    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Graph Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n        }\n        canvas {\n            border: 1px solid #ccc;\n            display: block;\n            margin: 20px auto;\n        }\n        button {\n            margin-top: 10px;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Graph Visualization</h1>\n<p>Select Graph Type:</p>\n<button onclick=\"drawGraph('undirected')\">Undirected Graph</button>\n<button onclick=\"drawGraph('directed')\">Directed Graph</button>\n\n<canvas id=\"graphCanvas\" width=\"600\" height=\"400\"></canvas>\n\n<script>\n    const canvas = document.getElementById('graphCanvas');\n    const ctx = canvas.getContext('2d');\n    \n    const nodes = [\n        { id: 1, x: 100, y: 200 },\n        { id: 2, x: 300, y: 100 },\n        { id: 3, x: 300, y: 300 },\n        { id: 4, x: 500, y: 200 }\n    ];\n    \n    const edges = [\n        { from: 1, to: 2 },\n        { from: 1, to: 3 },\n        { from: 2, to: 4 },\n        { from: 3, to: 4 }\n    ];\n    \n    function drawGraph(graphType) {\n        ctx.clearRect(0, 0, canvas.width, canvas.height);\n        \n        // Draw nodes\n        nodes.forEach(node => {\n            ctx.beginPath();\n            ctx.arc(node.x, node.y, 20, 0, Math.PI * 2);\n            ctx.fillStyle = '#f00';\n            ctx.fill();\n            ctx.fillStyle = '#000';\n            ctx.fillText(node.id, node.x-5, node.y+5);\n            ctx.closePath();\n        });\n\n        // Draw edges\n        edges.forEach(edge => {\n            const fromNode = nodes.find(n => n.id === edge.from);\n            const toNode = nodes.find(n => n.id === edge.to);\n\n            if (graphType === 'undirected') {\n                ctx.beginPath();\n                ctx.moveTo(fromNode.x, fromNode.y);\n                ctx.lineTo(toNode.x, toNode.y);\n                ctx.strokeStyle = '#000';\n                ctx.stroke();\n            } else if (graphType === 'directed') {\n                ctx.beginPath();\n                ctx.moveTo(fromNode.x, fromNode.y);\n                ctx.lineTo(toNode.x, toNode.y);\n                ctx.strokeStyle = '#000';\n                ctx.stroke();\n                \n                // Draw arrowhead\n                const headLength = 10; // Length of the arrowhead\n                const angle = Math.atan2(toNode.y - fromNode.y, toNode.x - fromNode.x);\n                ctx.beginPath();\n                ctx.moveTo(toNode.x, toNode.y);\n                ctx.lineTo(toNode.x - headLength * Math.cos(angle - Math.PI / 6), toNode.y - headLength * Math.sin(angle - Math.PI / 6));\n                ctx.lineTo(toNode.x - headLength * Math.cos(angle + Math.PI / 6), toNode.y - headLength * Math.sin(angle + Math.PI / 6));\n                ctx.lineTo(toNode.x, toNode.y);\n                ctx.fillStyle = '#000';\n                ctx.fill();\n                ctx.closePath();\n            }\n        });\n    }\n</script>\n\n</body>\n</html>"
