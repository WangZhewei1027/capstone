    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Adjacency Matrix Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n        }\n        #matrixContainer {\n            display: flex;\n            flex-direction: column;\n            margin-top: 20px;\n        }\n        .row {\n            display: flex;\n        }\n        .cell {\n            border: 1px solid #000;\n            padding: 10px;\n            width: 30px;\n            text-align: center;\n        }\n        input[type=\"number\"] {\n            width: 30px;\n            text-align: center;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Adjacency Matrix</h1>\n<label for=\"nodes\">Number of Nodes:</label>\n<input type=\"number\" id=\"nodes\" value=\"3\" min=\"1\" onchange=\"generateMatrix()\">\n<button onclick=\"generateMatrix()\">Generate Matrix</button>\n\n<div id=\"matrixContainer\"></div>\n\n<script>\n    function generateMatrix() {\n        const numNodes = parseInt(document.getElementById('nodes').value);\n        const matrixContainer = document.getElementById('matrixContainer');\n        matrixContainer.innerHTML = ''; // Clear previous matrix\n        \n        // Create matrix\n        for (let i = 0; i < numNodes; i++) {\n            const rowDiv = document.createElement('div');\n            rowDiv.className = 'row';\n            for (let j = 0; j < numNodes; j++) {\n                const cellInput = document.createElement('input');\n                cellInput.type = 'number';\n                cellInput.value = '0';\n                cellInput.id = `cell-${i}-${j}`;\n                cellInput.onchange = function() {\n                    if (this.value != '0' && this.value != '1') {\n                        this.value = '1'; // Only allow 0 or 1\n                    }\n                };\n                const cellDiv = document.createElement('div');\n                cellDiv.className = 'cell';\n                cellDiv.appendChild(cellInput);\n                rowDiv.appendChild(cellDiv);\n            }\n            matrixContainer.appendChild(rowDiv);\n        }\n    }\n</script>\n\n</body>\n</html>"
