    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Binary Search Tree Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            margin: 0;\n            height: 100vh;\n            justify-content: center;\n            background-color: #f4f4f4;\n        }\n        #bst {\n            margin: 20px 0;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n        }\n        .node {\n            border: 1px solid #333;\n            border-radius: 5px;\n            padding: 10px;\n            margin: 5px;\n            text-align: center;\n            background: #fff;\n            position: relative;\n        }\n        .edge {\n            position: absolute;\n            width: 2px;\n            background: #333;\n        }\n        #inputForm {\n            margin: 10px;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Binary Search Tree Visualization</h1>\n\n<div id=\"inputForm\">\n    <input type=\"number\" id=\"nodeValue\" placeholder=\"Enter a number\" />\n    <button onclick=\"insertNode()\">Insert Node</button>\n</div>\n\n<div id=\"bst\"></div>\n\n<script>\n    class Node {\n        constructor(value) {\n            this.value = value;\n            this.left = null;\n            this.right = null;\n        }\n    }\n\n    class BinarySearchTree {\n        constructor() {\n            this.root = null;\n        }\n\n        insert(value) {\n            const newNode = new Node(value);\n            if (!this.root) {\n                this.root = newNode;\n                return;\n            }\n            this.insertNode(this.root, newNode);\n        }\n\n        insertNode(node, newNode) {\n            if (newNode.value < node.value) {\n                if (!node.left) {\n                    node.left = newNode;\n                } else {\n                    this.insertNode(node.left, newNode);\n                }\n            } else {\n                if (!node.right) {\n                    node.right = newNode;\n                } else {\n                    this.insertNode(node.right, newNode);\n                }\n            }\n        }\n\n        display() {\n            const bstContainer = document.getElementById('bst');\n            bstContainer.innerHTML = '';\n            this.renderTree(this.root, bstContainer, 0, 200);\n        }\n\n        renderTree(node, container, depth, offset) {\n            if (!node) return;\n            const nodeDiv = document.createElement('div');\n            nodeDiv.className = 'node';\n            nodeDiv.innerText = node.value;\n            container.appendChild(nodeDiv);\n\n            if (node.left || node.right) {\n                const edgeLeft = document.createElement('div');\n                const edgeRight = document.createElement('div');\n                edgeLeft.className = 'edge';\n                edgeRight.className = 'edge';\n                edgeLeft.style.height = `${50}px`;\n                edgeRight.style.height = `${50}px`;\n                edgeLeft.style.left = `${-offset}px`;\n                edgeRight.style.left = `${offset}px`;\n                edgeLeft.style.top = `${70}px`;\n                edgeRight.style.top = `${70}px`;\n                container.appendChild(edgeLeft);\n                container.appendChild(edgeRight);\n\n                this.renderTree(node.left, container, depth + 1, offset / 2);\n                this.renderTree(node.right, container, depth + 1, offset / 2);\n            }\n        }\n    }\n\n    const bst = new BinarySearchTree();\n\n    function insertNode() {\n        const value = parseInt(document.getElementById('nodeValue').value);\n        if (!isNaN(value)) {\n            bst.insert(value);\n            bst.display();\n       