    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Priority Queue Demo</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n        }\n        #queue {\n            margin-top: 20px;\n            border: 1px solid #ccc;\n            padding: 10px;\n            min-height: 100px;\n        }\n        .item {\n            padding: 5px;\n            margin: 3px;\n            border-radius: 3px;\n            color: white;\n        }\n    </style>\n</head>\n<body>\n    <h1>Priority Queue Demo</h1>\n    <input type=\"text\" id=\"inputValue\" placeholder=\"Enter a value\">\n    <input type=\"number\" id=\"inputPriority\" placeholder=\"Enter priority (lower is higher)\">\n    <button onclick=\"enqueue()\">Enqueue</button>\n    <button onclick=\"dequeue()\">Dequeue</button>\n\n    <h3>Priority Queue:</h3>\n    <div id=\"queue\"></div>\n\n    <script>\n        class PriorityQueue {\n            constructor() {\n                this.items = [];\n            }\n\n            enqueue(element, priority) {\n                const queueElement = { element, priority };\n                let added = false;\n\n                for (let i = 0; i < this.items.length; i++) {\n                    if (queueElement.priority < this.items[i].priority) {\n                        this.items.splice(i, 0, queueElement);\n                        added = true;\n                        break;\n                    }\n                }\n\n                if (!added) {\n                    this.items.push(queueElement);\n                }\n            }\n\n            dequeue() {\n                return this.items.shift();\n            }\n\n            isEmpty() {\n                return this.items.length === 0;\n            }\n\n            printQueue() {\n                const queueDiv = document.getElementById(\"queue\");\n                queueDiv.innerHTML = \"\";\n                this.items.forEach(item => {\n                    const div = document.createElement(\"div\");\n                    div.className = \"item\";\n                    div.style.backgroundColor = `hsl(${Math.random() * 360}, 70%, 50%)`;\n                    div.innerText = `Value: ${item.element}, Priority: ${item.priority}`;\n                    queueDiv.appendChild(div);\n                });\n            }\n        }\n\n        const priorityQueue = new PriorityQueue();\n\n        function enqueue() {\n            const value = document.getElementById(\"inputValue\").value;\n            const priority = parseInt(document.getElementById(\"inputPriority\").value);\n            if (value && !isNaN(priority)) {\n                priorityQueue.enqueue(value, priority);\n                priorityQueue.printQueue();\n                document.getElementById(\"inputValue\").value = \"\";\n                document.getElementById(\"inputPriority\").value = \"\";\n            } else {\n                alert(\"Please enter valid value and priority.\");\n            }\n        }\n\n        function dequeue() {\n            if (!priorityQueue.isEmpty()) {\n                const dequeued = priorityQueue.dequeue();\n                alert(`Dequeued: ${dequeued.element} with priority ${dequeued.priority}`);\n                priorityQueue.printQueue();\n            } else {\n                alert(\"Queue is empty!\");\n            }\n        }\n    </script>\n</body>\n</html>"
