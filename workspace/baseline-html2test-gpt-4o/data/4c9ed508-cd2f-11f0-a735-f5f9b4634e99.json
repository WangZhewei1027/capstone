    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Prim's Algorithm Demo</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            min-height: 100vh;\n            margin: 0;\n            background-color: #f7f9fa;\n        }\n        #graph-container {\n            width: 600px;\n            height: 400px;\n            background-color: #ffffff;\n            border: 1px solid #cccccc;\n            position: relative;\n        }\n        .node {\n            width: 20px;\n            height: 20px;\n            background-color: #4299e1;\n            border-radius: 50%;\n            position: absolute;\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            color: white;\n            font-size: 12px;\n            cursor: pointer;\n        }\n        .edge {\n            position: absolute;\n            background-color: rgba(0, 0, 0, 0.1);\n            z-index: 0;\n        }\n        button {\n            margin-top: 20px;\n        }\n    </style>\n</head>\n<body>\n    <div id=\"graph-container\"></div>\n    <button id=\"run-algorithm\">Run Prim's Algorithm</button>\n    <script>\n        const graphContainer = document.getElementById('graph-container');\n        const nodes = [\n            { id: 'A', x: 50, y: 60 },\n            { id: 'B', x: 200, y: 100 },\n            { id: 'C', x: 400, y: 80 },\n            { id: 'D', x: 320, y: 280 },\n            { id: 'E', x: 150, y: 250 }\n        ];\n        \n        const edges = [\n            { from: 'A', to: 'B', weight: 4 },\n            { from: 'A', to: 'E', weight: 8 },\n            { from: 'B', to: 'C', weight: 8 },\n            { from: 'B', to: 'D', weight: 5 },\n            { from: 'C', to: 'D', weight: 5 },\n            { from: 'D', to: 'E', weight: 6 }\n        ];\n\n        function drawGraph() {\n            edges.forEach(edge => {\n                const nodeFrom = nodes.find(node => node.id === edge.from);\n                const nodeTo = nodes.find(node => node.id === edge.to);\n                const edgeElement = document.createElement('div');\n                edgeElement.className = 'edge';\n                const length = Math.sqrt(Math.pow(nodeTo.x - nodeFrom.x, 2) + Math.pow(nodeTo.y - nodeFrom.y, 2));\n                const angle = Math.atan2(nodeTo.y - nodeFrom.y, nodeTo.x - nodeFrom.x) * (180 / Math.PI);\n                edgeElement.style.width = `${length}px`;\n                edgeElement.style.height = '2px';\n                edgeElement.style.transform = `translate(${nodeFrom.x + 10}px, ${nodeFrom.y + 10}px) rotate(${angle}deg)`;\n                graphContainer.appendChild(edgeElement);\n            });\n\n            nodes.forEach(node => {\n                const nodeElement = document.createElement('div');\n                nodeElement.className = 'node';\n                nodeElement.textContent = node.id;\n                nodeElement.style.left = `${node.x}px`;\n                nodeElement.style.top = `${node.y}px`;\n                graphContainer.appendChild(nodeElement);\n            });\n        }\n\n        function primsAlgorithm() {\n            const mstSet = new Set();\n            const mstEdges = [];\n            mstSet.add(nodes[0].id);\n\n            while (mstSet.size < nodes.length) {\n                let minEdge = null;\n                for (let edge of edges) {\n                    if (mstSet.has(edge.from) && !mstSet.has(edge.to)) {\n                        if (!minEdge || edge.weight < minEdge.weight) {\n                            minEdge = edge;\n                        }\n                    } else if (!mstSet.has(edge.from) && mstSet.has(edge.to)) {\n                        if (!minEdge || edge.weight < minEdge.weight) {\n                            minEdge = edge;\n                        }\n                    }\n                }\n                mstSet.add(minEdge.from);\n                mstSet.add(minEdge.to);\n                mstEdges.push(minEdge);\n            }\n\n            mstEdges.forEach(edge => {\n                const edgeElements = Array.from(document.getElementsByClassName('edge'));\n                edgeElements.forEach(edgeElement => {\n                    const nodeFrom = nodes.find(node => node.id === edge.from);\n                    const nodeTo = nodes.find(node => node.id === edge.to);\n                    const length = Math.sqrt(Math.pow(nodeTo.x - nodeFrom.x, 2) + Math.pow(nodeTo.y - nodeFrom.y, 2)).toFixed();\n                    const style = window.getComputedStyle(edgeElement);\n                    if (parseFloat(style.width).toFixed() === length && style.transform.includes(`rotate(${Math.atan2(nodeTo.y - nodeFrom.y, nodeTo.x - nodeFrom.x) * (180 / Math.PI)}deg)`)) {\n                        edgeElement.style.backgroundColor = '#68d391'; // Light green color\n                    }\n                });\n            });\n        }\n\n        document.getElementById('run-algorithm').addEventListener('click', primsAlgorithm);\n        drawGraph();\n    </script>\n</body>\n</html>"
