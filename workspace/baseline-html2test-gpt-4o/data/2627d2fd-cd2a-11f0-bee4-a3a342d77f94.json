    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>K-Means Clustering Visualization</title>\n    <style>\n        body {\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            font-family: Arial, sans-serif;\n            margin: 0;\n            padding: 0;\n        }\n        canvas {\n            border: 1px solid #000;\n            margin-top: 20px;\n        }\n    </style>\n</head>\n<body>\n    <h1>K-Means Clustering Demo</h1>\n    <canvas id=\"canvas\" width=\"600\" height=\"600\"></canvas>\n    <button onclick=\"runKMeans()\">Run K-Means</button>\n\n    <script>\n        const canvas = document.getElementById('canvas');\n        const ctx = canvas.getContext('2d');\n\n        // Generate random points\n        const points = Array.from({length: 100}, () => ({\n            x: Math.floor(Math.random() * canvas.width),\n            y: Math.floor(Math.random() * canvas.height)\n        }));\n\n        // Number of clusters\n        const K = 3;\n        let centroids = Array.from({length: K}, () => ({\n            x: Math.floor(Math.random() * canvas.width),\n            y: Math.floor(Math.random() * canvas.height)\n        }));\n\n        const colors = ['red', 'green', 'blue'];\n\n        function drawPoints() {\n            points.forEach(point => {\n                ctx.fillStyle = 'gray';\n                ctx.fillRect(point.x, point.y, 5, 5);\n            });\n        }\n\n        function drawCentroids() {\n            centroids.forEach((centroid, i) => {\n                ctx.beginPath();\n                ctx.arc(centroid.x, centroid.y, 5, 0, Math.PI * 2, false);\n                ctx.fillStyle = colors[i];\n                ctx.fill();\n                ctx.stroke();\n            });\n        }\n\n        function distance(a, b) {\n            return Math.sqrt(Math.pow(a.x - b.x, 2) + Math.pow(a.y - b.y, 2));\n        }\n\n        function assignClusters() {\n            return points.map(point => {\n                let minDistance = Infinity;\n                let assignedCluster = -1;\n                centroids.forEach((centroid, i) => {\n                    const dist = distance(point, centroid);\n                    if (dist < minDistance) {\n                        minDistance = dist;\n                        assignedCluster = i;\n                    }\n                });\n                return assignedCluster;\n            });\n        }\n\n        function updateCentroids(clusters) {\n            for (let i = 0; i < K; i++) {\n                const clusterPoints = points.filter((_, index) => clusters[index] === i);\n                if (clusterPoints.length > 0) {\n                    const avgX = clusterPoints.reduce((sum, p) => sum + p.x, 0) / clusterPoints.length;\n                    const avgY = clusterPoints.reduce((sum, p) => sum + p.y, 0) / clusterPoints.length;\n                    centroids[i] = { x: avgX, y: avgY };\n                }\n            }\n        }\n\n        function runKMeans() {\n            let clusters;\n            for (let iteration = 0; iteration < 10; iteration++) {\n                ctx.clearRect(0, 0, canvas.width, canvas.height);\n                drawPoints();\n                clusters = assignClusters();\n                updateCentroids(clusters);\n                drawCentroids();\n\n                clusters.forEach((cluster, index) => {\n                    ctx.fillStyle = colors[cluster];\n                    ctx.fillRect(points[index].x, points[index].y, 5, 5);\n                });\n            }\n        }\n\n        drawPoints();\n        drawCentroids();\n    </script>\n</body>\n</html>"
