    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Heap (Min/Max) Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            padding: 20px;\n        }\n        input {\n            margin: 10px;\n            padding: 5px;\n            font-size: 16px;\n        }\n        button {\n            padding: 10px 20px;\n            font-size: 16px;\n            margin: 5px;\n        }\n        .heap-container {\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            margin-top: 30px;\n        }\n        .heap-row {\n            display: flex;\n            justify-content: center;\n            width: 100%;\n        }\n        .heap-node {\n            border: 2px solid #4CAF50;\n            border-radius: 50%;\n            width: 50px;\n            height: 50px;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            margin: 5px;\n            font-weight: bold;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Heap (Min/Max) Visualization</h1>\n\n<div>\n    <input type=\"number\" id=\"heap-value\" placeholder=\"Enter a number\">\n    <button onclick=\"insertMinHeap(parseInt(document.getElementById('heap-value').value))\">Insert Min-Heap</button>\n    <button onclick=\"insertMaxHeap(parseInt(document.getElementById('heap-value').value))\">Insert Max-Heap</button>\n</div>\n\n<div class=\"heap-container\" id=\"min-heap-container\">\n    <h2>Min-Heap</h2>\n    <div id=\"min-heap\"></div>\n</div>\n\n<div class=\"heap-container\" id=\"max-heap-container\">\n    <h2>Max-Heap</h2>\n    <div id=\"max-heap\"></div>\n</div>\n\n<script>\n    class MinHeap {\n        constructor() {\n            this.heap = [];\n        }\n\n        insert(value) {\n            this.heap.push(value);\n            this.bubbleUp(this.heap.length - 1);\n            this.display('min-heap');\n        }\n\n        bubbleUp(index) {\n            while (index > 0) {\n                let parentIndex = Math.floor((index - 1) / 2);\n                if (this.heap[parentIndex] <= this.heap[index]) break;\n                [this.heap[parentIndex], this.heap[index]] = [this.heap[index], this.heap[parentIndex]];\n                index = parentIndex;\n            }\n        }\n\n        display(containerId) {\n            const container = document.getElementById(containerId);\n            container.innerHTML = '';\n            this.heap.forEach((value, index) => {\n                const node = document.createElement('div');\n                node.className = 'heap-node';\n                node.innerText = value;\n                container.appendChild(node);\n            });\n        }\n    }\n\n    class MaxHeap {\n        constructor() {\n            this.heap = [];\n        }\n\n        insert(value) {\n            this.heap.push(value);\n            this.bubbleUp(this.heap.length - 1);\n            this.display('max-heap');\n        }\n\n        bubbleUp(index) {\n            while (index > 0) {\n                let parentIndex = Math.floor((index - 1) / 2);\n                if (this.heap[parentIndex] >= this.heap[index]) break;\n                [this.heap[parentIndex], this.heap[index]] = [this.heap[index], this.heap[parentIndex]];\n                index = parentIndex;\n            }\n        }\n\n        display(containerId) {\n            const container = document.getElementById(containerId);\n            container.innerHTML = '';\n            this.heap.forEach((value, index) => {\n                const node = document.createElement('div');\n                node.className = 'heap-node';\n                node.innerText = value;\n                container.appendChild(node);\n            });\n        }\n    }\n\n    const minHeap = new MinHeap();\n    const maxHeap = new MaxHeap();\n\n    function insertMinHeap(value) {\n        if (!isNaN(value)) {\n            minHeap.insert(value);\n        }\n    }\n\n    function insertMaxHeap(value) {\n        if (!isNaN(value)) {\n            maxHeap.insert(value);\n        }\n    }\n</script>\n\n</body>\n</html>"
