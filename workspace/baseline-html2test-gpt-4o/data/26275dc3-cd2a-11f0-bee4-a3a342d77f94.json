    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Binary Search Tree Demo</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            text-align: center;\n        }\n        #inputSection {\n            margin: 20px;\n        }\n        #treeContainer {\n            width: 80%;\n            margin: auto;\n        }\n        .node {\n            display: inline-block;\n            margin: 10px;\n            padding: 8px;\n            border: 2px solid #4CAF50;\n            border-radius: 50%;\n        }\n        .line {\n            width: 100%;\n            border-top: 2px solid #4CAF50;\n            margin-top: -20px;\n            position: relative;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Binary Search Tree Visualization</h1>\n<div id=\"inputSection\">\n    <input type=\"text\" id=\"bstInput\" placeholder=\"Enter values comma separated\" />\n    <button onclick=\"createBST()\">Create BST</button>\n    <button onclick=\"clearBST()\">Clear</button>\n</div>\n<div id=\"treeContainer\"></div>\n\n<script>\n    class Node {\n        constructor(data) {\n            this.data = data;\n            this.left = null;\n            this.right = null;\n        }\n    }\n\n    class BST {\n        constructor() {\n            this.root = null;\n        }\n\n        insert(data) {\n            const newNode = new Node(data);\n            if (this.root === null) {\n                this.root = newNode;\n            } else {\n                this.insertNode(this.root, newNode);\n            }\n        }\n\n        insertNode(node, newNode) {\n            if (newNode.data < node.data) {\n                if (node.left === null) {\n                    node.left = newNode;\n                } else {\n                    this.insertNode(node.left, newNode);\n                }\n            } else {\n                if (node.right === null) {\n                    node.right = newNode;\n                } else {\n                    this.insertNode(node.right, newNode);\n                }\n            }\n        }\n\n        visualize(node = this.root, depth = 0, output = []) {\n            if (!node) return output;\n            if (!output[depth]) output[depth] = [];\n            output[depth].push(node.data);\n            this.visualize(node.left, depth + 1, output);\n            this.visualize(node.right, depth + 1, output);\n            return output;\n        }\n    }\n\n    function createBST() {\n        const inputValues = document.getElementById('bstInput').value.split(',').map(Number);\n        const bst = new BST();\n        inputValues.forEach(value => bst.insert(value));\n        displayBST(bst.visualize());\n    }\n\n    function clearBST() {\n        document.getElementById('treeContainer').innerHTML = '';\n        document.getElementById('bstInput').value = '';\n    }\n\n    function displayBST(levels) {\n        const container = document.getElementById('treeContainer');\n        container.innerHTML = '';\n        levels.forEach((level, index) => {\n            const levelContainer = document.createElement('div');\n            level.forEach(node => {\n                const nodeDiv = document.createElement('div');\n                nodeDiv.classList.add('node');\n                nodeDiv.innerText = node;\n                levelContainer.appendChild(nodeDiv);\n            });\n            container.appendChild(levelContainer);\n            if (index < levels.length - 1) {\n                const lineDiv = document.createElement('div');\n                lineDiv.classList.add('line');\n                container.appendChild(lineDiv);\n            }\n        });\n    }\n</script>\n\n</body>\n</html>"
