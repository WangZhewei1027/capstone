    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Kruskal's Algorithm Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            height: 100vh;\n            margin: 0;\n            background-color: #f4f4f9;\n        }\n        .graph-container {\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            width: 470px;\n        }\n        .edge-list {\n            margin-top: 20px;\n            text-align: left;\n        }\n        .button-container {\n            margin-top: 20px;\n        }\n        .graph-visualization {\n            border: 1px solid #ccc;\n            width: 400px;\n            height: 400px;\n            position: relative;\n            background-color: #fafafa;\n            overflow: hidden;\n        }\n        .node {\n            position: absolute;\n            width: 30px;\n            height: 30px;\n            background-color: #3498db;\n            border-radius: 50%;\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            color: white;\n        }\n        .edge {\n            position: absolute;\n            background-color: #95a5a6;\n            height: 2px;\n            transform-origin: 0 0;\n        }\n        .edge.highlight {\n            background-color: #e74c3c;\n        }\n    </style>\n</head>\n<body>\n\n<div class=\"graph-container\">\n    <div id=\"graph\" class=\"graph-visualization\"></div>\n    <div class=\"edge-list\" id=\"edgeList\"></div>\n    <div class=\"button-container\">\n        <button onclick=\"runKruskal()\">Run Kruskal's Algorithm</button>\n    </div>\n</div>\n\n<script>\n    const nodes = [\n        { id: 0, x: 50, y: 50 },\n        { id: 1, x: 350, y: 50 },\n        { id: 2, x: 350, y: 350 },\n        { id: 3, x: 50, y: 350 },\n        { id: 4, x: 200, y: 200 }\n    ];\n\n    const edges = [\n        { u: 0, v: 1, weight: 10 },\n        { u: 0, v: 3, weight: 15 },\n        { u: 0, v: 4, weight: 5 },\n        { u: 1, v: 2, weight: 7 },\n        { u: 1, v: 4, weight: 6 },\n        { u: 2, v: 3, weight: 8 },\n        { u: 2, v: 4, weight: 10 },\n        { u: 3, v: 4, weight: 3 }\n    ];\n\n    const graphContainer = document.getElementById('graph');\n    const edgeListElem = document.getElementById('edgeList');\n\n    function createNodeElement(node) {\n        const div = document.createElement('div');\n        div.classList.add('node');\n        div.style.left = node.x + 'px';\n        div.style.top = node.y + 'px';\n        div.innerText = node.id;\n        return div;\n    }\n\n    function createEdgeElement(edge, nodes) {\n        const nodeA = nodes[edge.u];\n        const nodeB = nodes[edge.v];\n        const length = Math.sqrt(Math.pow(nodeB.x - nodeA.x, 2) + Math.pow(nodeB.y - nodeA.y, 2));\n        const angle = Math.atan2(nodeB.y - nodeA.y, nodeB.x - nodeA.x) * 180 / Math.PI;\n\n        const div = document.createElement('div');\n        div.classList.add('edge');\n        div.style.width = length + 'px';\n        div.style.left = nodeA.x + 15 + 'px';\n        div.style.top = nodeA.y + 15 + 'px';\n        div.style.transform = `rotate(${angle}deg)`;\n        return div;\n    }\n\n    function drawGraph() {\n        nodes.forEach(node => {\n            graphContainer.appendChild(createNodeElement(node));\n        });\n\n        edges.forEach(edge => {\n            graphContainer.appendChild(createEdgeElement(edge, nodes));\n        });\n    }\n\n    function updateEdgeList(edgeList) {\n        edgeListElem.innerHTML = '<strong>Edges (u, v, weight):</strong><br>' + edgeList.map(edge => `(${edge.u}, ${edge.v}, ${edge.weight})`).join('<br>');\n    }\n\n    function find(parent, i) {\n        if (parent[i] === i) return i;\n        return find(parent, parent[i]);\n    }\n\n    function union(parent, rank, x, y) {\n        const xroot = find(parent, x);\n        const yroot = find(parent, y);\n\n        if (rank[xroot] < rank[yroot]) {\n            parent[xroot] = yroot;\n        } else if (rank[xroot] > rank[yroot]) {\n            parent[yroot] = xroot;\n        } else {\n            parent[yroot] = xroot;\n            rank[xroot]++;\n        }\n    }\n\n    function runKruskal() {\n        const sortedEdges = [...edges].sort((a, b) => a.weight - b.weight);\n        const parent = [];\n        const rank = [];\n        const result = [];\n\n        for (let i = 0; i < nodes.length; i++) {\n            parent[i] = i;\n            rank[i] = 0;\n        }\n\n        let e = 0;\n        let i = 0;\n        while (e < nodes.length - 1) {\n            const { u, v, weight } = sortedEdges[i++];\n            const x = find(parent, u);\n            const y = find(parent, v);\n\n            if (x !== y) {\n                result.push({ u, v, weight });\n                union(parent, rank, x, y);\n                e++;\n            }\n        }\n\n        document.querySelectorAll('.edge').forEach(edge => edge.classList.remove('highlight'));\n        result.forEach(edge => {\n            const edgeElem = [...document.querySelectorAll('.edge')].find(domEdge => {\n                const [start, end] = domEdge.style.transform.match(/-?\\d+(\\.\\d+)?/g).map(Number);\n                return (start === nodes[edge.u].x + 15 && end === nodes[edge.u].y + 15) || (start === nodes[edge.v].x + 15 && end === nodes[edge.v].y + 15);\n            });\n            edgeElem.classList.add('highlight');\n        });\n        updateEdgeList(result);\n    }\n\n    drawGraph();\n    updateEdgeList(edges);\n\n</script>\n\n</body>\n</html>"
