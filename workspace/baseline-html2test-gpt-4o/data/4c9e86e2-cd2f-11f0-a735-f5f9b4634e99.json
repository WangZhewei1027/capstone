    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Binary Tree Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            height: 100vh;\n            margin: 0;\n            background-color: #f4f4f9;\n        }\n        .tree {\n            white-space: nowrap;\n        }\n        .node {\n            display: inline-block;\n            text-align: center;\n            padding: 10px;\n            border: 1px solid #333;\n            border-radius: 50%;\n            background-color: #61dafb;\n            box-shadow: 2px 2px 10px rgba(0,0,0,0.1);\n            margin: 10px;\n        }\n        .level {\n            display: flex;\n            justify-content: center;\n            margin-top: 10px;\n        }\n        .link {\n            text-align: center;\n            margin-bottom: -10px;\n        }\n        .link:before, .link:after {\n            content: '';\n            display: inline-block;\n            width: 20px;\n            height: 20px;\n            border-bottom: 1px solid #333;\n            margin: 0 -3px;\n        }\n        .link:before {\n            border-left: 1px solid #333;\n            border-bottom-left-radius: 10px;\n        }\n        .link:after {\n            border-right: 1px solid #333;\n            border-bottom-right-radius: 10px;\n        }\n    </style>\n</head>\n<body>\n    <div class=\"tree\" id=\"tree\">\n        <!-- Binary Tree will be generated here -->\n    </div>\n    <script>\n        class TreeNode {\n            constructor(value) {\n                this.value = value;\n                this.left = null;\n                this.right = null;\n            }\n        }\n\n        class BinaryTree {\n            constructor() {\n                this.root = null;\n            }\n\n            insert(value) {\n                const newNode = new TreeNode(value);\n                if (this.root === null) {\n                    this.root = newNode;\n                } else {\n                    this.insertNode(this.root, newNode);\n                }\n            }\n\n            insertNode(node, newNode) {\n                if (newNode.value < node.value) {\n                    if (node.left === null) {\n                        node.left = newNode;\n                    } else {\n                        this.insertNode(node.left, newNode);\n                    }\n                } else {\n                    if (node.right === null) {\n                        node.right = newNode;\n                    } else {\n                        this.insertNode(node.right, newNode);\n                    }\n                }\n            }\n\n            traverse() {\n                if (!this.root) return;\n                this._displayTree([this.root], 0);\n            }\n\n            _displayTree(nodes, level) {\n                if (!nodes.length) return;\n                const nextNodes = [];\n                const levelDiv = document.createElement('div');\n                levelDiv.className = 'level';\n\n                nodes.forEach(node => {\n                    if (node) {\n                        const nodeDiv = document.createElement('div');\n                        nodeDiv.className = 'node';\n                        nodeDiv.innerText = node.value;\n                        levelDiv.appendChild(nodeDiv);\n                        nextNodes.push(node.left, node.right);\n\n                        if (node.left || node.right) {\n                            const linkDiv = document.createElement('div');\n                            linkDiv.className = 'link';\n                            levelDiv.appendChild(linkDiv);\n                        }\n                    } else {\n                        nextNodes.push(null, null);\n                        const placeholderDiv = document.createElement('div');\n                        placeholderDiv.innerHTML = '&nbsp;';\n                        placeholderDiv.style.width = '40px';\n                        levelDiv.appendChild(placeholderDiv);\n                    }\n                });\n\n                document.getElementById('tree').appendChild(levelDiv);\n                this._displayTree(nextNodes, level + 1);\n            }\n        }\n\n        const binaryTree = new BinaryTree();\n        const values = [15, 10, 20, 5, 13, 18, 25];\n        values.forEach(value => binaryTree.insert(value));\n        binaryTree.traverse();\n    </script>\n</body>\n</html>"
