    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Binary Tree Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 0;\n            padding: 20px;\n            background-color: #f9f9f9;\n        }\n        #tree-container {\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            flex-direction: column;\n        }\n        .tree-node {\n            text-align: center;\n            margin: 10px;\n            background-color: #6ca0dc;\n            color: white;\n            padding: 10px;\n            border-radius: 5px;\n        }\n        .tree-connector {\n            width: 2px;\n            height: 30px;\n            background-color: #6ca0dc;\n            margin: 0 auto;\n        }\n    </style>\n</head>\n<body>\n    <h1>Binary Tree Visualization</h1>\n    <div id=\"tree-container\"></div>\n\n    <script>\n        class TreeNode {\n            constructor(value) {\n                this.value = value;\n                this.left = null;\n                this.right = null;\n            }\n        }\n\n        class BinaryTree {\n            constructor() {\n                this.root = null;\n            }\n\n            insert(value) {\n                const newNode = new TreeNode(value);\n                if (this.root === null) {\n                    this.root = newNode;\n                } else {\n                    this.insertNode(this.root, newNode);\n                }\n            }\n\n            insertNode(node, newNode) {\n                if (newNode.value < node.value) {\n                    if (node.left === null) {\n                        node.left = newNode;\n                    } else {\n                        this.insertNode(node.left, newNode);\n                    }\n                } else {\n                    if (node.right === null) {\n                        node.right = newNode;\n                    } else {\n                        this.insertNode(node.right, newNode);\n                    }\n                }\n            }\n\n            visualize() {\n                const container = document.getElementById('tree-container');\n                container.innerHTML = '';\n                if (this.root) {\n                    this.visualizeNode(this.root, container);\n                }\n            }\n\n            visualizeNode(node, parentElement) {\n                const nodeElement = document.createElement('div');\n                nodeElement.className = 'tree-node';\n                nodeElement.innerText = node.value;\n                parentElement.appendChild(nodeElement);\n\n                const connector = document.createElement('div');\n                connector.className = 'tree-connector';\n                parentElement.appendChild(connector);\n\n                if (node.left || node.right) {\n                    const childContainer = document.createElement('div');\n                    childContainer.style.display = 'flex';\n                    childContainer.style.justifyContent = 'space-around';\n                    parentElement.appendChild(childContainer);\n\n                    if (node.left) {\n                        this.visualizeNode(node.left, childContainer);\n                    }\n                    if (node.right) {\n                        this.visualizeNode(node.right, childContainer);\n                    }\n                }\n            }\n        }\n\n        // Create binary tree and insert nodes\n        const tree = new BinaryTree();\n        const values = [8, 3, 10, 1, 6, 14, 4, 7, 13];\n        values.forEach(value => tree.insert(value));\n\n        // Visualize the binary tree\n        tree.visualize();\n    </script>\n</body>\n</html>"
