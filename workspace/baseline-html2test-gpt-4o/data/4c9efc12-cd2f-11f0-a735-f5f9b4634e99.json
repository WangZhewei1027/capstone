    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>K-Means Clustering Demo</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            height: 100vh;\n            background-color: #f0f0f0;\n        }\n        canvas {\n            border: 1px solid #ccc;\n            background-color: white;\n        }\n        #controls {\n            position: absolute;\n            top: 20px;\n            left: 50%;\n            transform: translateX(-50%);\n        }\n        #controls button {\n            margin: 5px;\n        }\n    </style>\n</head>\n<body>\n    <div id=\"controls\">\n        <button onclick=\"init()\">Initialize</button>\n        <button onclick=\"runKMeans()\">Run K-Means</button>\n    </div>\n    <canvas id=\"canvas\" width=\"600\" height=\"400\"></canvas>\n\n    <script>\n        const canvas = document.getElementById(\"canvas\");\n        const context = canvas.getContext(\"2d\");\n        const numPoints = 100;\n        const numClusters = 3;\n        const points = [];\n        const clusters = [];\n        const colors = [\"red\", \"blue\", \"green\"];\n\n        function randomPoint() {\n            return { x: Math.random() * canvas.width, y: Math.random() * canvas.height };\n        }\n\n        function drawPoint(point, color) {\n            context.fillStyle = color;\n            context.beginPath();\n            context.arc(point.x, point.y, 5, 0, Math.PI * 2);\n            context.fill();\n        }\n\n        function init() {\n            context.clearRect(0, 0, canvas.width, canvas.height);\n            points.length = 0;\n            clusters.length = 0;\n\n            for (let i = 0; i < numPoints; i++) {\n                const point = randomPoint();\n                points.push(point);\n                drawPoint(point, \"black\");\n            }\n\n            // Initialize clusters\n            for (let i = 0; i < numClusters; i++) {\n                const cluster = randomPoint();\n                clusters.push(cluster);\n            }\n        }\n\n        function assignPointsToClusters() {\n            points.forEach(point => {\n                let closestCluster = null;\n                let closestDistance = Number.MAX_VALUE;\n                clusters.forEach((cluster, index) => {\n                    const distance = Math.hypot(point.x - cluster.x, point.y - cluster.y);\n                    if (distance < closestDistance) {\n                        closestDistance = distance;\n                        closestCluster = index;\n                    }\n                });\n                point.cluster = closestCluster;\n            });\n        }\n\n        function updateClusters() {\n            clusters.forEach((cluster, clusterIndex) => {\n                const assignedPoints = points.filter(point => point.cluster === clusterIndex);\n                if (assignedPoints.length === 0) return;\n                const avgX = assignedPoints.reduce((sum, point) => sum + point.x, 0) / assignedPoints.length;\n                const avgY = assignedPoints.reduce((sum, point) => sum + point.y, 0) / assignedPoints.length;\n                cluster.x = avgX;\n                cluster.y = avgY;\n            });\n        }\n\n        function render() {\n            context.clearRect(0, 0, canvas.width, canvas.height);\n            points.forEach(point => {\n                drawPoint(point, colors[point.cluster]);\n            });\n        }\n\n        function runKMeans() {\n            assignPointsToClusters();\n            updateClusters();\n            render();\n        }\n\n        window.onload = init;\n    </script>\n</body>\n</html>"
  },
