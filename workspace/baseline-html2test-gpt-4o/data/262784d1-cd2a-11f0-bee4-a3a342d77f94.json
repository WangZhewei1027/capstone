is.edges.forEach(edge => {\n                    const { node1, node2, directed } = edge;\n                    const x1 = node1.x + 10;\n                    const y1 = node1.y + 10;\n                    const x2 = node2.x + 10;\n                    const y2 = node2.y + 10;\n                    \n                    const length = Math.sqrt((x2 - x1) ** 2 + (y2 - y1) ** 2);\n                    \n                    const angle = Math.atan2(y2 - y1, x2 - x1) * (180 / Math.PI);\n                    \n                    const edgeDiv = document.createElement('div');\n                    edgeDiv.className = 'edge';\n                    edgeDiv.style.width = length + 'px';\n                    edgeDiv.style.left = x1 + 'px';\n                    edgeDiv.style.top = y1 + 'px';\n                    edgeDiv.style.transform = `rotate(${angle}deg)`;\n                    \n                    if(directed) {\n                        edgeDiv.style.borderRight = '6px solid transparent';\n                        edgeDiv.style.borderLeft = '6px solid transparent';\n                        edgeDiv.style.borderTop = '10px solid #000';\n                        edgeDiv.style.height = '0';\n                    }\n                    \n                    container.appendChild(edgeDiv);\n                });\n            }\n        }\n\n        class Node {\n            constructor(x, y, label) {\n                this.x = x;\n                this.y = y;\n                this.label = label;\n            }\n        }\n\n        // Create a new graph\n        const graph = new Graph();\n\n        // Add nodes\n        const nodeA = new Node(100, 100, 'A');\n        const nodeB = new Node(300, 100, 'B');\n        const nodeC = new Node(200, 300, 'C');\n        graph.addNode(nodeA);\n        graph.addNode(nodeB);\n        graph.addNode(nodeC);\n\n        // Add edges (True for directed, False for undirected)\n        graph.addEdge(nodeA, nodeB, true);  // Directed edge A -> B\n        graph.ad