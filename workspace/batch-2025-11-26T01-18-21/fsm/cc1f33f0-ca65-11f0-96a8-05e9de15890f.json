{
  "meta": {
    "concept": "KNN",
    "topic": "K-Nearest Neighbors (KNN) Interactive Demo",
    "educational_goal": "Interactive exploration of K selection, distance metrics, weighting, decision boundary, prediction and basic evaluation",
    "expected_interactions": [
      "add_point",
      "drag_point",
      "predict",
      "toggle_boundary",
      "generate_dataset",
      "clear_points",
      "k_change",
      "auto_k_toggle",
      "change_distance_metric",
      "change_weighting",
      "change_mode",
      "k_fold_eval",
      "confusion_matrix",
      "export_dataset",
      "normalize",
      "shuffle_labels",
      "add_noise"
    ]
  },
  "states": [
    {
      "id": "S0_Idle",
      "label": "idle",
      "type": "idle",
      "entry_actions": [
        "draw()",
        "updateLegend()",
        "kVal.textContent = kRange.value"
      ],
      "exit_actions": []
    },
    {
      "id": "S1_AddingPoint",
      "label": "adding_point",
      "type": "atomic",
      "entry_actions": [
        "toData(clickX,clickY)",
        "points.push(newPoint)",
        "lastPrediction = null",
        "nearestNeighbors = null",
        "lastPred.textContent = 'No predictions yet.'",
        "draw()"
      ],
      "exit_actions": []
    },
    {
      "id": "S2_Predicting",
      "label": "predicting_on_click",
      "type": "atomic",
      "entry_actions": [
        "pred = knnPredict(queryPoint, null, true)",
        "nearestNeighbors = pred.neighbors",
        "lastPrediction = pred",
        "lastPred.textContent = JSON.stringify(pred, null, 2)",
        "draw()"
      ],
      "exit_actions": [
        "nearestNeighbors = null"
      ]
    },
    {
      "id": "S3_Dragging",
      "label": "dragging_point",
      "type": "atomic",
      "entry_actions": [
        "startDrag(idx, offsetX, offsetY)",
        "highlightDraggedPoint(idx)",
        "draw()"
      ],
      "exit_actions": [
        "dragging = null",
        "draw()"
      ]
    },
    {
      "id": "S4_BoundaryVisible",
      "label": "boundary_visible",
      "type": "atomic",
      "entry_actions": [
        "showBoundary = true",
        "btnToggleBoundary.textContent = 'Hide decision boundary'",
        "draw()"
      ],
      "exit_actions": [
        "showBoundary = false",
        "btnToggleBoundary.textContent = 'Toggle decision boundary'",
        "draw()"
      ]
    },
    {
      "id": "S5_PredictModeOn",
      "label": "predict_mode_on",
      "type": "atomic",
      "entry_actions": [
        "predictOnClick = true",
        "btnPredictMode.textContent = 'Predict mode: ON'"
      ],
      "exit_actions": [
        "predictOnClick = false",
        "btnPredictMode.textContent = 'Predict on click'",
        "nearestNeighbors = null",
        "lastPrediction = null",
        "lastPred.textContent = 'No predictions yet.'"
      ]
    },
    {
      "id": "S6_Generating",
      "label": "generating_dataset",
      "type": "atomic",
      "entry_actions": [
        "generateClusters()",
        "lastPrediction = null",
        "nearestNeighbors = null",
        "lastPred.textContent = 'No predictions yet.'",
        "draw()"
      ],
      "exit_actions": []
    },
    {
      "id": "S7_Clearing",
      "label": "clearing_points",
      "type": "atomic",
      "entry_actions": [
        "points = []",
        "lastPrediction = null",
        "nearestNeighbors = null",
        "lastPred.textContent = 'No predictions yet.'",
        "draw()"
      ],
      "exit_actions": []
    },
    {
      "id": "S8_EvaluatingCV",
      "label": "evaluating_cross_validation",
      "type": "atomic",
      "entry_actions": [
        "perform k-fold evaluation (btnCV onclick handler)",
        "alert results"
      ],
      "exit_actions": []
    },
    {
      "id": "S9_ShowingConfusion",
      "label": "showing_confusion_matrix",
      "type": "atomic",
      "entry_actions": [
        "compute confusion matrix (btnConf handler)",
        "alert matrix"
      ],
      "exit_actions": []
    },
    {
      "id": "S10_Exporting",
      "label": "exporting_dataset",
      "type": "atomic",
      "entry_actions": [
        "data = JSON.stringify(points, null, 2)",
        "open new window and write data (btnExport handler)"
      ],
      "exit_actions": []
    },
    {
      "id": "S11_AddNoise",
      "label": "adding_noise",
      "type": "atomic",
      "entry_actions": [
        "for each p: p.x += noise; p.y += noise",
        "draw()"
      ],
      "exit_actions": []
    },
    {
      "id": "S12_Normalize",
      "label": "normalizing_points",
      "type": "atomic",
      "entry_actions": [
        "normalize coordinates to min/max",
        "draw()"
      ],
      "exit_actions": []
    },
    {
      "id": "S13_ShuffleLabels",
      "label": "shuffling_labels",
      "type": "atomic",
      "entry_actions": [
        "for each p: p.label = random label",
        "draw()"
      ],
      "exit_actions": []
    },
    {
      "id": "S14_Hover",
      "label": "hover_point",
      "type": "atomic",
      "entry_actions": [
        "hover = {idx}",
        "draw()"
      ],
      "exit_actions": [
        "hover = null",
        "draw()"
      ]
    },
    {
      "id": "S15_UpdatingParams",
      "label": "updating_parameters",
      "type": "atomic",
      "entry_actions": [
        "update param (k, autoK, dist, weighting, mode, nclasses, pointsPerClass, std, seed)",
        "if autoK then parseK()",
        "draw()"
      ],
      "exit_actions": []
    }
  ],
  "events": [
    {
      "id": "USER_MOUSEDOWN_ON_CANVAS",
      "event_type": "user_action",
      "description": "mousedown event on the canvas (canvas#grid)"
    },
    {
      "id": "USER_MOUSEMOVE",
      "event_type": "user_action",
      "description": "mousemove over canvas/window (used for dragging and hover)"
    },
    {
      "id": "USER_MOUSEUP",
      "event_type": "user_action",
      "description": "mouseup event (window/body)"
    },
    {
      "id": "USER_CLICK_BTN_GENERATE",
      "event_type": "user_action",
      "description": "User clicks Generate random dataset button (#btnGenerate)"
    },
    {
      "id": "USER_CLICK_BTN_CLEAR",
      "event_type": "user_action",
      "description": "User clicks Clear points button (#btnClear)"
    },
    {
      "id": "USER_CLICK_TOGGLE_BOUNDARY",
      "event_type": "user_action",
      "description": "User clicks Toggle decision boundary button (#btnToggleBoundary) or presses 'b'"
    },
    {
      "id": "USER_CLICK_PREDICT_MODE",
      "event_type": "user_action",
      "description": "User clicks Predict on click button (#btnPredictMode) or presses 'p'"
    },
    {
      "id": "USER_INPUT_K",
      "event_type": "user_action",
      "description": "User changes K range input (#k)"
    },
    {
      "id": "USER_CHANGE_AUTOK",
      "event_type": "user_action",
      "description": "User toggles auto K checkbox (#autoK)"
    },
    {
      "id": "USER_CHANGE_DIST",
      "event_type": "user_action",
      "description": "User changes distance metric select (#dist)"
    },
    {
      "id": "USER_CHANGE_WEIGHTING",
      "event_type": "user_action",
      "description": "User changes weighting select (#weighting)"
    },
    {
      "id": "USER_CHANGE_MODE",
      "event_type": "user_action",
      "description": "User changes Mode select (#mode)"
    },
    {
      "id": "USER_CHANGE_NCLASSES",
      "event_type": "user_action",
      "description": "User changes Number of classes select (#nclasses)"
    },
    {
      "id": "USER_CLICK_CV",
      "event_type": "user_action",
      "description": "User clicks K-fold eval button (#btnCV)"
    },
    {
      "id": "USER_CLICK_CONF",
      "event_type": "user_action",
      "description": "User clicks Show confusion matrix button (#btnConf)"
    },
    {
      "id": "USER_CLICK_EXPORT",
      "event_type": "user_action",
      "description": "User clicks Export dataset button (#btnExport)"
    },
    {
      "id": "USER_CLICK_ADD_NOISE",
      "event_type": "user_action",
      "description": "User clicks Add small noise button (#btnAddNoise)"
    },
    {
      "id": "USER_CLICK_NORMALIZE",
      "event_type": "user_action",
      "description": "User clicks Normalize coords button (#btnNormalize)"
    },
    {
      "id": "USER_CLICK_SHUFFLE",
      "event_type": "user_action",
      "description": "User clicks Shuffle labels button (#btnShuffle)"
    },
    {
      "id": "SYSTEM_GENERATE_COMPLETE",
      "event_type": "system_event",
      "description": "Dataset generation completed (generateClusters finished)"
    },
    {
      "id": "SYSTEM_CLEAR_COMPLETE",
      "event_type": "system_event",
      "description": "Clear operation completed"
    },
    {
      "id": "SYSTEM_CV_COMPLETE",
      "event_type": "system_event",
      "description": "Cross-validation evaluation completed"
    },
    {
      "id": "SYSTEM_CONF_COMPLETE",
      "event_type": "system_event",
      "description": "Confusion matrix computed and shown"
    },
    {
      "id": "SYSTEM_EXPORT_COMPLETE",
      "event_type": "system_event",
      "description": "Export operation completed (new window opened)"
    },
    {
      "id": "USER_KEYDOWN_B",
      "event_type": "user_action",
      "description": "User presses 'b' key to toggle boundary (keydown on window/body)"
    },
    {
      "id": "USER_KEYDOWN_P",
      "event_type": "user_action",
      "description": "User presses 'p' key to toggle predict mode (keydown on window/body)"
    },
    {
      "id": "USER_LESS_THAN_MIN_POINTS",
      "event_type": "system_event",
      "description": "Guard condition: too few points to evaluate (alerts shown by handlers)"
    }
  ],
  "transitions": [
    {
      "from": "S0_Idle",
      "to": "S1_AddingPoint",
      "event": "USER_MOUSEDOWN_ON_CANVAS",
      "guard": "predictOnClick === false && clickedOnExistingPoint === false",
      "actions": [
        "d = toData(cx,cy)",
        "points.push({x:d.x,y:d.y,label:selectedClass, val: (modeSelect.value==='regress'?computedVal:undefined)})",
        "lastPrediction = null",
        "nearestNeighbors = null",
        "lastPred.textContent = 'No predictions yet.'",
        "draw()"
      ],
      "triggers": "canvas#grid",
      "expected_observables": [
        "dom:canvas#grid mousedown"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S3_Dragging",
      "event": "USER_MOUSEDOWN_ON_CANVAS",
      "guard": "clickedOnExistingPoint === true",
      "actions": [
        "dragging = {idx:hitIdx, offsetX:cx - toCanvas(points[hitIdx]).cx, offsetY:cy - toCanvas(points[hitIdx]).cy}",
        "nearestNeighbors = null",
        "lastPrediction = null",
        "draw()"
      ],
      "triggers": "canvas#grid",
      "expected_observables": [
        "dom:canvas#grid mousedown over point"
      ]
    },
    {
      "from": "S3_Dragging",
      "to": "S3_Dragging",
      "event": "USER_MOUSEMOVE",
      "guard": "dragging != null",
      "actions": [
        "update dragging point coordinates based on mouse move",
        "clamp coordinates",
        "draw()"
      ],
      "triggers": "body, canvas#grid",
      "expected_observables": [
        "dom:window mousemove"
      ]
    },
    {
      "from": "S3_Dragging",
      "to": "S0_Idle",
      "event": "USER_MOUSEUP",
      "guard": "dragging != null",
      "actions": [
        "dragging = null",
        "draw()"
      ],
      "triggers": "body",
      "expected_observables": [
        "dom:window mouseup"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S2_Predicting",
      "event": "USER_MOUSEDOWN_ON_CANVAS",
      "guard": "predictOnClick === true && clickedOnExistingPoint === false",
      "actions": [
        "d = toData(cx,cy)",
        "pred = knnPredict(d, null, true)",
        "nearestNeighbors = pred ? pred.neighbors : null",
        "lastPrediction = pred",
        "lastPred.textContent = pred ? JSON.stringify(pred, null, 2) : 'No predictions.'",
        "draw()"
      ],
      "triggers": "canvas#grid",
      "expected_observables": [
        "dom:canvas#grid mousedown when predictOnClick"
      ]
    },
    {
      "from": "S2_Predicting",
      "to": "S0_Idle",
      "event": "USER_MOUSEMOVE",
      "guard": "true",
      "actions": [
        "/* predicting is immediate; stay idle until next action; clearing nearestNeighbors only if user interacts further */"
      ],
      "triggers": "canvas#grid, body",
      "expected_observables": [
        "dom:canvas#grid draw shows neighbors"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S6_Generating",
      "event": "USER_CLICK_BTN_GENERATE",
      "guard": "true",
      "actions": [
        "generateClusters()",
        "lastPrediction = null",
        "nearestNeighbors = null",
        "lastPred.textContent = 'No predictions yet.'",
        "draw()"
      ],
      "triggers": "#btnGenerate",
      "expected_observables": [
        "dom:#btnGenerate clicked",
        "dom:generateClusters executed"
      ]
    },
    {
      "from": "S6_Generating",
      "to": "S0_Idle",
      "event": "SYSTEM_GENERATE_COMPLETE",
      "guard": "true",
      "actions": [
        "draw()"
      ],
      "triggers": "#btnGenerate",
      "expected_observables": [
        "dom:points length > 0"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S7_Clearing",
      "event": "USER_CLICK_BTN_CLEAR",
      "guard": "true",
      "actions": [
        "points = []",
        "lastPrediction = null",
        "nearestNeighbors = null",
        "lastPred.textContent = 'No predictions yet.'",
        "draw()"
      ],
      "triggers": "#btnClear",
      "expected_observables": [
        "dom:#btnClear clicked",
        "dom:points length == 0"
      ]
    },
    {
      "from": "S7_Clearing",
      "to": "S0_Idle",
      "event": "SYSTEM_CLEAR_COMPLETE",
      "guard": "true",
      "actions": [
        "draw()"
      ],
      "triggers": "#btnClear",
      "expected_observables": [
        "dom:points cleared"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S4_BoundaryVisible",
      "event": "USER_CLICK_TOGGLE_BOUNDARY",
      "guard": "showBoundary === false",
      "actions": [
        "showBoundary = true",
        "btnToggleBoundary.textContent = 'Hide decision boundary'",
        "draw()"
      ],
      "triggers": "#btnToggleBoundary",
      "expected_observables": [
        "dom:#btnToggleBoundary clicked",
        "dom:boundary rendered"
      ]
    },
    {
      "from": "S4_BoundaryVisible",
      "to": "S0_Idle",
      "event": "USER_CLICK_TOGGLE_BOUNDARY",
      "guard": "showBoundary === true",
      "actions": [
        "showBoundary = false",
        "btnToggleBoundary.textContent = 'Toggle decision boundary'",
        "draw()"
      ],
      "triggers": "#btnToggleBoundary",
      "expected_observables": [
        "dom:#btnToggleBoundary clicked",
        "dom:boundary hidden"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S5_PredictModeOn",
      "event": "USER_CLICK_PREDICT_MODE",
      "guard": "predictOnClick === false",
      "actions": [
        "predictOnClick = true",
        "btnPredictMode.textContent = 'Predict mode: ON'"
      ],
      "triggers": "#btnPredictMode",
      "expected_observables": [
        "dom:#btnPredictMode clicked"
      ]
    },
    {
      "from": "S5_PredictModeOn",
      "to": "S0_Idle",
      "event": "USER_CLICK_PREDICT_MODE",
      "guard": "predictOnClick === true",
      "actions": [
        "predictOnClick = false",
        "btnPredictMode.textContent = 'Predict on click'",
        "nearestNeighbors = null",
        "lastPrediction = null",
        "lastPred.textContent = 'No predictions yet.'",
        "draw()"
      ],
      "triggers": "#btnPredictMode",
      "expected_observables": [
        "dom:#btnPredictMode clicked"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S15_UpdatingParams",
      "event": "USER_INPUT_K",
      "guard": "true",
      "actions": [
        "kVal.textContent = kRange.value",
        "lastPrediction = null",
        "draw()"
      ],
      "triggers": "#k",
      "expected_observables": [
        "dom:#k input changed"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S15_UpdatingParams",
      "event": "USER_CHANGE_AUTOK",
      "guard": "true",
      "actions": [
        "parseK()",
        "draw()"
      ],
      "triggers": "#autoK",
      "expected_observables": [
        "dom:#autoK change"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S15_UpdatingParams",
      "event": "USER_CHANGE_DIST",
      "guard": "true",
      "actions": [
        "lastPrediction = null",
        "draw()"
      ],
      "triggers": "#dist",
      "expected_observables": [
        "dom:#dist change"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S15_UpdatingParams",
      "event": "USER_CHANGE_WEIGHTING",
      "guard": "true",
      "actions": [
        "lastPrediction = null",
        "draw()"
      ],
      "triggers": "#weighting",
      "expected_observables": [
        "dom:#weighting change"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S15_UpdatingParams",
      "event": "USER_CHANGE_NCLASSES",
      "guard": "true",
      "actions": [
        "initColors(parseInt(nclassesSelect.value,10))",
        "updateLegend()",
        "draw()"
      ],
      "triggers": "#nclasses",
      "expected_observables": [
        "dom:#nclasses change"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S15_UpdatingParams",
      "event": "USER_CHANGE_MODE",
      "guard": "true",
      "actions": [
        "if(modeSelect.value==='regress'){ ensure points have .val } else { ensure labels integer }",
        "draw()"
      ],
      "triggers": "#mode",
      "expected_observables": [
        "dom:#mode change"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S8_EvaluatingCV",
      "event": "USER_CLICK_CV",
      "guard": "modeSelect.value === 'classify' && points.length >= 5",
      "actions": [
        "perform cross-validation as in btnCV handler",
        "alert result",
        "draw()"
      ],
      "triggers": "#btnCV",
      "expected_observables": [
        "dom:#btnCV clicked",
        "dom:alert shown"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S0_Idle",
      "event": "USER_CLICK_CV",
      "guard": "modeSelect.value !== 'classify' || points.length < 5",
      "actions": [
        "alert('Cross-validation accuracy is only implemented for classification mode in this demo.' or 'Too few points to evaluate.')"
      ],
      "triggers": "#btnCV",
      "expected_observables": [
        "dom:alert shown"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S9_ShowingConfusion",
      "event": "USER_CLICK_CONF",
      "guard": "modeSelect.value === 'classify' && points.length >= 5",
      "actions": [
        "compute confusion matrix (leave-one-out) as in btnConf handler",
        "alert matrix",
        "draw()"
      ],
      "triggers": "#btnConf",
      "expected_observables": [
        "dom:#btnConf clicked",
        "dom:alert shown"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S0_Idle",
      "event": "USER_CLICK_CONF",
      "guard": "modeSelect.value !== 'classify' || points.length < 5",
      "actions": [
        "alert('Confusion matrix is only for classification mode.' or 'Too few points to evaluate.')"
      ],
      "triggers": "#btnConf",
      "expected_observables": [
        "dom:alert shown"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S10_Exporting",
      "event": "USER_CLICK_EXPORT",
      "guard": "true",
      "actions": [
        "data = JSON.stringify(points, null, 2)",
        "open new window and write data",
        "draw()"
      ],
      "triggers": "#btnExport",
      "expected_observables": [
        "dom:#btnExport clicked",
        "dom:new window opened"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S11_AddNoise",
      "event": "USER_CLICK_ADD_NOISE",
      "guard": "points.length >= 1",
      "actions": [
        "for each p: p.x = clamp(p.x + (Math.random()-0.5)*0.02,0,1)",
        "for each p: p.y = clamp(p.y + (Math.random()-0.5)*0.02,0,1)",
        "draw()"
      ],
      "triggers": "#btnAddNoise",
      "expected_observables": [
        "dom:#btnAddNoise clicked"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S12_Normalize",
      "event": "USER_CLICK_NORMALIZE",
      "guard": "points.length > 0",
      "actions": [
        "compute min/max and normalize each p.x and p.y",
        "draw()"
      ],
      "triggers": "#btnNormalize",
      "expected_observables": [
        "dom:#btnNormalize clicked"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S13_ShuffleLabels",
      "event": "USER_CLICK_SHUFFLE",
      "guard": "points.length > 0",
      "actions": [
        "for each p: p.label = Math.floor(Math.random()*parseInt(nclassesSelect.value,10))",
        "draw()"
      ],
      "triggers": "#btnShuffle",
      "expected_observables": [
        "dom:#btnShuffle clicked"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S14_Hover",
      "event": "USER_MOUSEMOVE",
      "guard": "mouse over a point (getPointAtCanvas returns idx)",
      "actions": [
        "hover = {idx}",
        "draw()"
      ],
      "triggers": "canvas#grid",
      "expected_observables": [
        "dom:canvas#grid mousemove",
        "dom:inspector updated"
      ]
    },
    {
      "from": "S14_Hover",
      "to": "S0_Idle",
      "event": "USER_MOUSEMOVE",
      "guard": "mouse not over a point",
      "actions": [
        "hover = null",
        "draw()"
      ],
      "triggers": "canvas#grid",
      "expected_observables": [
        "dom:canvas#grid mousemove out of point"
      ]
    },
    {
      "from": "S0_Idle",
      "to": "S4_BoundaryVisible",
      "event": "USER_KEYDOWN_B",
      "guard": "showBoundary === false",
      "actions": [
        "showBoundary = !showBoundary",
        "btnToggleBoundary.textContent = (showBoundary ? 'Hide decision boundary' : 'Toggle decision boundary')",
        "draw()"
      ],
      "triggers": "body",
      "expected_observables": [
        "dom:window keydown 'b'"
      ]
    },
    {
