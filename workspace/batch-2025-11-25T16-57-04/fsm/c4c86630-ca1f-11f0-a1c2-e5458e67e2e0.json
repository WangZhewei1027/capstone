{
  "topic": "Queue",
  "description": "Models the interactive states of a simple Queue example page with enqueue/dequeue buttons, initial setup, and periodic timers; captures initialization, manual and automatic enqueue/dequeue, empty queue handling, and timer control.",
  "states": [
    {
      "name": "initializing",
      "onEnter": "runInitialSetupEnqueuesAndDequeue",
      "onExit": "noop",
      "on": {
        "INIT_COMPLETED": "idle",
        "ERROR": "error"
      }
    },
    {
      "name": "idle",
      "onEnter": "updateQueueDisplay",
      "onExit": "noop",
      "on": {
        "ENQUEUE_CLICKED": "enqueueing",
        "DEQUEUE_CLICKED": "dequeueing",
        "TIMER_ENQUEUE": "enqueueing",
        "TIMER_DEQUEUE": "dequeueing",
        "START_AUTO": "auto_running"
      }
    },
    {
      "name": "enqueueing",
      "onEnter": "enqueueItem",
      "onExit": "updateQueueDisplay",
      "on": {
        "ENQUEUE_COMPLETED": "idle",
        "ENQUEUE_FAILED": "error"
      }
    },
    {
      "name": "dequeueing",
      "onEnter": "dequeueItem",
      "onExit": "updateQueueDisplay",
      "on": {
        "DEQUEUE_COMPLETED": "idle",
        "DEQUEUE_EMPTY": "empty",
        "DEQUEUE_FAILED": "error"
      }
    },
    {
      "name": "empty",
      "onEnter": "alertQueueEmpty",
      "onExit": "noop",
      "on": {
        "ENQUEUE_CLICKED": "enqueueing",
        "TIMER_ENQUEUE": "enqueueing",
        "START_AUTO": "auto_running"
      }
    },
    {
      "name": "auto_running",
      "onEnter": "startTimers",
      "onExit": "stopTimers",
      "on": {
        "TIMER_ENQUEUE": "enqueueing",
        "TIMER_DEQUEUE": "dequeueing",
        "STOP_AUTO": "idle",
        "ERROR": "error"
      }
    },
    {
      "name": "error",
      "onEnter": "logError",
      "onExit": "noop",
      "on": {
        "RESET": "idle"
      }
    }
  ],
  "events": [
    "INIT_COMPLETED",
    "ENQUEUE_CLICKED",
    "DEQUEUE_CLICKED",
    "TIMER_ENQUEUE",
    "TIMER_DEQUEUE",
    "ENQUEUE_COMPLETED",
    "ENQUEUE_FAILED",
    "DEQUEUE_COMPLETED",
    "DEQUEUE_EMPTY",
    "DEQUEUE_FAILED",
    "START_AUTO",
    "STOP_AUTO",
    "ERROR",
    "RESET",
    "UPDATE_DISPLAY",
    "ALERT_EMPTY"
  ],
  "triggers": {
    "INIT_COMPLETED": "script",
    "ENQUEUE_CLICKED": "button[onclick=\"enqueue('Item 4')\"]",
    "DEQUEUE_CLICKED": "button[onclick=\"dequeue()\"]",
    "TIMER_ENQUEUE": "script",
    "TIMER_DEQUEUE": "script",
    "START_AUTO": "script",
    "STOP_AUTO": "script",
    "UPDATE_DISPLAY": "#queue, #queueText",
    "ALERT_EMPTY": "script",
    "RESET": "script"
  },
  "notes": "Triggers map events to actual DOM selectors present in the HTML. The page contains: div#queue, p#queueText, button[onclick=\"enqueue('Item 4')\"], button[onclick=\"dequeue()\"], and two <script> tags that include initial enqueues, dequeue, and setInterval calls (one setInterval call is malformed but is present in the script content)."
}