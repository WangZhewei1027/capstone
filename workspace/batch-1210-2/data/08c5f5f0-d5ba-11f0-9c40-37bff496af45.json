    "role": "assistant",
    "content": "<html>\n<head>\n    <title>Binary Search Tree</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        .tree {\n            width: 80%;\n            margin: 40px auto;\n            padding: 20px;\n            border: 1px solid #ddd;\n            border-radius: 10px;\n            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n        }\n        .node {\n            padding: 20px;\n            border-bottom: 1px solid #ccc;\n        }\n        .node:hover {\n            background-color: #f0f0f0;\n        }\n    </style>\n</head>\n<body>\n    <div class=\"tree\">\n        <h2>Binary Search Tree</h2>\n        <input type=\"text\" id=\"input\" placeholder=\"Enter a value to search for\">\n        <button id=\"search-btn\">Search</button>\n        <button id=\"add-btn\">Add</button>\n        <div id=\"tree\"></div>\n    </div>\n\n    <script>\n        class Node {\n            constructor(value) {\n                this.value = value;\n                this.left = null;\n                this.right = null;\n            }\n        }\n\n        class BinarySearchTree {\n            constructor() {\n                this.root = null;\n            }\n\n            insert(value) {\n                if (this.root === null) {\n                    this.root = new Node(value);\n                } else {\n                    this._insert(this.root, value);\n                }\n            }\n\n            _insert(node, value) {\n                if (value < node.value) {\n                    if (node.left === null) {\n                        node.left = new Node(value);\n                    } else {\n                        this._insert(node.left, value);\n                    }\n                } else {\n                    if (node.right === null) {\n                        node.right = new Node(value);\n                    } else {\n                        this._insert(node.right, value);\n                    }\n                }\n            }\n\n            search(value) {\n                return this._search(this.root, value);\n            }\n\n            _search(node, value) {\n                if (node === null) {\n                    return false;\n                } else if (value < node.value) {\n                    return this._search(node.left, value);\n                } else if (value > node.value) {\n                    return this._search(node.right, value);\n                } else {\n                    return true;\n                }\n            }\n\n            printTree(node) {\n                if (node !== null) {\n                    this.printTree(node.left);\n                    document.getElementById(\"tree\").innerHTML += `<p>${node.value}</p>`;\n                    this.printTree(node.right);\n                }\n            }\n        }\n\n        let bst = new BinarySearchTree();\n\n        document.getElementById(\"add-btn\").addEventListener(\"click\", () => {\n            bst.insert(document.getElementById(\"input\").value);\n            bst.printTree(bst.root);\n        });\n\n        document.getElementById(\"search-btn\").addEventListener(\"click\", () => {\n            let value = document.getElementById(\"input\").value;\n            if (bst.search(value)) {\n                document.getElementById(\"tree\").innerHTML += `<p>Found: ${value}</p>`;\n            } else {\n                document.getElementById(\"tree\").innerHTML += `<p>Not found</p>`;\n            }\n        });\n    </script>\n</body>\n</html>"
