    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Prim's Algorithm</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            text-align: center;\n        }\n        #graph {\n            border: 1px solid black;\n            padding: 20px;\n        }\n    </style>\n</head>\n<body>\n    <h1>Prim's Algorithm</h1>\n    <input id=\"vertices\" type=\"text\" placeholder=\"Enter number of vertices\">\n    <button id=\"start-button\">Start</button>\n    <button id=\"clear-button\">Clear</button>\n    <div id=\"graph\"></div>\n\n    <script>\n        const vertices = parseInt(document.getElementById('vertices').value);\n        let graph = new Array(vertices).fill(0).map(() => new Array(vertices).fill(0));\n\n        function addEdge(v1, v2) {\n            graph[v1][v2] = 1;\n            graph[v2][v1] = 1;\n        }\n\n        function primAlgorithm() {\n            const startVertex = parseInt(document.getElementById('start-button').value) - 1;\n            const visited = new Array(vertices).fill(false);\n\n            visited[startVertex] = true;\n\n            const parent = new Array(vertices).fill(-1);\n\n            const priorityQueue = [];\n\n            priorityQueue.push([startVertex, 0]);\n\n            while (priorityQueue.length > 0) {\n                priorityQueue.sort((a, b) => a[1] - b[1]);\n\n                const [v, d] = priorityQueue.shift();\n\n                if (visited[v]) {\n                    continue;\n                }\n\n                visited[v] = true;\n\n                for (let i = 0; i < vertices; i++) {\n                    if (graph[v][i] === 1 && !visited[i]) {\n                        priorityQueue.push([i, d + 1]);\n                        parent[i] = v;\n                    }\n                }\n            }\n\n            const result = {};\n\n            for (let i = 0; i < vertices; i++) {\n                result[i] = parent[i];\n            }\n\n            return result;\n        }\n\n        function displayGraph(result) {\n            const graphDiv = document.getElementById('graph');\n            graphDiv.innerHTML = '';\n\n            for (let i = 0; i < vertices; i++) {\n                for (let j = 0; j < vertices; j++) {\n                    if (graph[i][j] === 1 && result[i] === result[j]) {\n                        graphDiv.innerHTML += '<span class=\"edge\" style=\"width: 10px; height: 10px; border: 1px solid black; background-color: green;\"></span>';\n                    } else {\n                        graphDiv.innerHTML += '<span class=\"edge\" style=\"width: 10px; height: 10px; border: 1px solid black; background-color: gray;\"></span>';\n                    }\n                }\n            }\n        }\n\n        document.getElementById('start-button').addEventListener('click', () => {\n            const result = primAlgorithm();\n            displayGraph(result);\n        });\n\n        document.getElementById('clear-button').addEventListener('click', () => {\n            graph = new Array(vertices).fill(0).map(() => new Array(vertices).fill(0));\n            document.getElementById('graph').innerHTML = '';\n            displayGraph({});\n        });\n    </script>\n</body>\n</html>"
