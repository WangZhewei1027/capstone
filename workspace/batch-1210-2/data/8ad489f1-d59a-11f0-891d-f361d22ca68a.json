putNumbers).map((num) => {\n                let u = parseInt(num.value);\n                let v = parseInt(num.value + 10);\n                return { u, v };\n            });\n            let edgesArray = JSON.stringify(edges);\n            console.log('Edges:', edgesArray);\n            let result = kruskal(edgesArray);\n            console.log('Minimum Spanning Tree:', result);\n        });\n\n        mstButton.addEventListener('click', () => {\n            let edges = Array.from(inputNumbers).map((num) => {\n                let u = parseInt(num.value);\n                let v = parseInt(num.value + 10);\n                return { u, v };\n            });\n            let edgesArray = JSON.stringify(edges);\n            console.log('Edges:', edgesArray);\n            let result = mst(edgesArray);\n            console.log('Minimum Spanning Tree:', result);\n        });\n\n        function kruskal(edges) {\n            let edgesArray = JSON.parse(edges);\n            let edgesSet = new Set();\n            let result = [];\n            for (let edge of edgesArray) {\n                let [u, v] = edge;\n                if (!edgesSet.has(u) || !edgesSet.has(v)) {\n                    edgesSet.add(u);\n                    edgesSet.add(v);\n                    result.push({ u, v });\n                }\n            }\n            return result;\n        }\n\n        function mst(edges) {\n            let edgesArray = JSON.parse(edges);\n            let edgesSet = new Set();\n            let result = [];\n            for (let edge of edgesArray) {\n                let [u, v] = edge;\n                if (!edgesSet.has(u) || !edgesSet.has(v)) {\n                    edgesSet.add(u);\n                    edgesSet.add(v);\n                    result.push({ u, v });\n                    if (u === edgesSet.size - 1) {\n                        return result;\n                    }\n                }\n            }\n            return result;\n        }\n    </script>\n</body>\n</htm