    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Dijkstra's Algorithm</title>\n    <style>\n        #graph {\n            border: 1px solid black;\n            padding: 10px;\n        }\n        #graph table {\n            border-collapse: collapse;\n        }\n        #graph table td {\n            border: 1px solid black;\n            padding: 5px;\n        }\n    </style>\n</head>\n<body>\n    <h1>Dijkstra's Algorithm</h1>\n    <form id=\"graph-form\">\n        <label for=\"start\">Start:</label>\n        <input type=\"text\" id=\"start\" name=\"start\"><br><br>\n        <label for=\"end\">End:</label>\n        <input type=\"text\" id=\"end\" name=\"end\"><br><br>\n        <label for=\"graph\">Graph:</label>\n        <textarea id=\"graph\" name=\"graph\"></textarea><br><br>\n        <label for=\"num-vertices\">Number of Vertices:</label>\n        <input type=\"number\" id=\"num-vertices\" name=\"num-vertices\"><br><br>\n        <label for=\"weight\">Weight:</label>\n        <input type=\"number\" id=\"weight\" name=\"weight\"><br><br>\n        <button id=\"start-button\">Start</button>\n        <button id=\"end-button\">End</button>\n        <button id=\"graph-button\">Generate Graph</button>\n        <button id=\"result-button\">Get Result</button>\n    </form>\n    <div id=\"graph-container\"></div>\n\n    <script>\n        const start = document.getElementById('start');\n        const end = document.getElementById('end');\n        const graph = document.getElementById('graph');\n        const numVertices = document.getElementById('num-vertices');\n        const weight = document.getElementById('weight');\n        const startButton = document.getElementById('start-button');\n        const endButton = document.getElementById('end-button');\n        const graphButton = document.getElementById('graph-button');\n        const resultButton = document.getElementById('result-button');\n        let graphData = '';\n        let vertices = [];\n        let edges = [];\n        let visited = {};\n        let distances = {};\n        let previous = {};\n        let queue = [];\n\n        startButton.addEventListener('click', () => {\n            start.value = '';\n            graphData = '';\n            vertices = [];\n            edges = [];\n            visited = {};\n            distances = {};\n            previous = {};\n            queue = [];\n            graph.value = '';\n            end.value = '';\n            graphButton.value = 'Generate Graph';\n            resultButton.value = 'Get Result';\n            startButton.disabled = true;\n            endButton.disabled = true;\n            graphButton.disabled = true;\n            resultButton.disabled = true;\n        });\n\n        endButton.addEventListener('click', () => {\n            end.value = '';\n            graphButton.value = 'Generate Graph';\n            resultButton.value = 'Get Result';\n            startButton.disabled = true;\n            endButton.disabled = true;\n            graphButton.disabled = true;\n            resultButton.disabled = true;\n        });\n\n        graphButton.addEventListener('click', () => {\n            const graphStr = document.getElementById('graph').value;\n            graphData = graphStr;\n            graph.innerHTML = '';\n            graph.style.display = 'block';\n            const graphLines = graphStr.split('\\n');\n            for (let i = 0; i < graphLines.length; i++) {\n                const line = graphLines[i].split(',');\n                graph.appendChild(document.createElement('tr'));\n                for (let j = 0; j < line.length; j++) {\n                    const cell = document.createElement('td');\n                    cell.innerHTML = line[j];\n                    cell.style.width = '100px';\n                    cell.style.height = '50px';\n                    cell.style.border = '1px solid black';\n                    cell.style.padding = '5px';\n                    cell.style.backgroundColor = 'lightgreen';\n                    cell.onclick = () => {\n                        addVertex(line[j], i);\n                    };\n                    graph.appendChild(cell);\n                }\n            }\n        });\n\n        function addVertex(vertex, index) {\n            const row = document.createElement('tr');\n            row.innerHTML = vertex;\n            row.style.width = '100px';\n            row.style.height = '50px';\n            row.style.border = '1px solid black';\n            row.style.padding = '5px';\n            row.style.backgroundColor = 'lightgreen';\n            row.onclick = () => {\n                addEdge(index, vertex, index + 1);\n            };\n            graph.appendChild(row);\n        }\n\n        function addEdge(startVertex, endVertex, index) {\n            const row = document.createElement('tr');\n            row.innerHTML = `${startVertex},${endVertex},${index}`;\n            row.style.width = '100px';\n            row.style.height = '50px';\n            row.style.border = '1px solid black';\n            row.style.padding = '5px';\n            row.style.backgroundColor = 'lightgreen';\n            row.onclick = () => {\n                updateDistances(startVertex, endVertex, index);\n            };\n            graph.appendChild(row);\n        }\n\n        function updateDistances(startVertex, endVertex, index) {\n            const row = document.createElement('tr');\n            row.innerHTML = `${startVertex},${endVertex},${index}`;\n            row.style.width = '100px';\n            row.style.height = '50px';\n            row.style.border = '1px solid black';\n            row.style.padding = '5px';\n            row.style.backgroundColor = 'lightgreen';\n            row.onclick = () => {\n                const distance = getDistance(startVertex, endVertex, index);\n                updateVisited(startVertex, endVertex, index, distance);\n                updatePrevious(startVertex, endVertex, index, distance);\n                updateVisited(endVertex, startVertex, index, distance);\n            };\n            graph.appendChild(row);\n        }\n\n        function updateVisited(startVertex, endVertex, index, distance) {\n            visited[startVertex] = visited[endVertex] = true;\n            distances[index] = distance;\n            previous[index] = startVertex;\n        }\n\n        function updatePrevious(startVertex, endVertex, index, distance) {\n            if (distance < distances[endVertex]) {\n                distances[endVertex] = distance;\n                previous[endVertex] = startVertex;\n            }\n        }\n\n        function getDistance(startVertex, endVertex, index) {\n            const distance = Infinity;\n            if (visited[endVertex]) {\n                return distance;\n            }\n            queue.push(index);\n            while (queue.length > 0) {\n                const current = queue.shift();\n                if (current === index) {\n                    return distances[current];\n                }\n                for (let i = 0; i < edges.length; i++) {\n                    if (edges[i][0] === current && edges[i][1] === index) {\n                        const newDistance = distances[current] + edges[i][2];\n                        if (newDistance < distances[edges[i][1]]) {\n                            distances[edges[i][1]] = newDistance;\n                            previous[edges[i][1]] = edges[i][0];\n                        }\n                    }\n                }\n                queue.push(edges[current][0]);\n            }\n            return distance;\n        }\n    </script>\n</body>\n</html>"
