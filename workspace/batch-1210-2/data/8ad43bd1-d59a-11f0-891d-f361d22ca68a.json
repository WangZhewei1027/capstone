    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Depth-First Search</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        #graph {\n            border: 1px solid black;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Depth-First Search (DFS)</h1>\n\n<div id=\"graph\"></div>\n\n<input type=\"text\" id=\"start\" placeholder=\"Start node\">\n<input type=\"text\" id=\"end\" placeholder=\"End node\">\n\n<button id=\"dfs-button\">Perform DFS</button>\n\n<script>\n    const startNode = document.getElementById('start').value;\n    const endNode = document.getElementById('end').value;\n    const graph = new Array(5).fill(null).map(() => new Array(5).fill(null));\n\n    function addEdge(node1, node2) {\n        graph[node1].push(node2);\n        graph[node2].push(node1);\n    }\n\n    function dfs(node, visited, stack, result) {\n        visited[node] = true;\n        stack.push(node);\n\n        if (node === endNode) {\n            result.push(node);\n        }\n\n        for (const neighbor of graph[node]) {\n            if (!visited[neighbor]) {\n                dfs(neighbor, visited, stack, result);\n            }\n        }\n\n        stack.pop();\n    }\n\n    function drawGraph() {\n        const canvas = document.createElement('canvas');\n        canvas.width = 400;\n        canvas.height = 400;\n\n        const ctx = canvas.getContext('2d');\n        ctx.clearRect(0, 0, canvas.width, canvas.height);\n\n        for (let i = 0; i < 5; i++) {\n            for (let j = 0; j < 5; j++) {\n                if (graph[i][j] !== null) {\n                    ctx.beginPath();\n                    ctx.arc(j * 50 + 200, i * 50 + 200, 20, 0, 2 * Math.PI);\n                    ctx.fillStyle = 'blue';\n                    ctx.fill();\n                }\n            }\n        }\n\n        const x = canvas.width / 2 - 200;\n        const y = canvas.height / 2 - 200;\n\n        ctx.fillStyle = 'black';\n        ctx.beginPath();\n        ctx.arc(x, y, 50, 0, 2 * Math.PI);\n        ctx.fill();\n\n        const text = document.createElement('text');\n        text.text = 'Start Node: ' + startNode + '<br>End Node: ' + endNode;\n        text.style.position = 'absolute';\n        text.style.top = '10px';\n        text.style.left = '10px';\n        text.style.fontSize = '16px';\n        text.style.fontWeight = 'bold';\n        text.style.color = 'red';\n        canvas.appendChild(text);\n\n        const button = document.createElement('button');\n        button.text = 'Perform DFS';\n        button.style.position = 'absolute';\n        button.style.top = '20px';\n        button.style.left = '20px';\n        button.style.fontSize = '16px';\n        button.style.fontWeight = 'bold';\n        button.style.color = 'black';\n        button.onclick = function() {\n            const visited = new Array(5).fill(false);\n            const stack = [];\n            const result = [];\n\n            dfs(startNode, visited, stack, result);\n\n            for (const node of result) {\n                graph[node].forEach(neighbor => {\n                    if (neighbor !== endNode) {\n                        graph[neighbor].forEach(neighborNeighbor => {\n                            if (neighborNeighbor !== null) {\n                                addEdge(neighbor, neighborNeighbor);\n                            }\n                        });\n                    }\n                });\n            }\n\n            drawGraph();\n        };\n        canvas.appendChild(button);\n    }\n\n    addEdge(0, 1);\n    addEdge(0, 2);\n    addEdge(1, 3);\n    addEdge(2, 4);\n    drawGraph();\n</script>\n\n</body>\n</html>"
