    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Graph</title>\n    <style>\n        #graph {\n            border: 1px solid black;\n        }\n        #graph > * {\n            width: 50px;\n            height: 50px;\n            border: 1px solid black;\n            margin: 5px;\n        }\n    </style>\n</head>\n<body>\n    <h1>Graph</h1>\n    <button id=\"add-node\">Add Node</button>\n    <button id=\"add-edge\">Add Edge</button>\n    <div id=\"graph\"></div>\n\n    <script>\n        let graph = {\n            nodes: [],\n            edges: []\n        };\n\n        function addNode() {\n            const node = document.createElement('div');\n            node.classList.add('node');\n            document.getElementById('graph').appendChild(node);\n            graph.nodes.push(node);\n        }\n\n        function addEdge() {\n            const edge = document.createElement('div');\n            edge.classList.add('edge');\n            const source = document.createElement('div');\n            source.classList.add('source');\n            const target = document.createElement('div');\n            target.classList.add('target');\n            edge.appendChild(source);\n            edge.appendChild(target);\n            document.getElementById('graph').appendChild(edge);\n            graph.edges.push(edge);\n        }\n\n        function drawGraph() {\n            const canvas = document.createElement('canvas');\n            canvas.width = 400;\n            canvas.height = 400;\n            const ctx = canvas.getContext('2d');\n            graph.nodes.forEach(node => {\n                ctx.fillStyle = 'blue';\n                ctx.fillRect(node.style.left, node.style.top, 50, 50);\n            });\n            graph.edges.forEach(edge => {\n                const sourceX = parseInt(edge.style.source.style.left);\n                const sourceY = parseInt(edge.style.source.style.top);\n                const targetX = parseInt(edge.style.target.style.left);\n                const targetY = parseInt(edge.style.target.style.top);\n                ctx.beginPath();\n                ctx.moveTo(sourceX, sourceY);\n                ctx.lineTo(targetX, targetY);\n                ctx.stroke();\n            });\n            document.getElementById('graph').appendChild(canvas);\n        }\n\n        function clearGraph() {\n            graph.nodes = [];\n            graph.edges = [];\n            document.getElementById('graph').innerHTML = '';\n        }\n\n        addNode();\n        addEdge();\n        drawGraph();\n    </script>\n</body>\n</html>"
