    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Sliding Window</title>\n    <style>\n        #sliding-window {\n            position: absolute;\n            top: 50%;\n            left: 50%;\n            transform: translate(-50%, -50%);\n            width: 400px;\n            height: 200px;\n            border: 1px solid #ccc;\n            padding: 10px;\n            border-radius: 5px;\n            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n        }\n    </style>\n</head>\n<body>\n    <div id=\"sliding-window\"></div>\n    <script>\n        let window = document.getElementById('sliding-window');\n        let text = '';\n        let start = 0;\n        let end = 0;\n\n        function addText(text) {\n            window.innerHTML = '';\n            window.innerText = text;\n            start = 0;\n            end = text.length;\n        }\n\n        function updateText() {\n            if (start < end) {\n                let text = window.innerText;\n                let output = '';\n                for (let i = start; i < end; i++) {\n                    output += text.charAt(i);\n                    if (i < end - 1) {\n                        output += '<span style=\"background-color: yellow\">' + text.charAt(i + 1) + '</span>';\n                    }\n                }\n                window.innerText = output;\n            } else {\n                console.log('No more text to add');\n            }\n        }\n\n        function clearText() {\n            window.innerText = '';\n        }\n\n        function clearWindow() {\n            window.innerText = '';\n            start = 0;\n            end = 0;\n        }\n\n        addText('Hello, World!');\n        updateText();\n        console.log('Press F5 to add more text, press F8 to clear text');\n        window.addEventListener('keydown', function (e) {\n            if (e.key === 'F5') {\n                addText('Hello, ' + new Date().toLocaleTimeString() + '!');\n                updateText();\n            } else if (e.key === 'F8') {\n                clearText();\n            }\n        });\n    </script>\n</body>\n</html>"
