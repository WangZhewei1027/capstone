    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>HashMap Example</title>\n    <style>\n        #container {\n            width: 50%;\n            margin: 40px auto;\n            text-align: center;\n        }\n    </style>\n</head>\n<body>\n    <div id=\"container\">\n        <h2>HashMap Example</h2>\n        <form id=\"hashmap-form\">\n            <label for=\"key\">Enter Key:</label>\n            <input type=\"text\" id=\"key\" name=\"key\"><br><br>\n            <label for=\"value\">Enter Value:</label>\n            <input type=\"text\" id=\"value\" name=\"value\"><br><br>\n            <button id=\"add-btn\">Add</button>\n            <button id=\"remove-btn\">Remove</button>\n            <button id=\"clear-btn\">Clear</button>\n            <button id=\"print-btn\">Print</button>\n        </form>\n        <div id=\"hashmap-container\"></div>\n    </div>\n\n    <script>\n        const hashmap = new Map();\n        let inputKey, inputValue;\n\n        document.getElementById('add-btn').addEventListener('click', () => {\n            inputKey = document.getElementById('key').value;\n            hashmap.set(inputKey, inputValue);\n            updateHashmap();\n        });\n\n        document.getElementById('remove-btn').addEventListener('click', () => {\n            inputKey = document.getElementById('key').value;\n            if (hashmap.has(inputKey)) {\n                hashmap.delete(inputKey);\n                updateHashmap();\n            } else {\n                alert('Key not found in HashMap');\n            }\n        });\n\n        document.getElementById('clear-btn').addEventListener('click', () => {\n            hashmap.clear();\n            updateHashmap();\n        });\n\n        document.getElementById('print-btn').addEventListener('click', () => {\n            const sortedKeys = Array.from(hashmap.keys()).sort().join(', ');\n            const sortedValues = Array.from(hashmap.values()).sort().join(', ');\n            const hashmapString = `${sortedKeys}, ${sortedValues}`;\n            document.getElementById('hashmap-container').innerHTML = hashmapString;\n        });\n\n        function updateHashmap() {\n            const sortedKeys = Array.from(hashmap.keys()).sort().join(', ');\n            const sortedValues = Array.from(hashmap.values()).sort().join(', ');\n            const hashmapString = `${sortedKeys}, ${sortedValues}`;\n            document.getElementById('hashmap-container').innerHTML = hashmapString;\n        }\n    </script>\n</body>\n</html>"
