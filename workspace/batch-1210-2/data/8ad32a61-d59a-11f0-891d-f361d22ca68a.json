    "content": "<html>\n<head>\n    <title>Adjacency Matrix</title>\n    <style>\n        table {\n            border-collapse: collapse;\n        }\n        th, td {\n            border: 1px solid #ddd;\n            padding: 10px;\n            text-align: left;\n        }\n    </style>\n</head>\n<body>\n    <h1>Adjacency Matrix</h1>\n    <form id=\"matrix-form\">\n        <label for=\"rows\">Number of rows:</label>\n        <input type=\"number\" id=\"rows\" required><br><br>\n        <label for=\"columns\">Number of columns:</label>\n        <input type=\"number\" id=\"columns\" required><br><br>\n        <button type=\"submit\">Generate Matrix</button>\n    </form>\n    <div id=\"result\"></div>\n\n    <script>\n        const form = document.getElementById('matrix-form');\n        const resultDiv = document.getElementById('result');\n\n        form.addEventListener('submit', (e) => {\n            e.preventDefault();\n\n            const rows = parseInt(document.getElementById('rows').value);\n            const columns = parseInt(document.getElementById('columns').value);\n\n            if (rows <= 0 || columns <= 0) {\n                alert('Please enter a positive number for both rows and columns.');\n                return;\n            }\n\n            const matrix = Array(rows).fill(0).map(() => Array(columns).fill(0));\n\n            for (let i = 0; i < rows; i++) {\n                for (let j = 0; j < columns; j++) {\n                    const cell = document.createElement('input');\n                    cell.type = 'number';\n                    cell.value = i * columns + j;\n                    matrix[i][j] = parseInt(cell.value);\n                    document.getElementById('result').appendChild(cell);\n                }\n            }\n\n            const matrixHtml = matrix.map(row => row.join(' | ')).join('\\n');\n            const resultHtml = `\n                <table>\n                    ${matrixHtml}\n                </table>\n            `;\n\n            resultDiv.innerHTML = resultHtml;\n        });\n    </script>\n</body>\n</html>"
