    "role": "assistant",
    "content": "<html>\n<head>\n    <title>Radix Sort</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        #output {\n            margin-top: 20px;\n            padding: 10px;\n            border: 1px solid #ccc;\n            border-radius: 5px;\n            width: 300px;\n            text-align: center;\n        }\n    </style>\n</head>\n<body>\n    <h1>Radix Sort</h1>\n    <form id=\"radix-sort-form\">\n        <label for=\"number\">Enter a number:</label>\n        <input type=\"number\" id=\"number\" required>\n        <button type=\"submit\">Sort</button>\n    </form>\n    <div id=\"output\"></div>\n\n    <script>\n        function radixSort(num) {\n            const maxLen = num.toString().length;\n            const buckets = Array(10).fill(0).map(() => []);\n\n            for (let i = 0; i < maxLen; i++) {\n                for (let j = 0; j < num.toString().length; j++) {\n                    const digit = parseInt(num.toString()[j]);\n                    const bucketIndex = (digit / 10) % 10;\n                    buckets[bucketIndex].push(digit);\n                }\n            }\n\n            let output = '';\n            for (let i = 0; i < 10; i++) {\n                output += buckets[i].join(' ') + '\\n';\n            }\n\n            return output;\n        }\n\n        const number = document.getElementById('number').value;\n        const output = document.getElementById('output');\n\n        document.getElementById('radix-sort-form').addEventListener('submit', function(event) {\n            event.preventDefault();\n            const sortedNum = radixSort(number);\n            output.innerText = sortedNum;\n        });\n    </script>\n</body>\n</html>"
