    "content": "<html>\n<head>\n    <title>Array Demo</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        #output {\n            font-size: 24px;\n            font-weight: bold;\n            margin-bottom: 20px;\n        }\n    </style>\n</head>\n<body>\n    <h1>Array Demo</h1>\n    <input type=\"text\" id=\"array-input\" placeholder=\"Enter elements separated by comma and space\">\n    <button id=\"add-btn\">Add Elements</button>\n    <button id=\"clear-btn\">Clear Array</button>\n    <div id=\"output\"></div>\n\n    <script>\n        let array = [];\n        const addBtn = document.getElementById('add-btn');\n        const clearBtn = document.getElementById('clear-btn');\n        const outputDiv = document.getElementById('output');\n\n        // Function to add elements to the array\n        function addElements() {\n            const input = document.getElementById('array-input').value.trim();\n            if (input) {\n                array = input.split(',').map(Number);\n                outputDiv.innerText = `Array: ${array.join(', ')}`;\n            } else {\n                outputDiv.innerText = 'Please enter elements separated by comma and space.';\n            }\n        }\n\n        // Function to clear the array\n        function clearArray() {\n            array = [];\n            outputDiv.innerText = 'Array cleared.';\n        }\n\n        addBtn.addEventListener('click', addElements);\n        clearBtn.addEventListener('click', clearArray);\n\n        // Initialize the array with some values\n        array = [1, 2, 3, 4, 5];\n    </script>\n</body>\n</html>"
