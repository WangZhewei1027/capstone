    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Binary Tree</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        \n        .tree {\n            width: 80%;\n            margin: 40px auto;\n            padding: 20px;\n            border: 1px solid #ddd;\n            border-radius: 10px;\n            box-shadow: 0 0 10px rgba(0,0,0,0.1);\n        }\n        \n        .node {\n            padding: 20px;\n            border-bottom: 1px solid #ddd;\n        }\n        \n        .node:hover {\n            background-color: #f0f0f0;\n        }\n    </style>\n</head>\n<body>\n    <h1>Binary Tree</h1>\n    <p id=\"node-value\"></p>\n    <p id=\"node-parent\"></p>\n    <p id=\"node-left\"></p>\n    <p id=\"node-right\"></p>\n    <input id=\"node-value-input\" type=\"number\" placeholder=\"Enter node value\">\n    <button id=\"add-node-btn\">Add Node</button>\n    <button id=\"delete-node-btn\">Delete Node</button>\n    <button id=\"update-node-btn\">Update Node</button>\n    \n    <div id=\"tree\"></div>\n\n    <script>\n        class Node {\n            constructor(value) {\n                this.value = value;\n                this.parent = null;\n                this.left = null;\n                this.right = null;\n            }\n        }\n\n        function createTree(nodes) {\n            let root = new Node(nodes[0].value);\n            if (nodes.length > 1) {\n                let middleIndex = Math.floor(nodes.length / 2);\n                root.left = createTree(nodes.slice(0, middleIndex));\n                root.right = createTree(nodes.slice(middleIndex + 1));\n            }\n            return root;\n        }\n\n        function printTree(node, indent = 0) {\n            if (node) {\n                let str = '  '.repeat(indent) + node.value;\n                if (node.left) {\n                    str += ' -> Left: ' + node.left.value;\n                }\n                if (node.right) {\n                    str += ' -> Right: ' + node.right.value;\n                }\n                document.getElementById(\"tree\").innerHTML += str + '<br>';\n                printTree(node.left, indent + 2);\n                printTree(node.right, indent + 2);\n            }\n        }\n\n        let tree = null;\n        let nodes = [];\n\n        function addNode(value) {\n            if (nodes.length < 10) {\n                nodes.push(new Node(value));\n                printTree(nodes[nodes.length - 1]);\n                updateNodeInput();\n            } else {\n                alert(\"Tree is full. Cannot add more nodes.\");\n            }\n        }\n\n        function deleteNode(index) {\n            if (index >= nodes.length) {\n                alert(\"Node not found.\");\n            } else {\n                nodes.splice(index, 1);\n                printTree(nodes[nodes.length - 1]);\n                updateNodeInput();\n            }\n        }\n\n        function updateNode(index, value) {\n            if (index >= nodes.length) {\n                alert(\"Node not found.\");\n            } else {\n                nodes[index].value = value;\n                printTree(nodes[nodes.length - 1]);\n                updateNodeInput();\n            }\n        }\n\n        function updateNodeInput() {\n            let input = document.getElementById(\"node-value-input\");\n            input.value = \"\";\n            input.addEventListener(\"input\", function() {\n                let value = parseInt(input.value);\n                let index = Array.prototype.indexOf.call(nodes, node => node.value === value);\n                if (index !== -1) {\n                    node.value = value;\n                    updateNodeInput();\n                }\n            });\n        }\n\n        let addNodeBtn = document.getElementById(\"add-node-btn\");\n        let deleteNodeBtn = document.getElementById(\"delete-node-btn\");\n        let updateNodeBtn = document.getElementById(\"update-node-btn\");\n\n        addNodeBtn.addEventListener(\"click\", addNode);\n        deleteNodeBtn.addEventListener(\"click\", deleteNode);\n        updateNodeBtn.addEventListener(\"click\", updateNode);\n    </script>\n</body>\n</html>"
