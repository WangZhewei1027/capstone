    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Breadth-First Search</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        #graph {\n            width: 400px;\n            height: 400px;\n            border: 1px solid black;\n        }\n    </style>\n</head>\n<body>\n    <h1>Breadth-First Search</h1>\n    <div id=\"graph\"></div>\n\n    <script>\n        let graph = {\n            vertices: ['A', 'B', 'C', 'D', 'E'],\n            edges: [\n                ['A', 'B'],\n                ['B', 'C'],\n                ['C', 'D'],\n                ['D', 'E'],\n                ['E', 'A']\n            ]\n        };\n\n        let visited = new Set();\n        let queue = [];\n\n        function bfs(start) {\n            queue.push(start);\n            visited.add(start);\n            while (queue.length > 0) {\n                let vertex = queue.shift();\n                console.log(`Visited ${vertex}`);\n                for (let [neighbor, weight] of graph.edges) {\n                    if (!visited.has(neighbor) && weight > 0) {\n                        queue.push(neighbor);\n                        visited.add(neighbor);\n                        console.log(`Edge from ${vertex} to ${neighbor} with weight ${weight}`);\n                    }\n                }\n            }\n        }\n\n        bfs('A');\n    </script>\n</body>\n</html>"
