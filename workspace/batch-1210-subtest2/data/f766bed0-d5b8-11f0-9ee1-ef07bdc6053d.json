    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Binary Search Tree Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            justify-content: center;\n            height: 100vh;\n            background-color: #f4f4f4;\n        }\n        #bst-container {\n            display: flex;\n            justify-content: center;\n            align-items: flex-start;\n            margin-top: 20px;\n        }\n        .node {\n            border: 2px solid #4CAF50;\n            border-radius: 5px;\n            padding: 8px 12px;\n            margin: 5px;\n            background-color: #fff;\n            position: relative;\n        }\n        .line {\n            width: 2px;\n            background-color: #4CAF50;\n            position: absolute;\n        }\n        h2 {\n            margin: 0;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Binary Search Tree Visualization</h1>\n<input type=\"number\" id=\"value\" placeholder=\"Enter a number\" />\n<button onclick=\"insertNode()\">Insert</button>\n<button onclick=\"clearTree()\">Clear Tree</button>\n<div id=\"bst-container\"></div>\n\n<script>\n    class Node {\n        constructor(value) {\n            this.value = value;\n            this.left = null;\n            this.right = null;\n        }\n    }\n\n    class BinarySearchTree {\n        constructor() {\n            this.root = null;\n        }\n\n        insert(value) {\n            const newNode = new Node(value);\n            if (!this.root) {\n                this.root = newNode;\n                return;\n            }\n            let current = this.root;\n            while (true) {\n                if (value < current.value) {\n                    if (!current.left) {\n                        current.left = newNode;\n                        return;\n                    }\n                    current = current.left;\n                } else {\n                    if (!current.right) {\n                        current.right = newNode;\n                        return;\n                    }\n                    current = current.right;\n                }\n            }\n        }\n    }\n\n    const bst = new BinarySearchTree();\n    const bstContainer = document.getElementById('bst-container');\n\n    function insertNode() {\n        const value = parseInt(document.getElementById('value').value);\n        if (!isNaN(value)) {\n            bst.insert(value);\n            renderTree();\n            document.getElementById('value').value = '';\n        } else {\n            alert('Please enter a valid number');\n        }\n    }\n\n    function clearTree() {\n        bst.root = null;\n        bstContainer.innerHTML = '';\n    }\n\n    function renderTree() {\n        bstContainer.innerHTML = '';\n        if (!bst.root) return;\n        drawNode(bst.root, 0, 300, 100);\n    }\n\n    function drawNode(node, x, y, offset) {\n        if (!node) return;\n\n        const nodeElement = document.createElement('div');\n        nodeElement.className = 'node';\n        nodeElement.textContent = node.value;\n\n        bstContainer.appendChild(nodeElement);\n        nodeElement.style.position = 'absolute';\n        nodeElement.style.left = `${x}px`;\n        nodeElement.style.top = `${y}px`;\n\n        if (node.left) {\n            const leftX = x - offset;\n            const leftY = y + 60;\n            createConnectingLine(x + 30, y + 30, leftX + 30, leftY);\n            drawNode(node.left, leftX, leftY, offset / 2);\n        }\n\n        if (node.right) {\n            const rightX = x + offset;\n            const rightY = y + 60;\n            createConnectingLine(x + 30, y + 30, rightX + 30, rightY);\n            drawNode(node.right, rightX, rightY, offset / 2);\n        }\n    }\n\n    function createConnectingLine(fromX, fromY, toX, toY) {\n        const line = document.createElement('div');\n        line.className = 'line';\n        const length = Math.sqrt((toX - fromX) ** 2 + (toY - fromY) ** 2);\n        const angle = Math.atan2(toY - fromY, toX - fromX) * 180 / Math.PI;\n        line.style.height = length + 'px';\n        line.style.left = fromX + 'px';\n        line.style.top = fromY + 'px';\n        line.style.transform = `rotate(${angle}deg)`;\n        bstContainer.appendChild(line);\n    }\n</script>\n\n</body>\n</html>"
