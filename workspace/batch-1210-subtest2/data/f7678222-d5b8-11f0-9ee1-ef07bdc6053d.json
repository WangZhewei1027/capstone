    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Quick Sort Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            text-align: center;\n            margin-top: 50px;\n        }\n        #arrayContainer {\n            display: flex;\n            justify-content: center;\n            margin-bottom: 20px;\n        }\n        .bar {\n            width: 30px;\n            margin: 0 2px;\n            background-color: teal;\n            display: inline-block;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Quick Sort Visualization</h1>\n<button onclick=\"startQuickSort()\">Start Quick Sort</button>\n<div id=\"arrayContainer\"></div>\n\n<script>\n    let array = [];\n    const arraySize = 20;\n\n    // Generate random array\n    function generateRandomArray(size) {\n        return Array.from({ length: size }, () => Math.floor(Math.random() * 100) + 1);\n    }\n\n    function displayArray(arr) {\n        const container = document.getElementById('arrayContainer');\n        container.innerHTML = '';\n        arr.forEach(value => {\n            const bar = document.createElement('div');\n            bar.style.height = value * 3 + 'px'; // Scale height for visibility\n            bar.className = 'bar';\n            container.appendChild(bar);\n        });\n    }\n\n    async function quickSort(arr, left = 0, right = arr.length - 1) {\n        if (left < right) {\n            const pivotIndex = await partition(arr, left, right);\n            await quickSort(arr, left, pivotIndex - 1);\n            await quickSort(arr, pivotIndex + 1, right);\n        }\n        displayArray(arr);\n    }\n\n    async function partition(arr, low, high) {\n        const pivot = arr[high];\n        let i = low - 1;\n        \n        for (let j = low; j < high; j++) {\n            if (arr[j] < pivot) {\n                i++;\n                [arr[i], arr[j]] = [arr[j], arr[i]]; // Swap\n                displayArray(arr);\n                await new Promise(resolve => setTimeout(resolve, 100)); // Delay for visualization\n            }\n        }\n        [arr[i + 1], arr[high]] = [arr[high], arr[i + 1]]; // Swap pivot\n        displayArray(arr);\n        await new Promise(resolve => setTimeout(resolve, 100)); // Delay for visualization\n        return i + 1;\n    }\n\n    function startQuickSort() {\n        array = generateRandomArray(arraySize);\n        displayArray(array);\n        quickSort(array);\n    }\n\n    // Initial array display\n    array = generateRandomArray(arraySize);\n    displayArray(array);\n</script>\n\n</body>\n</html>"
