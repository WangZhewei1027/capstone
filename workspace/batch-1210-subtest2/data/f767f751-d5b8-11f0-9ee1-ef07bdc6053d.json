    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Breadth-First Search (BFS) Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            padding: 20px;\n        }\n        #graph {\n            display: flex;\n            flex-direction: column;\n            margin-bottom: 20px;\n        }\n        .node {\n            width: 50px;\n            height: 50px;\n            border: 2px solid #007BFF;\n            border-radius: 5px;\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            margin: 5px;\n            position: relative;\n        }\n        .edge {\n            height: 2px;\n            background: #007BFF;\n            position: absolute;\n            z-index: -1;\n        }\n        button {\n            padding: 10px 20px;\n            font-size: 16px;\n            cursor: pointer;\n        }\n        .visited {\n            background-color: #28a745;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Breadth-First Search (BFS) Visualization</h1>\n<div id=\"graph\"></div>\n<button id=\"startBtn\">Start BFS</button>\n\n<script>\n    const graphData = {\n        A: ['B', 'C'],\n        B: ['D', 'E'],\n        C: ['F'],\n        D: [],\n        E: ['F'],\n        F: []\n    };\n\n    function createGraph() {\n        const graphDiv = document.getElementById('graph');\n        for (const key in graphData) {\n            const node = document.createElement('div');\n            node.className = 'node';\n            node.id = key;\n            node.innerText = key;\n            graphDiv.appendChild(node);\n        }\n    }\n\n    function createEdges() {\n        for (const key in graphData) {\n            const node = document.getElementById(key);\n            graphData[key].forEach(child => {\n                const childNode = document.getElementById(child);\n                if (childNode) {\n                    const edge = document.createElement('div');\n                    edge.className = 'edge';\n                    const top = node.offsetTop + node.offsetHeight / 2;\n                    const left = node.offsetLeft + node.offsetWidth;\n                    const childTop = childNode.offsetTop + childNode.offsetHeight / 2;\n                    const childLeft = childNode.offsetLeft;\n                    const height = Math.abs(childTop - top);\n                    edge.style.width = `${height}px`;\n                    edge.style.left = `${left}px`;\n                    edge.style.top = `${top + (childTop > top ? 0 : -height)}px`;\n                    edge.style.transform = `rotate(${childTop > top ? 90 : -90}deg)`;\n                    graphDiv.appendChild(edge);\n                }\n            });\n        }\n    }\n\n    async function bfs(start) {\n        const visited = new Set();\n        const queue = [start];\n\n        while (queue.length > 0) {\n            const currentNode = queue.shift();\n            if (!visited.has(currentNode)) {\n                visited.add(currentNode);\n                document.getElementById(currentNode).classList.add('visited');\n                await new Promise(r => setTimeout(r, 1000));\n                graphData[currentNode].forEach(node => {\n                    if (!visited.has(node)) {\n                        queue.push(node);\n                    }\n                });\n            }\n        }\n    }\n\n    document.getElementById('startBtn').addEventListener('click', function(){\n        document.querySelectorAll('.node').forEach(node => {\n            node.classList.remove('visited');\n        });\n        bfs('A');\n    });\n\n    // Initialize the graph and edges\n    createGraph();\n    createEdges();\n</script>\n\n</body>\n</html>"
