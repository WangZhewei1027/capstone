    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Priority Queue Demo</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n        }\n        #queue {\n            margin-top: 20px;\n        }\n        .node {\n            display: inline-block;\n            margin: 5px;\n            padding: 10px;\n            border: 1px solid #ccc;\n            border-radius: 5px;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Priority Queue Demo</h1>\n<input type=\"text\" id=\"element\" placeholder=\"Element (number)\">\n<input type=\"number\" id=\"priority\" placeholder=\"Priority (lower number = higher priority)\">\n<button onclick=\"addElement()\">Add to Queue</button>\n<button onclick=\"removeElement()\">Remove Highest Priority</button>\n\n<div id=\"queue\"></div>\n\n<script>\n    class PriorityQueue {\n        constructor() {\n            this.elements = [];\n        }\n\n        enqueue(element, priority) {\n            this.elements.push({element, priority});\n            this.elements.sort((a, b) => a.priority - b.priority);\n            this.render();\n        }\n\n        dequeue() {\n            if (this.isEmpty()) {\n                alert(\"Queue is empty!\");\n                return;\n            }\n            this.elements.shift();\n            this.render();\n        }\n\n        isEmpty() {\n            return this.elements.length === 0;\n        }\n\n        render() {\n            const queueDiv = document.getElementById('queue');\n            queueDiv.innerHTML = '';\n            this.elements.forEach(el => {\n                const nodeDiv = document.createElement('div');\n                nodeDiv.className = 'node';\n                nodeDiv.innerText = `Element: ${el.element}, Priority: ${el.priority}`;\n                queueDiv.appendChild(nodeDiv);\n            });\n        }\n    }\n\n    const priorityQueue = new PriorityQueue();\n\n    function addElement() {\n        const elementInput = document.getElementById('element');\n        const priorityInput = document.getElementById('priority');\n\n        const element = elementInput.value;\n        const priority = parseInt(priorityInput.value);\n        \n        if (!element || isNaN(priority)) {\n            alert(\"Please enter a valid element and priority.\");\n            return;\n        }\n\n        priorityQueue.enqueue(element, priority);\n        elementInput.value = '';\n        priorityInput.value = '';\n    }\n\n    function removeElement() {\n        priorityQueue.dequeue();\n    }\n</script>\n\n</body>\n</html>"
