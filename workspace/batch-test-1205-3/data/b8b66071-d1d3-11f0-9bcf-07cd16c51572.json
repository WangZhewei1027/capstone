    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Graph Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            background-color: #f4f4f4;\n            margin: 0;\n            padding: 20px;\n        }\n        canvas {\n            border: 1px solid #aaa;\n            background-color: #fff;\n        }\n        #controls {\n            margin-bottom: 20px;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Graph Visualization</h1>\n<div id=\"controls\">\n    <button onclick=\"addNode()\">Add Node</button>\n    <button onclick=\"addEdge()\">Add Edge</button>\n    <button onclick=\"toggleDirection()\">Toggle Directed/Undirected</button>\n    <button onclick=\"clearGraph()\">Clear Graph</button>\n</div>\n<canvas id=\"graphCanvas\" width=\"800\" height=\"600\"></canvas>\n\n<script>\n    const canvas = document.getElementById('graphCanvas');\n    const ctx = canvas.getContext('2d');\n\n    let nodes = [];\n    let edges = [];\n    let isDirected = true;\n\n    function addNode() {\n        const nodeName = prompt(\"Enter node name:\");\n        if (nodeName && !nodes.includes(nodeName)) {\n            nodes.push(nodeName);\n            drawGraph();\n        } else {\n            alert(\"Invalid or duplicate node name.\");\n        }\n    }\n\n    function addEdge() {\n        const fromNode = prompt(\"Enter from node:\");\n        const toNode = prompt(\"Enter to node:\");\n        if (nodes.includes(fromNode) && nodes.includes(toNode)) {\n            edges.push({ from: fromNode, to: toNode });\n            if (!isDirected) {\n                edges.push({ from: toNode, to: fromNode });\n            }\n            drawGraph();\n        } else {\n            alert(\"Both nodes must exist in the graph.\");\n        }\n    }\n\n    function toggleDirection() {\n        isDirected = !isDirected;\n        alert(`Graph is now ${isDirected ? \"directed\" : \"undirected\"}.`);\n        drawGraph();\n    }\n\n    function clearGraph() {\n        nodes = [];\n        edges = [];\n        drawGraph();\n    }\n\n    function drawGraph() {\n        ctx.clearRect(0, 0, canvas.width, canvas.height);\n        const nodeRadius = 20;\n        const angleIncrement = (2 * Math.PI) / nodes.length;\n        \n        nodes.forEach((node, index) => {\n            const angle = angleIncrement * index;\n            const x = canvas.width / 2 + Math.cos(angle) * 200;\n            const y = canvas.height / 2 + Math.sin(angle) * 200;\n\n            // Draw node\n            ctx.beginPath();\n            ctx.arc(x, y, nodeRadius, 0, 2 * Math.PI);\n            ctx.fillStyle = 'lightblue';\n            ctx.fill();\n            ctx.stroke();\n            ctx.fillStyle = 'black';\n            ctx.fillText(node, x - 10, y + 5);\n\n            // Draw edges\n            edges.forEach(edge => {\n                if (edge.from === node) {\n                    const toNodeIndex = nodes.indexOf(edge.to);\n                    const toAngle = angleIncrement * toNodeIndex;\n                    const toX = canvas.width / 2 + Math.cos(toAngle) * 200;\n                    const toY = canvas.height / 2 + Math.sin(toAngle) * 200;\n\n                    ctx.beginPath();\n                    ctx.moveTo(x, y);\n                    ctx.lineTo(toX, toY);\n                    ctx.stroke();\n                    if (isDirected) {\n                        drawArrowHead(x, y, toX, toY);\n                    }\n                }\n            });\n        });\n    }\n\n    function drawArrowHead(fromX, fromY, toX, toY) {\n        const headlen = 10; // length of head in pixels\n        const angle = Math.atan2(toY - fromY, toX - fromX);\n        ctx.beginPath();\n        ctx.moveTo(toX, toY);\n        ctx.lineTo(toX - headlen * Math.cos(angle - Math.PI / 6), toY - headlen * Math.sin(angle - Math.PI / 6));\n        ctx.lineTo(toX - headlen * Math.cos(angle + Math.PI / 6), toY - headlen * Math.sin(angle + Math.PI / 6));\n        ctx.lineTo(toX, toY);\n        ctx.fillStyle = 'black';\n        ctx.fill();\n    }\n</script>\n\n</body>\n</html>"
