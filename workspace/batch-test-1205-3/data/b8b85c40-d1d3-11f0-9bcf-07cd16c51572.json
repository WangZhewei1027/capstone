    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>K-Means Clustering Demonstration</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            margin: 20px;\n        }\n        #canvas {\n            border: 1px solid black;\n            margin-top: 20px;\n            cursor: pointer;\n        }\n        #result {\n            margin-top: 20px;\n        }\n    </style>\n</head>\n<body>\n    <h1>K-Means Clustering Demonstration</h1>\n    <canvas id=\"canvas\" width=\"500\" height=\"400\"></canvas>\n    <button id=\"kmeans\">Run K-Means</button>\n    <div id=\"result\"></div>\n\n    <script>\n        const canvas = document.getElementById('canvas');\n        const ctx = canvas.getContext('2d');\n        const points = [];\n        const centroids = [];\n        const k = 3;\n\n        // Function to draw points and centroids\n        function draw() {\n            ctx.clearRect(0, 0, canvas.width, canvas.height);\n            points.forEach(point => {\n                ctx.fillStyle = point.cluster !== undefined ? `hsl(${point.cluster * 360 / k}, 100%, 50%)` : 'black';\n                ctx.beginPath();\n                ctx.arc(point.x, point.y, 5, 0, Math.PI * 2);\n                ctx.fill();\n            });\n\n            centroids.forEach((centroid, index) => {\n                ctx.fillStyle = `hsl(${index * 360 / k}, 100%, 50%)`;\n                ctx.beginPath();\n                ctx.arc(centroid.x, centroid.y, 10, 0, Math.PI * 2);\n                ctx.fill();\n            });\n        }\n\n        // Function to generate random points\n        function generatePoints(numPoints) {\n            for (let i = 0; i < numPoints; i++) {\n                points.push({x: Math.random() * canvas.width, y: Math.random() * canvas.height});\n            }\n        }\n\n        function initializeCentroids() {\n            centroids.length = 0;\n            for (let i = 0; i < k; i++) {\n                centroids.push({x: Math.random() * canvas.width, y: Math.random() * canvas.height});\n            }\n        }\n\n        function assignClusters() {\n            points.forEach(point => {\n                let closestIndex = -1;\n                let minDistance = Infinity;\n                centroids.forEach((centroid, index) => {\n                    const distance = Math.sqrt(Math.pow(point.x - centroid.x, 2) + Math.pow(point.y - centroid.y, 2));\n                    if (distance < minDistance) {\n                        minDistance = distance;\n                        closestIndex = index;\n                    }\n                });\n                point.cluster = closestIndex;\n            });\n        }\n\n        function updateCentroids() {\n            centroids.forEach((centroid, index) => {\n                let clusterPoints = points.filter(point => point.cluster === index);\n                if (clusterPoints.length > 0) {\n                    centroid.x = clusterPoints.reduce((sum, point) => sum + point.x, 0) / clusterPoints.length;\n                    centroid.y = clusterPoints.reduce((sum, point) => sum + point.y, 0) / clusterPoints.length;\n                }\n            });\n        }\n\n        function kMeans() {\n            initializeCentroids();\n            let changes;\n            do {\n                assignClusters();\n                const previousCentroids = JSON.stringify(centroids);\n                updateCentroids();\n                changes = previousCentroids !== JSON.stringify(centroids);\n            } while (changes);\n            draw();\n        }\n\n        canvas.addEventListener('click', function(event) {\n            const rect = canvas.getBoundingClientRect();\n            const x = event.clientX - rect.left;\n            const y = event.clientY - rect.top;\n            points.push({x: x, y: y});\n            draw();\n        });\n\n        document.getElementById('kmeans').addEventListener('click', function() {\n            kMeans();\n            document.getElementById('result').innerText = `${points.length} points clustered into ${k} clusters.`;\n        });\n\n        generatePoints(20);\n        draw();\n    </script>\n</body>\n</html>"
