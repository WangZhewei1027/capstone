    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Prim's Algorithm Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            text-align: center;\n        }\n        #canvas {\n            border: 1px solid black;\n            margin: 20px auto;\n            display: block;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Prim's Algorithm Visualization</h1>\n<canvas id=\"canvas\" width=\"600\" height=\"600\"></canvas>\n<button onclick=\"runPrim()\">Run Prim's Algorithm</button>\n\n<script>\n    const canvas = document.getElementById('canvas');\n    const ctx = canvas.getContext('2d');\n\n    const vertices = [\n        { x: 100, y: 100 },\n        { x: 500, y: 100 },\n        { x: 300, y: 300 },\n        { x: 100, y: 500 },\n        { x: 500, y: 500 },\n        { x: 300, y: 400 },\n    ];\n\n    const edges = [\n        [0, 1, 200], [0, 2, 200], [0, 3, 400],\n        [1, 2, 200], [1, 4, 400], [2, 3, 200],\n        [2, 5, 100], [3, 4, 200], [4, 5, 400],\n    ];\n\n    const visited = new Set();\n    const mstEdges = [];\n\n    function drawGraph() {\n        ctx.clearRect(0, 0, canvas.width, canvas.height);\n        \n        // Draw vertices\n        vertices.forEach((vertex, index) => {\n            ctx.beginPath();\n            ctx.arc(vertex.x, vertex.y, 10, 0, Math.PI * 2);\n            ctx.fillStyle = visited.has(index) ? \"green\" : \"blue\";\n            ctx.fill();\n            ctx.strokeText(index, vertex.x - 5, vertex.y - 15);\n        });\n\n        // Draw edges\n        edges.forEach(edge => {\n            const [start, end] = edge.slice(0, 2);\n            ctx.beginPath();\n            ctx.moveTo(vertices[start].x, vertices[start].y);\n            ctx.lineTo(vertices[end].x, vertices[end].y);\n            ctx.strokeStyle = mstEdges.includes(edge) ? \"red\" : \"black\";\n            ctx.stroke();\n        });\n    }\n\n    function runPrim() {\n        const startVertex = 0; // Starting vertex\n        visited.add(startVertex);\n        let edgesPriorityQueue = edges\n            .filter(edge => edge[0] === startVertex || edge[1] === startVertex)\n            .sort((a, b) => a[2] - b[2]); // Sort by weight\n\n        while (edgesPriorityQueue.length > 0) {\n            const [u, v, weight] = edgesPriorityQueue.shift();\n            if (!visited.has(v)) {\n                mstEdges.push([u, v, weight]);\n                visited.add(v);\n                edgesPriorityQueue.push(...edges\n                    .filter(edge => (edge[0] === v || edge[1] === v) && !visited.has(edge[0]) && !visited.has(edge[1]))\n                );\n                edgesPriorityQueue.sort((a, b) => a[2] - b[2]); // Sort again\n            }\n            drawGraph();\n        }\n    }\n\n    drawGraph();\n</script>\n\n</body>\n</html>"
