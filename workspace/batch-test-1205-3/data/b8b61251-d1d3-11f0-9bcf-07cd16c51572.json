    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Binary Tree Visualization</title>\n    <style>\n        body { font-family: Arial, sans-serif; }\n        #tree {\n            display: flex;\n            justify-content: center;\n            margin-top: 20px;\n        }\n        .node {\n            border: 1px solid #333;\n            border-radius: 5px;\n            padding: 10px;\n            text-align: center;\n            margin: 5px;\n            position: relative;\n        }\n        .line {\n            position: absolute;\n            left: 50%;\n            width: 2px;\n            background: #333;\n        }\n        .horizontal-line {\n            height: 20px;\n            top: 20px;\n          }\n        .vertical-line {\n            height: 50px;\n            top: -50px;\n        }\n        .level {\n            display: flex;\n            justify-content: center;\n        }\n    </style>\n</head>\n<body>\n    <h1>Binary Tree Visualization</h1>\n    <div id=\"tree\"></div>\n    <input type=\"text\" id=\"valueInput\" placeholder=\"Enter a value\">\n    <button onclick=\"addNode()\">Add Node</button>\n\n    <script>\n        class TreeNode {\n            constructor(data) {\n                this.data = data;\n                this.left = null;\n                this.right = null;\n            }\n        }\n\n        class BinaryTree {\n            constructor() {\n                this.root = null;\n                this.nodes = [];\n            }\n\n            add(data) {\n                const newNode = new TreeNode(data);\n                if (this.root === null) {\n                    this.root = newNode;\n                } else {\n                    this.insertNode(this.root, newNode);\n                }\n                this.nodes.push(newNode);\n                this.displayTree();\n            }\n\n            insertNode(node, newNode) {\n                if (newNode.data < node.data) {\n                    if (node.left === null) {\n                        node.left = newNode;\n                    } else {\n                        this.insertNode(node.left, newNode);\n                    }\n                } else {\n                    if (node.right === null) {\n                        node.right = newNode;\n                    } else {\n                        this.insertNode(node.right, newNode);\n                    }\n                }\n            }\n\n            displayTree() {\n                const treeContainer = document.getElementById('tree');\n                treeContainer.innerHTML = ''; // Clear existing nodes\n                this.generateTreeHTML(this.root, treeContainer);\n            }\n\n            generateTreeHTML(node, container) {\n                if (!node) return;\n                const nodeDiv = document.createElement('div');\n                nodeDiv.className = 'node';\n                nodeDiv.innerHTML = node.data;\n                container.appendChild(nodeDiv);\n\n                const childrenContainer = document.createElement('div');\n                childrenContainer.className = 'level';\n                container.appendChild(childrenContainer);\n\n                if (node.left || node.right) {\n                    const lineLeft = document.createElement('div');\n                    const lineRight = document.createElement('div');\n                    lineLeft.className = 'line vertical-line';\n                    lineRight.className = 'line vertical-line';\n                    \n                    if (node.left) {\n                        lineLeft.style.borderLeft = \"2px solid #333\";\n                        lineLeft.style.left = `${-50}px`;\n                        lineLeft.style.top = '10px';\n                        childrenContainer.appendChild(lineLeft);\n                    }\n\n                    if (node.right) {\n                        lineRight.style.borderLeft = \"2px solid #333\";\n                        lineRight.style.left = `${50}px`;\n                        lineRight.style.top = '10px';\n                        childrenContainer.appendChild(lineRight);\n                    }\n                }\n\n                const leftContainer = document.createElement('div');\n                const rightContainer = document.createElement('div');\n                leftContainer.className = 'level';\n                rightContainer.className = 'level';\n                childrenContainer.appendChild(leftContainer);\n                childrenContainer.appendChild(rightContainer);\n\n                this.generateTreeHTML(node.left, leftContainer);\n                this.generateTreeHTML(node.right, rightContainer);\n            }\n        }\n\n        const binaryTree = new BinaryTree();\n\n        function addNode() {\n            const valueInput = document.getElementById('valueInput');\n            const value = valueInput.value;\n            if (value) {\n                binaryTree.add(value);\n                valueInput.value = ''; // Clear input\n            }\n        }\n    </script>\n</body>\n</html>"
