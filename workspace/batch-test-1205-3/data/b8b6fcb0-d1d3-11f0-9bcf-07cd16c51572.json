    "content": "<html lang=\"en\">\n\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Quick Sort Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        #array {\n            display: flex;\n            justify-content: center;\n            margin: 20px 0;\n        }\n        .bar {\n            width: 20px;\n            margin: 0 2px;\n            background-color: teal;\n            display: inline-block;\n            transition: height 0.5s;\n        }\n    </style>\n</head>\n\n<body>\n\n    <h1>Quick Sort Visualization</h1>\n    <div id=\"array\"></div>\n    <button onclick=\"startQuickSort()\">Start Quick Sort</button>\n\n    <script>\n        let array = [50, 10, 20, 80, 30, 90, 70, 40, 60];\n        const arrayContainer = document.getElementById('array');\n\n        function drawArray(arr) {\n            arrayContainer.innerHTML = '';\n            arr.forEach(value => {\n                const bar = document.createElement('div');\n                bar.style.height = value * 3 + 'px'; // Scale height\n                bar.classList.add('bar');\n                arrayContainer.appendChild(bar);\n            });\n        }\n\n        function swap(arr, i, j) {\n            [arr[i], arr[j]] = [arr[j], arr[i]];\n        }\n\n        async function quickSort(arr, left, right) {\n            if (left < right) {\n                const pivotIndex = await partition(arr, left, right);\n                await quickSort(arr, left, pivotIndex - 1);\n                await quickSort(arr, pivotIndex + 1, right);\n            }\n        }\n\n        async function partition(arr, left, right) {\n            let pivot = arr[right];\n            let i = left - 1;\n            for (let j = left; j < right; j++) {\n                if (arr[j] < pivot) {\n                    i++;\n                    swap(arr, i, j);\n                    drawArray(arr);\n                    await sleep(500);\n                }\n            }\n            swap(arr, i + 1, right);\n            drawArray(arr);\n            await sleep(500);\n            return i + 1;\n        }\n\n        function sleep(ms) {\n            return new Promise(resolve => setTimeout(resolve, ms));\n        }\n\n        async function startQuickSort() {\n            drawArray(array);\n            await quickSort(array, 0, array.length - 1);\n            alert('Sorting completed!');\n        }\n\n        // Initial draw\n        drawArray(array);\n    </script>\n\n</body>\n\n</html>"
