    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Heap Visualization (Min & Max)</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            margin-top: 50px;\n        }\n        #heap {\n            margin-top: 20px;\n            display: grid;\n            grid-template-columns: repeat(5, 1fr);\n            gap: 10px;\n        }\n        .node {\n            width: 50px;\n            height: 50px;\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            border: 2px solid #333;\n            border-radius: 5px;\n        }\n        .min-heap {\n            border-color: green;\n        }\n        .max-heap {\n            border-color: red;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Min and Max Heap Visualization</h1>\n<div>\n    <input type=\"text\" id=\"valueInput\" placeholder=\"Enter a value\" />\n    <button id=\"insertMinHeap\">Insert into Min Heap</button>\n    <button id=\"insertMaxHeap\">Insert into Max Heap</button>\n</div>\n<div id=\"heap\">\n    <h2>Min Heap</h2>\n    <div id=\"minHeap\"></div>\n    <h2>Max Heap</h2>\n    <div id=\"maxHeap\"></div>\n</div>\n\n<script>\n    class MinHeap {\n        constructor() {\n            this.heap = [];\n        }\n\n        insert(num) {\n            this.heap.push(num);\n            this.bubbleUp();\n        }\n\n        bubbleUp() {\n            let index = this.heap.length - 1;\n            while (index > 0) {\n                let parentIndex = Math.floor((index - 1) / 2);\n                if (this.heap[index] >= this.heap[parentIndex]) break;\n                [this.heap[index], this.heap[parentIndex]] = [this.heap[parentIndex], this.heap[index]];\n                index = parentIndex;\n            }\n        }\n\n        getHeap() {\n            return this.heap;\n        }\n    }\n\n    class MaxHeap {\n        constructor() {\n            this.heap = [];\n        }\n\n        insert(num) {\n            this.heap.push(num);\n            this.bubbleUp();\n        }\n\n        bubbleUp() {\n            let index = this.heap.length - 1;\n            while (index > 0) {\n                let parentIndex = Math.floor((index - 1) / 2);\n                if (this.heap[index] <= this.heap[parentIndex]) break;\n                [this.heap[index], this.heap[parentIndex]] = [this.heap[parentIndex], this.heap[index]];\n                index = parentIndex;\n            }\n        }\n\n        getHeap() {\n            return this.heap;\n        }\n    }\n\n    const minHeap = new MinHeap();\n    const maxHeap = new MaxHeap();\n\n    function renderHeaps() {\n        const minHeapContainer = document.getElementById('minHeap');\n        const maxHeapContainer = document.getElementById('maxHeap');\n        minHeapContainer.innerHTML = '';\n        maxHeapContainer.innerHTML = '';\n\n        minHeap.getHeap().forEach(value => {\n            const node = document.createElement('div');\n            node.classList.add('node', 'min-heap');\n            node.textContent = value;\n            minHeapContainer.appendChild(node);\n        });\n\n        maxHeap.getHeap().forEach(value => {\n            const node = document.createElement('div');\n            node.classList.add('node', 'max-heap');\n            node.textContent = value;\n            maxHeapContainer.appendChild(node);\n        });\n    }\n\n    document.getElementById('insertMinHeap').addEventListener('click', () => {\n        const value = parseInt(document.getElementById('valueInput').value);\n        if (!isNaN(value)) {\n            minHeap.insert(value);\n            renderHeaps();\n            document.getElementById('valueInput').value = '';\n        }\n    });\n\n    document.getElementById('insertMaxHeap').addEventListener('click', () => {\n        const value = parseInt(document.getElementById('valueInput').value);\n        if (!isNaN(value)) {\n            maxHeap.insert(value);\n            renderHeaps();\n            document.getElementById('valueInput').value = '';\n        }\n    });\n</script>\n\n</body>\n</html>"
