    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Kruskal's Algorithm Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            background-color: #f8f9fa;\n            margin: 0;\n            padding: 20px;\n            text-align: center;\n        }\n        canvas {\n            border: 1px solid #000;\n            margin: 20px 0;\n        }\n        #output {\n            margin-top: 20px;\n            padding: 10px;\n            background-color: #e9ecef;\n            display: inline-block;\n        }\n    </style>\n</head>\n<body>\n    <h1>Kruskal's Algorithm Visualization</h1>\n    <canvas id=\"graphCanvas\" width=\"600\" height=\"400\"></canvas>\n    <div id=\"output\"></div>\n    <button onclick=\"runKruskal()\">Run Kruskal's Algorithm</button>\n\n    <script>\n        const canvas = document.getElementById('graphCanvas');\n        const ctx = canvas.getContext('2d');\n\n        const edges = [\n            { start: { x: 100, y: 100 }, end: { x: 300, y: 100 }, weight: 4 },\n            { start: { x: 100, y: 150 }, end: { x: 300, y: 150 }, weight: 1 },\n            { start: { x: 100, y: 200 }, end: { x: 300, y: 200 }, weight: 3 },\n            { start: { x: 100, y: 250 }, end: { x: 300, y: 250 }, weight: 2 },\n            { start: { x: 200, y: 100 }, end: { x: 200, y: 250 }, weight: 5 },\n            { start: { x: 100, y: 100 }, end: { x: 100, y: 250 }, weight: 6 },\n            { start: { x: 300, y: 100 }, end: { x: 300, y: 250 }, weight: 8 },\n        ];\n\n        function drawEdges(visibleEdges = []) {\n            ctx.clearRect(0, 0, canvas.width, canvas.height);\n            for (let edge of edges) {\n                const { start, end, weight } = edge;\n                ctx.beginPath();\n                ctx.moveTo(start.x, start.y);\n                ctx.lineTo(end.x, end.y);\n                ctx.strokeStyle = visibleEdges.includes(edge) ? 'green' : 'gray';\n                ctx.lineWidth = visibleEdges.includes(edge) ? 2 : 1;\n                ctx.stroke();\n                ctx.fillStyle = 'black';\n                ctx.fillText(weight, (start.x + end.x) / 2, (start.y + end.y) / 2 - 5);\n            }\n        }\n\n        function find(parent, i) {\n            if (parent[i] === -1) {\n                return i;\n            }\n            return find(parent, parent[i]);\n        }\n\n        function union(parent, x, y) {\n            parent[x] = y;\n        }\n\n        function runKruskal() {\n            const sortedEdges = edges.sort((a, b) => a.weight - b.weight);\n            const parent = Array(edges.length).fill(-1);\n            const mst = [];\n            const visibleEdges = [];\n\n            for (let edge of sortedEdges) {\n                const x = find(parent, edges.indexOf(edge.start));\n                const y = find(parent, edges.indexOf(edge.end));\n                \n                if (x !== y) {\n                    union(parent, x, y);\n                    mst.push(edge);\n                    visibleEdges.push(edge);\n                }\n            }\n            drawEdges(visibleEdges);\n            document.getElementById('output').innerText = \"Minimum Spanning Tree Edges: \" + mst.map(e => e.weight).join(', ');\n        }\n\n        drawEdges();\n    </script>\n</body>\n</html>"
