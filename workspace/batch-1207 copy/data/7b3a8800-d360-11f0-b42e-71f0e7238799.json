    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Binary Tree Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            background-color: #f4f4f4;\n            padding: 20px;\n        }\n        .node {\n            display: inline-block;\n            padding: 10px;\n            margin: 5px;\n            border: 1px solid #333;\n            border-radius: 5px;\n            background-color: #fff;\n            text-align: center;\n        }\n        .tree {\n            display: flex;\n            justify-content: center;\n            margin: 20px 0;\n        }\n        .level {\n            display: flex;\n            justify-content: center;\n            margin: 10px 0;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Binary Tree Visualization</h1>\n<div id=\"treeContainer\" class=\"tree\"></div>\n<input type=\"number\" id=\"valueInput\" placeholder=\"Enter a number\" />\n<button onclick=\"addNode()\">Add Node</button>\n<button onclick=\"resetTree()\">Reset Tree</button>\n\n<script>\n    class TreeNode {\n        constructor(value) {\n            this.value = value;\n            this.left = null;\n            this.right = null;\n        }\n    }\n\n    class BinaryTree {\n        constructor() {\n            this.root = null;\n        }\n\n        insert(value) {\n            const newNode = new TreeNode(value);\n            if (this.root === null) {\n                this.root = newNode;\n            } else {\n                this.insertNode(this.root, newNode);\n            }\n        }\n\n        insertNode(node, newNode) {\n            if (newNode.value < node.value) {\n                if (node.left === null) {\n                    node.left = newNode;\n                } else {\n                    this.insertNode(node.left, newNode);\n                }\n            } else {\n                if (node.right === null) {\n                    node.right = newNode;\n                } else {\n                    this.insertNode(node.right, newNode);\n                }\n            }\n        }\n\n        drawTree(node, level = 0, levels = []) {\n            if (!node) return;\n\n            if (!levels[level]) {\n                levels[level] = [];\n            }\n            levels[level].push(node);\n\n            this.drawTree(node.left, level + 1, levels);\n            this.drawTree(node.right, level + 1, levels);\n\n            return levels;\n        }\n\n        visualize() {\n            const levels = this.drawTree(this.root);\n            const container = document.getElementById('treeContainer');\n            container.innerHTML = '';\n\n            levels.forEach((level) => {\n                const levelDiv = document.createElement('div');\n                levelDiv.className = 'level';\n                level.forEach(node => {\n                    const nodeDiv = document.createElement('div');\n                    nodeDiv.className = 'node';\n                    nodeDiv.textContent = node.value;\n                    levelDiv.appendChild(nodeDiv);\n                });\n                container.appendChild(levelDiv);\n            });\n        }\n    }\n\n    const tree = new BinaryTree();\n\n    function addNode() {\n        const inputElement = document.getElementById('valueInput');\n        const value = parseInt(inputElement.value, 10);\n        if (!isNaN(value)) {\n            tree.insert(value);\n            tree.visualize();\n            inputElement.value = '';\n        } else {\n            alert(\"Please enter a valid number.\");\n        }\n    }\n\n    function resetTree() {\n        tree.root = null;\n        document.getElementById('treeContainer').innerHTML = '';\n        document.getElementById('valueInput').value = '';\n    }\n</script>\n\n</body>\n</html>"
