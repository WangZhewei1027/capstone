    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Priority Queue Demo</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            background-color: #f5f5f5;\n            margin: 20px;\n        }\n        #queue {\n            list-style-type: none;\n            padding: 0;\n        }\n        #queue li {\n            padding: 10px;\n            margin: 5px 0;\n            background-color: #007bff;\n            color: white;\n            border-radius: 5px;\n        }\n        #controls {\n            margin-bottom: 20px;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Priority Queue Demo</h1>\n<div id=\"controls\">\n    <input type=\"text\" id=\"item\" placeholder=\"Item (e.g. Task1)\" />\n    <input type=\"number\" id=\"priority\" placeholder=\"Priority (1-10)\" min=\"1\" max=\"10\" />\n    <button onclick=\"addToQueue()\">Add to Queue</button>\n    <button onclick=\"removeFromQueue()\">Remove Highest Priority</button>\n</div>\n<h2>Current Priority Queue</h2>\n<ul id=\"queue\"></ul>\n\n<script>\n    class PriorityQueue {\n        constructor() {\n            this.items = [];\n        }\n        \n        enqueue(item, priority) {\n            const queueElement = { item, priority };\n            let added = false;\n\n            for (let i = 0; i < this.items.length; i++) {\n                if (queueElement.priority < this.items[i].priority) {\n                    this.items.splice(i, 0, queueElement);\n                    added = true;\n                    break;\n                }\n            }\n            \n            if (!added) {\n                this.items.push(queueElement);\n            }\n        }\n\n        dequeue() {\n            return this.items.shift();\n        }\n\n        isEmpty() {\n            return this.items.length === 0;\n        }\n\n        printQueue() {\n            return this.items.map(element => `${element.item} (Priority: ${element.priority})`).join(', ');\n        }\n    }\n\n    const priorityQueue = new PriorityQueue();\n\n    function addToQueue() {\n        const itemInput = document.getElementById('item');\n        const priorityInput = document.getElementById('priority');\n\n        const item = itemInput.value;\n        const priority = parseInt(priorityInput.value);\n\n        if (item && priority) {\n            priorityQueue.enqueue(item, priority);\n            itemInput.value = '';\n            priorityInput.value = '';\n            updateQueueDisplay();\n        } else {\n            alert('Both fields must be filled out.');\n        }\n    }\n\n    function removeFromQueue() {\n        if (priorityQueue.isEmpty()) {\n            alert('The queue is empty!');\n            return;\n        }\n        \n        const removed = priorityQueue.dequeue();\n        alert(`Removed: ${removed.item} (Priority: ${removed.priority})`);\n        updateQueueDisplay();\n    }\n\n    function updateQueueDisplay() {\n        const queueList = document.getElementById('queue');\n        queueList.innerHTML = '';\n\n        priorityQueue.items.forEach(element => {\n            const li = document.createElement('li');\n            li.textContent = `${element.item} (Priority: ${element.priority})`;\n            queueList.appendChild(li);\n        });\n    }\n</script>\n\n</body>\n</html>"
