    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Prim's Algorithm Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            background-color: #f4f4f4;\n            margin: 20px;\n            padding: 20px;\n        }\n        canvas {\n            border: 1px solid #000;\n            margin-top: 10px;\n        }\n        #controls {\n            margin-bottom: 10px;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Prim's Algorithm Visualization</h1>\n<p>Click on the canvas to generate a random graph, then click \"Generate MST\" to create a Minimum Spanning Tree (MST) using Prim's Algorithm.</p>\n\n<div id=\"controls\">\n    <button id=\"generateGraph\">Generate Random Graph</button>\n    <button id=\"generateMST\">Generate MST</button>\n</div>\n<canvas id=\"canvas\" width=\"600\" height=\"400\"></canvas>\n\n<script>\n    const canvas = document.getElementById('canvas');\n    const ctx = canvas.getContext('2d');\n    const nodes = [];\n    const edges = [];\n    const radius = 5;  // Node draw radius\n\n    function resetGraph() {\n        nodes.length = 0;\n        edges.length = 0;\n        ctx.clearRect(0, 0, canvas.width, canvas.height);\n    }\n\n    function drawNode(x, y) {\n        ctx.fillStyle = '#3498db';\n        ctx.beginPath();\n        ctx.arc(x, y, radius, 0, 2 * Math.PI);\n        ctx.fill();\n        ctx.closePath();\n        nodes.push({x, y});\n    }\n\n    function drawEdge(x1, y1, x2, y2) {\n        ctx.strokeStyle = '#2ecc71';\n        ctx.beginPath();\n        ctx.moveTo(x1, y1);\n        ctx.lineTo(x2, y2);\n        ctx.stroke();\n        ctx.closePath();\n    }\n\n    function generateRandomGraph() {\n        resetGraph();\n        const numNodes = 10;\n        const maxWidth = canvas.width;\n        const maxHeight = canvas.height;\n\n        for(let i = 0; i < numNodes; i++) {\n            drawNode(Math.random() * maxWidth, Math.random() * maxHeight);\n        }\n\n        // Generate edges\n        for(let i = 0; i < nodes.length; i++) {\n            for(let j = i + 1; j < nodes.length; j++) {\n                const distance = Math.hypot(nodes[i].x - nodes[j].x, nodes[i].y - nodes[j].y);\n                edges.push({from: i, to: j, weight: distance});\n            }\n        }\n    }\n\n    function primsAlgorithm() {\n        const mstEdges = [];\n        const visited = Array(nodes.length).fill(false);\n        const minHeap = [{weight: 0, from: -1, to: 0}]; // Start from first node\n\n        while (minHeap.length) {\n            // Sort minHeap by edge weight\n            minHeap.sort((a, b) => a.weight - b.weight);\n            const {from, to, weight} = minHeap.shift();\n\n            if (visited[to]) continue; // Skip if already visited\n\n            visited[to] = true; // Mark this vertex as visited\n            if (from !== -1) {\n                mstEdges.push({from, to, weight});\n                drawEdge(nodes[from].x, nodes[from].y, nodes[to].x, nodes[to].y);\n            }\n\n            // Add new edges to the heap\n            for (let i = 0; i < edges.length; i++) {\n                const edge = edges[i];\n                if (edge.from === to && !visited[edge.to]) {\n                    minHeap.push(edge);\n                }\n                if (edge.to === to && !visited[edge.from]) {\n                    minHeap.push({...edge, from: edge.to, to: edge.from}); // Reverse direction for undirected\n                }\n            }\n        }\n\n        return mstEdges;\n    }\n\n    document.getElementById('generateGraph').addEventListener('click', generateRandomGraph);\n    document.getElementById('generateMST').addEventListener('click', primsAlgorithm);\n</script>\n\n</body>\n</html>"
