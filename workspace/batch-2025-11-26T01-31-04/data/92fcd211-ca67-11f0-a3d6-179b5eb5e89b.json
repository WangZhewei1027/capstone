    "content": "<html lang=\"en\">\n<head>\n<meta charset=\"UTF-8\" />\n<meta name=\"viewport\" content=\"width=device-width, initial-scale=1\" />\n<title>Stack Demonstration</title>\n<style>\n  body {\n    font-family: Arial, sans-serif;\n    margin: 30px;\n    background: #f9f9f9;\n    color: #333;\n  }\n  h1 {\n    text-align: center;\n    margin-bottom: 0.5em;\n  }\n  .container {\n    max-width: 400px;\n    margin: auto;\n    background: white;\n    border: 2px solid #0077cc;\n    border-radius: 8px;\n    padding: 20px;\n    box-shadow: 0 2px 8px rgb(0 0 0 / 0.1);\n  }\n  label {\n    display: block;\n    margin-bottom: 8px;\n    font-weight: bold;\n  }\n  input[type=\"text\"] {\n    width: calc(100% - 16px);\n    padding: 6px 8px;\n    margin-bottom: 10px;\n    border: 1px solid #bbb;\n    border-radius: 4px;\n    font-size: 1em;\n  }\n  button {\n    background: #0077cc;\n    color: white;\n    border: none;\n    padding: 10px 16px;\n    margin: 0 5px 10px 0;\n    font-size: 1em;\n    cursor: pointer;\n    border-radius: 4px;\n    transition: background-color 0.2s ease;\n  }\n  button:hover:not(:disabled) {\n    background: #005fa3;\n  }\n  button:disabled {\n    background: #ccc;\n    cursor: not-allowed;\n  }\n  #stackDisplay {\n    min-height: 120px;\n    border: 1px solid #ccc;\n    border-radius: 4px;\n    background: #fff;\n    padding: 10px;\n    font-family: monospace;\n    font-size: 1.2em;\n    display: flex;\n    flex-direction: column-reverse;\n    align-items: center;\n  }\n  .stack-element {\n    width: 80%;\n    margin: 4px 0;\n    padding: 8px;\n    background: #0077cc;\n    color: white;\n    text-align: center;\n    border-radius: 4px;\n    box-shadow: 0 2px 5px rgb(0 119 204 / 0.5);\n  }\n  .stack-empty {\n    color: #999;\n    font-style: italic;\n  }\n  #message {\n    min-height: 1.4em;\n    margin-top: 8px;\n    font-weight: bold;\n    color: #d9534f;\n    text-align: center;\n  }\n</style>\n</head>\n<body>\n  <h1>Stack Data Structure Demo</h1>\n  <div class=\"container\">\n    <label for=\"itemInput\">Add Item to Stack</label>\n    <input id=\"itemInput\" type=\"text\" placeholder=\"Enter item...\" />\n    <div>\n      <button id=\"pushBtn\">Push</button>\n      <button id=\"popBtn\" disabled>Pop</button>\n      <button id=\"peekBtn\" disabled>Peek</button>\n      <button id=\"clearBtn\" disabled>Clear</button>\n    </div>\n    <div id=\"message\"></div>\n    <h3>Stack (Top at the top):</h3>\n    <div id=\"stackDisplay\" aria-live=\"polite\" aria-atomic=\"true\">\n      <div class=\"stack-empty\">Stack is empty</div>\n    </div>\n  </div>\n\n<script>\n  // Basic stack class implemented in JS\n  class Stack {\n    constructor() {\n      this.items = [];\n    }\n    push(element) {\n      this.items.push(element);\n    }\n    pop() {\n      if (this.isEmpty()) return null;\n      return this.items.pop();\n    }\n    peek() {\n      if (this.isEmpty()) return null;\n      return this.items[this.items.length - 1];\n    }\n    isEmpty() {\n      return this.items.length === 0;\n    }\n    size() {\n      return this.items.length;\n    }\n    clear() {\n      this.items = [];\n    }\n  }\n\n  // DOM references\n  const itemInput = document.getElementById('itemInput');\n  const pushBtn = document.getElementById('pushBtn');\n  const popBtn = document.getElementById('popBtn');\n  const peekBtn = document.getElementById('peekBtn');\n  const clearBtn = document.getElementById('clearBtn');\n  const stackDisplay = document.getElementById('stackDisplay');\n  const message = document.getElementById('message');\n\n  // Initialize stack\n  const stack = new Stack();\n\n  // Update buttons state depending on stack size\n  function updateButtons() {\n    const empty = stack.isEmpty();\n    popBtn.disabled = empty;\n    peekBtn.disabled = empty;\n    clearBtn.disabled = empty;\n  }\n\n  // Show stack visually\n  function renderStack() {\n    stackDisplay.innerHTML = '';\n    if (stack.isEmpty()) {\n      const emptyDiv = document.createElement('div');\n      emptyDiv.className = 'stack-empty';\n      emptyDiv.textContent = 'Stack is empty';\n      stackDisplay.appendChild(emptyDiv);\n      return;\n    }\n    // Display from top to bottom (last element first)\n    for(let i = stack.items.length - 1; i >= 0; i--) {\n      const elemDiv = document.createElement('div');\n      elemDiv.className = 'stack-element';\n      elemDiv.textContent = stack.items[i];\n      if (i === stack.items.length - 1) {\n        elemDiv.title = 'Top of Stack';\n        elemDiv.style.backgroundColor = '#28a745'; // shape top as green\n      }\n      stackDisplay.appendChild(elemDiv);\n    }\n  }\n\n  // Clear message\n  function clearMessage() {\n    message.textContent = '';\n    message.style.color = '#d9534f'; // red by default (errors)\n  }\n\n  // Event Handlers\n  pushBtn.onclick = () => {\n    clearMessage();\n    const val = itemInput.value.trim();\n    if (!val) {\n      message.textContent = 'Please enter a valid item to push.';\n      return;\n    }\n    stack.push(val);\n    itemInput.value = '';\n    renderStack();\n    updateButtons();\n    message.style.color = '#28a745';\n    message.textContent = `\"${val}\" pushed onto stack.`;\n    itemInput.focus();\n  };\n\n  popBtn.onclick = () => {\n    clearMessage();\n    if (stack.isEmpty()) {\n      message.textContent = 'Stack is empty, cannot pop.';\n      return;\n    }\n    const popped = stack.pop();\n    renderStack();\n    updateButtons();\n    message.style.color = '#28a745';\n    message.textContent = `Popped \"${popped}\" from stack.`;\n  };\n\n  peekBtn.onclick = () => {\n    clearMessage();\n    if (stack.isEmpty()) {\n      message.textContent = 'Stack is empty, nothing to peek.';\n      return;\n    }\n    const top = stack.peek();\n    message.style.color = '#007bff';\n    message.textContent = `Top item: \"${top}\"`;\n  };\n\n  clearBtn.onclick = () => {\n    clearMessage();\n    stack.clear();\n    renderStack();\n    updateButtons();\n    message.style.color = '#28a745';\n    message.textContent = 'Stack cleared.';\n  };\n\n  // Enable push only if input is not empty\n  function togglePushButton() {\n    pushBtn.disabled = itemInput.value.trim() === '';\n  }\n\n  itemInput.addEventListener('input', togglePushButton);\n\n  // Initialize\n  togglePushButton();\n  renderStack();\n  updateButtons();\n\n  // Accessibility: focus input on load\n  itemInput.focus();\n</script>\n</body>\n</html>"
