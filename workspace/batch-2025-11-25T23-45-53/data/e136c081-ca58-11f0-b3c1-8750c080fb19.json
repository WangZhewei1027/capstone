    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Binary Tree Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            text-align: center;\n        }\n        #tree {\n            position: relative;\n            margin: 20px auto;\n            width: 100%;\n            max-width: 800px;\n            height: 400px;\n            border: 1px solid #ccc;\n        }\n        .node {\n            position: absolute;\n            background: #f0f0f0;\n            border: 1px solid #333;\n            border-radius: 5px;\n            padding: 5px 10px;\n            transition: background 0.3s;\n        }\n        .node:hover {\n            background: #e0e0e0;\n        }\n        .line {\n            position: absolute;\n            background: #333;\n            width: 2px;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Binary Tree Visualization</h1>\n<button onclick=\"createBinaryTree()\">Create Binary Tree</button>\n<div id=\"tree\"></div>\n\n<script>\n    class Node {\n        constructor(value) {\n            this.value = value;\n            this.left = null;\n            this.right = null;\n        }\n    }\n\n    class BinaryTree {\n        constructor() {\n            this.root = null;\n        }\n\n        insert(value) {\n            const newNode = new Node(value);\n            if (this.root === null) {\n                this.root = newNode;\n            } else {\n                this._insertNode(this.root, newNode);\n            }\n        }\n\n        _insertNode(node, newNode) {\n            if (newNode.value < node.value) {\n                if (node.left === null) {\n                    node.left = newNode;\n                } else {\n                    this._insertNode(node.left, newNode);\n                }\n            } else {\n                if (node.right === null) {\n                    node.right = newNode;\n                } else {\n                    this._insertNode(node.right, newNode);\n                }\n            }\n        }\n\n        getElements() {\n            let result = [];\n            this._inOrder(this.root, result);\n            return result;\n        }\n\n        _inOrder(node, result) {\n            if (node !== null) {\n                this._inOrder(node.left, result);\n                result.push(node.value);\n                this._inOrder(node.right, result);\n            }\n        }\n    }\n\n    function createBinaryTree() {\n        const values = [30, 15, 45, 10, 20, 40, 50];\n        const tree = new BinaryTree();\n        values.forEach(value => tree.insert(value));\n        drawTree(tree);\n    }\n\n    function drawTree(tree) {\n        const container = document.getElementById('tree');\n        container.innerHTML = '';\n        const elements = tree.getElements();\n        const coordinates = calculateCoordinates(elements);\n        \n        coordinates.forEach((coord, index) => {\n            const nodeDiv = document.createElement('div');\n            nodeDiv.className = 'node';\n            nodeDiv.style.left = `${coord.x}px`;\n            nodeDiv.style.top = `${coord.y}px`;\n            nodeDiv.innerText = elements[index];\n            container.appendChild(nodeDiv);\n            \n            // Draw lines to children\n            if (coord.parentIndex !== null) {\n                drawLine(\n                    coordinates[coord.parentIndex].x + 30,\n                    coordinates[coord.parentIndex].y + 20,\n                    coord.x + 30,\n                    coord.y\n                );\n            }\n        });\n    }\n\n    function drawLine(startX, startY, endX, endY) {\n        const container = document.getElementById('tree');\n        const lineDiv = document.createElement('div');\n        const length = Math.sqrt(Math.pow(endX - startX, 2) + Math.pow(endY - startY, 2));\n        lineDiv.className = 'line';\n        lineDiv.style.height = `${length}px`;\n        const angle = Math.atan2(endY - startY, endX - startX) * (180 / Math.PI);\n        lineDiv.style.transform = `rotate(${angle}deg)`;\n        lineDiv.style.transformOrigin = '0 0';\n        lineDiv.style.left = `${startX}px`;\n        lineDiv.style.top = `${startY}px`;\n        container.appendChild(lineDiv);\n    }\n\n    function calculateCoordinates(elements) {\n        const coords = [];\n        const depth = Math.ceil(Math.log2(elements.length + 1));\n        const width = Math.pow(2, depth) * 60; \n\n        function placeNode(index, depth, pos) {\n            if (index < elements.length) {\n                coords[index] = {\n                    x: pos,\n                    y: depth * 60,\n                    parentIndex: Math.floor((index - 1) / 2)\n                };\n                placeNode(2 * index + 1, depth + 1, pos - width / (Math.pow(2, depth + 1)));\n                placeNode(2 * index + 2, depth + 1, pos + width / (Math.pow(2, depth + 1)));\n            }\n        }\n\n        placeNode(0, 0, width / 2);\n        return coords;\n    }\n</script>\n\n</body>\n</html>"
