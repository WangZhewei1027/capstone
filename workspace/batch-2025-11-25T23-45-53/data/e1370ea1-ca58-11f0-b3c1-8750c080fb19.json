    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Graph Visualization</title>\n    <style>\n        #graph {\n            width: 600px;\n            height: 400px;\n            border: 1px solid #ccc;\n            position: relative;\n        }\n        .node {\n            position: absolute;\n            width: 30px;\n            height: 30px;\n            background-color: #007bff;\n            border-radius: 15px;\n            text-align: center;\n            line-height: 30px;\n            color: white;\n            cursor: pointer;\n        }\n        .edge {\n            position: absolute;\n            width: 2px;\n            background-color: #333;\n        }\n    </style>\n</head>\n<body>\n    <h1>Graph Visualization</h1>\n    <button onclick=\"addNode()\">Add Node</button>\n    <button onclick=\"addEdge()\">Add Edge</button>\n    <button onclick=\"toggleDirected()\">Toggle Directed/Undirected</button>\n    <div id=\"graph\"></div>\n\n    <script>\n        let nodes = [];\n        let edges = [];\n        let directed = true;\n\n        function addNode() {\n            const id = nodes.length;\n            const node = document.createElement('div');\n            node.className = 'node';\n            node.textContent = id;\n            node.style.top = Math.random() * 370 + 'px';\n            node.style.left = Math.random() * 570 + 'px';\n            node.onclick = () => removeNode(id);\n            document.getElementById('graph').appendChild(node);\n            nodes.push(node);\n        }\n\n        function removeNode(id) {\n            nodes[id].remove();\n            nodes[id] = null;\n\n            edges = edges.filter(edge => edge.source !== id && edge.target !== id);\n            redrawEdges();\n        }\n\n        function addEdge() {\n            const sourceId = Math.floor(Math.random() * nodes.length);\n            const targetId = Math.floor(Math.random() * nodes.length);\n            if (sourceId !== targetId && nodes[sourceId] && nodes[targetId]) {\n                edges.push({source: sourceId, target: targetId});\n                redrawEdges();\n            }\n        }\n\n        function toggleDirected() {\n            directed = !directed;\n            redrawEdges();\n        }\n\n        function redrawEdges() {\n            const graph = document.getElementById('graph');\n            const edgeElements = document.querySelectorAll('.edge');\n            edgeElements.forEach(edge => edge.remove());\n            \n            edges.forEach(edge => {\n                const sourceNode = nodes[edge.source];\n                const targetNode = nodes[edge.target];\n\n                if (sourceNode && targetNode) {\n                    const edgeDiv = document.createElement('div');\n                    edgeDiv.className = 'edge';\n                    const x1 = sourceNode.offsetLeft + 15;\n                    const y1 = sourceNode.offsetTop + 15;\n                    const x2 = targetNode.offsetLeft + 15;\n                    const y2 = targetNode.offsetTop + 15;\n\n                    edgeDiv.style.transform = `rotate(${Math.atan2(y2 - y1, x2 - x1)}rad)`;\n                    edgeDiv.style.width = `${Math.sqrt((x2 - x1) ** 2 + (y2 - y1) ** 2)}px`;\n                    edgeDiv.style.left = `${x1}px`;\n                    edgeDiv.style.top = `${y1}px`;\n\n                    graph.appendChild(edgeDiv);\n                }\n            });\n        }\n    </script>\n</body>\n</html>"
