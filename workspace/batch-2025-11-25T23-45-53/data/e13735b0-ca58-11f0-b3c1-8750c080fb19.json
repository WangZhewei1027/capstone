    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Adjacency Matrix Demonstration</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n        }\n        input[type=\"number\"] {\n            width: 50px;\n        }\n        table {\n            border-collapse: collapse;\n            margin-top: 20px;\n        }\n        th, td {\n            border: 1px solid #000;\n            padding: 10px;\n            text-align: center;\n        }\n        th {\n            background-color: #f2f2f2;\n        }\n        .hidden {\n            display: none;\n        }\n    </style>\n</head>\n<body>\n\n    <h1>Adjacency Matrix Demonstration</h1>\n    <label for=\"numVertices\">Number of vertices:</label>\n    <input type=\"number\" id=\"numVertices\" min=\"1\" value=\"3\">\n    <button id=\"createMatrix\">Create Adjacency Matrix</button>\n\n    <div id=\"matrixContainer\" class=\"hidden\">\n        <h2>Adjacency Matrix</h2>\n        <table id=\"adjMatrix\"></table>\n        <button id=\"submitMatrix\">Submit Matrix</button>\n    </div>\n\n    <div id=\"resultContainer\" class=\"hidden\">\n        <h2>Matrix Result</h2>\n        <pre id=\"matrixResult\"></pre>\n    </div>\n\n    <script>\n        document.getElementById('createMatrix').onclick = function() {\n            const numVertices = parseInt(document.getElementById('numVertices').value);\n            const adjMatrix = createMatrix(numVertices);\n            displayMatrix(adjMatrix);\n            document.getElementById('matrixContainer').classList.remove('hidden');\n        };\n\n        function createMatrix(size) {\n            const matrix = [];\n            for (let i = 0; i < size; i++) {\n                matrix[i] = new Array(size).fill(0);\n            }\n            return matrix;\n        }\n\n        function displayMatrix(matrix) {\n            const table = document.getElementById('adjMatrix');\n            table.innerHTML = ''; // Clear previous matrix if any\n\n            // Create table header\n            let headerRow = '<tr><th></th>';\n            for (let i = 0; i < matrix.length; i++) {\n                headerRow += `<th>${i}</th>`;\n            }\n            headerRow += '</tr>';\n            table.innerHTML += headerRow;\n\n            // Create table rows\n            for (let i = 0; i < matrix.length; i++) {\n                let row = `<tr><th>${i}</th>`;\n                for (let j = 0; j < matrix[i].length; j++) {\n                    row += `<td><input type=\"number\" value=\"${matrix[i][j]}\" min=\"0\" max=\"1\" /></td>`;\n                }\n                row += '</tr>';\n                table.innerHTML += row;\n            }\n        }\n\n        document.getElementById('submitMatrix').onclick = function() {\n            const adjMatrixInputs = document.querySelectorAll(\"#adjMatrix input[type='number']\");\n            const matrixSize = Math.sqrt(adjMatrixInputs.length);\n            const resultMatrix = [];\n\n            for (let i = 0; i < matrixSize; i++) {\n                resultMatrix[i] = [];\n                for (let j = 0; j < matrixSize; j++) {\n                    const index = i * matrixSize + j;\n                    resultMatrix[i][j] = parseInt(adjMatrixInputs[index].value);\n                }\n            }\n\n            document.getElementById('matrixResult').textContent = JSON.stringify(resultMatrix);\n            document.getElementById('resultContainer').classList.remove('hidden');\n        };\n    </script>\n</body>\n</html>"
