    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Dijkstra's Algorithm Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n        }\n        #canvas {\n            border: 1px solid #ccc;\n            margin-top: 20px;\n        }\n        .button {\n            padding: 10px 20px;\n            background-color: #007BFF;\n            color: white;\n            border: none;\n            cursor: pointer;\n            margin-right: 10px;\n        }\n        .button:hover {\n            background-color: #0056b3;\n        }\n    </style>\n</head>\n<body>\n    <h1>Dijkstra's Algorithm Visualization</h1>\n    <p>\n        Click the button below to visualize Dijkstra's algorithm on the graph.\n    </p>\n    <button class=\"button\" id=\"start\">Start Dijkstra's Algorithm</button>\n    <canvas id=\"canvas\" width=\"600\" height=\"400\"></canvas>\n    <script>\n        const canvas = document.getElementById('canvas');\n        const ctx = canvas.getContext('2d');\n\n        const nodes = [\n            { id: 'A', x: 50, y: 50 },\n            { id: 'B', x: 200, y: 50 },\n            { id: 'C', x: 350, y: 50 },\n            { id: 'D', x: 50, y: 200 },\n            { id: 'E', x: 200, y: 200 },\n            { id: 'F', x: 350, y: 200 },\n        ];\n\n        const edges = [\n            { from: 'A', to: 'B', weight: 1 },\n            { from: 'A', to: 'D', weight: 4 },\n            { from: 'B', to: 'C', weight: 2 },\n            { from: 'B', to: 'E', weight: 5 },\n            { from: 'C', to: 'F', weight: 3 },\n            { from: 'D', to: 'E', weight: 1 },\n            { from: 'E', to: 'F', weight: 2 },\n        ];\n\n        const startNode = 'A';\n        const endNode = 'F';\n\n        function drawGraph() {\n            ctx.clearRect(0, 0, canvas.width, canvas.height);\n            edges.forEach(edge => {\n                const fromNode = nodes.find(n => n.id === edge.from);\n                const toNode = nodes.find(n => n.id === edge.to);\n                ctx.beginPath();\n                ctx.moveTo(fromNode.x, fromNode.y);\n                ctx.lineTo(toNode.x, toNode.y);\n                ctx.strokeStyle = '#ccc';\n                ctx.stroke();\n                const midX = (fromNode.x + toNode.x) / 2;\n                const midY = (fromNode.y + toNode.y) / 2;\n                ctx.fillText(edge.weight, midX, midY);\n            });\n\n            nodes.forEach(node => {\n                ctx.beginPath();\n                ctx.arc(node.x, node.y, 20, 0, Math.PI * 2);\n                ctx.fillStyle = '#fff';\n                ctx.fill();\n                ctx.strokeStyle = '#000';\n                ctx.stroke();\n                ctx.fillText(node.id, node.x - 5, node.y + 5);\n            });\n        }\n\n        function dijkstra() {\n            const distances = {};\n            const previousNodes = {};\n            const unvisitedNodes = new Set(nodes.map(node => node.id));\n\n            nodes.forEach(node => {\n                distances[node.id] = Infinity;\n                previousNodes[node.id] = null;\n            });\n            distances[startNode] = 0;\n\n            while (unvisitedNodes.size > 0) {\n                const closestNode = [...unvisitedNodes].reduce((closest, nodeId) => {\n                    return distances[nodeId] < distances[closest] ? nodeId : closest;\n                });\n                \n                if (distances[closestNode] === Infinity) break; // All remaining nodes are inaccessible\n\n                unvisitedNodes.delete(closestNode);\n\n                const neighbors = edges.filter(edge => edge.from === closestNode);\n                neighbors.forEach(edge => {\n                    const alt = distances[closestNode] + edge.weight;\n                    if (alt < distances[edge.to]) {\n                        distances[edge.to] = alt;\n                        previousNodes[edge.to] = closestNode;\n                    }\n                });\n            }\n\n            return previousNodes;\n        }\n\n        function reconstructPath(previousNodes) {\n            const path = [];\n            let currentNode = endNode;\n            while (currentNode) {\n                path.unshift(currentNode);\n                currentNode = previousNodes[currentNode];\n            }\n            return path;\n        }\n\n        document.getElementById('start').addEventListener('click', () => {\n            const previousNodes = dijkstra();\n            const path = reconstructPath(previousNodes);\n\n            path.forEach((nodeId, index) => {\n                const node = nodes.find(n => n.id === nodeId);\n                if (index < path.length - 1) {\n                    ctx.beginPath();\n                    ctx.arc(node.x, node.y, 20, 0, Math.PI * 2);\n                    ctx.fillStyle = 'lightgreen';\n                    ctx.fill();\n                    ctx.stroke();\n                } else {\n                    ctx.beginPath();\n                    ctx.arc(node.x, node.y, 20, 0, Math.PI * 2);\n                    ctx.fillStyle = 'lightcoral';\n                    ctx.fill();\n                    ctx.stroke();\n                }\n            });\n            drawGraph();\n        });\n\n        drawGraph();\n    </script>\n</body>\n</html>"
