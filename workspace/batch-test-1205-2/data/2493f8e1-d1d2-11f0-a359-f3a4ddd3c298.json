    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Priority Queue Demo</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n        }\n        #queue {\n            margin-top: 20px;\n            border: 1px solid #ccc;\n            padding: 10px;\n        }\n        .queue-item {\n            padding: 5px;\n            border: 1px solid #aaa;\n            margin: 5px 0;\n            display: flex;\n            justify-content: space-between;\n        }\n    </style>\n</head>\n<body>\n    <h1>Priority Queue Demonstration</h1>\n    <input type=\"text\" id=\"element\" placeholder=\"Enter element\">\n    <input type=\"number\" id=\"priority\" placeholder=\"Enter priority\" min=\"1\">\n    <button onclick=\"enqueue()\">Add to Queue</button>\n    <button onclick=\"dequeue()\">Remove from Queue</button>\n\n    <div id=\"queue\">\n        <h3>Priority Queue</h3>\n        <div id=\"queueItems\"></div>\n    </div>\n\n    <script>\n        class PriorityQueue {\n            constructor() {\n                this.items = [];\n            }\n\n            enqueue(element, priority) {\n                const queueElement = { element, priority };\n                let added = false;\n\n                for (let i = 0; i < this.items.length; i++) {\n                    if (queueElement.priority < this.items[i].priority) {\n                        this.items.splice(i, 0, queueElement);\n                        added = true;\n                        break;\n                    }\n                }\n\n                if (!added) {\n                    this.items.push(queueElement);\n                }\n                this.updateQueueDisplay();\n            }\n        \n            dequeue() {\n                if (this.isEmpty()) {\n                    return \"Underflow\";\n                }\n                const removedElement = this.items.shift(); // Remove the element with the highest priority\n                this.updateQueueDisplay();\n                return removedElement;\n            }\n\n            isEmpty() {\n                return this.items.length === 0;\n            }\n\n            updateQueueDisplay() {\n                const queueDisplay = document.getElementById('queueItems');\n                queueDisplay.innerHTML = '';\n                this.items.forEach(item => {\n                    const div = document.createElement('div');\n                    div.className = 'queue-item';\n                    div.innerHTML = `${item.element} (Priority: ${item.priority})`;\n                    queueDisplay.appendChild(div);\n                });\n            }\n        }\n\n        const pq = new PriorityQueue();\n\n        function enqueue() {\n            const element = document.getElementById('element').value;\n            const priority = parseInt(document.getElementById('priority').value);\n            if (element && !isNaN(priority)) {\n                pq.enqueue(element, priority);\n                document.getElementById('element').value = '';\n                document.getElementById('priority').value = '';\n            } else {\n                alert('Please enter a valid element and priority.');\n            }\n        }\n\n        function dequeue() {\n            const result = pq.dequeue();\n            if (result === \"Underflow\") {\n                alert('Queue is empty!');\n            }\n        }\n    </script>\n</body>\n</html>"
