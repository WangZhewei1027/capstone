e.left = newNode;\n                } else {\n                    this.insertNode(node.left, newNode);\n                }\n            } else {\n                if (!node.right) {\n                    node.right = newNode;\n                } else {\n                    this.insertNode(node.right, newNode);\n                }\n            }\n        }\n\n        // Function to visualize the tree\n        visualize(node, level = 0, x = 0, y = 0) {\n            const treeDiv = document.getElementById('tree');\n            if (node) {\n                // Create a node element\n                const nodeDiv = document.createElement('div');\n                nodeDiv.classList.add('node');\n                nodeDiv.textContent = node.value;\n                treeDiv.appendChild(nodeDiv);\n                \n                // Positioning the node\n                nodeDiv.style.marginLeft = `${x}px`;\n                nodeDiv.style.marginTop = `${y}px`;\n\n                // Draw lines to the children\n                if (node.left) {\n                    this.drawLine(treeDiv, x + 20, y + 40, x, y + 70);\n                    this.visualize(node.left, level + 1, x - 40, y + 100);\n                }\n                if (node.right) {\n                    this.drawLine(treeDiv, x + 30, y + 40, x + 50, y + 70);\n                    this.visualize(node.right, level + 1, x + 40, y + 100);\n                }\n            }\n        }\n\n        // Method to draw lines between nodes\n        drawLine(parent, startX, startY, endX, endY) {\n            const line = document.createElement('div');\n            line.classList.add('line');\n            const length = Math.sqrt((endX - startX) ** 2 + (endY - startY) ** 2);\n            line.style.height = `${length}px`;\n            const angle = Math.atan2(endY - startY, endX - startX) * (180 / Math.PI);\n            line.style.transform = `translate(${startX}px, ${startY}px) rotate(${angle}deg)`;\n            parent.appendChild(line);\n        }\n    }\n\n    // Create a binary tree and insert nodes\n    const tree = new BinaryTree();\n    const values = [10, 5, 15, 3, 7, 12, 18];\n\n    values.forEach(value => tree.insert(value));\n\n    // Visualize the binary tree\n    tree.visualize(tree.root);\n</script>\n\n</body>\n</html>"
