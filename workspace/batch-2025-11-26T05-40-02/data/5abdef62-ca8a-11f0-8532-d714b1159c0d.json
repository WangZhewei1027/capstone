    "content": "<html>\n<head>\n    <title>Union-Find (Disjoint Set)</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        #graph {\n            border: 1px solid #ccc;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Union-Find (Disjoint Set)</h1>\n\n<div id=\"graph\"></div>\n\n<script>\n    let graph = {\n        'A': ['B', 'C'],\n        'B': ['A', 'D', 'E'],\n        'C': ['A', 'F'],\n        'D': ['B'],\n        'E': ['B', 'F'],\n        'F': ['C', 'E']\n    };\n\n    function find(node) {\n        if (graph[node] === node) {\n            return node;\n        } else {\n            return graph[node] = find(graph[node]);\n        }\n    }\n\n    function union(node1, node2) {\n        let root1 = find(node1);\n        let root2 = find(node2);\n\n        if (root1 !== root2) {\n            graph[root1] = root2;\n        }\n    }\n\n    let nodes = Object.keys(graph);\n    let colors = Array(nodes.length).fill(0).map((_, i) => i);\n\n    let graphDiv = document.getElementById('graph');\n    graphDiv.innerHTML = '';\n\n    for (let node of nodes) {\n        let nodeDiv = document.createElement('div');\n        nodeDiv.className = 'node';\n        nodeDiv.style.width = '50px';\n        nodeDiv.style.height = '50px';\n        nodeDiv.style.border = '1px solid #ccc';\n        nodeDiv.style.margin = '10px';\n        nodeDiv.style.position = 'absolute';\n        nodeDiv.style.top = '50%';\n        nodeDiv.style.left = '50%';\n        nodeDiv.style.transform = 'translate(-50%, -50%)';\n\n        if (colors[node] === 0) {\n            nodeDiv.style.background = 'lightblue';\n        } else {\n            nodeDiv.style.background = 'lightgreen';\n        }\n\n        graphDiv.appendChild(nodeDiv);\n    }\n\n    for (let node of nodes) {\n        for (let neighbor of graph[node]) {\n            let neighborNodeDiv = document.createElement('div');\n            neighborNodeDiv.className = 'node';\n            neighborNodeDiv.style.width = '50px';\n            neighborNodeDiv.style.height = '50px';\n            neighborNodeDiv.style.border = '1px solid #ccc';\n            neighborNodeDiv.style.margin = '10px';\n            neighborNodeDiv.style.position = 'absolute';\n            neighborNodeDiv.style.top = '50%';\n            neighborNodeDiv.style.left = '50%';\n            neighborNodeDiv.style.transform = 'translate(-50%, -50%)';\n\n            if (colors[neighbor] === 0) {\n                neighborNodeDiv.style.background = 'lightblue';\n            } else {\n                neighborNodeDiv.style.background = 'lightgreen';\n            }\n\n            neighborNodeDiv.style.border = '1px solid #ccc';\n            neighborNodeDiv.style.width = '50px';\n            neighborNodeDiv.style.height = '50px';\n            neighborNodeDiv.style.margin = '10px';\n            neighborNodeDiv.style.position = 'absolute';\n            neighborNodeDiv.style.top = '50%';\n            neighborNodeDiv.style.left = '50%';\n            neighborNodeDiv.style.transform = 'translate(-50%, -50%)';\n\n            graphDiv.appendChild(neighborNodeDiv);\n\n            union(node, neighbor);\n        }\n    }\n</script>\n\n</body>\n</html>"
