    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Topological Sort</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        .container {\n            width: 80%;\n            margin: 40px auto;\n            padding: 20px;\n            background-color: #f9f9f9;\n            border: 1px solid #ddd;\n            border-radius: 10px;\n            box-shadow: 0 0 10px rgba(0,0,0,0.1);\n        }\n        .header {\n            text-align: center;\n            margin-bottom: 20px;\n        }\n        .footer {\n            text-align: center;\n            margin-top: 20px;\n        }\n    </style>\n</head>\n<body>\n    <div class=\"container\">\n        <h2>Topological Sort</h2>\n        <div class=\"header\">Given Graph:</div>\n        <div class=\"footer\"></div>\n        <input type=\"text\" id=\"graph\" placeholder=\"Enter graph data (nodes and edges separated by comma)\">\n        <button id=\"sort-btn\">Sort</button>\n        <div class=\"footer\"></div>\n        <p id=\"result\"></p>\n\n        <script>\n            function topologicalSort(graph) {\n                const visited = {};\n                const stack = [];\n\n                function dfs(node) {\n                    visited[node] = true;\n                    for (const neighbor of graph[node]) {\n                        if (!visited[neighbor]) {\n                            dfs(neighbor);\n                        } else if (stack.includes(neighbor)) {\n                            stack.splice(stack.indexOf(neighbor), 1);\n                        }\n                    }\n                    stack.push(node);\n                }\n\n                for (const node in graph) {\n                    if (!visited[node]) {\n                        dfs(node);\n                    }\n                }\n\n                return stack;\n            }\n\n            const graph = {\n                'A': ['B', 'C'],\n                'B': ['A', 'D', 'E'],\n                'C': ['A', 'F'],\n                'D': [],\n                'E': ['B', 'F'],\n                'F': ['C', 'E']\n            };\n\n            document.getElementById('sort-btn').addEventListener('click', () => {\n                const graphData = document.getElementById('graph').value;\n                const sortedNodes = topologicalSort(graphData);\n                const sortedGraph = sortedNodes.map(node => graph[node]).join(' -> ');\n                document.getElementById('result').innerHTML = 'Topological Sort: ' + sortedGraph;\n            });\n        </script>\n    </div>\n</body>\n</html>"
