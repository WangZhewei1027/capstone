    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Binary Search</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            text-align: center;\n        }\n        #binary-search {\n            width: 50%;\n            margin: 40px auto;\n            padding: 20px;\n            border: 1px solid #ccc;\n            border-radius: 10px;\n            box-shadow: 0 0 10px rgba(0,0,0,0.1);\n        }\n        #binary-search input[type=\"number\"] {\n            width: 100%;\n            height: 30px;\n            margin-bottom: 10px;\n            padding: 5px;\n            box-sizing: border-box;\n        }\n    </style>\n</head>\n<body>\n    <div id=\"binary-search\">\n        <h2>Binary Search</h2>\n        <input type=\"number\" id=\"search-input\" placeholder=\"Enter a number\">\n        <button id=\"search-button\">Search</button>\n        <div id=\"result\"></div>\n    </div>\n\n    <script>\n        const searchInput = document.getElementById('search-input');\n        const searchButton = document.getElementById('search-button');\n        const resultDiv = document.getElementById('result');\n\n        searchButton.addEventListener('click', () => {\n            const target = searchInput.value;\n            const array = Array.from({ length: 10000 }, (_, i) => i);\n\n            const low = 0;\n            const high = array.length - 1;\n\n            while (low <= high) {\n                const mid = Math.floor((low + high) / 2);\n                const midValue = array[mid];\n\n                if (midValue === target) {\n                    resultDiv.innerText = `Found: ${midValue}`;\n                    return;\n                } else if (midValue < target) {\n                    low = mid + 1;\n                } else {\n                    high = mid - 1;\n                }\n            }\n\n            resultDiv.innerText = `Not Found: ${target}`;\n        });\n    </script>\n</body>\n</html>"
