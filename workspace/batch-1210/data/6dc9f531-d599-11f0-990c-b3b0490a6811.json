    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Hash Table</title>\n    <style>\n        table {\n            border-collapse: collapse;\n        }\n        th, td {\n            border: 1px solid black;\n            padding: 10px;\n        }\n    </style>\n</head>\n<body>\n    <h1>Hash Table</h1>\n    <form id=\"hash-table-form\">\n        <label for=\"key\">Key:</label>\n        <input type=\"text\" id=\"key\" required><br><br>\n        <label for=\"value\">Value:</label>\n        <input type=\"text\" id=\"value\" required><br><br>\n        <button id=\"add-button\">Add</button>\n        <button id=\"clear-button\">Clear</button>\n    </form>\n    <div id=\"hash-table-container\"></div>\n\n    <script>\n        let hashTable = {};\n\n        document.getElementById('add-button').addEventListener('click', function() {\n            let key = document.getElementById('key').value;\n            let value = document.getElementById('value').value;\n\n            if (key in hashTable) {\n                hashTable[key] += value;\n            } else {\n                hashTable[key] = value;\n            }\n\n            document.getElementById('hash-table-container').innerHTML += `<tr><td>${key}</td><td>${hashTable[key]}</td></tr>`;\n        });\n\n        document.getElementById('clear-button').addEventListener('click', function() {\n            for (let key in hashTable) {\n                hashTable[key] = 0;\n            }\n\n            document.getElementById('hash-table-container').innerHTML = '';\n        });\n    </script>\n</body>\n</html>"
