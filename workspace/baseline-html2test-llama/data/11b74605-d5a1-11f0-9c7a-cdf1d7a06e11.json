n\n        function addNode(value) {\n            if (currentNode) {\n                currentNode.appendChild(createNode(value));\n            } else {\n                treeNodeLeft.appendChild(createNode(value));\n                treeNodeRight.appendChild(createNode(value));\n                treeNode.appendChild(createNode(value));\n            }\n        }\n\n        function createNode(value) {\n            let node = document.createElement('div');\n            node.className = 'node';\n            node.innerHTML = value + ' ';\n            return node;\n        }\n\n        function removeNode(value) {\n            if (currentNode) {\n                if (currentNode.value === value) {\n                    currentNode = null;\n                    treeNodeLeft.appendChild(createNode(value));\n                    treeNodeRight.appendChild(createNode(value));\n                    treeNode.appendChild(createNode(value));\n                } else {\n                    currentNode = currentNode.parentNode;\n                    currentNode.removeChild(currentNode);\n                }\n            }\n        }\n\n        function clearTree() {\n            treeNodeLeft.innerHTML = '';\n            treeNodeRight.innerHTML = '';\n            treeNode.appendChild(createNode(''));\n        }\n\n        addButton.addEventListener('click', () => {\n            let value = valueInput.value;\n            addNode(value);\n            valueInput.value = '';\n        });\n\n        removeButton.addEventListener('click', () => {\n            removeNode(valueInput.value);\n            valueInput.value = '';\n        });\n\n        clearButton.addEventListener('click', clearTree);\n\n        function printTree(node) {\n            if (node) {\n                treeNodeLeft.appendChild(createNode(node.value));\n                treeNodeRight.appendChild(createNode(node.value));\n                treeNode.appendChild(createNode(node.value));\n                printTree(node.left);\n        