\n            <input type=\"number\" id=\"feature3\" name=\"feature3\" value=\"30\">\n            <br><br>\n            <label for=\"target\">Target Variable:</label>\n            <input type=\"number\" id=\"target\" name=\"target\" value=\"50\">\n            <br><br>\n            <button id=\"predict-button\">Predict</button>\n        </div>\n        <div class=\"button-container\">\n            <button id=\"reset-button\">Reset</button>\n        </div>\n    </div>\n\n    <script>\n        let x = [];\n        let y = [];\n        let target;\n        let prediction = 0;\n\n        document.getElementById('feature1').addEventListener('input', (e) => {\n            x.push(e.target.value);\n            updateTarget();\n        });\n\n        document.getElementById('feature2').addEventListener('input', (e) => {\n            x.push(e.target.value);\n            updateTarget();\n        });\n\n        document.getElementById('feature3').addEventListener('input', (e) => {\n            x.push(e.target.value);\n            updateTarget();\n        });\n\n        document.getElementById('target').addEventListener('input', (e) => {\n            target = e.target.value;\n            updateTarget();\n        });\n\n        document.getElementById('predict-button').addEventListener('click', () => {\n            let features = x.map(Number);\n            let targetValue = Number(target);\n\n            // Linear Regression formula\n            let sumX = features.reduce((acc, curr) => acc + curr, 0);\n            let sumY = targetValue;\n            let sumX2 = features.reduce((acc, curr) => acc + curr * curr, 0);\n            let sumXY = features.reduce((acc, curr) => acc + curr * targetValue, 0);\n\n            let slope = (sumXY - (sumX * sumY) / x.length) / (sumX2 - (sumX * sumX) / x.length);\n            let intercept = (sumY - slope * sumX) / x.length;\n\n            prediction = slope * x[0] + intercept;\n\n            alert(`Your predicted value is: ${prediction}`);\n  