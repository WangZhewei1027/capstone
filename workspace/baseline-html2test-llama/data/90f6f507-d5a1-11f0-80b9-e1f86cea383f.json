    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>BFS Algorithm</title>\n    <style>\n        #graph {\n            border: 1px solid black;\n        }\n    </style>\n</head>\n<body>\n    <h1>BFS Algorithm</h1>\n    <input id=\"graph\" type=\"text\" placeholder=\"Enter vertices separated by space\">\n    <button id=\"bfs-button\">Find BFS</button>\n    <div id=\"graph-container\"></div>\n\n    <script>\n        let graph = {};\n        let visited = {};\n        let queue = [];\n\n        function addEdge(u, v) {\n            if (!graph[u]) graph[u] = [];\n            graph[u].push(v);\n        }\n\n        function bfs(start) {\n            visited[start] = true;\n            queue.push(start);\n\n            while (queue.length > 0) {\n                let vertex = queue.shift();\n\n                console.log(vertex);\n\n                for (let neighbor of graph[vertex]) {\n                    if (!visited[neighbor]) {\n                        visited[neighbor] = true;\n                        queue.push(neighbor);\n                    }\n                }\n            }\n        }\n\n        function displayGraph() {\n            let graphElement = document.getElementById(\"graph-container\");\n            graphElement.innerHTML = \"\";\n\n            for (let vertex in graph) {\n                let row = document.createElement(\"div\");\n                row.style.display = \"flex\";\n                row.style.justifyContent = \"space-around\";\n                row.style.padding = \"10px\";\n\n                for (let neighbor of graph[vertex]) {\n                    let cell = document.createElement(\"div\");\n                    cell.style.width = \"20px\";\n                    cell.style.height = \"20px\";\n                    cell.style.background = \"lightgray\";\n                    cell.style.border = \"1px solid black\";\n                    cell.style.padding = \"2px\";\n                    cell.style.margin = \"2px\";\n\n                    cell.textContent = neighbor;\n\n                    row.appendChild(cell);\n                }\n\n                graphElement.appendChild(row);\n            }\n        }\n\n        function main() {\n            let input = document.getElementById(\"graph\").value;\n            let start = input.split(\" \").pop();\n\n            if (!graph[start]) {\n                console.log(\"Graph does not exist.\");\n            } else {\n                addEdge(start, 0);\n                addEdge(0, 1);\n                addEdge(0, 2);\n                addEdge(1, 3);\n                addEdge(2, 3);\n                addEdge(3, 4);\n\n                bfs(start);\n            }\n        }\n\n        document.getElementById(\"bfs-button\").addEventListener(\"click\", main);\n        displayGraph();\n    </script>\n</body>\n</html>"
