    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Array Demo</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n    </style>\n</head>\n<body>\n    <h1>Array Demo</h1>\n    <input type=\"text\" id=\"input\" placeholder=\"Enter elements to create an array\">\n    <button id=\"add-btn\">Add Element</button>\n    <button id=\"clear-btn\">Clear Array</button>\n    <div id=\"array\"></div>\n\n    <script>\n        let array = [];\n        const input = document.getElementById('input');\n        const addBtn = document.getElementById('add-btn');\n        const clearBtn = document.getElementById('clear-btn');\n        const arrayDiv = document.getElementById('array');\n\n        addBtn.addEventListener('click', addElement);\n        clearBtn.addEventListener('click', clearArray);\n\n        function addElement() {\n            const element = input.value;\n            array.push(element);\n            renderArray();\n            input.value = '';\n        }\n\n        function clearArray() {\n            array = [];\n            arrayDiv.innerHTML = '';\n            renderArray();\n        }\n\n        function renderArray() {\n            arrayDiv.innerHTML = '';\n            array.forEach((element, index) => {\n                const elementDiv = document.createElement('div');\n                elementDiv.textContent = `${index + 1}. ${element}`;\n                arrayDiv.appendChild(elementDiv);\n            });\n        }\n    </script>\n</body>\n</html>"
