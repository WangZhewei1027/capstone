    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Adjacency List</title>\n    <style>\n        table {\n            border-collapse: collapse;\n            width: 100%;\n        }\n        \n        th, td {\n            border: 1px solid #ddd;\n            padding: 10px;\n            text-align: left;\n        }\n        \n        th {\n            background-color: #f0f0f0;\n        }\n    </style>\n</head>\n<body>\n    <h1>Adjacency List</h1>\n    <input id=\"name\" type=\"text\" placeholder=\"Enter a node name\">\n    <button id=\"add-node-btn\">Add Node</button>\n    <button id=\"remove-node-btn\">Remove Node</button>\n    <button id=\"add-edge-btn\">Add Edge</button>\n    <button id=\"remove-edge-btn\">Remove Edge</button>\n    <button id=\"print-edge-btn\">Print Edges</button>\n    <button id=\"clear-btn\">Clear</button>\n    <table id=\"adjacency-list\"></table>\n\n    <script>\n        let nodes = {};\n        let edges = {};\n\n        function addNode(name) {\n            if (!nodes[name]) {\n                nodes[name] = {};\n                edges[name] = [];\n            }\n            console.log(`Node ${name} added.`);\n        }\n\n        function removeNode(name) {\n            if (nodes[name]) {\n                delete nodes[name];\n                edges[name] = [];\n                console.log(`Node ${name} removed.`);\n            } else {\n                console.log(`Node ${name} not found.`);\n            }\n        }\n\n        function addEdge(from, to) {\n            if (nodes[from] && nodes[to]) {\n                edges[from].push(to);\n                console.log(`Edge ${from} -> ${to} added.`);\n            } else {\n                console.log(`Either ${from} or ${to} not found.`);\n            }\n        }\n\n        function removeEdge(from, to) {\n            if (nodes[from] && nodes[to]) {\n                let index = edges[from].indexOf(to);\n                if (index !== -1) {\n                    edges[from].splice(index, 1);\n                    console.log(`Edge ${from} -> ${to} removed.`);\n                } else {\n                    console.log(`Either ${from} or ${to} not found.`);\n                }\n            } else {\n                console.log(`Either ${from} or ${to} not found.`);\n            }\n        }\n\n        function printEdges() {\n            console.log(\"Edges:\");\n            for (let node in edges) {\n                console.log(`${node}: ${edges[node].join(\", \")}`);\n            }\n        }\n\n        document.getElementById(\"add-node-btn\").addEventListener(\"click\", addNode);\n        document.getElementById(\"remove-node-btn\").addEventListener(\"click\", removeNode);\n        document.getElementById(\"add-edge-btn\").addEventListener(\"click\", addEdge);\n        document.getElementById(\"remove-edge-btn\").addEventListener(\"click\", removeEdge);\n        document.getElementById(\"print-edge-btn\").addEventListener(\"click\", printEdges);\n        document.getElementById(\"clear-btn\").addEventListener(\"click\", () => {\n            nodes = {};\n            edges = {};\n            printEdges();\n        });\n    </script>\n</body>\n</html>"
