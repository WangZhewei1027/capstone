    "role": "assistant",
    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Merge Sort</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        #output {\n            margin-top: 20px;\n        }\n    </style>\n</head>\n<body>\n    <h1>Merge Sort</h1>\n    <form id=\"sort-form\">\n        <label for=\"array\">Enter numbers separated by spaces:</label>\n        <input type=\"text\" id=\"array\" name=\"array\" required>\n        <button type=\"submit\">Sort</button>\n    </form>\n    <div id=\"output\"></div>\n\n    <script>\n        function mergeSort(arr) {\n            if (arr.length <= 1) {\n                return arr;\n            }\n            const mid = Math.floor(arr.length / 2);\n            const left = mergeSort(arr.slice(0, mid));\n            const right = mergeSort(arr.slice(mid));\n            return merge(left, right);\n        }\n\n        function merge(left, right) {\n            const merged = [];\n            let leftIndex = 0;\n            let rightIndex = 0;\n            while (leftIndex < left.length && rightIndex < right.length) {\n                if (left[leftIndex] <= right[rightIndex]) {\n                    merged.push(left[leftIndex]);\n                    leftIndex++;\n                } else {\n                    merged.push(right[rightIndex]);\n                    rightIndex++;\n                }\n            }\n            merged.push(...left.slice(leftIndex));\n            merged.push(...right.slice(rightIndex));\n            return merged;\n        }\n\n        document.getElementById('sort-form').addEventListener('submit', (e) => {\n            e.preventDefault();\n            const array = document.getElementById('array').value.split(' ').map(Number);\n            const sortedArray = mergeSort(array);\n            const output = document.getElementById('output');\n            output.innerHTML = 'Sorted array: ';\n            sortedArray.forEach((num) => {\n                output.innerHTML += num + ' ';\n            });\n            output.innerHTML += '\\n';\n        });\n    </script>\n</body>\n</html>"
