    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Array Demo</title>\n    <style>\n        #array-container {\n            border: 1px solid #ccc;\n            padding: 10px;\n            border-radius: 10px;\n            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n        }\n    </style>\n</head>\n<body>\n    <h1>Array Demo</h1>\n    <div id=\"array-container\">\n        <h2>Array Length: <span id=\"array-length\">0</span></h2>\n        <button id=\"generate-array-button\">Generate Array</button>\n        <button id=\"clear-array-button\">Clear Array</button>\n        <div id=\"array-list\"></div>\n    </div>\n\n    <script>\n        let arrayLength = 0;\n\n        const generateArrayButton = document.getElementById('generate-array-button');\n        const clearArrayButton = document.getElementById('clear-array-button');\n        const arrayList = document.getElementById('array-list');\n\n        generateArrayButton.addEventListener('click', () => {\n            const arrayLengthInput = document.getElementById('array-length');\n            arrayLength = parseInt(arrayLengthInput.value);\n            arrayLengthInput.value = '';\n            generateArray();\n        });\n\n        clearArrayButton.addEventListener('click', () => {\n            arrayLength = 0;\n            generateArray();\n        });\n\n        function generateArray() {\n            const array = [];\n            for (let i = 0; i < arrayLength; i++) {\n                array.push(Math.floor(Math.random() * 100));\n            }\n            array.sort((a, b) => a - b);\n            array.forEach((element) => {\n                const listElement = document.createElement('div');\n                listElement.textContent = element;\n                listElement.style.width = '50px';\n                listElement.style.height = '20px';\n                listElement.style.background = 'lightgray';\n                listElement.style.border = '1px solid black';\n                listElement.style.padding = '5px';\n                listElement.style.margin = '5px';\n                listElement.style.position = 'absolute';\n                listElement.style.left = '10px';\n                listElement.style.top = '10px';\n                listElement.style.zIndex = '1';\n                listElement.addEventListener('click', () => {\n                    alert(`Element: ${element}`);\n                });\n                arrayList.appendChild(listElement);\n            });\n        }\n    </script>\n</body>\n</html>"
