    "role": "assistant",
    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Merge Sort</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        table {\n            border-collapse: collapse;\n        }\n        th, td {\n            border: 1px solid #ddd;\n            padding: 8px;\n            text-align: left;\n        }\n    </style>\n</head>\n<body>\n    <h1>Merge Sort</h1>\n    <form id=\"mergeSortForm\">\n        <label for=\"array\">Enter an array of numbers:</label>\n        <input type=\"text\" id=\"array\" name=\"array\" required>\n        <button type=\"submit\">Sort</button>\n    </form>\n    <div id=\"result\"></div>\n\n    <script>\n        function mergeSort(arr) {\n            if (arr.length <= 1) {\n                return arr;\n            }\n\n            const mid = Math.floor(arr.length / 2);\n            const left = arr.slice(0, mid);\n            const right = arr.slice(mid);\n\n            left = mergeSort(left);\n            right = mergeSort(right);\n\n            return merge(left, right);\n        }\n\n        function merge(left, right) {\n            const result = [];\n            while (left.length > 0 && right.length > 0) {\n                if (left[0] <= right[0]) {\n                    result.push(left.shift());\n                } else {\n                    result.push(right.shift());\n                }\n            }\n            result.push(...left);\n            result.push(...right);\n            return result;\n        }\n\n        const array = parseInt(document.getElementById(\"array\").value);\n        const result = mergeSort(array);\n\n        document.getElementById(\"result\").innerHTML = \"Sorted array: \" + result.join(\" \");\n    </script>\n</body>\n</html>"
