    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Binary Tree</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            text-align: center;\n        }\n        #tree {\n            width: 50%;\n            margin: 40px auto;\n            padding: 20px;\n            border: 1px solid #ccc;\n            border-radius: 10px;\n            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n        }\n        #tree h1 {\n            margin-bottom: 20px;\n        }\n        #tree pre {\n            margin-bottom: 20px;\n        }\n    </style>\n</head>\n<body>\n    <h1>Binary Tree</h1>\n    <pre id=\"tree\"></pre>\n\n    <input type=\"text\" id=\"input\" placeholder=\"Enter node value\">\n    <button id=\"add\">Add</button>\n\n    <script>\n        let tree = {\n            root: null\n        };\n\n        function addNode(value) {\n            if (tree.root === null) {\n                tree.root = { value, left: null, right: null };\n            } else {\n                let node = tree.root;\n                while (true) {\n                    if (value < node.value) {\n                        if (node.left === null) {\n                            node.left = { value, left: null, right: null };\n                            break;\n                        }\n                        node = node.left;\n                    } else {\n                        if (node.right === null) {\n                            node.right = { value, left: null, right: null };\n                            break;\n                        }\n                        node = node.right;\n                    }\n                }\n            }\n        }\n\n        function printTree(node, indent = 0) {\n            if (node !== null) {\n                let str = '  '.repeat(indent) + node.value + '\\n';\n                str += '  '.repeat(indent + 1) + '<pre>' + str + '</pre>\\n';\n                printTree(node.left, indent + 1);\n                printTree(node.right, indent + 1);\n            }\n        }\n\n        let inputElement = document.getElementById('input');\n        let addButton = document.getElementById('add');\n        addButton.addEventListener('click', addNode);\n\n        function printTreeInOrder(node) {\n            if (node !== null) {\n                printTreeInOrder(node.left);\n                console.log(node.value);\n                printTreeInOrder(node.right);\n            }\n        }\n\n        addButton.addEventListener('click', () => {\n            let value = inputElement.value;\n            addNode(value);\n            printTreeInOrder(tree.root);\n        });\n    </script>\n</body>\n</html>"
