    "content": "<html>\n<head>\n    <title>Binary Search Tree</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        .container {\n            width: 50%;\n            margin: 40px auto;\n            text-align: center;\n        }\n        .tree {\n            width: 50%;\n            margin: 40px auto;\n            padding: 20px;\n            border: 1px solid #ddd;\n            border-radius: 10px;\n            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n        }\n        .node {\n            padding: 20px;\n            border-bottom: 1px solid #ddd;\n            cursor: pointer;\n        }\n        .node:hover {\n            background-color: #f0f0f0;\n        }\n        .node:hover .left-child {\n            display: block;\n        }\n        .left-child {\n            float: left;\n            padding: 10px;\n            border-right: 1px solid #ddd;\n        }\n        .right-child {\n            float: right;\n            padding: 10px;\n            border-left: 1px solid #ddd;\n        }\n    </style>\n</head>\n<body>\n<div class=\"container\">\n    <h1>Binary Search Tree</h1>\n    <p>Insert a node into the tree:</p>\n    <input type=\"text\" id=\"node-value\" placeholder=\"Enter value\">\n    <button id=\"insert-btn\">Insert</button>\n    <p>Search for a value:</p>\n    <input type=\"text\" id=\"search-value\" placeholder=\"Enter value\">\n    <button id=\"search-btn\">Search</button>\n    <p>Display the tree:</p>\n    <div class=\"tree\"></div>\n    <script>\n        let root = null;\n        let nodes = [];\n\n        function insert(nodeValue) {\n            if (root === null) {\n                root = { value: nodeValue, left: null, right: null };\n                nodes.push(root);\n            } else {\n                let currentNode = root;\n                while (currentNode !== null) {\n                    if (nodeValue < currentNode.value) {\n                        currentNode = currentNode.left;\n                    } else if (nodeValue > currentNode.value) {\n                        currentNode = currentNode.right;\n                    } else {\n                        console.log(`Value ${nodeValue} already exists in the tree.`);\n                        return;\n                    }\n                }\n                if (currentNode === null) {\n                    console.log(`Value ${nodeValue} not found in the tree.`);\n                    return;\n                }\n                currentNode.value = nodeValue;\n                nodes.push(currentNode);\n            }\n        }\n\n        function search(nodeValue) {\n            let currentNode = root;\n            while (currentNode !== null) {\n                if (nodeValue === currentNode.value) {\n                    return currentNode;\n                } else if (nodeValue < currentNode.value) {\n                    currentNode = currentNode.left;\n                } else {\n                    currentNode = currentNode.right;\n                }\n            }\n            return null;\n        }\n\n        function displayTree(node) {\n            let html = '';\n            if (node !== null) {\n                html += `<div class=\"node\">${node.value} <button class=\"left-child\">Left Child</button> <button class=\"right-child\">Right Child</button></div>`;\n                if (node.left !== null) {\n                    html += `<div class=\"left-child\">${node.left.value} <button class=\"left-child\">Left Child</button> <button class=\"right-child\">Right Child</button></div>`;\n                }\n                if (node.right !== null) {\n                    html += `<div class=\"right-child\">${node.right.value} <button class=\"left-child\">Left Child</button> <button class=\"right-child\">Right Child</button></div>`;\n                }\n            }\n            return html;\n        }\n\n        function displayTreeRecursive(node) {\n            if (node !== null) {\n                console.log(displayTree(node));\n            }\n        }\n\n        document.getElementById('insert-btn').addEventListener('click', () => {\n            let nodeValue = parseInt(document.getElementById('node-value').value);\n            insert(nodeValue);\n            displayTreeRecursive(root);\n        });\n\n        document.getElementById('search-btn').addEventListener('click', () => {\n            let searchValue = parseInt(document.getElementById('search-value').value);\n            let result = search(searchValue);\n            if (result !== null) {\n                console.log(`Value ${searchValue} found in the tree.`);\n                displayTreeRecursive(result);\n            } else {\n                console.log(`Value ${searchValue} not found in the tree.`);\n            }\n        });\n    </script>\n</div>\n</body>\n</html>"
