    "content": "<html>\n<head>\n    <title>Priority Queue</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        .container {\n            width: 50%;\n            margin: 40px auto;\n            padding: 20px;\n            border: 1px solid #ccc;\n            border-radius: 10px;\n            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n        }\n        .container h2 {\n            margin-top: 0;\n        }\n    </style>\n</head>\n<body>\n    <div class=\"container\">\n        <h2>Priority Queue</h2>\n        <form id=\"priority-queue-form\">\n            <label for=\"priority\">Priority:</label>\n            <input type=\"number\" id=\"priority\" name=\"priority\" required>\n            <br><br>\n            <label for=\"elements\">Elements:</label>\n            <input type=\"number\" id=\"elements\" name=\"elements\" required>\n            <br><br>\n            <label for=\"operation\">Operation:</label>\n            <select id=\"operation\" name=\"operation\" required>\n                <option value=\"insert\">Insert</option>\n                <option value=\"delete\">Delete</option>\n            </select>\n            <br><br>\n            <button type=\"submit\">Submit</button>\n        </form>\n        <div id=\"priority-queue\"></div>\n    </div>\n\n    <script>\n        let priorityQueue = new PriorityQueue();\n        let form = document.getElementById('priority-queue-form');\n        let priorityInput = document.getElementById('priority');\n        let elementsInput = document.getElementById('elements');\n        let operationSelect = document.getElementById('operation');\n        let priorityQueueDiv = document.getElementById('priority-queue');\n\n        form.addEventListener('submit', (e) => {\n            e.preventDefault();\n            let priority = parseInt(priorityInput.value);\n            let elements = parseInt(elementsInput.value);\n            let operation = operationSelect.value;\n\n            if (operation === 'insert') {\n                priorityQueue.insert(priority, elements);\n                updatePriorityQueueDiv(priorityQueueDiv, priorityQueue);\n            } else if (operation === 'delete') {\n                priorityQueue.delete(priority, elements);\n                updatePriorityQueueDiv(priorityQueueDiv, priorityQueue);\n            }\n        });\n\n        function updatePriorityQueueDiv(div, queue) {\n            let elements = queue.elements;\n            let queueText = queue.queueText;\n            div.innerHTML = `Elements: ${elements}\\nQueue: ${queueText}`;\n        }\n\n        class PriorityQueue {\n            constructor() {\n                this.queue = [];\n                this.elements = [];\n                this.queueText = '';\n            }\n\n            insert(priority, elements) {\n                this.queue.push({ priority, elements });\n                this.queue.sort((a, b) => a.priority - b.priority);\n                this.elements.push(elements);\n                this.queueText = this.queue.map((item, index) => `Element ${index + 1}: ${item.elements}`).join('\\n');\n            }\n\n            delete(priority, elements) {\n                for (let i = 0; i < this.queue.length; i++) {\n                    if (this.queue[i].priority === priority && this.queue[i].elements === elements) {\n                        this.queue.splice(i, 1);\n                        this.elements.splice(this.elements.indexOf(elements), 1);\n                        this.queueText = this.queue.map((item, index) => `Element ${index + 1}: ${item.elements}`).join('\\n');\n                        return true;\n                    }\n                }\n                return false;\n            }\n\n            getQueueText() {\n                return this.queueText;\n            }\n        }\n\n        priorityQueue.init();\n    </script>\n</body>\n</html>"
