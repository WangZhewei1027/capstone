    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Kruskal's Algorithm</title>\n    <style>\n        table {\n            border-collapse: collapse;\n        }\n        th, td {\n            border: 1px solid black;\n            padding: 10px;\n            text-align: left;\n        }\n    </style>\n</head>\n<body>\n    <h1>Kruskal's Algorithm</h1>\n    <p>Input: <input type=\"text\" id=\"vertices\" placeholder=\"Enter number of vertices\"></p>\n    <p>Input: <input type=\"number\" id=\"edges\" placeholder=\"Enter number of edges\"></p>\n    <button id=\"calculate\">Calculate</button>\n    <button id=\"display\">Display</button>\n    <div id=\"graph\"></div>\n\n    <script>\n        const inputVertices = document.getElementById('vertices');\n        const inputEdges = document.getElementById('edges');\n        const calculateButton = document.getElementById('calculate');\n        const displayButton = document.getElementById('display');\n        const graph = document.getElementById('graph');\n\n        calculateButton.addEventListener('click', calculateGraph);\n        displayButton.addEventListener('click', displayGraph);\n\n        function calculateGraph() {\n            const vertices = parseInt(inputVertices.value);\n            const edges = parseInt(inputEdges.value);\n\n            if (vertices < 3 || edges < 3) {\n                alert('Minimum number of vertices and edges are 3');\n                return;\n            }\n\n            const graph = new Array(vertices).fill(0).map(() => new Array(vertices).fill(0));\n            const edgesArray = Array.from({ length: vertices }, () => []);\n\n            for (let i = 0; i < vertices; i++) {\n                for (let j = i + 1; j < vertices; j++) {\n                    edgesArray[i].push([j, 1]);\n                    edgesArray[j].push([i, 1]);\n                }\n            }\n\n            edgesArray.forEach((edge, index) => {\n                if (edge.length === 2) {\n                    const [u, v] = edge;\n                    if (u < v) {\n                        graph[u][v] = 1;\n                        graph[v][u] = 1;\n                    }\n                }\n            });\n\n            const visited = Array(vertices).fill(false);\n            const stack = [];\n            const result = [];\n\n            function dfs(node) {\n                visited[node] = true;\n                stack.push(node);\n                result.push(node);\n\n                for (const neighbor of graph[node]) {\n                    if (!visited[neighbor]) {\n                        dfs(neighbor);\n                    }\n                }\n            }\n\n            for (let i = 0; i < vertices; i++) {\n                if (!visited[i]) {\n                    dfs(i);\n                }\n            }\n\n            graph.forEach((row, rowIndex) => {\n                row.forEach((value, columnIndex) => {\n                    if (value === 1) {\n                        const node1 = rowIndex + 1;\n                        const node2 = columnIndex + 1;\n                        const edge = [node1, node2];\n                        graph[node1][node2] = 1;\n                        graph[node2][node1] = 1;\n                        result.push(edge);\n                    }\n                });\n            });\n\n            graph.forEach((row, rowIndex) => {\n                row.forEach((value, columnIndex) => {\n                    if (value === 0) {\n                        const node1 = rowIndex + 1;\n                        const node2 = columnIndex + 1;\n                        const edge = [node1, node2];\n                        graph[node1][node2] = 0;\n                        graph[node2][node1] = 0;\n                    }\n                });\n            });\n\n            displayGraph(graph);\n        }\n\n        function displayGraph(graph) {\n            graph.forEach((row, rowIndex) => {\n                row.forEach((value, columnIndex) => {\n                    if (value === 1) {\n                        const node = rowIndex + 1;\n                        const edge = columnIndex + 1;\n                        const label = `(${node}, ${edge})`;\n                        graph[rowIndex][columnIndex].style.text = label;\n                    } else {\n                        graph[rowIndex][columnIndex].style.display = 'none';\n                    }\n                });\n            });\n\n            graph.forEach((row, rowIndex) => {\n                row.forEach((value, columnIndex) => {\n                    if (value === 0) {\n                        const node = rowIndex + 1;\n                        const edge = columnIndex + 1;\n                        const label = `(${node}, ${edge})`;\n                        graph[rowIndex][columnIndex].style.text = label;\n                    } else {\n                        graph[rowIndex][columnIndex].style.display = 'none';\n                    }\n                });\n            });\n        }\n    </script>\n</body>\n</html>"
