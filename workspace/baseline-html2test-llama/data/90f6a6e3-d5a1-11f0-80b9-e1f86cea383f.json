    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Weighted Graph</title>\n    <style>\n        #graph {\n            border: 1px solid black;\n        }\n    </style>\n</head>\n<body>\n    <h1>Weighted Graph</h1>\n    <canvas id=\"graph\" width=\"400\" height=\"400\"></canvas>\n\n    <script>\n        const canvas = document.getElementById('graph');\n        const ctx = canvas.getContext('2d');\n\n        function drawGraph(graph) {\n            for (const node in graph) {\n                ctx.beginPath();\n                ctx.arc(node, graph[node].x, graph[node].y, 5, 0, 2 * Math.PI);\n                ctx.fillStyle = 'blue';\n                ctx.fill();\n                ctx.beginPath();\n                ctx.arc(node, graph[node].x, graph[node].y, 10, 0, 2 * Math.PI);\n                ctx.fillStyle = 'red';\n                ctx.fill();\n                ctx.beginPath();\n                ctx.arc(node, graph[node].x, graph[node].y, 5, 0, 2 * Math.PI);\n                ctx.strokeStyle = 'black';\n                ctx.stroke();\n            }\n        }\n\n        function addEdge(graph, start, end, weight) {\n            graph[start] = { x: graph[start].x, y: graph[start].y, weight };\n            graph[end] = { x: graph[end].x, y: graph[end].y, weight };\n        }\n\n        const graph = {\n            'A': { x: 100, y: 100, weight: 5 },\n            'B': { x: 150, y: 100, weight: 3 },\n            'C': { x: 100, y: 200, weight: 7 },\n            'D': { x: 150, y: 200, weight: 2 }\n        };\n\n        addEdge(graph, 'A', 'B', 5);\n        addEdge(graph, 'A', 'C', 7);\n        addEdge(graph, 'B', 'D', 3);\n\n        drawGraph(graph);\n    </script>\n</body>\n</html>"
