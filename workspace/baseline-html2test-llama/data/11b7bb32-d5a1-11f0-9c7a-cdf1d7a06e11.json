as.height);\n                    graphCanvasContext.fillStyle = 'black';\n                    graphCanvasContext.font = '14px Arial';\n                    graphCanvasContext.textAlign = 'center';\n                    graphCanvasContext.textBaseline = 'middle';\n                    graphCanvasContext.fillText(currentVertex, graphCanvas.width / 2, graphCanvas.height / 2);\n\n                    for (let i = 0; i < graphVertices.length; i++) {\n                        if (!visited.has(i)) {\n                            visited.add(i);\n                            stack.push([i, true]);\n                        }\n                    }\n                } else {\n                    for (let i = 0; i < graphVertices.length; i++) {\n                        if (!visited.has(i)) {\n                            visited.add(i);\n                            stack.push([i, false]);\n                        }\n                    }\n                }\n            }\n        }\n    </script>\n</body>\n</html>"
