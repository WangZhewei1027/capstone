{
  "topic": "Queue",
  "description": "FSM modeling the interactive Queue Visualizer (dynamic and circular implementations, enqueue/dequeue/peek/clear, auto-enqueue, demo fill/drain, and logging).",
  "states": [
    {
      "name": "dynamic_empty",
      "onEnter": "render",
      "onExit": "noop",
      "on": {
        "ENQUEUE_CLICKED": "dynamic_active",
        "ENQUEUE_FAIL": "dynamic_empty",
        "DEMO_FILL_CLICKED": "dynamic_active",
        "CLEAR_CLICKED": "dynamic_empty",
        "IMPL_CHANGED_CIRCULAR": "circular_empty",
        "AUTO_TOGGLED_START": "auto_running",
        "CLEAR_LOG_CLICKED": "dynamic_empty",
        "PEEK_CLICKED": "dynamic_empty"
      }
    },
    {
      "name": "dynamic_active",
      "onEnter": "render",
      "onExit": "noop",
      "on": {
        "ENQUEUE_SUCCESS": "dynamic_active",
        "ENQUEUE_FAIL": "dynamic_active",
        "DEQUEUE_SUCCESS_TO_EMPTY": "dynamic_empty",
        "DEQUEUE_SUCCESS_TO_NONEMPTY": "dynamic_active",
        "DEQUEUE_FAIL": "dynamic_active",
        "PEEK_SUCCESS": "dynamic_active",
        "PEEK_FAIL": "dynamic_active",
        "CLEAR_CLICKED": "dynamic_empty",
        "IMPL_CHANGED_CIRCULAR": "circular_empty",
        "AUTO_TOGGLED_START": "auto_running",
        "DEMO_DRAIN_CLICKED": "dynamic_empty",
        "CLEAR_LOG_CLICKED": "dynamic_active"
      }
    },
    {
      "name": "circular_empty",
      "onEnter": "render",
      "onExit": "noop",
      "on": {
        "ENQUEUE_CLICKED": "circular_nonfull",
        "ENQUEUE_FAIL": "circular_empty",
        "IMPL_CHANGED_DYNAMIC": "dynamic_empty",
        "CAPACITY_CHANGED": "circular_empty",
        "DEMO_FILL_CLICKED": "circular_full",
        "AUTO_TOGGLED_START": "auto_running",
        "CLEAR_CLICKED": "circular_empty",
        "CLEAR_LOG_CLICKED": "circular_empty",
        "PEEK_CLICKED": "circular_empty"
      }
    },
    {
      "name": "circular_nonfull",
      "onEnter": "render",
      "onExit": "noop",
      "on": {
        "ENQUEUE_SUCCESS": "circular_nonfull",
        "ENQUEUE_FAIL": "circular_full",
        "DEQUEUE_SUCCESS_TO_EMPTY": "circular_empty",
        "DEQUEUE_SUCCESS_TO_NONEMPTY": "circular_nonfull",
        "DEQUEUE_FAIL": "circular_nonfull",
        "PEEK_SUCCESS": "circular_nonfull",
        "PEEK_FAIL": "circular_nonfull",
        "IMPL_CHANGED_DYNAMIC": "dynamic_empty",
        "AUTO_TOGGLED_START": "auto_running",
        "CLEAR_CLICKED": "circular_empty",
        "CLEAR_LOG_CLICKED": "circular_nonfull"
      }
    },
    {
      "name": "circular_full",
      "onEnter": "render",
      "onExit": "noop",
      "on": {
        "DEQUEUE_SUCCESS_TO_NONFULL": "circular_nonfull",
        "ENQUEUE_FAIL": "circular_full",
        "IMPL_CHANGED_DYNAMIC": "dynamic_empty",
        "AUTO_TOGGLED_START": "auto_running",
        "CLEAR_CLICKED": "circular_empty",
        "CLEAR_LOG_CLICKED": "circular_full",
        "PEEK_CLICKED": "circular_full"
      }
    },
    {
      "name": "auto_running",
      "onEnter": "startAutoEnqueue",
      "onExit": "stopAutoEnqueue",
      "on": {
        "AUTO_INTERVAL_ENQUEUE_SUCCESS": "auto_running",
        "AUTO_INTERVAL_ENQUEUE_FAIL": "auto_stopping_on_full",
        "AUTO_TOGGLED_STOP": "impl_reset_to_empty",
        "ENQUEUE_FAIL": "auto_running",
        "DEQUEUE_CLICKED": "auto_running",
        "IMPL_CHANGED_DYNAMIC": "auto_stopping_reset",
        "IMPL_CHANGED_CIRCULAR": "auto_stopping_reset",
        "CLEAR_CLICKED": "auto_running",
        "CLEAR_LOG_CLICKED": "auto_running"
      }
    },
    {
      "name": "auto_stopping_on_full",
      "onEnter": "logAutoStoppedDueToFull",
      "onExit": "noop",
      "on": {
        "AUTO_TOGGLED_STOP": "impl_reset_to_empty",
        "AUTO_INTERVAL_ENQUEUE_FAIL": "auto_stopping_on_full",
        "IMPL_CHANGED_DYNAMIC": "auto_stopping_reset",
        "IMPL_CHANGED_CIRCULAR": "auto_stopping_reset"
      }
    },
    {
      "name": "auto_stopping_reset",
      "onEnter": "stopAutoEnqueue",
      "onExit": "noop",
      "on": {
        "IMPL_CHANGED_DYNAMIC": "dynamic_empty",
        "IMPL_CHANGED_CIRCULAR": "circular_empty",
        "AUTO_TOGGLED_STOP": "impl_reset_to_empty"
      }
    },
    {
      "name": "impl_reset_to_empty",
      "onEnter": "render",
      "onExit": "noop",
      "on": {
        "IMPL_CHANGED_DYNAMIC": "dynamic_empty",
        "IMPL_CHANGED_CIRCULAR": "circular_empty",
        "AUTO_TOGGLED_STOP": "dynamic_empty"
      }
    }
  ],
  "events": [
    "ENQUEUE_CLICKED",
    "ENQUEUE_SUCCESS",
    "ENQUEUE_FAIL",
    "DEQUEUE_CLICKED",
    "DEQUEUE_SUCCESS_TO_EMPTY",
    "DEQUEUE_SUCCESS_TO_NONEMPTY",
    "DEQUEUE_FAIL",
    "PEEK_CLICKED",
    "PEEK_SUCCESS",
    "PEEK_FAIL",
    "CLEAR_CLICKED",
    "CLEAR_LOG_CLICKED",
    "IMPL_CHANGED_DYNAMIC",
    "IMPL_CHANGED_CIRCULAR",
    "CAPACITY_CHANGED",
    "DEMO_FILL_CLICKED",
    "DEMO_DRAIN_CLICKED",
    "AUTO_TOGGLED_START",
    "AUTO_TOGGLED_STOP",
    "AUTO_INTERVAL_ENQUEUE_SUCCESS",
    "AUTO_INTERVAL_ENQUEUE_FAIL"
  ],
  "triggers": {
    "ENQUEUE_CLICKED": "#enqueueBtn, #valueInput",
    "ENQUEUE_SUCCESS": "#enqueueBtn, #randBtn, #demoFill, #autoBtn",
    "ENQUEUE_FAIL": "#enqueueBtn, #randBtn, #autoBtn, #demoFill",
    "DEQUEUE_CLICKED": "#dequeueBtn",
    "DEQUEUE_SUCCESS_TO_EMPTY": "#dequeueBtn, #demoEmpty",
    "DEQUEUE_SUCCESS_TO_NONEMPTY": "#dequeueBtn, #demoEmpty",
    "DEQUEUE_FAIL": "#dequeueBtn, #demoEmpty",
    "PEEK_CLICKED": "#peekBtn",
    "PEEK_SUCCESS": "#peekBtn",
    "PEEK_FAIL": "#peekBtn",
    "CLEAR_CLICKED": "#clearBtn",
    "CLEAR_LOG_CLICKED": "#clearLog",
    "IMPL_CHANGED_DYNAMIC": "#implSelect",
    "IMPL_CHANGED_CIRCULAR": "#implSelect",
    "CAPACITY_CHANGED": "#capacityInput",
    "DEMO_FILL_CLICKED": "#demoFill",
    "DEMO_DRAIN_CLICKED": "#demoEmpty",
    "AUTO_TOGGLED_START": "#autoBtn",
    "AUTO_TOGGLED_STOP": "#autoBtn",
    "AUTO_INTERVAL_ENQUEUE_SUCCESS": "#autoBtn",
    "AUTO_INTERVAL_ENQUEUE_FAIL": "#autoBtn"
  },
  "notes": "Triggers map events to actual DOM selectors present in the HTML. States model the two implementations (dynamic / circular), fullness (empty, nonfull, full for circular) and the auto-enqueue lifecycle. onEnter/onExit actions reference implemented behaviors: render, start/stop auto-enqueue, and logging/animation hooks (animateLastEnqueue, animateDequeue, flash) invoked by event handlers."
}