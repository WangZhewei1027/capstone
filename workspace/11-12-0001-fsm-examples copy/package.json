{
  "name": "fsm-extractor",
  "version": "1.0.0",
  "type": "module",
  "description": "智能FSM提取工具 - 从HTML页面自动分析交互逻辑并构建FSM模型",
  "main": "fsm-interactive-capture.spec.js",
  "scripts": {
    "extract": "node run_fsm_extraction.mjs",
    "extract:full": "node run_fsm_extraction.mjs \"\" full",
    "extract:only": "node run_fsm_extraction.mjs \"\" extract",
    "compare:only": "node run_fsm_extraction.mjs \"\" compare",
    "test": "npx playwright test fsm-interactive-capture.spec.js",
    "test:headed": "npx playwright test fsm-interactive-capture.spec.js --headed",
    "test:debug": "npx playwright test fsm-interactive-capture.spec.js --debug",
    "install": "npm install && npx playwright install chromium"
  },
  "keywords": [
    "fsm",
    "finite-state-machine",
    "html-analysis",
    "interaction-testing",
    "playwright",
    "educational-content",
    "evaluation-metrics"
  ],
  "author": "CS-Capstone Team",
  "license": "MIT",
  "dependencies": {
    "@playwright/test": "^1.56.0"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WangZhewei1027/capstone.git"
  },
  "directories": {
    "html": "./html",
    "output": {
      "visuals": "./visuals",
      "fsm": "./fsm"
    }
  }
}
