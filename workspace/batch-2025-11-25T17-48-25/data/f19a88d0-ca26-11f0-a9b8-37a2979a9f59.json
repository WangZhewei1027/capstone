    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Binary Search Tree Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            text-align: center;\n            margin: 0;\n        }\n        #bst-container {\n            display: flex;\n            justify-content: center;\n            margin-top: 20px;\n        }\n        .node {\n            border: 1px solid #333;\n            border-radius: 5px;\n            padding: 10px;\n            margin: 5px;\n            display: inline-block;\n            position: relative;\n            background-color: #f9f9f9;\n        }\n        .line {\n            position: absolute;\n            border-left: 1px solid #333;\n            height: 20px;\n            top: 20px;\n            left: 50%;\n            margin-left: -1px;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Binary Search Tree Visualization</h1>\n<input type=\"number\" id=\"value\" placeholder=\"Enter a number\">\n<button onclick=\"insertNode()\">Insert Node</button>\n<button onclick=\"clearTree()\">Clear Tree</button>\n\n<div id=\"bst-container\"></div>\n\n<script>\n    class Node {\n        constructor(value) {\n            this.value = value;\n            this.left = null;\n            this.right = null;\n        }\n    }\n\n    class BinarySearchTree {\n        constructor() {\n            this.root = null;\n        }\n        \n        insert(value) {\n            const newNode = new Node(value);\n            if (!this.root) {\n                this.root = newNode;\n                return;\n            }\n            this.insertNode(this.root, newNode);\n        }\n        \n        insertNode(node, newNode) {\n            if (newNode.value < node.value) {\n                if (!node.left) {\n                    node.left = newNode;\n                } else {\n                    this.insertNode(node.left, newNode);\n                }\n            } else {\n                if (!node.right) {\n                    node.right = newNode;\n                } else {\n                    this.insertNode(node.right, newNode);\n                }\n            }\n        }\n\n        clear() {\n            this.root = null;\n        }\n    }\n\n    const bst = new BinarySearchTree();\n\n    function insertNode() {\n        const input = document.getElementById(\"value\");\n        const value = parseInt(input.value);\n        if (!isNaN(value)) {\n            bst.insert(value);\n            input.value = \"\";\n            renderTree(bst.root);\n        }\n    }\n\n    function clearTree() {\n        bst.clear();\n        document.getElementById(\"bst-container\").innerHTML = \"\";\n    }\n\n    function renderTree(root) {\n        const container = document.getElementById(\"bst-container\");\n        container.innerHTML = \"\"; // Clear previous drawing\n        if (!root) return;\n        drawNode(root, container);\n    }\n\n    function drawNode(node, container) {\n        const divNode = document.createElement('div');\n        divNode.className = 'node';\n        divNode.innerText = node.value;\n\n        if (node.left) {\n            const leftLine = document.createElement('div');\n            leftLine.className = 'line';\n            divNode.appendChild(leftLine);\n            drawNode(node.left, container);\n        }\n\n        if (node.right) {\n            const rightLine = document.createElement('div');\n            rightLine.className = 'line';\n            divNode.appendChild(rightLine);\n            drawNode(node.right, container);\n        }\n\n        container.appendChild(divNode);\n    }\n</script>\n\n</body>\n</html>"
