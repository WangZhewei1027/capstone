    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Linked List Demonstration</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            background-color: #f4f4f4;\n            margin: 0;\n            padding: 20px;\n        }\n        h1 {\n            color: #333;\n        }\n        #list {\n            list-style: none;\n            padding: 0;\n        }\n        #list li {\n            background: #fff;\n            margin: 5px 0;\n            padding: 10px;\n            border-radius: 5px;\n            box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\n        }\n    </style>\n</head>\n<body>\n\n<h1>Linked List Demonstration</h1>\n<input type=\"text\" id=\"nodeValue\" placeholder=\"Enter node value\">\n<button onclick=\"addNode()\">Add Node</button>\n<button onclick=\"removeNode()\">Remove Last Node</button>\n\n<ul id=\"list\"></ul>\n\n<script>\n    class Node {\n        constructor(value) {\n            this.value = value;\n            this.next = null;\n        }\n    }\n\n    class LinkedList {\n        constructor() {\n            this.head = null;\n            this.tail = null;\n        }\n\n        add(value) {\n            const newNode = new Node(value);\n            if (!this.head) {\n                this.head = newNode;\n                this.tail = newNode;\n            } else {\n                this.tail.next = newNode;\n                this.tail = newNode;\n            }\n            this.render();\n        }\n\n        remove() {\n            if (!this.head) return;\n\n            if (this.head === this.tail) {\n                this.head = null;\n                this.tail = null;\n            } else {\n                let current = this.head;\n                while (current.next && current.next !== this.tail) {\n                    current = current.next;\n                }\n                current.next = null;\n                this.tail = current;\n            }\n            this.render();\n        }\n\n        render() {\n            const listElement = document.getElementById('list');\n            listElement.innerHTML = '';\n            let current = this.head;\n            while (current) {\n                const li = document.createElement('li');\n                li.textContent = current.value;\n                listElement.appendChild(li);\n                current = current.next;\n            }\n        }\n    }\n\n    const linkedList = new LinkedList();\n\n    function addNode() {\n        const input = document.getElementById('nodeValue');\n        const value = input.value.trim();\n        if (value) {\n            linkedList.add(value);\n            input.value = '';\n        }\n    }\n\n    function removeNode() {\n        linkedList.remove();\n    }\n</script>\n\n</body>\n</html>"
