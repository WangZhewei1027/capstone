{
  "topic": "Deque",
  "description": "FSM modeling the interactive states of a simple Deque web UI with Add/Remove operations using prompt/alert and display updates.",
  "states": [
    {
      "name": "idle",
      "onEnter": "noop",
      "onExit": "noop",
      "on": {
        "ADD_FRONT_CLICKED": "prompting_add_front",
        "ADD_REAR_CLICKED": "prompting_add_rear",
        "REMOVE_FRONT_CLICKED": "removing_front",
        "REMOVE_REAR_CLICKED": "removing_rear"
      }
    },
    {
      "name": "prompting_add_front",
      "onEnter": "showPromptFront",
      "onExit": "noop",
      "on": {
        "PROMPT_SUBMIT": "adding_front",
        "PROMPT_CANCEL": "idle"
      }
    },
    {
      "name": "adding_front",
      "onEnter": "addFrontValueAndUpdateDisplay",
      "onExit": "noop",
      "on": {
        "ADDED_FRONT": "idle"
      }
    },
    {
      "name": "prompting_add_rear",
      "onEnter": "showPromptRear",
      "onExit": "noop",
      "on": {
        "PROMPT_SUBMIT": "adding_rear",
        "PROMPT_CANCEL": "idle"
      }
    },
    {
      "name": "adding_rear",
      "onEnter": "addRearValueAndUpdateDisplay",
      "onExit": "noop",
      "on": {
        "ADDED_REAR": "idle"
      }
    },
    {
      "name": "removing_front",
      "onEnter": "removeFrontValueAndMaybeAlertAndUpdateDisplay",
      "onExit": "noop",
      "on": {
        "REMOVED_FRONT": "idle",
        "NO_ELEMENT_FRONT": "idle"
      }
    },
    {
      "name": "removing_rear",
      "onEnter": "removeRearValueAndMaybeAlertAndUpdateDisplay",
      "onExit": "noop",
      "on": {
        "REMOVED_REAR": "idle",
        "NO_ELEMENT_REAR": "idle"
      }
    }
  ],
  "events": [
    "ADD_FRONT_CLICKED",
    "ADD_REAR_CLICKED",
    "REMOVE_FRONT_CLICKED",
    "REMOVE_REAR_CLICKED",
    "PROMPT_SUBMIT",
    "PROMPT_CANCEL",
    "ADDED_FRONT",
    "ADDED_REAR",
    "REMOVED_FRONT",
    "REMOVED_REAR",
    "NO_ELEMENT_FRONT",
    "NO_ELEMENT_REAR"
  ],
  "triggers": {
    "ADD_FRONT_CLICKED": ".deque-operations > button:nth-child(1)",
    "ADD_REAR_CLICKED": ".deque-operations > button:nth-child(2)",
    "REMOVE_FRONT_CLICKED": ".deque-operations > button:nth-child(3)",
    "REMOVE_REAR_CLICKED": ".deque-operations > button:nth-child(4)"
  },
  "notes": "Selectors extracted from HTML: .deque-container, .deque-operations, .deque-display, #deque-display, and the four buttons as .deque-operations > button:nth-child(1/2/3/4). Prompt/alert interactions (PROMPT_SUBMIT, PROMPT_CANCEL) are produced by browser dialogs and are not DOM-selectable elements."
}