{
  "meta": {
    "concept": "BinarySearchTree",
    "topic": "Binary Search Tree Visualization",
    "educational_goal": "Demonstrate BST insertion, deletion, search, traversals and animations",
    "expected_interactions": [
      "insert",
      "delete",
      "search",
      "random_populate",
      "clear",
      "traversal",
      "pan",
      "node_click",
      "speed_change"
    ]
  },
  "states": [
    {
      "id": "S0_Idle",
      "label": "Idle",
      "type": "idle",
      "entry_actions": [
        "draw()",
        "disableControls(false)",
        "animating = false"
      ],
      "exit_actions": []
    },
    {
      "id": "S1_Inserting",
      "label": "Inserting",
      "type": "atomic",
      "entry_actions": [
        "const v = parseInt(valueInput.value)",
        "disableControls(true)"
      ],
      "exit_actions": []
    },
    {
      "id": "S2_InsertResult",
      "label": "InsertResult",
      "type": "atomic",
      "entry_actions": [
        "const ok = tree.insert(v)",
        "valueInput.value = ''",
        "draw()"
      ],
      "exit_actions": [
        "disableControls(false)"
      ]
    },
    {
      "id": "S3_InsertDuplicateAlert",
      "label": "InsertDuplicateAlert",
      "type": "atomic",
      "entry_actions": [
        "alert('Value already exists in BST (duplicates ignored).')"
      ],
      "exit_actions": []
    },
    {
      "id": "S4_Deleting",
      "label": "Deleting",
      "type": "atomic",
      "entry_actions": [
        "const v = parseInt(valueInput.value)",
        "disableControls(true)"
      ],
      "exit_actions": []
    },
    {
      "id": "S5_DeleteResult",
      "label": "DeleteResult",
      "type": "atomic",
      "entry_actions": [
        "const ok = tree.delete(v)",
        "valueInput.value = ''",
        "draw()"
      ],
      "exit_actions": [
        "disableControls(false)"
      ]
    },
    {
      "id": "S6_DeleteNotFoundAlert",
      "label": "DeleteNotFoundAlert",
      "type": "atomic",
      "entry_actions": [
        "alert('Value not found.')"
      ],
      "exit_actions": []
    },
    {
      "id": "S7_ClearConfirm",
      "label": "ClearConfirm",
      "type": "atomic",
      "entry_actions": [
        "const ok = confirm('Clear the entire tree?')"
      ],
      "exit_actions": []
    },
    {
      "id": "S8_Clearing",
      "label": "Clearing",
      "type": "atomic",
      "entry_actions": [
        "tree.root = null",
        "tree._size = 0",
        "draw()"
      ],
      "exit_actions": []
    },
    {
      "id": "S9_RandomPopulating",
      "label": "RandomPopulating",
      "type": "atomic",
      "entry_actions": [
        "generate 7 unique random integers between 1..99",
        "insert each into tree",
        "draw()"
      ],
      "exit_actions": []
    },
    {
      "id": "S10_Searching",
      "label": "Searching",
      "type": "atomic",
      "entry_actions": [
        "const v = parseInt(searchInput.value)",
        "const {found,path} = tree.find(v)"
      ],
      "exit_actions": []
    },
    {
      "id": "S11_SearchAnimating",
      "label": "SearchAnimating",
      "type": "atomic",
      "entry_actions": [
        "animating = true",
        "disableControls(true)",
        "highlightPath(path, found, parseInt(speedInput.value), callback)"
      ],
      "exit_actions": [
        "animating = false",
        "disableControls(false)"
      ]
    },
    {
      "id": "S12_TraversalAnimating",
      "label": "TraversalAnimating",
      "type": "atomic",
      "entry_actions": [
        "animating = true",
        "disableControls(true)",
        "animateSequence(arr, parseInt(speedInput.value), callback)"
      ],
      "exit_actions": [
        "animating = false",
        "disableControls(false)"
      ]
    },
    {
      "id": "S13_NodePrefill",
      "label": "NodePrefill",
      "type": "atomic",
      "entry_actions": [
        "valueInput.value = node.v",
        "searchInput.value = node.v"
      ],
      "exit_actions": []
    },
    {
      "id": "S14_Panning",
      "label": "Panning",
      "type": "atomic",
      "entry_actions": [
        "isDown = true",
        "store startX,startY and viewBox"
      ],
      "exit_actions": [
        "isDown = false"
      ]
    },
    {
      "id": "S15_InputErrorAlert",
      "label": "InputErrorAlert",
      "type": "atomic",
      "entry_actions": [
        "alert('Enter an integer to insert/delete/search.')"
      ],
      "exit_actions": []
    },
    {
      "id": "S16_TreeEmptyAlert",
      "label": "TreeEmptyAlert",
      "type": "atomic",
      "entry_actions": [
        "alert('Tree is empty.')"
      ],
      "exit_actions": []
    }
  ],
  "events": [
    {
      "id": "CLICK_INSERT",
      "event_type": "user_action",
      "description": "User clicks the Insert button",
      "triggers": "#insertBtn"
    },
    {
      "id": "CLICK_DELETE",
      "event_type": "user_action",
      "description": "User clicks the Delete button",
      "triggers": "#deleteBtn"
    },
    {
      "id": "CLICK_SEARCH",
      "event_type": "user_action",
      "description": "User clicks the Search button",
      "triggers": "#searchBtn"
    },
    {
      "id": "CLICK_RANDOM",
      "event_type": "user_action",
      "description": "User clicks the Random (7) button",
      "triggers": "#randomBtn"
    },
    {
      "id": "CLICK_CLEAR",
      "event_type": "user_action",
      "description": "User clicks the Clear button",
      "triggers": "#clearBtn"
    },
    {
      "id": "CLICK_TRAVERSAL_INORDER",
      "event_type": "user_action",
      "description": "User clicks the In-order traversal button",
      "triggers": "#inorderBtn"
    },
    {
      "id": "CLICK_TRAVERSAL_PREORDER",
      "event_type": "user_action",
      "description": "User clicks the Pre-order traversal button",
      "triggers": "#preorderBtn"
    },
    {
      "id": "CLICK_TRAVERSAL_POSTORDER",
      "event_type": "user_action",
      "description": "User clicks the Post-order traversal button",
      "triggers": "#postorderBtn"
    },
    {
      "id": "CLICK_TRAVERSAL_LEVEL",
      "event_type": "user_action",
      "description": "User clicks the Level-order traversal button",
      "triggers": "#levelBtn"
    },
    {
      "id": "SVG_NODE_CLICK",
      "event_type": "user_action",
      "description": "User clicks on a rendered node in the SVG to prefill inputs",
      "triggers": "#svgCanvas .node"
    },
    {
      "id": "KEYPRESS_ENTER_VALUE",
      "event_type": "user_action",
      "description": "User presses Enter in the value input (maps to insert)",
      "triggers": "#valueInput"
    },
    {
      "id": "KEYPRESS_ENTER_SEARCH",
      "event_type": "user_action",
      "description": "User presses Enter in the search input (maps to search)",
      "triggers": "#searchInput"
    },
    {
      "id": "SPEED_CHANGE",
      "event_type": "user_action",
      "description": "User changes animation speed range input",
      "triggers": "#speed"
    },
    {
      "id": "MOUSEDOWN_SVG",
      "event_type": "user_action",
      "description": "User starts panning by mousedown on the SVG canvas",
      "triggers": "#svgCanvas"
    },
    {
      "id": "MOUSEMOVE_WINDOW",
      "event_type": "user_action",
      "description": "Mouse moves (while panning) - global window listener",
      "triggers": "window"
    },
    {
      "id": "MOUSEUP_WINDOW",
      "event_type": "user_action",
      "description": "Mouse up ends panning - global window listener",
      "triggers": "window"
    },
    {
      "id": "INSERT_PROCESSED",
      "event_type": "system_event",
      "description": "Insert operation processed (ok or duplicate)"
    },
    {
      "id": "DELETE_PROCESSED",
      "event_type": "system_event",
      "description": "Delete operation processed (deleted or not found)"
    },
    {
      "id": "CLEAR_CONFIRMED",
      "event_type": "system_event",
      "description": "User confirmed clearing the tree"
    },
    {
      "id": "CLEAR_CANCELLED",
      "event_type": "system_event",
      "description": "User cancelled clearing the tree"
    },
    {
      "id": "SEARCH_ANIMATION_STARTED",
      "event_type": "system_event",
      "description": "Search highlightPath animation started"
    },
    {
      "id": "SEARCH_ANIMATION_COMPLETE",
      "event_type": "system_event",
      "description": "Search highlightPath animation completed"
    },
    {
      "id": "TRAVERSAL_ANIMATION_STARTED",
      "event_type": "system_event",
      "description": "Traversal animateSequence started"
    },
    {
      "id": "TRAVERSAL_ANIMATION_COMPLETE",
      "event_type": "system_event",
      "description": "Traversal animateSequence completed"
    },
    {
      "id": "PAN_STARTED",
      "event_type": "system_event",
      "description": "Panning started"
    },
    {
      "id": "PAN_MOVED",
      "event_type": "system_event",
      "description": "Panning moved (viewBox updated)"
    },
    {
      "id": "PAN_ENDED",
      "event_type": "system_event",
      "description": "Panning ended"
    }
  ],
  "transitions": [
    {
      "from": "S0_Idle",
      "to": "S1_Inserting",
      "event": "CLICK_INSERT",
      "guard": "valueInput contains a valid integer && animating === false",
      "actions": [
        "parseInt(#valueInput.value) -> v",
        "disableControls(true)"
      ],
      "expected_observables": [
        "dom:#valueInput value read",
        "dom:#insertBtn click"
      ],
      "triggers": "#insertBtn",
      "timeout": 2000
    },
    {
      "from": "S1_Inserting",
      "to": "S2_InsertResult",
      "event": "INSERT_PROCESSED",
      "guard": "true (synchronous continuation)",
      "actions": [
        "const ok = tree.insert(v)",
        "valueInput.value = ''",
        "draw()"
      ],
      "expected_observables": [
        "dom:#nodeCount text changed",
        "dom:#svgCanvas updated"
      ],
      "triggers": "#insertBtn",
      "timeout": 1000
    },
    {
      "from": "S2_InsertResult",
      "to": "S0_Idle",
      "event": "INSERT_PROCESSED",
      "guard": "ok === true",
      "actions": [
        "disableControls(false)"
      ],
      "expected_observables": [
        "dom:#traversalResult updated if any",
        "dom:#valueInput cleared"
      ],
      "triggers": "#insertBtn",
      "timeout": 500
    },
    {
      "from": "S2_InsertResult",
      "to": "S3_InsertDuplicateAlert",
      "event": "INSERT_PROCESSED",
      "guard": "ok === false",
      "actions": [
        "alert('Value already exists in BST (duplicates ignored).')",
        "disableControls(false)"
      ],
      "expected_observables": [
        "dom:alert dialog shown",
        "dom:#valueInput cleared"
      ],
      "triggers": "#insertBtn",
      "timeout": 2000
    },
    {
      "from": "S3_InsertDuplicateAlert",
      "to": "S0_Idle",
      "event": "USER_ACK_ALERT",
      "guard": "true",
      "actions": [],
      "expected_observables": [
        "dom:alert closed"
      ],
      "triggers": "#insertBtn",
      "timeout": 500
    },
    {
      "from": "S0_Idle",
      "to": "S4_Deleting",
      "event": "CLICK_DELETE",
      "guard": "valueInput contains a valid integer && animating === false",
      "actions": [
        "parseInt(#valueInput.value) -> v",
        "disableControls(true)"
      ],
      "expected_observables": [
        "dom:#valueInput value read",
        "dom:#deleteBtn click"
      ],
      "triggers": "#deleteBtn",
      "timeout": 2000
    },
    {
      "from": "S4_Deleting",
      "to": "S5_DeleteResult",
      "event": "DELETE_PROCESSED",
      "guard": "true (synchronous continuation)",
      "actions": [
        "const ok = tree.delete(v)",
        "valueInput.value = ''",
        "draw()"
      ],
      "expected_observables": [
        "dom:#nodeCount changed",
        "dom:#svgCanvas updated"
      ],
      "triggers": "#deleteBtn",
      "timeout": 1500
    },
    {
      "from": "S5_DeleteResult",
      "to": "S0_Idle",
      "event": "DELETE_PROCESSED",
      "guard": "ok === true",
      "actions": [
        "disableControls(false)"
      ],
      "expected_observables": [
        "dom:#valueInput cleared"
      ],
      "triggers": "#deleteBtn",
      "timeout": 500
    },
    {
      "from": "S5_DeleteResult",
      "to": "S6_DeleteNotFoundAlert",
      "event": "DELETE_PROCESSED",
      "guard": "ok === false",
      "actions": [
        "alert('Value not found.')",
        "disableControls(false)"
      ],
      "expected_observables": [
        "dom:alert shown"
      ],
      "triggers": "#deleteBtn",
      "timeout": 1000
    },
    {
      "from": "S6_DeleteNotFoundAlert",
      "to": "S0_Idle",
      "event": "USER_ACK_ALERT",
      "guard": "true",
      "actions": [],
      "expected_observables": [
        "dom:alert closed"
      ],
      "triggers": "#deleteBtn",
      "timeout": 500
    },
    {
      "from": "S0_Idle",
      "to": "S7_ClearConfirm",
      "event": "CLICK_CLEAR",
      "guard": "animating === false",
      "actions": [
        "invoke confirm('Clear the entire tree?')"
      ],
      "expected_observables": [
        "dom:#clearBtn click",
        "dom:confirm dialog shown"
      ],
      "triggers": "#clearBtn",
      "timeout": 2000
    },
    {
      "from": "S7_ClearConfirm",
      "to": "S8_Clearing",
      "event": "CLEAR_CONFIRMED",
      "guard": "user confirmed",
      "actions": [
        "tree.root = null",
        "tree._size = 0",
        "draw()"
      ],
      "expected_observables": [
        "dom:#svgCanvas cleared",
        "dom:#nodeCount becomes 0"
      ],
      "triggers": "#clearBtn",
      "timeout": 1000
    },
    {
      "from": "S7_ClearConfirm",
      "to": "S0_Idle",
      "event": "CLEAR_CANCELLED",
      "guard": "user cancelled",
      "actions": [],
      "expected_observables": [
        "dom:confirm closed"
      ],
      "triggers": "#clearBtn",
      "timeout": 500
    },
    {
      "from": "S8_Clearing",
      "to": "S0_Idle",
      "event": "CLEAR_CONFIRMED",
      "guard": "true",
      "actions": [],
      "expected_observables": [
        "dom:#structureText shows 'Empty'",
        "dom:#nodeCount shows '0'"
      ],
      "triggers": "#clearBtn",
      "timeout": 500
    },
    {
      "from": "S0_Idle",
      "to": "S9_RandomPopulating",
      "event": "CLICK_RANDOM",
      "guard": "animating === false",
      "actions": [
        "generate 7 unique random ints between 1..99",
        "insert each into tree",
        "draw()"
      ],
      "expected_observables": [
        "dom:#randomBtn click",
        "dom:#nodeCount increases",
        "dom:#svgCanvas updated"
      ],
      "triggers": "#randomBtn",
      "timeout": 2000
    },
    {
      "from": "S9_RandomPopulating",
      "to": "S0_Idle",
      "event": "RANDOM_POPULATED",
      "guard": "true",
      "actions": [],
      "expected_observables": [
        "dom:#nodeCount updated",
        "dom:#svgCanvas rendered with nodes"
      ],
      "triggers": "#randomBtn",
      "timeout": 1000
    },
    {
      "from": "S0_Idle",
      "to": "S10_Searching",
      "event": "CLICK_SEARCH",
      "guard": "searchInput contains a valid integer && animating === false",
      "actions": [
        "parseInt(#searchInput.value) -> v",
        "const {found,path} = tree.find(v)"
      ],
      "expected_observables": [
        "dom:#searchInput value read",
        "dom:#searchBtn click"
      ],
      "triggers": "#searchBtn",
      "timeout": 1000
    },
    {
      "from": "S10_Searching",
      "to": "S16_TreeEmptyAlert",
      "event": "SEARCH_CHECK",
      "guard": "path.length === 0",
      "actions": [
        "alert('Tree is empty.')"
      ],
      "expected_observables": [
        "dom:alert shown"
      ],
      "triggers": "#searchBtn",
      "timeout": 500
    },
    {
      "from": "S16_TreeEmptyAlert",
      "to": "S0_Idle",
      "event": "USER_ACK_ALERT",
      "guard": "true",
      "actions": [],
      "expected_observables": [
        "dom:alert closed"
      ],
      "triggers": "#searchBtn",
      "timeout": 300
    },
    {
      "from": "S10_Searching",
      "to": "S11_SearchAnimating",
      "event": "SEARCH_ANIMATION_STARTED",
      "guard": "path.length > 0",
      "actions": [
        "highlightPath(path, found, parseInt(speedInput.value), callback)",
        "animating = true",
        "disableControls(true)"
      ],
      "expected_observables": [
        "dom:#svgCanvas nodes get .highlight class",
        "dom:controls disabled"
      ],
      "triggers": "#searchBtn",
      "timeout": 100
    },
    {
      "from": "S11_SearchAnimating",
      "to": "S0_Idle",
      "event": "SEARCH_ANIMATION_COMPLETE",
      "guard": "true",
      "actions": [
        "animating = false",
        "disableControls(false)",
        "update #traversalResult: 'Found' or 'Not found' with visited count"
      ],
      "expected_observables": [
        "dom:.node classes updated (.visited or .found)",
        "dom:#traversalResult text updated"
      ],
      "triggers": "#svgCanvas",
      "timeout": 2000
    },
    {
      "from": "S0_Idle",
      "to": "S12_TraversalAnimating",
      "event": "CLICK_TRAVERSAL_INORDER",
      "guard": "tree.size > 0 && animating === false",
      "actions": [
        "const arr = tree.inorder()",
        "animating = true",
        "disableControls(true)",
        "animateSequence(arr, parseInt(speedInput.value), callback)"
      ],
      "expected_observables": [
        "dom:#inorderBtn click",
        "dom:#svgCanvas nodes get .highlight then .visited"
      ],
      "triggers": "#inorderBtn",
      "timeout": 100
    },
    {
      "from": "S0_Idle",
      "to": "S12_TraversalAnimating",
      "event": "CLICK_TRAVERSAL_PREORDER",
      "guard": "tree.size > 0 && animating === false",
      "actions": [
        "const arr = tree.preorder()",
        "animating = true",
        "disableControls(true)",
        "animateSequence(arr, parseInt(speedInput.value), callback)"
      ],
      "expected_observables": [
        "dom:#preorderBtn click"
      ],
      "triggers": "#preorderBtn",
      "timeout": 100
    },
    {
      "from": "S0_Idle",
      "to": "S12_TraversalAnimating",
      "event": "CLICK_TRAVERSAL_POSTORDER",
      "guard": "tree.size > 0 && animating === false",
      "actions": [
        "const arr = tree.postorder()",
        "animating = true",
        "disableControls(true)",
        "animateSequence(arr, parseInt(speedInput.value), callback)"
      ],
      "expected_observables": [
        "dom:#postorderBtn click"
      ],
      "triggers": "#postorderBtn",
      "timeout": 100
    },
    {
      "from": "S0_Idle",
      "to": "S12_TraversalAnimating",
      "event": "CLICK_TRAVERSAL_LEVEL",
      "guard": "tree.size > 0 && animating === false",
      "actions": [
        "const arr = tree.levelOrder()",
        "animating = true",
        "disableControls(true)",
        "animateSequence(arr, parseInt(speedInput.value), callback)"
      ],
      "expected_observables": [
        "dom:#levelBtn click"
      ],
      "triggers": "#levelBtn",
      "timeout": 100
    },
    {
      "from": "S12_TraversalAnimating",
      "to": "S0_Idle",
      "event": "TRAVERSAL_ANIMATION_COMPLETE",
      "guard": "true",
      "actions": [
        "animating = false",
        "disableControls(false)",
        "set #traversalResult to traversal string"
      ],
      "expected_observables": [
        "dom:#traversalResult updated",
        "dom:.node classes toggled"
      ],
      "triggers": "#inorderBtn,#preorderBtn,#postorderBtn,#levelBtn",
      "timeout": 3000
    },
    {
      "from": "S0_Idle",
      "to": "S13_NodePrefill",
      "event": "SVG_NODE_CLICK",
      "guard": "clicked element has .node and _nodeRef",
      "actions": [
        "valueInput.value = node.v",
        "searchInput.value = node.v"
      ],
      "expected_observables": [
        "dom:#valueInput filled",
        "dom:#searchInput filled"
      ],
      "triggers": "#svgCanvas .node",
      "timeout": 200
    },
    {
      "from": "S13_NodePrefill",
      "to": "S0_Idle",
      "event": "NODE_PREFILL_DONE",
      "guard": "true",
      "actions": [],
      "expected_observables": [
        "dom:#valueInput and dom:#searchInput updated"
      ],
      "triggers": "#svgCanvas .node",
      "timeout": 100
    },
    {
      "from": "S0_Idle",
      "to": "S1_Inserting",
      "event": "KEYPRESS_ENTER_VALUE",
      "guard": "valueInput contains a valid integer && animating === false",
      "actions": [
        "synthesize click on #insertBtn"
      ],
      "expected_observables": [
        "dom:#valueInput keypress Enter",
        "dom:#insertBtn click"
      ],
      "triggers": "#valueInput",
      "timeout": 200
    },
    {
      "from": "S0_Idle",
      "to": "S10_Searching",
      "event": "KEYPRESS_ENTER_SEARCH",
      "guard": "searchInput contains a valid integer && animating === false",
      "actions": [
        "synthesize click on #searchBtn"
      ],
      "expected_observables": [
        "dom:#searchInput keypress Enter",
        "dom:#searchBtn click"
      ],
      "triggers": "#searchInput",
      "timeout": 200
    },
    {
      "from": "S0_Idle",
      "to": "S14_Panning",
      "event": "MOUSEDOWN_SVG",
      "guard": "e.button === 0 (left mouse button)",
      "actions": [
        "isDown = true",
        "startX = e.clientX",
        "startY = e.clientY",
        "store viewBox"
      ],
      "expected_observables": [
        "dom:#svgCanvas mousedown"
      ],
      "triggers": "#svgCanvas",
      "timeout": 0
    },
    {
      "from": "S14_Panning",
      "to": "S14_Panning",
      "event": "MOUSEMOVE_WINDOW",
      "guard": "isDown === true",
      "actions": [
        "compute dx,dy and set svg viewBox attribute accordingly"
      ],
      "expected_observables": [
        "dom:#svgCanvas viewBox attribute changes"
      ],
      "triggers": "window",
      "timeout": 0
    },
    {
      "from": "S14_Panning",
      "to": "S0_Idle",
      "event": "MOUSEUP_WINDOW",
      "guard": "true",
      "actions": [
        "isDown = false",
        "svg.style.cursor = 'default'"
      ],
      "expected_observables": [
        "dom:#svgCanvas cursor style reset"
      ],
      "triggers": "window",
      "timeout": 0
    },
    {
      "from": "S0_Idle",
      "to": "S0_Idle",
      "event": "SPEED_CHANGE",
      "guard": "true",
      "actions": [
