{
  "meta": {
    "concept": "WeightedGraph",
    "topic": "Weighted Graph Interactive Demo",
    "educational_goal": "Teach graph construction (nodes/edges), weighted edges, directed vs undirected semantics, shortest path (Dijkstra) and MST algorithms (Prim/Kruskal), and interactive manipulation (move/delete/generate).",
    "expected_interactions": [
      "add_node",
      "add_edge",
      "move_node",
      "delete_node",
      "delete_edge",
      "generate_random",
      "clear_graph",
      "set_source",
      "set_target",
      "run_dijkstra",
      "run_prim",
      "run_kruskal"
    ]
  },
  "states": [
    {
      "id": "S0_Idle",
      "label": "Idle",
      "type": "idle",
      "entry_actions": [
        "render()",
        "updateStatus()",
        "clearTempSelection()"
      ],
      "exit_actions": []
    },
    {
      "id": "S1_Mode_AddNode",
      "label": "Mode_AddNode",
      "type": "atomic",
      "entry_actions": [
        "setMode('addNode')",
        "clearTempSelection()",
        "log('Mode: addNode')"
      ],
      "exit_actions": []
    },
    {
      "id": "S2_Mode_AddEdge",
      "label": "Mode_AddEdge",
      "type": "atomic",
      "entry_actions": [
        "setMode('addEdge')",
        "clearTempSelection()",
        "log('Mode: addEdge')"
      ],
      "exit_actions": []
    },
    {
      "id": "S3_Mode_Move",
      "label": "Mode_Move",
      "type": "atomic",
      "entry_actions": [
        "setMode('move')",
        "clearTempSelection()",
        "log('Mode: move')"
      ],
      "exit_actions": []
    },
    {
      "id": "S4_Mode_Delete",
      "label": "Mode_Delete",
      "type": "atomic",
      "entry_actions": [
        "setMode('del')",
        "clearTempSelection()",
        "log('Mode: delete')"
      ],
      "exit_actions": []
    },
    {
      "id": "S5_Edge_SourceSelected",
      "label": "EdgeSourceSelected",
      "type": "atomic",
      "entry_actions": [
        "tempSelection.firstNode = clickedId",
        "highlightNode(clickedId,'#2563eb')",
        "log('Selected source node ' + clickedId + '. Now click target.')"
      ],
      "exit_actions": [
        "clearTempSelection()"
      ]
    },
    {
      "id": "S6_Prompting_Weight",
      "label": "PromptingWeight",
      "type": "atomic",
      "entry_actions": [
        "prompt('Edge weight (number):', '1')",
        "log('Waiting for weight prompt response')"
      ],
      "exit_actions": []
    },
    {
      "id": "S7_Dragging_Node",
      "label": "DraggingNode",
      "type": "atomic",
      "entry_actions": [
        "dragging = node",
        "svg.setPointerCapture(pointerId)",
        "svg.onpointermove = nodePointerMove",
        "svg.onpointerup = nodePointerUp",
        "highlightNode(node.id,'#2563eb')"
      ],
      "exit_actions": [
        "svg.releasePointerCapture(pointerId)",
        "svg.onpointermove = null",
        "svg.onpointerup = null",
        "dragging = null",
        "updateAllEdges()"
      ]
    },
    {
      "id": "S8_Random_Generating",
      "label": "RandomGenerating",
      "type": "atomic",
      "entry_actions": [
        "document.getElementById('clear-graph').click()",
        "createNode(...) // multiple",
        "createEdge(...) // probabilistic",
        "render()",
        "log('Random graph generated')"
      ],
      "exit_actions": []
    },
    {
      "id": "S9_Clearing_Confirm",
      "label": "ClearingConfirm",
      "type": "atomic",
      "entry_actions": [
        "confirm('Clear entire graph?')"
      ],
      "exit_actions": []
    },
    {
      "id": "S10_SetSrc_Waiting",
      "label": "SetSourceWaiting",
      "type": "atomic",
      "entry_actions": [
        "tempSelection.setSrc = true",
        "tempSelection.setTgt = false",
        "log('Click a node to set Source')"
      ],
      "exit_actions": [
        "tempSelection.setSrc = false"
      ]
    },
    {
      "id": "S11_SetTgt_Waiting",
      "label": "SetTargetWaiting",
      "type": "atomic",
      "entry_actions": [
        "tempSelection.setTgt = true",
        "tempSelection.setSrc = false",
        "log('Click a node to set Target')"
      ],
      "exit_actions": [
        "tempSelection.setTgt = false"
      ]
    },
    {
      "id": "S12_Running_Dijkstra",
      "label": "RunningDijkstra",
      "type": "atomic",
      "entry_actions": [
        "runDijkstra(src,target)",
        "log('Running Dijkstra')"
      ],
      "exit_actions": []
    },
    {
      "id": "S13_Running_Prim",
      "label": "RunningPrim",
      "type": "atomic",
      "entry_actions": [
        "runPrim()",
        "log('Running Prim')"
      ],
      "exit_actions": []
    },
    {
      "id": "S14_Running_Kruskal",
      "label": "RunningKruskal",
      "type": "atomic",
      "entry_actions": [
        "runKruskal()",
        "log('Running Kruskal')"
      ],
      "exit_actions": []
    },
    {
      "id": "S15_Display_Result",
      "label": "DisplayResult",
      "type": "atomic",
      "entry_actions": [
        "clearHighlights()",
        "highlight path or MST (nodes/edges)",
        "log('Result displayed')"
      ],
      "exit_actions": []
    },
    {
      "id": "S16_Edge_Info_Shown",
      "label": "EdgeInfoShown",
      "type": "atomic",
      "entry_actions": [
        "alert(edge info)",
        "log('Edge info shown')"
      ],
      "exit_actions": []
    },
    {
      "id": "S17_Error_Alert",
      "label": "ErrorAlert",
      "type": "atomic",
      "entry_actions": [
        "alert(message)",
        "log('Error/alert shown')"
      ],
      "exit_actions": []
    }
  ],
  "events": [
    {
      "id": "CLICK_MODE_ADD_NODE",
      "event_type": "user_action",
      "description": "User clicks Add Node mode button",
      "triggers": "#mode-add-node"
    },
    {
      "id": "CLICK_MODE_ADD_EDGE",
      "event_type": "user_action",
      "description": "User clicks Add Edge mode button",
      "triggers": "#mode-add-edge"
    },
    {
      "id": "CLICK_MODE_MOVE",
      "event_type": "user_action",
      "description": "User clicks Move mode button",
      "triggers": "#mode-move"
    },
    {
      "id": "CLICK_MODE_DELETE",
      "event_type": "user_action",
      "description": "User clicks Delete mode button",
      "triggers": "#mode-delete"
    },
    {
      "id": "SVG_POINTERDOWN",
      "event_type": "user_action",
      "description": "Pointer down on SVG canvas",
      "triggers": "#svg"
    },
    {
      "id": "NODE_POINTERDOWN",
      "event_type": "user_action",
      "description": "Pointer down on a node group (g.node-group)",
      "triggers": "g.node-group"
    },
    {
      "id": "EDGE_POINTERDOWN",
      "event_type": "user_action",
      "description": "Pointer down on an edge line",
      "triggers": "g.edge-group line"
    },
    {
      "id": "CHANGE_DIRECTED",
      "event_type": "user_action",
      "description": "User toggles Directed checkbox",
      "triggers": "#directed"
    },
    {
      "id": "CHANGE_ALLOW_MULTI",
      "event_type": "user_action",
      "description": "User toggles Allow Multi-edges checkbox",
      "triggers": "#allow-multi"
    },
    {
      "id": "CLICK_CLEAR_GRAPH",
      "event_type": "user_action",
      "description": "User clicks Clear Graph button",
      "triggers": "#clear-graph"
    },
    {
      "id": "CLICK_RANDOM_GRAPH",
      "event_type": "user_action",
      "description": "User clicks Random Graph button",
      "triggers": "#random-graph,#rand-n,#rand-density,#rand-maxw"
    },
    {
      "id": "CLICK_SET_SRC",
      "event_type": "user_action",
      "description": "User clicks Set Src button (prepare to click a node)",
      "triggers": "#set-src"
    },
    {
      "id": "CLICK_SET_TGT",
      "event_type": "user_action",
      "description": "User clicks Set Tgt button (prepare to click a node)",
      "triggers": "#set-tgt"
    },
    {
      "id": "CLICK_DIJKSTRA",
      "event_type": "user_action",
      "description": "User clicks Dijkstra button to run shortest path",
      "triggers": "#dijkstra-btn,#alg-src,#alg-tgt"
    },
    {
      "id": "CLICK_PRIM",
      "event_type": "user_action",
      "description": "User clicks Prim's MST button",
      "triggers": "#prim-btn"
    },
    {
      "id": "CLICK_KRUSKAL",
      "event_type": "user_action",
      "description": "User clicks Kruskal's MST button",
      "triggers": "#kruskal-btn"
    },
    {
      "id": "PROMPT_WEIGHT_SUBMIT",
      "event_type": "system_event",
      "description": "User enters weight in prompt and confirms",
      "triggers": "window.prompt (synchronous)"
    },
    {
      "id": "PROMPT_WEIGHT_CANCEL",
      "event_type": "system_event",
      "description": "User cancels the weight prompt",
      "triggers": "window.prompt (synchronous)"
    },
    {
      "id": "CONFIRM_TRUE",
      "event_type": "system_event",
      "description": "User confirmed a confirm() dialog (true returned)",
      "triggers": "window.confirm (synchronous)"
    },
    {
      "id": "CONFIRM_FALSE",
      "event_type": "system_event",
      "description": "User canceled a confirm() dialog (false returned)",
      "triggers": "window.confirm (synchronous)"
    },
    {
      "id": "POINTER_UP",
      "event_type": "user_action",
      "description": "Pointer up event on SVG after dragging",
      "triggers": "#svg"
    },
    {
      "id": "EDGE_INFO_SHOWN",
      "event_type": "system_event",
      "description": "Alert showing edge info was displayed",
      "triggers": "alert()"
    },
    {
      "id": "ALERT_SHOWN",
      "event_type": "system_event",
      "description": "Generic alert shown (invalid operations)",
      "triggers": "alert()"
    }
  ],
  "transitions": [
    {
      "from": "S0_Idle",
      "to": "S1_Mode_AddNode",
      "event": "CLICK_MODE_ADD_NODE",
      "guard": "true",
      "actions": [
        "setMode('addNode')",
        "clearTempSelection()",
        "log('Mode: addNode')"
      ],
      "expected_observables": [
        "dom:#mode-add-node",
        "dom:#mode-add-node.active"
      ],
      "timeout": 500
    },
    {
      "from": "S0_Idle",
      "to": "S2_Mode_AddEdge",
      "event": "CLICK_MODE_ADD_EDGE",
      "guard": "true",
      "actions": [
        "setMode('addEdge')",
        "clearTempSelection()",
        "log('Mode: addEdge')"
      ],
      "expected_observables": [
        "dom:#mode-add-edge",
        "dom:#mode-add-edge.active"
      ],
      "timeout": 500
    },
    {
      "from": "S0_Idle",
      "to": "S3_Mode_Move",
      "event": "CLICK_MODE_MOVE",
      "guard": "true",
      "actions": [
        "setMode('move')",
        "clearTempSelection()",
        "log('Mode: move')"
      ],
      "expected_observables": [
        "dom:#mode-move",
        "dom:#mode-move.active"
      ],
      "timeout": 500
    },
    {
      "from": "S0_Idle",
      "to": "S4_Mode_Delete",
      "event": "CLICK_MODE_DELETE",
      "guard": "true",
      "actions": [
        "setMode('del')",
        "clearTempSelection()",
        "log('Mode: delete')"
      ],
      "expected_observables": [
        "dom:#mode-delete",
        "dom:#mode-delete.active"
      ],
      "timeout": 500
    },
    {
      "from": "S1_Mode_AddNode",
      "to": "S1_Mode_AddNode",
      "event": "SVG_POINTERDOWN",
      "guard": "mode === 'addNode'",
      "actions": [
        "createNode(atPointer.x, atPointer.y)",
        "render()",
        "log('Node created')"
      ],
      "expected_observables": [
        "dom:#svg",
        "dom:g.node-group"
      ],
      "timeout": 500
    },
    {
      "from": "S2_Mode_AddEdge",
      "to": "S5_Edge_SourceSelected",
      "event": "NODE_POINTERDOWN",
      "guard": "tempSelection.firstNode == null",
      "actions": [
        "tempSelection.firstNode = clickedId",
        "highlightNode(clickedId,'#2563eb')",
        "log('Selected source node ' + clickedId + '. Now click target.')"
      ],
      "expected_observables": [
        "dom:g.node-group",
        "dom:g.node-group[data-id]"
      ],
      "timeout": 1000
    },
    {
      "from": "S5_Edge_SourceSelected",
      "to": "S6_Prompting_Weight",
      "event": "NODE_POINTERDOWN",
      "guard": "clickedId !== tempSelection.firstNode",
      "actions": [
        "tempSelection.targetNode = clickedId",
        "log('Selected target node ' + clickedId + ' - prompting for weight')",
        "prompt('Edge weight (number):', '1')"
      ],
      "expected_observables": [
        "dom:g.node-group",
        "dom:window.prompt"
      ],
      "timeout": 2000
    },
    {
      "from": "S6_Prompting_Weight",
      "to": "S2_Mode_AddEdge",
      "event": "PROMPT_WEIGHT_SUBMIT",
      "guard": "Number(weight) === Number(weight) && !Number.isNaN(Number(weight))",
      "actions": [
        "createEdge(tempSelection.firstNode,tempSelection.targetNode,Number(weight), document.getElementById('directed').checked)",
        "log('Edge created: ' + tempSelection.firstNode + ' -> ' + tempSelection.targetNode + ' w=' + weight)",
        "clearTempSelection()",
        "render()"
      ],
      "expected_observables": [
        "dom:g.edge-group",
        "dom:.edge-weight-label"
      ],
      "timeout": 2000
    },
    {
      "from": "S6_Prompting_Weight",
      "to": "S2_Mode_AddEdge",
      "event": "PROMPT_WEIGHT_CANCEL",
      "guard": "true",
      "actions": [
        "clearTempSelection()",
        "log('Edge creation canceled')"
      ],
      "expected_observables": [
        "dom:window.prompt"
      ],
      "timeout": 500
    },
    {
      "from": "S3_Mode_Move",
      "to": "S7_Dragging_Node",
      "event": "NODE_POINTERDOWN",
      "guard": "mode === 'move'",
      "actions": [
        "dragging = nodeWithId(clickedId)",
        "dragOffset = computeOffset(pointer, dragging)",
        "svg.setPointerCapture(pointerId)",
        "svg.onpointermove = nodePointerMove",
        "svg.onpointerup = nodePointerUp",
        "highlightNode(clickedId,'#2563eb')"
      ],
      "expected_observables": [
        "dom:g.node-group",
        "dom:g.node-group[data-id]"
      ],
      "timeout": 0
    },
    {
      "from": "S7_Dragging_Node",
      "to": "S3_Mode_Move",
      "event": "POINTER_UP",
      "guard": "true",
      "actions": [
        "svg.releasePointerCapture(pointerId)",
        "svg.onpointermove = null",
        "svg.onpointerup = null",
        "dragging = null",
        "updateAllEdges()",
        "log('Drag ended')"
      ],
      "expected_observables": [
        "dom:g.node-group[transform]",
        "dom:g.edge-group line"
      ],
      "timeout": 0
    },
    {
      "from": "S4_Mode_Delete",
      "to": "S0_Idle",
      "event": "NODE_POINTERDOWN",
      "guard": "confirm('Delete node ' + clickedId + ' and its edges?') === true",
      "actions": [
        "removeNodeById(clickedId)",
        "render()",
        "log('Deleted node ' + clickedId)"
      ],
      "expected_observables": [
        "dom:g.node-group:not([data-id='",
        "dom:g.edge-group"
      ],
      "timeout": 1000
    },
    {
      "from": "S4_Mode_Delete",
      "to": "S4_Mode_Delete",
      "event": "NODE_POINTERDOWN",
      "guard": "confirm(...) === false",
      "actions": [
        "log('Node deletion canceled')"
      ],
      "expected_observables": [
        "dom:g.node-group"
      ],
      "timeout": 500
    },
    {
      "from": "S4_Mode_Delete",
      "to": "S0_Idle",
      "event": "EDGE_POINTERDOWN",
      "guard": "confirm('Delete this edge?') === true",
      "actions": [
        "removeEdgeById(clickedEdgeId)",
        "log('Deleted edge ' + clickedEdgeId)"
      ],
      "expected_observables": [
        "dom:g.edge-group line",
        "dom:g.edge-group:not(:has(line))"
      ],
      "timeout": 1000
    },
    {
      "from": "S4_Mode_Delete",
      "to": "S4_Mode_Delete",
      "event": "EDGE_POINTERDOWN",
      "guard": "confirm(...) === false",
      "actions": [
        "log('Edge deletion canceled')"
      ],
      "expected_observables": [
        "dom:g.edge-group line"
      ],
      "timeout": 500
    },
    {
      "from": "S0_Idle",
      "to": "S9_Clearing_Confirm",
      "event": "CLICK_CLEAR_GRAPH",
      "guard": "true",
      "actions": [
        "confirm('Clear entire graph?')"
      ],
      "expected_observables": [
        "dom:#clear-graph"
      ],
      "timeout": 200
    },
    {
      "from": "S9_Clearing_Confirm",
      "to": "S0_Idle",
      "event": "CONFIRM_TRUE",
      "guard": "true",
      "actions": [
        "nodes.forEach(remove DOM)",
        "edges.forEach(remove DOM)",
        "nodes = []",
        "edges = []",
        "updateStatus()",
        "log('Graph cleared')"
      ],
      "expected_observables": [
        "dom:g.node-group (none)",
        "dom:g.edge-group (none)"
      ],
      "timeout": 500
    },
    {
      "from": "S9_Clearing_Confirm",
      "to": "S0_Idle",
      "event": "CONFIRM_FALSE",
      "guard": "true",
      "actions": [
        "log('Clear canceled')"
      ],
      "expected_observables": [
        "dom:g.node-group",
        "dom:g.edge-group"
      ],
      "timeout": 200
    },
    {
      "from": "S0_Idle",
      "to": "S8_Random_Generating",
      "event": "CLICK_RANDOM_GRAPH",
      "guard": "true",
      "actions": [
        "document.getElementById('clear-graph').click()",
        "createNode(...) // add nodes at random positions",
        "createEdge(...) // add probabilistic edges",
        "render()",
        "log('Random graph: generated')"
      ],
      "expected_observables": [
        "dom:#random-graph",
        "dom:g.node-group",
        "dom:g.edge-group"
      ],
      "timeout": 2000
    },
    {
      "from": "S8_Random_Generating",
      "to": "S0_Idle",
      "event": "true",
      "guard": "true",
      "actions": [
        "updateStatus()",
        "log('Random graph complete')"
      ],
      "expected_observables": [
        "dom:g.node-group",
        "dom:g.edge-group"
      ],
      "timeout": 500
    },
    {
      "from": "S0_Idle",
      "to": "S10_SetSrc_Waiting",
      "event": "CLICK_SET_SRC",
      "guard": "true",
      "actions": [
        "tempSelection.setSrc = true",
        "tempSelection.setTgt = false",
        "log('Click a node to set Source')"
      ],
      "expected_observables": [
        "dom:#set-src",
        "dom:#alg-src"
      ],
      "timeout": 500
    },
    {
      "from": "S10_SetSrc_Waiting",
      "to": "S0_Idle",
      "event": "NODE_POINTERDOWN",
      "guard": "tempSelection.setSrc === true",
      "actions": [
        "document.getElementById('alg-src').value = clickedId",
        "tempSelection.setSrc = false",
        "highlightNode(clickedId,'#2563eb')",
        "log('Source set to ' + clickedId)"
      ],
      "expected_observables": [
        "dom:#alg-src[value]",
        "dom:g.node-group"
      ],
      "timeout": 500
    },
    {
      "from": "S0_Idle",
      "to": "S11_SetTgt_Waiting",
      "event": "CLICK_SET_TGT",
      "guard": "true",
      "actions": [
        "tempSelection.setTgt = true",
        "tempSelection.setSrc = false",
        "log('Click a node to set Target')"
      ],
      "expected_observables": [
        "dom:#set-tgt",
        "dom:#alg-tgt"
      ],
      "timeout": 500
    },
    {
      "from": "S11_SetTgt_Waiting",
      "to": "S0_Idle",
      "event": "NODE_POINTERDOWN",
      "guard": "tempSelection.setTgt === true",
      "actions": [
        "document.getElementById('alg-tgt').value = clickedId",
        "tempSelection.setTgt = false",
        "highlightNode(clickedId,'#2563eb')",
        "log('Target set to ' + clickedId)"
      ],
      "expected_observables": [
        "dom:#alg-tgt[value]",
        "dom:g.node-group"
      ],
      "timeout": 500
    },
    {
      "from": "S0_Idle",
      "to": "S12_Running_Dijkstra",
      "event": "CLICK_DIJKSTRA",
      "guard": "document.getElementById('alg-src').value && document.getElementById('alg-tgt').value && nodes.find(n=>n.id===Number(document.getElementById('alg-src').value)) && nodes.find(n=>n.id===Number(document.getElementById('alg-tgt').value))",
      "actions": [
        "runDijkstra(Number(document.getElementById('alg-src').value), Number(document.getElementById('alg-tgt').value))",
        "log('Dijkstra started')"
      ],
      "expected_observables": [
        "dom:#dijkstra-btn",
        "dom:.edge-weight-label",
        "dom:g.edge-group line"
      ],
      "timeout": 5000
    },
    {
      "from": "S12_Running_Dijkstra",
      "to": "S15_Display_Result",
      "event": "true",
      "guard": "true",
      "actions": [
        "clearHighlights()",
        "highlight path edges and nodes (in runDijkstra)",
        "log('Dijkstra complete')"
      ],
      "expected_observables": [
        "dom:g.edge-group line[stroke='#10b981']",
        "dom:g.node-group circle[fill='#10b981']"
      ],
      "timeout": 2000
    },
    {
      "from": "S0_Idle",
      "to": "S13_Running_Prim",
      "event": "CLICK_PRIM",
      "guard": "nodes.length > 0",
      "actions": [
        "runPrim()",
        "log('Prim started')"
      ],
      "expected_observables": [
        "dom:#prim-btn"
      ],
      "timeout": 5000
    },
    {
      "from": "S13_Running_Prim",
      "to": "S15_Display_Result",
      "event": "true",
      "guard": "true",
      "actions": [
        "clearHighlights()",
        "highlight MST edges/nodes (in runPrim)",
        "log('Prim complete')"
      ],
      "expected_observables": [
        "dom:g.edge-group line[stroke='#10b981']",
        "dom:g.node-group circle[fill='#10b981']"
      ],
      "timeout": 2000
    },
    {
      "from": "S0_Idle",
      "to": "S14_Running_Kruskal",
      "event": "CLICK_KRUSKAL",
      "guard": "nodes.length > 0",
      "actions": [
        "runKruskal()",
        "log('Kruskal started')"
