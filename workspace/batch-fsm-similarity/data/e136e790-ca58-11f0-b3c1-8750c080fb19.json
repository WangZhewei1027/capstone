else {\n                    this.insertNode(node.right, newNode);\n                }\n            }\n        }\n\n        reset() {\n            this.root = null;\n        }\n\n        getTreeElements() {\n            return this.toArray(this.root);\n        }\n\n        toArray(node) {\n            if (!node) {\n                return [];\n            }\n            return [\n                node,\n                ...this.toArray(node.left),\n                ...this.toArray(node.right),\n            ];\n        }\n    }\n\n    const bst = new BST();\n\n    function insertNode() {\n        const valueInput = document.getElementById('valueInput');\n        const value = parseInt(valueInput.value);\n        if (!isNaN(value)) {\n            bst.insert(value);\n            valueInput.value = '';\n            drawTree();\n        }\n    }\n\n    function resetTree() {\n        bst.reset();\n        document.getElementById('bst').innerHTML = '';\n    }\n\n    function drawTree() {\n        const container = document.getElementById('bst');\n        container.innerHTML = '';\n\n        const elements = bst.getTreeElements();\n        drawNodes(elements, container);\n    }\n\n    function drawNodes(nodes, container, x = 0, y = 0, gap = 50) {\n        if (nodes.length === 0) return;\n\n        const nodeWidth = 40;\n        nodes.forEach((node, index) => {\n            const div = document.createElement('div');\n            div.className = 'node';\n            div.innerText = node.value;\n            div.style.left = `${x + index * gap}px`;\n            div.style.top = `${y}px`;\n            container.appendChild(div);\n\n            const leftChild = node.left ? node.left.value : null;\n            const rightChild = node.right ? node.right.value : null;\n\n            if (leftChild) {\n                drawNodes([node.left], container, x + index * gap - gap / 2, y + 50, gap / 2);\n            }\n            if (rightChild) {\n                drawNodes([node.right], container, x + index * gap + gap / 2, y + 50, gap / 2);\n            }\n        });\n    }\n</script>\n\n</body>\n</html>"
