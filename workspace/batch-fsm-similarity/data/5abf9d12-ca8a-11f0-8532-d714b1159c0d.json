    "content": "<html>\n<head>\n    <title>K-Means Clustering</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        #chart {\n            width: 800px;\n            height: 600px;\n            border: 1px solid black;\n        }\n    </style>\n</head>\n<body>\n    <h1>K-Means Clustering</h1>\n    <p>Enter the number of clusters:</p>\n    <input type=\"number\" id=\"clusters\" value=\"3\">\n    <button id=\"run\">Run</button>\n    <div id=\"chart\"></div>\n\n    <script>\n        let clusters = document.getElementById('clusters').value;\n        let data = [];\n\n        function addData() {\n            let x = parseFloat(document.getElementById('x').value);\n            let y = parseFloat(document.getElementById('y').value);\n            let z = parseFloat(document.getElementById('z').value);\n            data.push([x, y, z]);\n        }\n\n        function drawChart() {\n            let ctx = document.getElementById('chart').getContext('2d');\n            let chart = new Chart(ctx, {\n                type: 'scatter',\n                data: {\n                    labels: data.map((d) => d[0]),\n                    datasets: [{\n                        label: 'Data Points',\n                        data: data.map((d) => d[1]),\n                        backgroundColor: 'rgba(255, 99, 132, 0.2)',\n                        borderColor: 'rgba(255, 99, 132, 1)',\n                        borderWidth: 1\n                    }]\n                },\n                options: {\n                    scales: {\n                        y: {\n                            beginAtZero: true\n                        }\n                    },\n                    tooltips: {\n                        callbacks: {\n                            label: (tooltipItem, data) => {\n                                return `x: ${data[0]}, y: ${data[1]}, z: ${data[2]}`;\n                            }\n                        }\n                    }\n                }\n            });\n        }\n\n        document.getElementById('run').addEventListener('click', function() {\n            addData();\n            drawChart();\n        });\n\n        document.getElementById('x').addEventListener('input', function() {\n            addData();\n            drawChart();\n        });\n\n        document.getElementById('y').addEventListener('input', function() {\n            addData();\n            drawChart();\n        });\n\n        document.getElementById('z').addEventListener('input', function() {\n            addData();\n            drawChart();\n        });\n    </script>\n</body>\n</html>"
