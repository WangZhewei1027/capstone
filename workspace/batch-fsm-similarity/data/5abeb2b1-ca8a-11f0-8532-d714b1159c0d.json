    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Breadth-First Search</title>\n    <style>\n        #graph {\n            border: 1px solid black;\n        }\n    </style>\n</head>\n<body>\n    <h1>Breadth-First Search</h1>\n    <button id=\"start-button\">Start</button>\n    <button id=\"stop-button\">Stop</button>\n    <div id=\"graph\"></div>\n\n    <script>\n        let graph = document.getElementById('graph');\n        let startButton = document.getElementById('start-button');\n        let stopButton = document.getElementById('stop-button');\n        let graphCanvas = document.getElementById('graph');\n        let graphCtx = graphCanvas.getContext('2d');\n\n        let nodes = [];\n        let edges = [];\n        let visited = new Set();\n\n        startButton.addEventListener('click', start);\n        stopButton.addEventListener('click', stop);\n\n        function start() {\n            startButton.disabled = true;\n            stopButton.disabled = false;\n            graphCanvas.width = window.innerWidth;\n            graphCanvas.height = window.innerHeight;\n            graphCtx.clearRect(0, 0, graphCanvas.width, graphCanvas.height);\n\n            for (let i = 0; i < 5; i++) {\n                for (let j = 0; j < 5; j++) {\n                    nodes.push({ x: i, y: j });\n                    edges.push({ from: i, to: j, weight: Math.random() });\n                }\n            }\n\n            for (let node of nodes) {\n                graphCtx.beginPath();\n                graphCtx.arc(node.x, node.y, 10, 0, 2 * Math.PI);\n                graphCtx.fillStyle = 'blue';\n                graphCtx.fill();\n            }\n\n            for (let edge of edges) {\n                graphCtx.beginPath();\n                graphCtx.moveTo(edge.from.x, edge.from.y);\n                graphCtx.lineTo(edge.to.x, edge.to.y);\n                graphCtx.strokeStyle = 'red';\n                graphCtx.lineWidth = 2;\n                graphCtx.stroke();\n            }\n\n            startButton.disabled = false;\n            stopButton.disabled = true;\n        }\n\n        function stop() {\n            startButton.disabled = true;\n            stopButton.disabled = false;\n            graphCanvas.width = window.innerWidth;\n            graphCanvas.height = window.innerHeight;\n            graphCtx.clearRect(0, 0, graphCanvas.width, graphCanvas.height);\n\n            for (let node of nodes) {\n                graphCtx.beginPath();\n                graphCtx.arc(node.x, node.y, 10, 0, 2 * Math.PI);\n                graphCtx.fillStyle = 'blue';\n                graphCtx.fill();\n            }\n\n            for (let edge of edges) {\n                graphCtx.beginPath();\n                graphCtx.moveTo(edge.from.x, edge.from.y);\n                graphCtx.lineTo(edge.to.x, edge.to.y);\n                graphCtx.strokeStyle = 'red';\n                graphCtx.lineWidth = 2;\n                graphCtx.stroke();\n            }\n\n            startButton.disabled = false;\n            stopButton.disabled = true;\n        }\n\n        function bfs(start, end) {\n            let queue = [start];\n            visited.add(start);\n\n            while (queue.length > 0) {\n                let node = queue.shift();\n                for (let edge of edges) {\n                    if (edge.to.x === node.x && edge.to.y === node.y && !visited.has(edge.to)) {\n                        visited.add(edge.to);\n                        queue.push(edge.to);\n                    }\n                }\n            }\n\n            if (start.x === end.x && start.y === end.y) {\n                console.log(\"BFS: Found path from\", start, \"to\", end);\n            } else {\n                console.log(\"BFS: No path found\");\n            }\n        }\n\n        startButton.addEventListener('click', function() {\n            bfs(nodes[0], nodes[4]);\n        });\n\n        stopButton.addEventListener('click', function() {\n            bfs(nodes[0], nodes[4]);\n        });\n    </script>\n</body>\n</html>"
