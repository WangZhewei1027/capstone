    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Union-Find Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            background-color: #f4f4f4;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            padding: 20px;\n        }\n        #output {\n            margin-top: 20px;\n            background: white;\n            padding: 10px;\n            border: 1px solid #ccc;\n            width: 300px;\n            text-align: left;\n        }\n        button {\n            margin: 5px;\n            padding: 10px;\n        }\n    </style>\n</head>\n<body>\n    <h1>Union-Find (Disjoint Set) Demonstration</h1>\n    <input type=\"number\" id=\"size\" placeholder=\"Enter number of elements\" min=\"1\" max=\"100\">\n    <button id=\"initialize\">Initialize</button>\n    <button id=\"unionButton\">Union</button>\n    <button id=\"findButton\">Find</button>\n    <div id=\"output\"></div>\n\n    <script>\n        class UnionFind {\n            constructor(size) {\n                this.parent = Array.from({ length: size }, (_, i) => i);\n                this.rank = Array(size).fill(1);\n                this.size = size;\n            }\n\n            find(x) {\n                if (this.parent[x] !== x) {\n                    this.parent[x] = this.find(this.parent[x]); // Path compression\n                }\n                return this.parent[x];\n            }\n\n            union(x, y) {\n                let rootX = this.find(x);\n                let rootY = this.find(y);\n\n                if (rootX !== rootY) {\n                    // Union by rank\n                    if (this.rank[rootX] > this.rank[rootY]) {\n                        this.parent[rootY] = rootX;\n                    } else if (this.rank[rootX] < this.rank[rootY]) {\n                        this.parent[rootX] = rootY;\n                    } else {\n                        this.parent[rootY] = rootX;\n                        this.rank[rootX]++;\n                    }\n                    return true;\n                }\n                return false;\n            }\n\n            getParentArray() {\n                return this.parent;\n            }\n        }\n\n        let uf, currentSize;\n\n        document.getElementById('initialize').onclick = function() {\n            currentSize = parseInt(document.getElementById('size').value);\n            if (isNaN(currentSize) || currentSize <= 0) {\n                alert(\"Please enter a valid size.\");\n                return;\n            }\n            uf = new UnionFind(currentSize);\n            displayOutput(`Union-Find initialized with ${currentSize} elements.`);\n        }\n\n        document.getElementById('unionButton').onclick = function() {\n            if (!uf) {\n                alert(\"Please initialize Union-Find first.\");\n                return;\n            }\n            let x = Math.floor(Math.random() * currentSize);\n            let y = Math.floor(Math.random() * currentSize);\n            if (uf.union(x, y)) {\n                displayOutput(`Union performed between ${x} and ${y}.`);\n            } else {\n                displayOutput(`Elements ${x} and ${y} are already in the same set.`);\n            }\n        }\n\n        document.getElementById('findButton').onclick = function() {\n            if (!uf) {\n                alert(\"Please initialize Union-Find first.\");\n                return;\n            }\n            let x = Math.floor(Math.random() * currentSize);\n            let root = uf.find(x);\n            displayOutput(`The root of element ${x} is ${root}.`);\n        }\n\n        function displayOutput(message) {\n            document.getElementById('output').innerText += message + '\\n';\n        }\n    </script>\n</body>\n</html>"
