    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Kruskal's Algorithm Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 0;\n            padding: 20px;\n            background-color: #f4f4f4;\n        }\n        canvas {\n            border: 1px solid #cccccc;\n            background-color: #ffffff;\n        }\n        #controls {\n            margin-bottom: 20px;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Kruskal's Algorithm Visualization</h1>\n<div id=\"controls\">\n    <button onclick=\"generateGraph()\">Generate Random Graph</button>\n    <button onclick=\"runKruskal()\">Run Kruskal's Algorithm</button>\n</div>\n<canvas id=\"graphCanvas\" width=\"800\" height=\"600\"></canvas>\n\n<script>\nconst canvas = document.getElementById('graphCanvas');\nconst ctx = canvas.getContext('2d');\n\nlet edges = [];\nlet vertices = [];\nconst numVertices = 6;\n\nfunction generateGraph() {\n    edges = [];\n    vertices = [];\n    ctx.clearRect(0, 0, canvas.width, canvas.height);\n    \n    for (let i = 0; i < numVertices; i++) {\n        vertices.push({ x: Math.random() * canvas.width, y: Math.random() * canvas.height });\n    }\n\n    for (let i = 0; i < numVertices; i++) {\n        for (let j = i + 1; j < numVertices; j++) {\n            const weight = Math.floor(Math.random() * 10) + 1; // random weight between 1 and 10\n            edges.push({ source: i, target: j, weight: weight });\n        }\n    }\n    \n    drawGraph();\n}\n\nfunction drawGraph() {\n    edges.forEach(edge => {\n        const { source, target, weight } = edge;\n        drawEdge(vertices[source], vertices[target], weight);\n    });\n\n    vertices.forEach((vertex, index) => {\n        drawVertex(vertex, index);\n    });\n}\n\nfunction drawEdge(v1, v2, weight) {\n    ctx.beginPath();\n    ctx.moveTo(v1.x, v1.y);\n    ctx.lineTo(v2.x, v2.y);\n    ctx.strokeStyle = '#cccccc';\n    ctx.stroke();\n    \n    const midX = (v1.x + v2.x) / 2;\n    const midY = (v1.y + v2.y) / 2;\n    ctx.fillStyle = 'black';\n    ctx.fillText(weight, midX, midY);\n}\n\nfunction drawVertex(vertex, index) {\n    ctx.fillStyle = 'blue';\n    ctx.beginPath();\n    ctx.arc(vertex.x, vertex.y, 5, 0, Math.PI * 2);\n    ctx.fill();\n    ctx.fillStyle = 'black';\n    ctx.fillText(index, vertex.x - 5, vertex.y - 10);\n}\n\nfunction find(parent, i) {\n    if (parent[i] === -1) {\n        return i;\n    }\n    return find(parent, parent[i]);\n}\n\nfunction union(parent, x, y) {\n    parent[x] = y;\n}\n\nfunction runKruskal() {\n    let parent = Array(numVertices).fill(-1);\n    edges.sort((a, b) => a.weight - b.weight);\n    let mstEdges = [];\n    \n    edges.forEach(edge => {\n        const { source, target } = edge;\n        const xset = find(parent, source);\n        const yset = find(parent, target);\n        \n        if (xset !== yset) {\n            mstEdges.push(edge);\n            union(parent, xset, yset);\n        }\n    });\n\n    highlightMST(mstEdges);\n}\n\nfunction highlightMST(mstEdges) {\n    ctx.clearRect(0, 0, canvas.width, canvas.height);\n    drawGraph();\n    \n    mstEdges.forEach(edge => {\n        const { source, target } = edge;\n        ctx.beginPath();\n        ctx.moveTo(vertices[source].x, vertices[source].y);\n        ctx.lineTo(vertices[target].x, vertices[target].y);\n        ctx.strokeStyle = 'red';\n        ctx.lineWidth = 2;\n        ctx.stroke();\n    });\n}\n\n</script>\n</body>\n</html>"
