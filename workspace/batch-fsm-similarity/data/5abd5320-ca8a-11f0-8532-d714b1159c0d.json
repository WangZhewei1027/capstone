    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Binary Tree</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        .binary-tree {\n            width: 50%;\n            margin: 40px auto;\n            padding: 20px;\n            border: 1px solid #ddd;\n            border-radius: 10px;\n            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n        }\n        .node {\n            padding: 20px;\n            border-bottom: 1px solid #ddd;\n        }\n        .node:hover {\n            background-color: #f0f0f0;\n        }\n    </style>\n</head>\n<body>\n    <div class=\"binary-tree\">\n        <h2>Binary Tree</h2>\n        <pre id=\"tree\"></pre>\n        <form id=\"input-form\">\n            <label for=\"value\">Enter a value:</label>\n            <input type=\"number\" id=\"value\" />\n            <button id=\"add-btn\">Add</button>\n        </form>\n        <pre id=\"output\"></pre>\n    </div>\n\n    <script>\n        class Node {\n            constructor(value, left = null, right = null) {\n                this.value = value;\n                this.left = left;\n                this.right = right;\n            }\n        }\n\n        class BinaryTree {\n            constructor() {\n                this.root = null;\n            }\n\n            add(value) {\n                if (!this.root) {\n                    this.root = new Node(value);\n                } else {\n                    this._add(value, this.root);\n                }\n            }\n\n            _add(value, node) {\n                if (value < node.value) {\n                    if (node.left) {\n                        this._add(value, node.left);\n                    } else {\n                        node.left = new Node(value);\n                    }\n                } else {\n                    if (node.right) {\n                        this._add(value, node.right);\n                    } else {\n                        node.right = new Node(value);\n                    }\n                }\n            }\n\n            printTree(node, level = 0) {\n                const str = '  '.repeat(level) + node.value;\n                if (node.left) {\n                    str += ' <- ' + this.printTree(node.left, level + 1);\n                }\n                if (node.right) {\n                    str += ' -> ' + this.printTree(node.right, level + 1);\n                }\n                document.getElementById('output').innerHTML += str + '<br>';\n            }\n        }\n\n        const tree = new BinaryTree();\n        const inputForm = document.getElementById('input-form');\n        const addBtn = document.getElementById('add-btn');\n        const treeElement = document.getElementById('tree');\n        const outputElement = document.getElementById('output');\n\n        addBtn.addEventListener('click', () => {\n            const value = parseInt(document.getElementById('value').value);\n            tree.add(value);\n            tree.printTree(tree.root);\n        });\n\n        inputForm.addEventListener('submit', (e) => {\n            e.preventDefault();\n            const value = parseInt(document.getElementById('value').value);\n            tree.add(value);\n            tree.printTree(tree.root);\n        });\n    </script>\n</body>\n</html>"
