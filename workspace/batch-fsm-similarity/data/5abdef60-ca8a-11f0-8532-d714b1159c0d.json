    "role": "assistant",
    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Adjacency Matrix</title>\n    <style>\n        table {\n            border-collapse: collapse;\n        }\n        th, td {\n            border: 1px solid black;\n            padding: 5px;\n            text-align: left;\n        }\n    </style>\n</head>\n<body>\n    <h1>Adjacency Matrix</h1>\n    <input id=\"num-adj\" type=\"number\" placeholder=\"Enter the number of vertices\">\n    <button id=\"generate-btn\">Generate Adjacency Matrix</button>\n    <button id=\"clear-btn\">Clear Matrix</button>\n    <table id=\"adj-matrix\"></table>\n\n    <script>\n        let numVertices = document.getElementById('num-adj').value;\n        let adjMatrix = new Array(numVertices).fill(0).map(() => new Array(numVertices).fill(0));\n\n        document.getElementById('generate-btn').addEventListener('click', () => {\n            for (let i = 0; i < numVertices; i++) {\n                for (let j = 0; j < numVertices; j++) {\n                    if (i === j) {\n                        adjMatrix[i][j] = 1;\n                    }\n                }\n            }\n            renderMatrix();\n        });\n\n        document.getElementById('clear-btn').addEventListener('click', () => {\n            adjMatrix = new Array(numVertices).fill(0).map(() => new Array(numVertices).fill(0));\n            renderMatrix();\n        });\n\n        function renderMatrix() {\n            document.getElementById('adj-matrix').innerHTML = '';\n            for (let i = 0; i < numVertices; i++) {\n                for (let j = 0; j < numVertices; j++) {\n                    document.getElementById('adj-matrix').appendChild(document.createElement('tr'));\n                    document.getElementById('adj-matrix').appendChild(document.createElement('td'));\n                    document.getElementById('adj-matrix').appendChild(document.createElement('td')).textContent = adjMatrix[i][j];\n                    document.getElementById('adj-matrix').appendChild(document.createElement('td')).textContent = (i === j ? '1' : '0');\n                }\n            }\n        }\n    </script>\n</body>\n</html>"
