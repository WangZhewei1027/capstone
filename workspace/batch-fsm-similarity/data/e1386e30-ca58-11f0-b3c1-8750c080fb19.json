    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Topological Sort Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            background-color: #f4f4f4;\n            padding: 20px;\n        }\n        canvas {\n            border: 1px solid #ccc;\n            margin-bottom: 20px;\n        }\n        button {\n            padding: 10px 15px;\n            background-color: #28a745;\n            color: white;\n            border: none;\n            cursor: pointer;\n        }\n        button:hover {\n            background-color: #218838;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Topological Sort Visualization</h1>\n<canvas id=\"canvas\" width=\"600\" height=\"400\"></canvas>\n<br>\n<button id=\"runSort\">Perform Topological Sort</button>\n<p id=\"result\"></p>\n\n<script>\n    // Create a graph using an adjacency list\n    const graph = {\n        0: [1, 2],\n        1: [2],\n        2: [3],\n        3: [],\n        4: [0, 1]\n    };\n\n    // Function to perform topological sort\n    function topologicalSort(graph) {\n        const visited = new Set();\n        const result = [];\n        const tempMark = new Set();\n\n        function visit(node) {\n            if (tempMark.has(node)) throw new Error(\"Graph is not a Directed Acyclic Graph (DAG)\");\n            if (!visited.has(node)) {\n                tempMark.add(node);\n                if (graph[node]) {\n                    for (const neighbor of graph[node]) {\n                        visit(neighbor);\n                    }\n                }\n                tempMark.delete(node);\n                visited.add(node);\n                result.push(node);\n            }\n        }\n\n        for (let node in graph) {\n            visit(parseInt(node));\n        }\n\n        return result.reverse();\n    }\n\n    // Function to visualize the graph and the sort\n    function visualize() {\n        const canvas = document.getElementById('canvas');\n        const ctx = canvas.getContext('2d');\n        ctx.clearRect(0, 0, canvas.width, canvas.height);\n        \n        // Draw the graph\n        const positions = {\n            0: { x: 50, y: 200 },\n            1: { x: 150, y: 100 },\n            2: { x: 150, y: 300 },\n            3: { x: 250, y: 200 },\n            4: { x: 50, y: 100 }\n        };\n\n        // Draw nodes\n        for (const node in positions) {\n            ctx.beginPath();\n            ctx.arc(positions[node].x, positions[node].y, 20, 0, Math.PI * 2, false);\n            ctx.fillStyle = '#007bff';\n            ctx.fill();\n            ctx.stroke();\n            ctx.fillStyle = 'white';\n            ctx.fillText(node, positions[node].x - 5, positions[node].y + 5);\n        }\n\n        // Draw edges\n        for (const [from, edges] of Object.entries(graph)) {\n            edges.forEach(to => {\n                ctx.beginPath();\n                ctx.moveTo(positions[from].x, positions[from].y);\n                ctx.lineTo(positions[to].x, positions[to].y);\n                ctx.stroke();\n            });\n        }\n    }\n\n    // Run sort on button click\n    document.getElementById('runSort').onclick = function() {\n        try {\n            const sortedOrder = topologicalSort(graph);\n            document.getElementById('result').textContent = 'Topological Sort Order: ' + sortedOrder.join(', ');\n        } catch (e) {\n            document.getElementById('result').textContent = e.message;\n        }\n    };\n\n    // Initial visualization\n    visualize();\n</script>\n\n</body>\n</html>"
