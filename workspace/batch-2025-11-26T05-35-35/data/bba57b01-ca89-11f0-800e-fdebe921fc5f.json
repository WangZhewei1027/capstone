  }\n        }\n\n        function addVertexClick() {\n            let x = parseInt(input.value);\n            if (x <= unionFind.length) {\n                let node = document.createElement('div');\n                node.classList.add('node');\n                node.innerHTML = `Node ${x}`;\n                graph.appendChild(node);\n                unionFind[x] = x;\n                drawGraph();\n            }\n        }\n\n        function mergeVerticesClick() {\n            let x1 = parseInt(input1.value);\n            let y1 = parseInt(input2.value);\n            let x2 = parseInt(input3.value);\n            let y2 = parseInt(input4.value);\n            if (x1 <= unionFind.length && y1 <= unionFind.length && x2 <= unionFind.length && y2 <= unionFind.length) {\n                union(x1, y1);\n                union(x2, y2);\n                drawGraph();\n            }\n        }\n\n        input.addEventListener('click', addVertexClick);\n        input1.addEventListener('click', mergeVerticesClick);\n        input2.addEventListener('click', mergeVerticesClick);\n        input3.addEventListener('click', mergeVerticesClick);\n        input4.addEventListener('click', mergeVerticesClick);\n\n        drawGraph();\n    </script>\n</body>\n</html>"
