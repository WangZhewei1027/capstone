    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Weighted Graph</title>\n    <style>\n        #graph {\n            border: 1px solid black;\n            border-radius: 10px;\n            padding: 20px;\n            width: 50%;\n            margin: 40px auto;\n        }\n        #graph > .vertex {\n            display: inline-block;\n            margin-right: 10px;\n        }\n        #graph > .edge {\n            border: 1px solid black;\n        }\n    </style>\n</head>\n<body>\n    <h1>Weighted Graph</h1>\n    <input id=\"vertices\" type=\"text\" placeholder=\"Enter vertices (separated by comma)\">\n    <input id=\"edges\" type=\"text\" placeholder=\"Enter edges (separated by comma)\">\n    <button id=\"add-vertex\">Add Vertex</button>\n    <button id=\"add-edge\">Add Edge</button>\n    <canvas id=\"graph\"></canvas>\n\n    <script>\n        const graph = document.getElementById('graph');\n        const verticesInput = document.getElementById('vertices');\n        const edgesInput = document.getElementById('edges');\n        const addVertexButton = document.getElementById('add-vertex');\n        const addEdgeButton = document.getElementById('add-edge');\n        const graphCanvas = document.getElementById('graph');\n\n        let vertices = [];\n        let edges = [];\n        let graphData = {};\n\n        addVertexButton.addEventListener('click', () => {\n            const vertex = verticesInput.value.trim();\n            if (vertex) {\n                vertices.push(vertex);\n                verticesInput.value = '';\n                renderGraph();\n            }\n        });\n\n        addEdgeButton.addEventListener('click', () => {\n            const edge = edgesInput.value.trim();\n            if (edge) {\n                edges.push(edge);\n                edgesInput.value = '';\n                renderGraph();\n            }\n        });\n\n        function renderGraph() {\n            graphData = {};\n            graph.innerHTML = '';\n\n            vertices.forEach((vertex) => {\n                const vertexElement = document.createElement('div');\n                vertexElement.classList.add('vertex');\n                vertexElement.textContent = vertex;\n                graph.appendChild(vertexElement);\n            });\n\n            edges.forEach((edge) => {\n                const edgeElement = document.createElement('div');\n                edgeElement.classList.add('edge');\n                const source = document.createElement('span');\n                source.textContent = edge[0];\n                const target = document.createElement('span');\n                target.textContent = edge[1];\n                edgeElement.appendChild(source);\n                edgeElement.appendChild(target);\n                graph.appendChild(edgeElement);\n            });\n\n            const vertexSize = 20;\n            const edgeWidth = 5;\n            const vertexSpacing = 10;\n            const edgeSpacing = 10;\n            const maxVertex = vertices.length;\n            const maxEdge = edges.length;\n\n            for (let i = 0; i < maxVertex; i++) {\n                const x = i * (vertexSize + vertexSpacing);\n                const y = graphCanvas.offsetTop + graphCanvas.offsetHeight / 2;\n                const rect = document.createElement('rect');\n                rect.style.width = `${vertexSize}px`;\n                rect.style.height = `${vertexSize}px`;\n                rect.style.left = x + 'px';\n                rect.style.top = y + 'px';\n                rect.style.fill = 'none';\n                rect.style.stroke = 'black';\n                rect.style.strokeWidth = `${edgeWidth}px`;\n                rect.style.strokeDasharray = '2 ${edgeWidth}px';\n                graphCanvas.appendChild(rect);\n            }\n\n            for (let i = 0; i < maxEdge; i++) {\n                const x = i * (vertexSize + vertexSpacing) + vertexSpacing;\n                const y = graphCanvas.offsetTop + graphCanvas.offsetHeight / 2;\n                const rect = document.createElement('rect');\n                rect.style.width = `${edgeWidth}px`;\n                rect.style.height = `${edgeWidth}px`;\n                rect.style.left = x + 'px';\n                rect.style.top = y + 'px';\n                rect.style.fill = 'none';\n                rect.style.stroke = 'black';\n                graphCanvas.appendChild(rect);\n            }\n        }\n\n        verticesInput.addEventListener('input', (e) => {\n            vertices = e.target.value.trim().split(',').map((vertex) => vertex.trim());\n            renderGraph();\n        });\n\n        edgesInput.addEventListener('input', (e) => {\n            edges = e.target.value.trim().split(',').map((edge) => edge.trim());\n            renderGraph();\n        });\n    </script>\n</body>\n</html>"
