    "content": "<html>\n<head>\n    <title>Binary Search</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        #result {\n            font-size: 24px;\n            font-weight: bold;\n            margin-bottom: 20px;\n        }\n    </style>\n</head>\n<body>\n    <h1>Binary Search</h1>\n    <form id=\"search-form\">\n        <label for=\"low\">Low:</label>\n        <input type=\"number\" id=\"low\" name=\"low\" required>\n        <label for=\"high\">High:</label>\n        <input type=\"number\" id=\"high\" name=\"high\" required>\n        <button type=\"submit\">Search</button>\n    </form>\n    <div id=\"result\"></div>\n\n    <script>\n        let low = 0;\n        let high = 100;\n        let result = '';\n\n        function binarySearch() {\n            if (low > high) {\n                return;\n            }\n            let mid = Math.floor((low + high) / 2);\n            let guess = mid;\n            let guessCount = 0;\n            while (low <= high) {\n                if (guess === high) {\n                    result = \"Element is present at index \" + guessCount + \" \";\n                    break;\n                } else if (guess < high) {\n                    low = guess + 1;\n                    guessCount++;\n                } else {\n                    high = guess - 1;\n                    guessCount++;\n                }\n                guess = mid;\n            }\n            if (guess === low) {\n                result = \"Element is not present at any index \";\n            }\n        }\n\n        document.getElementById('search-form').addEventListener('submit', binarySearch);\n\n        binarySearch();\n    </script>\n</body>\n</html>"
