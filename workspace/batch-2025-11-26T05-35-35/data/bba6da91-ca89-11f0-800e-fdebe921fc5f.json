e = generateHuffmanCode();\n            let code = '';\n            function traverse(node, currentCode) {\n                if (node === null) {\n                    return;\n                }\n                code += currentCode + node.symbol;\n                traverse(node.left, currentCode + '0');\n                traverse(node.right, currentCode + '1');\n            }\n            traverse(huffmanTree, '');\n            document.getElementById('display-huffman-code').innerHTML = `Huffman Code: ${code}`;\n        }\n\n        document.getElementById('symbol').addEventListener('input', function() {\n            addSymbol(this.value);\n            displayHuffmanCode();\n        });\n\n        document.getElementById('generate-huffman-code').addEventListener('click', generateHuffmanCode);\n        document.getElementById('display-huffman-code').addEventListener('click', displayHuffmanCode);\n    </script>\n</body>\n</html>"
