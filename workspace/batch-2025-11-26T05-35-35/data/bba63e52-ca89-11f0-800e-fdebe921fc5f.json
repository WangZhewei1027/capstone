    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Breadth-First Search (BFS)</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        #graph {\n            width: 800px;\n            height: 600px;\n            border: 1px solid black;\n        }\n    </style>\n</head>\n<body>\n    <h1>Breadth-First Search (BFS)</h1>\n    <input id=\"graph\" type=\"text\" placeholder=\"Enter graph vertices\">\n    <button id=\"bfs-btn\">BFS</button>\n    <div id=\"graph-container\"></div>\n\n    <script>\n        let graph = {};\n        let visited = {};\n        let result = [];\n\n        function addEdge(v1, v2) {\n            if (!graph[v1]) graph[v1] = [];\n            if (!graph[v2]) graph[v2] = [];\n            graph[v1].push(v2);\n            graph[v2].push(v1);\n        }\n\n        function dfs(v, parent) {\n            visited[v] = true;\n            result.push(v);\n            for (let neighbor of graph[v]) {\n                if (!visited[neighbor]) {\n                    dfs(neighbor, v);\n                }\n            }\n        }\n\n        function bfs() {\n            let startVertex = +document.getElementById('graph').value;\n            let startNode = startVertex;\n            visited[startNode] = true;\n            result = [];\n            let queue = [startNode];\n\n            while (queue.length > 0) {\n                let currentNode = queue.shift();\n                for (let neighbor of graph[currentNode]) {\n                    if (!visited[neighbor]) {\n                        visited[neighbor] = true;\n                        result.push(neighbor);\n                        queue.push(neighbor);\n                    }\n                }\n            }\n            return result;\n        }\n\n        let graphInput = document.getElementById('graph');\n        let bfsBtn = document.getElementById('bfs-btn');\n\n        graphInput.addEventListener('input', function() {\n            let vertices = graphInput.value.split(' ');\n            for (let vertex of vertices) {\n                addEdge(vertex, parseInt(vertex));\n            }\n            bfsBtn.textContent = 'BFS';\n        });\n\n        bfsBtn.addEventListener('click', function() {\n            let result = bfs();\n            console.log(result);\n            let resultHtml = '<ul>';\n            for (let vertex of result) {\n                resultHtml += `<li>${vertex}</li>`;\n            }\n            resultHtml += '</ul>';\n            document.getElementById('graph-container').innerHTML = resultHtml;\n        });\n    </script>\n</body>\n</html>"
