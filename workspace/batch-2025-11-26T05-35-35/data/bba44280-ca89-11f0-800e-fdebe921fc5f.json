    "content": "<html>\n<head>\n    <title>Linked List</title>\n    <style>\n        table {\n            border-collapse: collapse;\n        }\n        th, td {\n            border: 1px solid #ddd;\n            padding: 8px;\n            text-align: left;\n        }\n    </style>\n</head>\n<body>\n    <h1>Linked List</h1>\n    <form>\n        <label for=\"data\">Enter data:</label>\n        <input type=\"text\" id=\"data\" name=\"data\"><br><br>\n        <label for=\"size\">Enter size of linked list:</label>\n        <input type=\"number\" id=\"size\" name=\"size\"><br><br>\n        <label for=\"list\">List:</label>\n        <table>\n            <tr>\n                <th>Index</th>\n                <th>Data</th>\n            </tr>\n        </table>\n        <button id=\"add\">Add</button>\n        <button id=\"remove\">Remove</button>\n        <button id=\"print\">Print</button>\n    </form>\n\n    <script>\n        let size = parseInt(document.getElementById(\"size\").value);\n        let head = new Node(0, null);\n        let tail = new Node(0, null);\n\n        function Node(index, data) {\n            this.index = index;\n            this.data = data;\n            this.next = null;\n        }\n\n        function append(data) {\n            let newNode = new Node(size, data);\n            if (head === null) {\n                head = newNode;\n                tail = newNode;\n            } else {\n                tail.next = newNode;\n                tail = newNode;\n            }\n        }\n\n        function printList() {\n            let result = \"\";\n            let current = head;\n            while (current !== null) {\n                result += current.index + \" -> \" + current.data + \"<br>\";\n                current = current.next;\n            }\n            document.getElementById(\"list\").innerHTML = result;\n        }\n\n        function remove(index) {\n            if (index < 0 || index >= size) {\n                return;\n            }\n            if (index === 0) {\n                head = null;\n                tail = null;\n            } else {\n                let current = head;\n                for (let i = 0; i < index - 1; i++) {\n                    current = current.next;\n                }\n                current.next = current.next.next;\n                if (current.next === null) {\n                    tail = current;\n                }\n            }\n        }\n\n        function add(data) {\n            append(data);\n            printList();\n        }\n\n        let data = \"\";\n        let sizeInput = document.getElementById(\"size\");\n        let addButton = document.getElementById(\"add\");\n        let removeButton = document.getElementById(\"remove\");\n        let printButton = document.getElementById(\"print\");\n\n        addButton.addEventListener(\"click\", function() {\n            data = document.getElementById(\"data\").value;\n            append(parseInt(data));\n            printList();\n        });\n\n        removeButton.addEventListener(\"click\", function() {\n            let index = parseInt(document.getElementById(\"data\").value);\n            remove(index);\n            printList();\n        });\n\n        printButton.addEventListener(\"click\", function() {\n            printList();\n        });\n    </script>\n</body>\n</html>"
