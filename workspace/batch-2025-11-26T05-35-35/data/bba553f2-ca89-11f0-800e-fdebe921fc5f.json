in(\"\");\n        }\n\n        const matrix = [];\n        const matrixInput = document.getElementById(\"matrix\");\n\n        matrixInput.addEventListener(\"input\", (e) => {\n            const matrixValue = e.target.value;\n            generateMatrix(matrixValue.split(\"\"));\n        });\n\n        matrixInput.addEventListener(\"submit\", (e) => {\n            e.preventDefault();\n            const matrixValue = matrixInput.value;\n            generateMatrix(matrixValue.split(\"\"));\n            printMatrix(matrixValue.split(\"\"));\n        });\n    </script>\n</body>\n</html>"
