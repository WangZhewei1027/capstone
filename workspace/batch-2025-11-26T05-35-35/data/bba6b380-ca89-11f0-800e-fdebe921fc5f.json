    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Topological Sort</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        .container {\n            width: 50%;\n            margin: 40px auto;\n            text-align: center;\n        }\n        .input-group {\n            margin-bottom: 20px;\n        }\n        .button-group {\n            margin-bottom: 20px;\n        }\n    </style>\n</head>\n<body>\n    <div class=\"container\">\n        <h1>Topological Sort</h1>\n        <div class=\"input-group\">\n            <label for=\"graph\">Graph:</label>\n            <textarea id=\"graph\" rows=\"10\" cols=\"30\"></textarea>\n        </div>\n        <div class=\"button-group\">\n            <button id=\"start-button\">Start</button>\n            <button id=\"done-button\">Done</button>\n        </div>\n        <script src=\"script.js\"></script>\n    </div>\n\n    <script>\n        function topologicalSort(graph) {\n            const inDegree = {};\n            const graphData = JSON.parse(graph);\n\n            for (const node in graphData) {\n                inDegree[node] = 0;\n            }\n\n            for (const node in graphData) {\n                for (const neighbor of graphData[node]) {\n                    inDegree[neighbor] += 1;\n                }\n            }\n\n            const queue = [];\n            for (const node in inDegree) {\n                if (inDegree[node] === 0) {\n                    queue.push(node);\n                }\n            }\n\n            const sortedNodes = [];\n\n            while (queue.length > 0) {\n                const node = queue.shift();\n                sortedNodes.push(node);\n\n                for (const neighbor of graphData[node]) {\n                    inDegree[neighbor] -= 1;\n                    if (inDegree[neighbor] === 0) {\n                        queue.push(neighbor);\n                    }\n                }\n            }\n\n            return sortedNodes;\n        }\n\n        function displayGraph(graph) {\n            const graphData = JSON.parse(graph);\n            const nodes = Object.keys(graphData);\n\n            console.log(\"Graph:\");\n            for (const node of nodes) {\n                console.log(node, graphData[node].length);\n            }\n        }\n\n        const graph = `\n        1\n        2\n        3\n        4\n        2 -> 3\n        3 -> 4\n        4 -> 5\n        5\n       `;\n\n        const startButton = document.getElementById('start-button');\n        const doneButton = document.getElementById('done-button');\n        startButton.addEventListener('click', () => {\n            const graph = graph;\n            displayGraph(graph);\n            const sortedNodes = topologicalSort(graph);\n            console.log('Sorted nodes:', sortedNodes);\n            displayGraph(sortedNodes);\n        });\n\n        doneButton.addEventListener('click', () => {\n            const graph = graph;\n            displayGraph(graph);\n            const sortedNodes = topologicalSort(graph);\n            console.log('Sorted nodes:', sortedNodes);\n            displayGraph(sortedNodes);\n        });\n    </script>\n</body>\n</html>"
