ode);\n        \n        // Add event listener to the add edge button\n        edgeInput.addEventListener('click', addEdge);\n        \n        // Add event listener to the clear graph button\n        clearGraphButton.addEventListener('click', clearGraph);\n        \n        function addNode() {\n            let node = nodeInput.value;\n            if (node) {\n                nodes.push(node);\n                let nodeDiv = document.createElement('div');\n                nodeDiv.classList.add('node');\n                nodeDiv.textContent = node;\n                graph.appendChild(nodeDiv);\n                nodeInput.value = '';\n            }\n        }\n        \n        function addEdge() {\n            let edge = edgeInput.value;\n            if (edge) {\n                edges.push(edge);\n                let edgeDiv = document.createElement('div');\n                edgeDiv.classList.add('edge');\n                if (edge === 'blue') {\n                    edgeDiv.classList.add('edge-blue');\n                } else {\n                    edgeDiv.classList.add('edge-red');\n                }\n                edgeDiv.textContent = edge;\n                graph.appendChild(edgeDiv);\n                edgeInput.value = '';\n            }\n        }\n        \n        function clearGraph() {\n            graph.innerHTML = '';\n            nodes = [];\n            edges = [];\n        }\n    </script>\n</body>\n</html>"
