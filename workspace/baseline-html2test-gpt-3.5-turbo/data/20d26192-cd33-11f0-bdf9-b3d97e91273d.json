    "content": "<html lang=\"en\">\n<head>\n<meta charset=\"UTF-8\" />\n<meta name=\"viewport\" content=\"width=device-width, initial-scale=1\" />\n<title>Queue Demonstration</title>\n<style>\n  body {\n    font-family: Arial, sans-serif;\n    margin: 2rem;\n    background: #f9f9f9;\n    color: #333;\n  }\n  h1 {\n    text-align: center;\n  }\n  .container {\n    max-width: 500px;\n    margin: 1rem auto;\n    background: white;\n    padding: 1.5rem;\n    border-radius: 8px;\n    box-shadow: 0 0 10px rgb(0 0 0 / 0.1);\n  }\n  label {\n    display: block;\n    margin-bottom: 0.25rem;\n  }\n  input[type=\"text\"] {\n    width: 100%;\n    padding: 0.5rem;\n    font-size: 1rem;\n  }\n  button {\n    margin-top: 0.75rem;\n    padding: 0.5rem 1rem;\n    font-size: 1rem;\n    cursor: pointer;\n    margin-right: 10px;\n  }\n  button:disabled {\n    opacity: 0.5;\n    cursor: default;\n  }\n  .queue-display {\n    margin-top: 1.5rem;\n    min-height: 50px;\n    border: 2px dashed #777;\n    border-radius: 6px;\n    display: flex;\n    gap: 8px;\n    align-items: center;\n    justify-content: start;\n    flex-wrap: wrap;\n    padding: 0.5rem;\n    background: #eef6fc;\n  }\n  .queue-item {\n    background: #4A90E2;\n    color: white;\n    padding: 0.4rem 0.8rem;\n    border-radius: 4px;\n    font-weight: bold;\n    min-width: 40px;\n    text-align: center;\n    user-select: none;\n  }\n  .info-text {\n    margin-top: 0.5rem;\n    font-size: 0.9rem;\n    color: #555;\n  }\n  .flex-row {\n    display: flex;\n    gap: 1rem;\n    flex-wrap: wrap;\n  }\n\n</style>\n</head>\n<body>\n  <h1>Queue Demonstration</h1>\n  <div class=\"container\">\n    <label for=\"enqueueInput\">Enter value to enqueue:</label>\n    <input type=\"text\" id=\"enqueueInput\" placeholder=\"Value (e.g. 5, apple)\" />\n    <div class=\"flex-row\">\n      <button id=\"enqueueBtn\">Enqueue</button>\n      <button id=\"dequeueBtn\" disabled>Dequeue</button>\n      <button id=\"peekBtn\" disabled>Peek</button>\n      <button id=\"clearBtn\" disabled>Clear Queue</button>\n    </div>\n    <div class=\"info-text\" id=\"message\"></div>\n    <h3>Queue Contents (Front â†’ Rear):</h3>\n    <div class=\"queue-display\" id=\"queueDisplay\">\n      <!-- Queue items will appear here -->\n    </div>\n  </div>\n\n<script>\n  /*\n   * Simple Queue class implementation with FIFO behavior\n   */\n  class Queue {\n    constructor() {\n      this.items = [];\n    }\n    enqueue(element) {\n      this.items.push(element);\n    }\n    dequeue() {\n      if (this.isEmpty()) return null;\n      return this.items.shift();\n    }\n    peek() {\n      if (this.isEmpty()) return null;\n      return this.items[0];\n    }\n    isEmpty() {\n      return this.items.length === 0;\n    }\n    clear() {\n      this.items = [];\n    }\n    getItems() {\n      return this.items;\n    }\n  }\n\n  const queue = new Queue();\n\n  const enqueueInput = document.getElementById('enqueueInput');\n  const enqueueBtn = document.getElementById('enqueueBtn');\n  const dequeueBtn = document.getElementById('dequeueBtn');\n  const peekBtn = document.getElementById('peekBtn');\n  const clearBtn = document.getElementById('clearBtn');\n  const queueDisplay = document.getElementById('queueDisplay');\n  const message = document.getElementById('message');\n\n  function updateButtons() {\n    const empty = queue.isEmpty();\n    dequeueBtn.disabled = empty;\n    peekBtn.disabled = empty;\n    clearBtn.disabled = empty;\n  }\n\n  function renderQueue() {\n    queueDisplay.innerHTML = '';\n    const items = queue.getItems();\n    for (let i = 0; i < items.length; i++) {\n      const div = document.createElement('div');\n      div.className = 'queue-item';\n      div.textContent = items[i];\n      if (i === 0) {\n        div.title = 'Front of Queue';\n        div.style.border = '2px solid #2a65d7';\n        div.style.fontWeight = '900';\n      }\n      queueDisplay.appendChild(div);\n    }\n  }\n\n  function setMessage(text, isError = false) {\n    message.textContent = text;\n    message.style.color = isError ? 'crimson' : '#333';\n  }\n\n  enqueueBtn.addEventListener('click', () => {\n    const val = enqueueInput.value.trim();\n    if (val === '') {\n      setMessage('Please enter a value to enqueue.', true);\n      return;\n    }\n    queue.enqueue(val);\n    setMessage(`Enqueued \"${val}\" successfully.`);\n    enqueueInput.value = '';\n    enqueueInput.focus();\n    renderQueue();\n    updateButtons();\n  });\n\n  dequeueBtn.addEventListener('click', () => {\n    const removed = queue.dequeue();\n    if (removed === null) {\n      setMessage('Queue is empty, cannot dequeue.', true);\n    } else {\n      setMessage(`Dequeued \"${removed}\".`);\n      renderQueue();\n      updateButtons();\n    }\n  });\n\n  peekBtn.addEventListener('click', () => {\n    const front = queue.peek();\n    if (front === null) {\n      setMessage('Queue is empty, nothing to peek.', true);\n    } else {\n      setMessage(`Front of the queue: \"${front}\".`);\n    }\n  });\n\n  clearBtn.addEventListener('click', () => {\n    queue.clear();\n    setMessage('Queue cleared.');\n    renderQueue();\n    updateButtons();\n  });\n\n  // Allow pressing Enter in input to enqueue\n  enqueueInput.addEventListener('keyup', (e) => {\n    if (e.key === 'Enter') {\n      enqueueBtn.click();\n    }\n  });\n\n  // Initialize\n  renderQueue();\n  updateButtons();\n  setMessage('Enter values to enqueue. Use buttons to interact with the queue.');\n</script>\n</body>\n</html>"
