    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Graph</title>\n    <style>\n        /* Basic styling for the graph */\n        canvas {\n            border: 1px solid black;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Graph</h1>\n<input id=\"n\" type=\"number\" placeholder=\"Number of vertices\">\n<button id=\"add-vertex\">Add Vertex</button>\n<button id=\"add-edge\">Add Edge</button>\n\n<div id=\"graph\"></div>\n\n<script>\n    // Get the graph elements\n    const graph = document.getElementById('graph');\n    const input = document.getElementById('n');\n    const addVertex = document.getElementById('add-vertex');\n    const addEdge = document.getElementById('add-edge');\n\n    // Get the canvas element\n    const canvas = document.createElement('canvas');\n    graph.appendChild(canvas);\n\n    // Set the canvas context\n    canvas.width = 400;\n    canvas.height = 400;\n    const ctx = canvas.getContext('2d');\n\n    // Initialize the graph with an empty adjacency list\n    let graphData = {\n        vertices: [],\n        edges: []\n    };\n\n    // Function to draw the graph\n    function drawGraph() {\n        ctx.clearRect(0, 0, canvas.width, canvas.height);\n        for (const vertex of graphData.vertices) {\n            ctx.beginPath();\n            ctx.arc(vertex.x, vertex.y, 5, 0, 2 * Math.PI);\n            ctx.fillStyle = 'black';\n            ctx.fill();\n            ctx.fillStyle = 'gray';\n            ctx.font = '12px Arial';\n            ctx.textAlign = 'center';\n            ctx.textBaseline = 'middle';\n            ctx.fillText(vertex.name, vertex.x, vertex.y);\n        }\n        for (const edge of graphData.edges) {\n            ctx.beginPath();\n            ctx.moveTo(edge.start.x, edge.start.y);\n            ctx.lineTo(edge.end.x, edge.end.y);\n            ctx.strokeStyle = 'black';\n            ctx.lineWidth = 2;\n            ctx.stroke();\n        }\n    }\n\n    // Function to add a vertex\n    function addVertexClick(event) {\n        const vertex = {\n            x: event.clientX - canvas.width / 2,\n            y: event.clientY - canvas.height / 2,\n            name: `Vertex ${graphData.vertices.length + 1}`\n        };\n        graphData.vertices.push(vertex);\n        drawGraph();\n    }\n\n    // Function to add an edge\n    function addEdgeClick(event) {\n        const startVertex = graphData.vertices.find(vertex => vertex.x === event.clientX - canvas.width / 2);\n        const endVertex = graphData.vertices.find(vertex => vertex.y === event.clientY - canvas.height / 2);\n        if (startVertex && endVertex) {\n            const edge = {\n                start: startVertex,\n                end: endVertex,\n                weight: 1 // Assuming a simple weight of 1\n            };\n            graphData.edges.push(edge);\n            drawGraph();\n        }\n    }\n\n    // Add event listeners\n    addVertex.addEventListener('click', addVertexClick);\n    addEdge.addEventListener('click', addEdgeClick);\n\n    // Initial graph data\n    for (let i = 0; i < 10; i++) {\n        graphData.vertices.push({ x: i * 20, y: 20 });\n    }\n    for (let i = 0; i < 5; i++) {\n        graphData.edges.push({ start: { x: i * 20, y: 20 }, end: { x: (i + 1) * 20, y: 20 } });\n    }\n\n    // Draw the initial graph\n    drawGraph();\n</script>\n\n</body>\n</html>"
