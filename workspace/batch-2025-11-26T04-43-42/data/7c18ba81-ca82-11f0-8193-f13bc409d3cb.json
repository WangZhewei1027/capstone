    "content": "<html>\n<head>\n    <title>Linear Search</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        #result {\n            font-weight: bold;\n            font-size: 24px;\n            color: green;\n        }\n    </style>\n</head>\n<body>\n    <h1>Linear Search</h1>\n    <p>Enter the number of elements to search:</p>\n    <input type=\"number\" id=\"numElements\" value=\"10\">\n    <button id=\"searchButton\">Search</button>\n    <div id=\"result\"></div>\n\n    <script>\n        let numElements = parseInt(document.getElementById(\"numElements\").value);\n        let resultDiv = document.getElementById(\"result\");\n\n        function linearSearch(arr, target) {\n            for (let i = 0; i < arr.length; i++) {\n                if (arr[i] === target) {\n                    resultDiv.innerText = `Element found: ${arr[i]}`;\n                    return;\n                }\n            }\n            resultDiv.innerText = `Element not found`;\n        }\n\n        function search() {\n            let arr = Array.from({ length: numElements }, (_, i) => i + 1);\n            linearSearch(arr, numElements);\n        }\n\n        searchButton.addEventListener(\"click\", search);\n    </script>\n</body>\n</html>"
