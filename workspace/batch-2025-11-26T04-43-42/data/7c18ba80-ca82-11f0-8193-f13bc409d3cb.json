    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Radix Sort</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        .container {\n            max-width: 400px;\n            margin: 40px auto;\n            padding: 20px;\n            background-color: #f9f9f9;\n            border: 1px solid #ddd;\n            border-radius: 10px;\n            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n        }\n        .container h2 {\n            margin-top: 0;\n        }\n        .container p {\n            margin-bottom: 20px;\n        }\n        .button {\n            padding: 10px 20px;\n            border: none;\n            border-radius: 5px;\n            cursor: pointer;\n        }\n        .button:hover {\n            background-color: #ccc;\n        }\n    </style>\n</head>\n<body>\n    <div class=\"container\">\n        <h2>Radix Sort Example</h2>\n        <p>Enter numbers separated by space:</p>\n        <input type=\"text\" id=\"numbers\" />\n        <button id=\"sort-btn\">Sort</button>\n        <div id=\"result\"></div>\n\n        <script>\n            let numbers = document.getElementById('numbers').value;\n            let result = document.getElementById('result');\n            let numbersArray = numbers.split(' ').map(Number);\n\n            function radixSort(arr) {\n                let buckets = {};\n                for (let i = 0; i < arr.length; i++) {\n                    if (!buckets[arr[i]]) {\n                        buckets[arr[i]] = [];\n                    }\n                    buckets[arr[i]].push(arr[i]);\n                }\n\n                for (let key in buckets) {\n                    result.innerHTML += `<p>Radix Sort: ${key} = ${buckets[key].join(', ')}</p>`;\n                    for (let i = 0; i < buckets[key].length; i++) {\n                        let item = buckets[key][i];\n                        result.innerHTML += `<p>Radix Sort: ${item} = ${item} (Original: ${arr.indexOf(item)})</p>`;\n                        arr[arr.indexOf(item)] = item;\n                    }\n                    buckets[key] = [];\n                }\n            }\n\n            function sort(arr) {\n                let max = Math.max(...arr);\n                let index = arr.indexOf(max);\n                let temp = arr.splice(index, 1);\n                return [max, index, temp];\n            }\n\n            radixSort(numbersArray);\n            console.log(\"Sorted numbers:\", numbersArray);\n        </script>\n    </div>\n\n    <script>\n        document.getElementById('sort-btn').addEventListener('click', () => {\n            let numbers = document.getElementById('numbers').value;\n            let result = document.getElementById('result');\n            let numbersArray = numbers.split(' ').map(Number);\n\n            function radixSort(arr) {\n                let buckets = {};\n                for (let i = 0; i < arr.length; i++) {\n                    if (!buckets[arr[i]]) {\n                        buckets[arr[i]] = [];\n                    }\n                    buckets[arr[i]].push(arr[i]);\n                }\n\n                for (let key in buckets) {\n                    result.innerHTML += `<p>Radix Sort: ${key} = ${buckets[key].join(', ')}</p>`;\n                    for (let i = 0; i < buckets[key].length; i++) {\n                        let item = buckets[key][i];\n                        result.innerHTML += `<p>Radix Sort: ${item} = ${item} (Original: ${arr.indexOf(item)})</p>`;\n                        arr[arr.indexOf(item)] = item;\n                    }\n                    buckets[key] = [];\n                }\n            }\n\n            function sort(arr) {\n                let max = Math.max(...arr);\n                let index = arr.indexOf(max);\n                let temp = arr.splice(index, 1);\n                return [max, index, temp];\