    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Breadth-First Search</title>\n    <style>\n        #graph {\n            border: 1px solid black;\n        }\n    </style>\n</head>\n<body>\n    <h1>Breadth-First Search</h1>\n    <canvas id=\"graph\" width=\"400\" height=\"400\"></canvas>\n\n    <script>\n        let graph = new Array(4).fill(0).map(() => new Array(4).fill(0));\n        let startNode = 0;\n        let endNode = 3;\n\n        function drawGraph() {\n            const canvas = document.getElementById('graph');\n            const ctx = canvas.getContext('2d');\n            ctx.clearRect(0, 0, canvas.width, canvas.height);\n            for (let i = 0; i < graph.length; i++) {\n                for (let j = 0; j < graph[i].length; j++) {\n                    if (graph[i][j] === 1) {\n                        ctx.fillStyle = 'red';\n                        ctx.fillRect(j * 50, i * 50, 50, 50);\n                    }\n                }\n            }\n        }\n\n        function bfs(startNode, endNode) {\n            const visited = new Array(4).fill(false);\n            const queue = [[startNode, 0]];\n            visited[startNode] = true;\n\n            while (queue.length > 0) {\n                const [node, distance] = queue.shift();\n                if (node === endNode) {\n                    console.log(`Path found: ${startNode} -> ${endNode}`);\n                    return;\n                }\n\n                for (let i = 0; i < 4; i++) {\n                    if (graph[node][i] === 1 && !visited[i]) {\n                        queue.push([i, distance + 1]);\n                        visited[i] = true;\n                    }\n                }\n            }\n\n            console.log(`No path found from ${startNode} to ${endNode}`);\n        }\n\n        drawGraph();\n        const startInput = document.getElementById('start');\n        const endInput = document.getElementById('end');\n\n        startInput.addEventListener('input', () => {\n            startNode = parseInt(startInput.value);\n            endInput.value = parseInt(endInput.value);\n            bfs(startNode, endNode);\n        });\n    </script>\n</body>\n</html>"
