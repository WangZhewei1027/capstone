    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Heap (Min/Max)</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        .container {\n            width: 50%;\n            margin: 40px auto;\n            padding: 20px;\n            border: 1px solid #ccc;\n            border-radius: 10px;\n            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n        }\n        .heap {\n            width: 100%;\n            height: 300px;\n            border: 1px solid #ccc;\n            border-radius: 10px;\n            overflow-y: auto;\n        }\n        .heap .item {\n            padding: 10px;\n            border: 1px solid #ccc;\n            border-radius: 10px;\n            cursor: move;\n            user-select: none;\n        }\n        .heap .item:hover {\n            background-color: #f0f0f0;\n        }\n    </style>\n</head>\n<body>\n    <div class=\"container\">\n        <h2>Heap (Min/Max)</h2>\n        <input id=\"input\" type=\"number\" placeholder=\"Enter a number\">\n        <button id=\"add-btn\">Add</button>\n        <button id=\"remove-btn\">Remove</button>\n        <button id=\"clear-btn\">Clear</button>\n        <div class=\"heap\">\n            <div class=\"item\" id=\"heap\"></div>\n        </div>\n        <div id=\"output\"></div>\n    </div>\n\n    <script>\n        let heap = [];\n        let output = document.getElementById('output');\n\n        function add(num) {\n            heap.push(num);\n            heapifyUp(heap.length - 1);\n            updateHeap();\n        }\n\n        function remove() {\n            if (heap.length === 0) return;\n            let min = heap[0];\n            heap[0] = heap[heap.length - 1];\n            heap.pop();\n            heapifyDown(0);\n            updateHeap();\n        }\n\n        function clear() {\n            heap = [];\n            updateHeap();\n        }\n\n        function heapifyUp(index) {\n            if (index === 0) return;\n            let parentIndex = Math.floor((index - 1) / 2);\n            if (heap[parentIndex] >= heap[index]) return;\n            let temp = heap[parentIndex];\n            heap[parentIndex] = heap[index];\n            heap[index] = temp;\n            heapifyUp(parentIndex);\n        }\n\n        function heapifyDown(index) {\n            let leftChildIndex = 2 * index + 1;\n            let rightChildIndex = 2 * index + 2;\n            let smallest = index;\n            if (leftChildIndex < heap.length && heap[leftChildIndex] < heap[smallest]) smallest = leftChildIndex;\n            if (rightChildIndex < heap.length && heap[rightChildIndex] < heap[smallest]) smallest = rightChildIndex;\n            if (smallest !== index) {\n                let temp = heap[index];\n                heap[index] = heap[smallest];\n                heap[smallest] = temp;\n                heapifyDown(smallest);\n            }\n        }\n\n        function updateHeap() {\n            let outputHtml = '';\n            for (let item of heap) {\n                outputHtml += `<div class=\"item\">${item}</div>`;\n            }\n            output.innerHTML = outputHtml;\n        }\n\n        document.getElementById('add-btn').addEventListener('click', add);\n        document.getElementById('remove-btn').addEventListener('click', remove);\n        document.getElementById('clear-btn').addEventListener('click', clear);\n    </script>\n</body>\n</html>"
