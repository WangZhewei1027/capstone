    "role": "assistant",
    "content": "<html>\n<head>\n    <title>Binary Search</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            text-align: center;\n        }\n        #binary-search {\n            width: 50%;\n            margin: 40px auto;\n            padding: 20px;\n            border: 1px solid #ccc;\n            border-radius: 10px;\n            box-shadow: 0 0 10px rgba(0,0,0,0.1);\n        }\n    </style>\n</head>\n<body>\n    <h1>Binary Search</h1>\n    <form id=\"binary-search\">\n        <label for=\"low\">Low:</label>\n        <input type=\"number\" id=\"low\" required>\n        <label for=\"high\">High:</label>\n        <input type=\"number\" id=\"high\" required>\n        <button type=\"submit\">Search</button>\n    </form>\n    <div id=\"result\"></div>\n\n    <script>\n        function binarySearch(low, high) {\n            if (low > high) {\n                return;\n            }\n            let mid = Math.floor((low + high) / 2);\n            if (low === high) {\n                return low;\n            }\n            if (low > high) {\n                return binarySearch(low, mid - 1);\n            }\n            if (low === mid) {\n                return binarySearch(mid + 1, high);\n            }\n            if (high >= mid) {\n                return binarySearch(low, mid - 1);\n            }\n            return binarySearch(mid + 1, high);\n        }\n\n        document.getElementById(\"binary-search\").addEventListener(\"submit\", function(e) {\n            e.preventDefault();\n            let low = parseInt(document.getElementById(\"low\").value);\n            let high = parseInt(document.getElementById(\"high\").value);\n            let result = binarySearch(low, high);\n            if (result !== null) {\n                document.getElementById(\"result\").innerHTML = \"Low: \" + low + \", High: \" + result + \", Result: \" + result;\n            } else {\n                document.getElementById(\"result\").innerHTML = \"No result found\";\n            }\n        });\n    </script>\n</body>\n</html>"
