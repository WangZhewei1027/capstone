tree.selectionStart;\n            tree.deleteText(branch);\n            addBranch.value = '';\n        }\n\n        function traverse(node, level = 0) {\n            let text = '';\n            if (node.textContent) {\n                text += node.textContent + ' ';\n            }\n            if (node.children) {\n                for (let child of node.children) {\n                    text += traverse(child, level + 1) + ' ';\n                }\n            }\n            tree.value = text;\n            return text;\n        }\n\n        function insert(root, node) {\n            if (root === null) {\n                root = node;\n                return root;\n            }\n\n            if (node.textContent < root.textContent) {\n                root.children.push(node);\n            } else {\n                let child = root.children[0];\n                if (node.textContent < child.textContent) {\n                    root.children.push(node);\n                } else {\n                    root.children.splice(root.children.indexOf(child), 0, node);\n                }\n            }\n            return root;\n        }\n\n        function printTree(node) {\n            let text = '';\n            if (node.textContent) {\n                text += node.textContent + ' ';\n            }\n            for (let child of node.children) {\n                text += printTree(child) + ' ';\n            }\n            return text;\n        }\n\n        addLeaf();\n        addBranch();\n        addLeaf();\n        addBranch();\n\n        let treeValue = '';\n        tree.addEventListener('input', function() {\n            treeValue = traverse(this);\n            console.log('Tree Value:', treeValue);\n        });\n\n        root = insert(root, {\n            text: 'Root',\n            children: []\n        });\n\n        console.log('Tree Value before deletion:', traverse(root));\n\n        deleteLeaf();\n        console.log('Tree Value after deletion:', traverse(root));\n\n 