    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Prim's Algorithm Visualization</title>\n    <style>\n        body { font-family: Arial, sans-serif; }\n        canvas { border: 1px solid black; margin-top: 20px; }\n        #controls { margin-bottom: 20px; }\n    </style>\n</head>\n<body>\n    <h1>Prim's Algorithm Visualization</h1>\n    <div id=\"controls\">\n        <button onclick=\"startPrims()\">Start Prim's Algorithm</button>\n        <button onclick=\"reset()\">Reset</button>\n    </div>\n    <canvas id=\"canvas\" width=\"600\" height=\"400\"></canvas>\n\n    <script>\n        const canvas = document.getElementById(\"canvas\");\n        const ctx = canvas.getContext(\"2d\");\n\n        let vertices = [\n            { x: 100, y: 50, label: 'A' },\n            { x: 200, y: 150, label: 'B' },\n            { x: 300, y: 50, label: 'C' },\n            { x: 400, y: 200, label: 'D' },\n            { x: 500, y: 100, label: 'E' }\n        ];\n\n        let edges = [\n            { from: 0, to: 1, weight: 4 },\n            { from: 0, to: 2, weight: 3 },\n            { from: 1, to: 2, weight: 1 },\n            { from: 1, to: 3, weight: 2 },\n            { from: 2, to: 3, weight: 5 },\n            { from: 3, to: 4, weight: 6 },\n            { from: 1, to: 4, weight: 7 }\n        ];\n\n        let visited = [];\n        let mst = [];\n        let currentVertex = 0;\n\n        function drawGraph() {\n            ctx.clearRect(0, 0, canvas.width, canvas.height);\n            edges.forEach(edge => {\n                const from = vertices[edge.from];\n                const to = vertices[edge.to];\n                ctx.beginPath();\n                ctx.moveTo(from.x, from.y);\n                ctx.lineTo(to.x, to.y);\n                ctx.strokeStyle = 'black';\n                ctx.stroke();\n                ctx.closePath();\n                \n                ctx.fillText(edge.weight, (from.x + to.x) / 2, (from.y + to.y) / 2);\n            });\n\n            vertices.forEach((vertex, index) => {\n                ctx.beginPath();\n                ctx.arc(vertex.x, vertex.y, 20, 0, Math.PI * 2);\n                ctx.fillStyle = visited[index] ? 'green' : 'red';\n                ctx.fill();\n                ctx.closePath();\n                ctx.fillStyle = 'black';\n                ctx.fillText(vertex.label, vertex.x - 5, vertex.y + 5);\n            });\n        }\n\n        function startPrims() {\n            visited = Array(vertices.length).fill(false);\n            mst = [];\n            currentVertex = 0;\n            visited[currentVertex] = true;\n            drawGraph();\n            primsAlgorithm();\n        }\n\n        function primsAlgorithm() {\n            const edgesInMST = [];\n            let totalWeight = 0;\n\n            for (let i = 0; i < vertices.length - 1; i++) {\n                let minEdge = { weight: Infinity };\n                let fromIndex = -1, toIndex = -1;\n\n                edges.forEach(edge => {\n                    if (visited[edge.from] && !visited[edge.to] && edge.weight < minEdge.weight) {\n                        minEdge = edge;\n                        fromIndex = edge.from;\n                        toIndex = edge.to;\n                    }\n                });\n\n                if (minEdge.weight !== Infinity) {\n                    visited[toIndex] = true;\n                    edgesInMST.push(minEdge);\n                    totalWeight += minEdge.weight;\n                    drawGraph();\n\n                    // Delay to visualize the process\n                    setTimeout(() => {\n                        ctx.beginPath();\n                        ctx.moveTo(vertices[fromIndex].x, vertices[fromIndex].y);\n                        ctx.lineTo(vertices[toIndex].x, vertices[toIndex].y);\n                        ctx.strokeStyle = 'blue';\n                        ctx.lineWidth = 4;\n                        ctx.stroke();\n                        ctx.closePath();\n                    }, 1000 * i);\n                }\n            }\n\n            setTimeout(() => {\n                // Display total weight of MST\n                ctx.fillStyle = 'black';\n                ctx.fillText(`Total Weight of MST: ${totalWeight}`, 10, canvas.height - 10);\n            }, 1000 * (vertices.length - 1));\n        }\n\n        function reset() {\n            visited = [];\n            mst = [];\n            drawGraph();\n        }\n\n        drawGraph();\n    </script>\n</body>\n</html>"
