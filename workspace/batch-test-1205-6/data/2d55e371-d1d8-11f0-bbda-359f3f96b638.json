    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Binary Tree Visualization</title>\n    <style>\n        #tree-container {\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            flex-direction: column;\n        }\n        .node {\n            width: 50px;\n            height: 50px;\n            background-color: #f9c74f;\n            border: 2px solid #fca311;\n            border-radius: 50%;\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            position: relative;\n            margin: 20px;\n        }\n        .line {\n            width: 2px;\n            background-color: #fca311;\n            position: absolute;\n        }\n    </style>\n</head>\n<body>\n\n<h2>Binary Tree Visualization</h2>\n<div id=\"tree-container\"></div>\n<button onclick=\"buildTree()\">Build Sample Binary Tree</button>\n\n<script>\n    class Node {\n        constructor(value) {\n            this.value = value;\n            this.left = null;\n            this.right = null;\n        }\n    }\n\n    class BinaryTree {\n        constructor() {\n            this.root = null;\n        }\n\n        insert(value) {\n            const newNode = new Node(value);\n            if (!this.root) {\n                this.root = newNode;\n                return;\n            }\n            const queue = [this.root];\n            while (queue.length) {\n                const current = queue.shift();\n                if (!current.left) {\n                    current.left = newNode;\n                    return;\n                } else if (!current.right) {\n                    current.right = newNode;\n                    return;\n                }\n                queue.push(current.left);\n                queue.push(current.right);\n            }\n        }\n    }\n\n    function createNodeElement(value) {\n        const nodeDiv = document.createElement('div');\n        nodeDiv.className = 'node';\n        nodeDiv.innerText = value;\n        return nodeDiv;\n    }\n\n    function buildTree() {\n        const tree = new BinaryTree();\n        const values = [1, 2, 3, 4, 5, 6, 7];\n        values.forEach(value => tree.insert(value));\n        drawTree(tree.root);\n    }\n\n    function drawTree(node, level = 0, pos = 0, parentPos = null) {\n        if (!node) return;\n\n        const container = document.getElementById('tree-container');\n\n        const nodeElement = createNodeElement(node.value);\n        container.appendChild(nodeElement);\n\n        if (parentPos !== null) {\n            const line = document.createElement('div');\n            line.className = 'line';\n            line.style.height = '20px';\n            line.style.left = `${(pos + 0.5) * 50}px`;\n            line.style.top = '0px';\n            container.appendChild(line);\n        }\n\n        const currentPos = pos + (1 << (3 - level));\n        drawTree(node.left, level + 1, currentPos * 2 - 1, pos);\n        drawTree(node.right, level + 1, currentPos * 2, pos);\n    }\n</script>\n</body>\n</html>"
