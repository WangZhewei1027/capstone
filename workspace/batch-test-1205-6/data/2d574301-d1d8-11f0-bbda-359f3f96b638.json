    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Dijkstra's Algorithm Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n        }\n        #graph {\n            display: grid;\n            grid-template-columns: repeat(5, 100px);\n            gap: 10px;\n        }\n        .node {\n            width: 100px;\n            height: 100px;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            border: 1px solid black;\n            position: relative;\n            text-align: center;\n        }\n        .node.start {\n            background-color: lightgreen;\n        }\n        .node.end {\n            background-color: lightcoral;\n        }\n        .path {\n            background-color: lightyellow;\n        }\n    </style>\n</head>\n<body>\n    <h1>Dijkstra's Algorithm Visualization</h1>\n    <div id=\"graph\"></div>\n    <button id=\"run\">Run Dijkstra's Algorithm</button>\n    <script>\n        const graphElement = document.getElementById('graph');\n        const runButton = document.getElementById('run');\n\n        const graph = [\n            [0, 7, 0, 0, 0],\n            [7, 0, 5, 0, 0],\n            [0, 5, 0, 2, 0],\n            [0, 0, 2, 0, 1],\n            [0, 0, 0, 1, 0]\n        ];\n\n        const nodes = [\"A\", \"B\", \"C\", \"D\", \"E\"];\n        let startNode = 0; // A\n        let endNode = 4; // E\n\n        function createGraph() {\n            nodes.forEach((node, index) => {\n                const nodeElement = document.createElement('div');\n                nodeElement.className = 'node';\n                nodeElement.textContent = node;\n                nodeElement.dataset.index = index;\n                graphElement.appendChild(nodeElement);\n            });\n            graphElement.children[startNode].classList.add('start');\n            graphElement.children[endNode].classList.add('end');\n        }\n\n        async function dijkstra() {\n            const distances = Array(graph.length).fill(Infinity);\n            const prevNodes = Array(graph.length).fill(null);\n            const visited = new Set();\n            distances[startNode] = 0;\n\n            const unvisited = new Set(nodes.map((_, index) => index));\n\n            while (unvisited.size) {\n                const currentNodeIndex = Array.from(unvisited).reduce((lowestIndex, index) => {\n                    if (distances[index] < distances[lowestIndex]) {\n                        return index;\n                    }\n                    return lowestIndex;\n                }, Array.from(unvisited)[0]);\n\n                if (distances[currentNodeIndex] === Infinity) break;\n\n                unvisited.delete(currentNodeIndex);\n                visited.add(currentNodeIndex);\n\n                graph[currentNodeIndex].forEach((distance, neighborIndex) => {\n                    if (distance > 0 && !visited.has(neighborIndex)) {\n                        const newDistance = distances[currentNodeIndex] + distance;\n                        if (newDistance < distances[neighborIndex]) {\n                            distances[neighborIndex] = newDistance;\n                            prevNodes[neighborIndex] = currentNodeIndex;\n                        }\n                    }\n                });\n\n                await new Promise(resolve => setTimeout(resolve, 500));\n                highlightNode(currentNodeIndex);\n            }\n\n            const path = [];\n            let currentNode = endNode;\n            while (currentNode !== null) {\n                path.push(currentNode);\n                currentNode = prevNodes[currentNode];\n            }\n\n            path.reverse().forEach(nodeIndex => {\n                graphElement.children[nodeIndex].classList.add('path');\n            });\n        }\n\n        function highlightNode(index) {\n            graphElement.children[index].style.backgroundColor = 'lightblue';\n        }\n\n        runButton.addEventListener('click', async () => {\n            resetGraph();\n            await dijkstra();\n        });\n\n        function resetGraph() {\n            const nodes = document.querySelectorAll('.node');\n            nodes.forEach(node => {\n                node.classList.remove('path', 'start', 'end');\n                node.style.backgroundColor = '';\n            });\n            graphElement.children[startNode].classList.add('start');\n            graphElement.children[endNode].classList.add('end');\n        }\n\n        createGraph();\n    </script>\n</body>\n</html>"
