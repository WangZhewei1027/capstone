    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Priority Queue Demo</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n            padding: 20px;\n            border: 1px solid #ccc;\n        }\n        input[type=\"text\"] {\n            padding: 5px;\n            margin-right: 10px;\n        }\n        input[type=\"number\"] {\n            padding: 5px;\n            margin-right: 10px;\n        }\n        button {\n            padding: 5px 10px;\n            margin-top: 10px;\n        }\n        .queue {\n            margin-top: 20px;\n        }\n    </style>\n</head>\n<body>\n    <h1>Priority Queue Demo</h1>\n    <label for=\"element\">Element:</label>\n    <input type=\"text\" id=\"element\" placeholder=\"Enter element\">\n    <label for=\"priority\">Priority:</label>\n    <input type=\"number\" id=\"priority\" placeholder=\"Enter priority (higher = more important)\">\n    <button onclick=\"enqueue()\">Enqueue</button>\n    <button onclick=\"dequeue()\">Dequeue</button>\n\n    <div class=\"queue\">\n        <h2>Current Priority Queue</h2>\n        <ul id=\"queueList\"></ul>\n    </div>\n\n    <script>\n        class PriorityQueue {\n            constructor() {\n                this.items = [];\n            }\n\n            enqueue(element, priority) {\n                const queueElement = { element, priority };\n                let added = false;\n\n                for (let i = 0; i < this.items.length; i++) {\n                    if (queueElement.priority > this.items[i].priority) {\n                        this.items.splice(i, 0, queueElement);\n                        added = true;\n                        break;\n                    }\n                }\n\n                if (!added) {\n                    this.items.push(queueElement);\n                }\n            }\n\n            dequeue() {\n                return this.items.shift(); // Remove and return the highest priority element\n            }\n\n            getQueue() {\n                return this.items;\n            }\n        }\n\n        const priorityQueue = new PriorityQueue();\n\n        function enqueue() {\n            const elementInput = document.getElementById('element');\n            const priorityInput = document.getElementById('priority');\n\n            const element = elementInput.value;\n            const priority = parseFloat(priorityInput.value);\n\n            if (element && !isNaN(priority)) {\n                priorityQueue.enqueue(element, priority);\n                elementInput.value = '';\n                priorityInput.value = '';\n                updateQueueList();\n            } else {\n                alert('Please enter a valid element and priority.');\n            }\n        }\n\n        function dequeue() {\n            const removedElement = priorityQueue.dequeue();\n            if (removedElement) {\n                alert(`Removed: ${removedElement.element}`);\n            } else {\n                alert('Queue is empty.');\n            }\n            updateQueueList();\n        }\n\n        function updateQueueList() {\n            const queueList = document.getElementById('queueList');\n            queueList.innerHTML = '';\n            priorityQueue.getQueue().forEach(item => {\n                const li = document.createElement('li');\n                li.textContent = `Element: ${item.element}, Priority: ${item.priority}`;\n                queueList.appendChild(li);\n            });\n        }\n    </script>\n</body>\n</html>"
