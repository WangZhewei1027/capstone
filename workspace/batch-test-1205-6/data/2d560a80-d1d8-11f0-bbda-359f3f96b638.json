    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Binary Search Tree Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        .tree {\n            display: flex;\n            justify-content: center;\n            margin-top: 20px;\n        }\n        .node {\n            border: 1px solid #000;\n            padding: 10px;\n            margin: 5px;\n            text-align: center;\n            position: relative;\n        }\n        .line {\n            display: block;\n            width: 2px;\n            background-color: #000;\n            position: absolute;\n            top: 100%;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Binary Search Tree Visualization</h1>\n<input type=\"number\" id=\"inputValue\" placeholder=\"Enter a number\">\n<button onclick=\"insertNode()\">Insert</button>\n\n<div class=\"tree\" id=\"treeContainer\"></div>\n\n<script>\n    class TreeNode {\n        constructor(value) {\n            this.value = value;\n            this.left = null;\n            this.right = null;\n        }\n    }\n\n    class BST {\n        constructor() {\n            this.root = null;\n        }\n\n        insert(value) {\n            const newNode = new TreeNode(value);\n            if (!this.root) {\n                this.root = newNode;\n            } else {\n                this.insertNode(this.root, newNode);\n            }\n        }\n\n        insertNode(node, newNode) {\n            if (newNode.value < node.value) {\n                if (!node.left) {\n                    node.left = newNode;\n                } else {\n                    this.insertNode(node.left, newNode);\n                }\n            } else {\n                if (!node.right) {\n                    node.right = newNode;\n                } else {\n                    this.insertNode(node.right, newNode);\n                }\n            }\n        }\n\n        getTreeNodes() {\n            const nodes = [];\n            this.inOrderTraversal(this.root, nodes);\n            return nodes;\n        }\n\n        inOrderTraversal(node, nodes) {\n            if (node) {\n                this.inOrderTraversal(node.left, nodes);\n                nodes.push(node);\n                this.inOrderTraversal(node.right, nodes);\n            }\n        }\n    }\n\n    const bst = new BST();\n\n    function insertNode() {\n        const value = parseInt(document.getElementById('inputValue').value);\n        if (!isNaN(value)) {\n            bst.insert(value);\n            drawTree();\n            document.getElementById('inputValue').value = '';\n        }\n    }\n\n    function drawTree() {\n        const treeContainer = document.getElementById('treeContainer');\n        treeContainer.innerHTML = ''; // Clear previous tree\n        const nodes = bst.getTreeNodes();\n\n        const createNodeElement = (node) => {\n            const div = document.createElement('div');\n            div.className = 'node';\n            div.innerText = node.value;\n\n            // Create connecting lines if parent exists\n            const parent = nodes.find(n => n.left === node || n.right === node);\n            if (parent) {\n                const line = document.createElement('span');\n                line.className = 'line';\n                line.style.height = '30px';\n                line.style.left = (node.value < parent.value ? '50%' : '50%');\n                line.style.transform = `translateX(${node.value < parent.value ? '-50%' : '50%'})`;\n                div.appendChild(line);\n            }\n            return div;\n        };\n\n        nodes.forEach(node => {\n            const nodeElement = createNodeElement(node);\n            treeContainer.appendChild(nodeElement);\n        });\n    }\n</script>\n\n</body>\n</html>"
