    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Weighted Graph Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            text-align: center;\n            margin: 50px;\n        }\n        #graph {\n            border: 1px solid #ccc;\n            width: 600px;\n            height: 400px;\n            margin: 0 auto;\n            position: relative;\n        }\n        .node {\n            position: absolute;\n            width: 30px;\n            height: 30px;\n            background-color: #4CAF50;\n            border-radius: 50%;\n            color: white;\n            text-align: center;\n            line-height: 30px;\n            font-weight: bold;\n        }\n        .edge {\n            position: absolute;\n            height: 2px;\n            background-color: black;\n            transform-origin: top left;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Weighted Graph Visualization</h1>\n<div id=\"graph\"></div>\n\n<script>\n    const graph = {\n        A: { B: 2, C: 3 },\n        B: { A: 2, D: 5 },\n        C: { A: 3, D: 1 },\n        D: { B: 5, C: 1 }\n    };\n\n    const positions = {\n        A: { x: 100, y: 50 },\n        B: { x: 250, y: 150 },\n        C: { x: 250, y: 50 },\n        D: { x: 400, y: 150 }\n    };\n\n    const graphContainer = document.getElementById('graph');\n\n    // Draw nodes\n    for (const node in graph) {\n        const nodeDiv = document.createElement('div');\n        nodeDiv.className = 'node';\n        nodeDiv.textContent = node;\n        nodeDiv.style.left = positions[node].x + 'px';\n        nodeDiv.style.top = positions[node].y + 'px';\n        graphContainer.appendChild(nodeDiv);\n    }\n\n    // Draw edges\n    for (const node in graph) {\n        for (const neighbor in graph[node]) {\n            const weight = graph[node][neighbor];\n            const startPos = positions[node];\n            const endPos = positions[neighbor];\n\n            const edgeDiv = document.createElement('div');\n            edgeDiv.className = 'edge';\n           \n            const length = Math.sqrt(Math.pow(endPos.x - startPos.x, 2) + Math.pow(endPos.y - startPos.y, 2));\n            edgeDiv.style.width = length + 'px';\n            edgeDiv.style.left = startPos.x + 'px';\n            edgeDiv.style.top = startPos.y + 'px';\n            edgeDiv.style.transform = `rotate(${Math.atan2(endPos.y - startPos.y, endPos.x - startPos.x) * 180 / Math.PI}deg)`;\n\n            const textDiv = document.createElement('span');\n            textDiv.textContent = weight;\n            textDiv.style.position = 'absolute';\n            textDiv.style.left = (startPos.x + endPos.x) / 2 + 'px';\n            textDiv.style.top = (startPos.y + endPos.y) / 2 + 'px';\n            textDiv.style.transform = 'translate(-50%, -50%)';\n\n            graphContainer.appendChild(edgeDiv);\n            graphContainer.appendChild(textDiv);\n        }\n    }\n\n</script>\n</body>\n</html>"
