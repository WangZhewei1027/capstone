    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Prim's Algorithm Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n            background-color: #f4f4f4;\n        }\n        #canvas {\n            border: 1px solid #ccc;\n            margin-top: 20px;\n        }\n        #controls {\n            margin-bottom: 20px;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Prim's Algorithm Visualization</h1>\n<p>This visualization demonstrates Prim's Algorithm for finding a Minimum Spanning Tree (MST) in a graph.</p>\n\n<div id=\"controls\">\n    <button onclick=\"runPrimsAlgorithm()\">Run Prim's Algorithm</button>\n</div>\n\n<canvas id=\"canvas\" width=\"600\" height=\"400\"></canvas>\n\n<script>\nconst canvas = document.getElementById('canvas');\nconst ctx = canvas.getContext('2d');\n\nconst vertices = [\n    { x: 50, y: 50, id: 'A' },\n    { x: 200, y: 50, id: 'B' },\n    { x: 350, y: 50, id: 'C' },\n    { x: 100, y: 150, id: 'D' },\n    { x: 300, y: 150, id: 'E' },\n    { x: 175, y: 250, id: 'F' },\n];\n\nconst edges = [\n    { from: 'A', to: 'B', weight: 4 },\n    { from: 'A', to: 'D', weight: 1 },\n    { from: 'B', to: 'C', weight: 2 },\n    { from: 'B', to: 'E', weight: 7 },\n    { from: 'C', to: 'E', weight: 3 },\n    { from: 'D', to: 'E', weight: 6 },\n    { from: 'D', to: 'F', weight: 5 },\n    { from: 'E', to: 'F', weight: 4 },\n];\n\nconst graph = {};\n\nfunction setupGraph() {\n    vertices.forEach(v => {\n        graph[v.id] = [];\n    });\n    \n    edges.forEach(e => {\n        graph[e.from].push({ to: e.to, weight: e.weight });\n        graph[e.to].push({ to: e.from, weight: e.weight });\n    });\n}\n\nfunction drawGraph(selectedEdges = []) {\n    ctx.clearRect(0, 0, canvas.width, canvas.height);\n    \n    // Draw vertices\n    vertices.forEach(v => {\n        ctx.beginPath();\n        ctx.arc(v.x, v.y, 20, 0, Math.PI * 2);\n        ctx.fillStyle = 'lightblue';\n        ctx.fill();\n        ctx.stroke();\n        ctx.fillStyle = 'black';\n        ctx.fillText(v.id, v.x - 5, v.y + 5);\n    });\n\n    // Draw edges\n    edges.forEach(e => {\n        const v1 = vertices.find(v => v.id === e.from);\n        const v2 = vertices.find(v => v.id === e.to);\n        ctx.beginPath();\n        ctx.moveTo(v1.x, v1.y);\n        ctx.lineTo(v2.x, v2.y);\n        ctx.strokeStyle = 'gray';\n        ctx.stroke();\n    });\n\n    // Draw selected edges for MST\n    selectedEdges.forEach(e => {\n        const v1 = vertices.find(v => v.id === e.from);\n        const v2 = vertices.find(v => v.id === e.to);\n        ctx.beginPath();\n        ctx.moveTo(v1.x, v1.y);\n        ctx.lineTo(v2.x, v2.y);\n        ctx.strokeStyle = 'green';\n        ctx.lineWidth = 2;\n        ctx.stroke();\n    });\n}\n\nfunction runPrimsAlgorithm() {\n    const mstEdges = [];\n    const visited = new Set();\n    const edgeQueue = [];\n\n    const startVertex = vertices[0].id;\n    visited.add(startVertex);\n    \n    graph[startVertex].forEach(e => {\n        edgeQueue.push({ ...e, from: startVertex });\n    });\n\n    while (edgeQueue.length > 0) {\n        edgeQueue.sort((a, b) => a.weight - b.weight);\n        const { from, to, weight } = edgeQueue.shift();\n\n        if (!visited.has(to)) {\n            visited.add(to);\n            mstEdges.push({ from, to });\n\n            graph[to].forEach(e => {\n                edgeQueue.push({ ...e, from: to });\n            });\n        }\n    }\n\n    drawGraph(mstEdges);\n}\n\nsetupGraph();\ndrawGraph();\n</script>\n\n</body>\n</html>"
