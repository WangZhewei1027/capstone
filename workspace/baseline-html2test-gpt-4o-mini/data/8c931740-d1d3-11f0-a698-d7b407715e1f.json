    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Heap Visualization (Min/Max)</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            text-align: center;\n            margin: 20px;\n        }\n        #heap {\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            margin-top: 20px;\n        }\n        .heap-node {\n            display: inline-block;\n            margin: 5px;\n            padding: 10px;\n            border: 1px solid #777;\n            border-radius: 5px;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Min Heap / Max Heap Demonstration</h1>\n\n<button onclick=\"insertValue('min', parseInt(document.getElementById('minInput').value))\">Insert into Min Heap</button>\n<button onclick=\"insertValue('max', parseInt(document.getElementById('maxInput').value))\">Insert into Max Heap</button>\n\n<div>\n    <input type=\"number\" id=\"minInput\" placeholder=\"Value for Min Heap\">\n    <input type=\"number\" id=\"maxInput\" placeholder=\"Value for Max Heap\">\n</div>\n\n<div id=\"heap\">\n    <div>\n        <h2>Min Heap</h2>\n        <div id=\"minHeap\" class=\"heap-visualization\"></div>\n    </div>\n    <div>\n        <h2>Max Heap</h2>\n        <div id=\"maxHeap\" class=\"heap-visualization\"></div>\n    </div>\n</div>\n\n<script>\n    class MinHeap {\n        constructor() {\n            this.heap = [];\n        }\n\n        insert(value) {\n            this.heap.push(value);\n            this.bubbleUp();\n        }\n\n        bubbleUp() {\n            let index = this.heap.length - 1;\n            while (index > 0) {\n                let parentIndex = Math.floor((index - 1) / 2);\n                if (this.heap[index] >= this.heap[parentIndex]) break;\n                [this.heap[index], this.heap[parentIndex]] = [this.heap[parentIndex], this.heap[index]];\n                index = parentIndex;\n            }\n        }\n\n        display() {\n            const minHeapDiv = document.getElementById('minHeap');\n            minHeapDiv.innerHTML = this.heap.map(value => `<div class=\"heap-node\">${value}</div>`).join('');\n        }\n    }\n\n    class MaxHeap {\n        constructor() {\n            this.heap = [];\n        }\n\n        insert(value) {\n            this.heap.push(value);\n            this.bubbleUp();\n        }\n\n        bubbleUp() {\n            let index = this.heap.length - 1;\n            while (index > 0) {\n                let parentIndex = Math.floor((index - 1) / 2);\n                if (this.heap[index] <= this.heap[parentIndex]) break;\n                [this.heap[index], this.heap[parentIndex]] = [this.heap[parentIndex], this.heap[index]];\n                index = parentIndex;\n            }\n        }\n\n        display() {\n            const maxHeapDiv = document.getElementById('maxHeap');\n            maxHeapDiv.innerHTML = this.heap.map(value => `<div class=\"heap-node\">${value}</div>`).join('');\n        }\n    }\n\n    const minHeap = new MinHeap();\n    const maxHeap = new MaxHeap();\n\n    function insertValue(type, value) {\n        if (!isNaN(value)) {\n            if (type === 'min') {\n                minHeap.insert(value);\n                minHeap.display();\n            } else if (type === 'max') {\n                maxHeap.insert(value);\n                maxHeap.display();\n            }\n        } else {\n            alert('Please enter a valid number.');\n        }\n    }\n</script>\n\n</body>\n</html>"
