    "content": "<html lang=\"en\">\n\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Topological Sort Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            text-align: center;\n            margin: 20px;\n        }\n\n        #graph {\n            margin: 20px auto;\n            max-width: 800px;\n        }\n\n        .node {\n            border: 1px solid #444;\n            border-radius: 5px;\n            padding: 10px;\n            display: inline-block;\n            margin: 5px;\n        }\n\n        .edge {\n            display: inline-block;\n            margin: 5px;\n        }\n\n        .sort-button {\n            padding: 10px 20px;\n            font-size: 16px;\n            cursor: pointer;\n        }\n\n        .result {\n            margin-top: 20px;\n            font-weight: bold;\n        }\n    </style>\n</head>\n\n<body>\n    <h1>Topological Sort Demo</h1>\n    <div id=\"graph\">\n        <div class=\"node\">A</div>\n        <div class=\"edge\">→</div>\n        <div class=\"node\">B</div>\n        <div class=\"edge\">→</div>\n        <div class=\"node\">C</div>\n        <div class=\"edge\">→</div>\n        <div class=\"node\">D</div>\n        <div class=\"edge\">→</div>\n        <div class=\"node\">E</div>\n    </div>\n\n    <button onclick=\"performTopologicalSort()\" class=\"sort-button\">Perform Topological Sort</button>\n\n    <div class=\"result\" id=\"result\"></div>\n\n    <script>\n        function performTopologicalSort() {\n            const adjacencyList = {\n                A: ['B', 'C'],\n                B: ['D'],\n                C: ['D'],\n                D: ['E'],\n                E: []\n            };\n\n            const visited = {};\n            const result = [];\n            const tempMark = {};\n\n            function visit(node) {\n                if (tempMark[node]) {\n                    throw new Error(\"Graph is not a DAG (contains a cycle)\");\n                }\n                if (!visited[node]) {\n                    tempMark[node] = true;\n\n                    for (const neighbor of adjacencyList[node]) {\n                        visit(neighbor);\n                    }\n\n                    tempMark[node] = false;\n                    visited[node] = true;\n                    result.push(node);\n                }\n            }\n\n            try {\n                for (const node in adjacencyList) {\n                    visit(node);\n                }\n                result.reverse(); // Reverse to get the correct order\n                document.getElementById('result').innerText = \"Topological Sort Order: \" + result.join(', ');\n            } catch (error) {\n                document.getElementById('result').innerText = error.message;\n            }\n        }\n    </script>\n</body>\n\n</html>"
