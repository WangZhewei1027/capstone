    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Union-Find (Disjoint Set) Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            background-color: #f4f4f4;\n            padding: 20px;\n        }\n        .container {\n            margin: 0 auto;\n            max-width: 600px;\n        }\n        #output {\n            margin-top: 20px;\n            font-size: 18px;\n        }\n        .node {\n            display: inline-block;\n            padding: 10px;\n            margin: 5px;\n            background: #aeeeee;\n            border: 1px solid #007d7d;\n            border-radius: 5px;\n            cursor: pointer;\n        }\n        .node.selected {\n            background: #007d7d;\n            color: #fff;\n        }\n    </style>\n</head>\n<body>\n\n<div class=\"container\">\n    <h1>Union-Find (Disjoint Set) Visualization</h1>\n    <div id=\"nodes\"></div>\n    <button onclick=\"performUnion()\">Union Selected</button>\n    <div id=\"output\"></div>\n</div>\n\n<script>\n    class UnionFind {\n        constructor(size) {\n            this.parent = Array.from({ length: size }, (_, i) => i);\n            this.rank = Array(size).fill(0);\n        }\n\n        find(x) {\n            if (this.parent[x] !== x) {\n                this.parent[x] = this.find(this.parent[x]); // Path compression\n            }\n            return this.parent[x];\n        }\n\n        union(x, y) {\n            const rootX = this.find(x);\n            const rootY = this.find(y);\n            if (rootX !== rootY) {\n                // Union by rank\n                if (this.rank[rootX] > this.rank[rootY]) {\n                    this.parent[rootY] = rootX;\n                } else if (this.rank[rootX] < this.rank[rootY]) {\n                    this.parent[rootX] = rootY;\n                } else {\n                    this.parent[rootY] = rootX;\n                    this.rank[rootX]++;\n                }\n            }\n        }\n\n        getComponents() {\n            const components = {};\n            for (let i = 0; i < this.parent.length; i++) {\n                const root = this.find(i);\n                if (!components[root]) {\n                    components[root] = [];\n                }\n                components[root].push(i);\n            }\n            return Object.values(components);\n        }\n    }\n\n    const uf = new UnionFind(10);\n    const nodesContainer = document.getElementById('nodes');\n    const outputDiv = document.getElementById('output');\n\n    // Create nodes\n    for (let i = 0; i < 10; i++) {\n        const node = document.createElement('div');\n        node.classList.add('node');\n        node.textContent = i;\n        node.dataset.index = i;\n        node.onclick = () => {\n            node.classList.toggle('selected');\n        };\n        nodesContainer.appendChild(node);\n    }\n\n    function performUnion() {\n        const selectedNodes = Array.from(document.querySelectorAll('.node.selected'));\n        if (selectedNodes.length === 2) {\n            const index1 = parseInt(selectedNodes[0].dataset.index);\n            const index2 = parseInt(selectedNodes[1].dataset.index);\n            uf.union(index1, index2);\n            outputDiv.textContent = `Union(${index1}, ${index2}) performed.`;\n            updateComponents();\n        } else {\n            outputDiv.textContent = 'Please select exactly two nodes to perform a union.';\n        }\n    }\n\n    function updateComponents() {\n        const components = uf.getComponents();\n        outputDiv.textContent += `\\nCurrent components: ${JSON.stringify(components)}`;\n        const nodes = document.querySelectorAll('.node');\n        nodes.forEach(node => {\n            node.classList.remove('selected');\n        });\n    }\n</script>\n\n</body>\n</html>"
