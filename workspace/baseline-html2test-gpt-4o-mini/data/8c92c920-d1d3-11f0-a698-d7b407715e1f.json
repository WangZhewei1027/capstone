    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Binary Tree Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            justify-content: center;\n            height: 100vh;\n            margin: 0;\n            background-color: #f0f0f0;\n        }\n        #tree {\n            position: relative;\n            height: 300px;\n            width: 100%;\n            margin: 20px;\n        }\n        .node {\n            position: absolute;\n            width: 40px;\n            height: 40px;\n            background-color: #4CAF50;\n            border-radius: 50%;\n            text-align: center;\n            line-height: 40px;\n            color: white;\n            font-weight: bold;\n        }\n        .line {\n            position: absolute;\n            background: black;\n            height: 2px;\n            width: 100px;\n            transform: rotate(0deg);\n            transform-origin: left;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Binary Tree Visualization</h1>\n<div id=\"tree\"></div>\n<button onclick=\"addNode()\">Add Node</button>\n<button onclick=\"resetTree()\">Reset Tree</button>\n\n<script>\n    class TreeNode {\n        constructor(value) {\n            this.value = value;\n            this.left = null;\n            this.right = null;\n        }\n    }\n\n    class BinaryTree {\n        constructor() {\n            this.root = null;\n            this.nodes = [];\n        }\n\n        add(value) {\n            const newNode = new TreeNode(value);\n            if (!this.root) {\n                this.root = newNode;\n            } else {\n                this._addNode(this.root, newNode);\n            }\n            this.nodes.push(newNode);\n            this.drawTree();\n        }\n\n        _addNode(root, newNode) {\n            if (newNode.value < root.value) {\n                if (root.left) {\n                    this._addNode(root.left, newNode);\n                } else {\n                    root.left = newNode;\n                }\n            } else {\n                if (root.right) {\n                    this._addNode(root.right, newNode);\n                } else {\n                    root.right = newNode;\n                }\n            }\n        }\n\n        clear() {\n            this.root = null;\n            this.nodes = [];\n        }\n\n        drawTree() {\n            const treeDiv = document.getElementById('tree');\n            treeDiv.innerHTML = '';\n            if (this.root) {\n                this._drawNode(this.root, treeDiv, window.innerWidth / 2, 20, 100);\n            }\n        }\n\n        _drawNode(node, container, x, y, offset) {\n            if (!node) return;\n\n            // Create node element\n            const nodeDiv = document.createElement('div');\n            nodeDiv.className = 'node';\n            nodeDiv.style.left = `${x}px`;\n            nodeDiv.style.top = `${y}px`;\n            nodeDiv.textContent = node.value;\n            container.appendChild(nodeDiv);\n\n            if (node.left) {\n                const leftX = x - offset;\n                const leftY = y + 100;\n                this._drawLine(container, x, y, leftX, leftY);\n                this._drawNode(node.left, container, leftX, leftY, offset / 2);\n            }\n\n            if (node.right) {\n                const rightX = x + offset;\n                const rightY = y + 100;\n                this._drawLine(container, x, y, rightX, rightY);\n                this._drawNode(node.right, container, rightX, rightY, offset / 2);\n            }\n        }\n\n        _drawLine(container, x1, y1, x2, y2) {\n            const line = document.createElement('div');\n            line.className = 'line';\n            const length = Math.sqrt((x2 - x1) ** 2 + (y2 - y1) ** 2);\n            line.style.width = `${length}px`;\n            const angle = Math.atan2(y2 - y1, x2 - x1) * (180 / Math.PI);\n            line.style.transform = `rotate(${angle}deg)`;\n            line.style.left = `${x1}px`;\n            line.style.top = `${y1}px`;\n            container.appendChild(line);\n        }\n    }\n\n    const tree = new BinaryTree();\n\n    function addNode() {\n        const value = Math.floor(Math.random() * 100);\n        tree.add(value);\n    }\n\n    function resetTree() {\n        tree.clear();\n        tree.drawTree();\n    }\n</script>\n\n</body>\n</html>"
