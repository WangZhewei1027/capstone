    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Binary Tree Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            text-align: center;\n        }\n        #tree {\n            margin: 20px auto;\n            width: 80%;\n        }\n        .node {\n            display: inline-block;\n            width: 50px;\n            height: 50px;\n            line-height: 50px;\n            border: 1px solid #333;\n            border-radius: 50%;\n            position: relative;\n            margin: 10px;\n        }\n        .connector {\n            position: absolute;\n            width: 2px;\n            background-color: #333;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Binary Tree Visualization</h1>\n<div id=\"tree\"></div>\n<button onclick=\"addNode()\">Add Node</button>\n\n<script>\n    class TreeNode {\n        constructor(value) {\n            this.value = value;\n            this.left = null;\n            this.right = null;\n        }\n    }\n\n    class BinaryTree {\n        constructor() {\n            this.root = null;\n        }\n\n        add(value) {\n            const newNode = new TreeNode(value);\n            if (!this.root) {\n                this.root = newNode;\n            } else {\n                this._insertNode(this.root, newNode);\n            }\n        }\n\n        _insertNode(node, newNode) {\n            if (newNode.value < node.value) {\n                if (!node.left) {\n                    node.left = newNode;\n                } else {\n                    this._insertNode(node.left, newNode);\n                }\n            } else {\n                if (!node.right) {\n                    node.right = newNode;\n                } else {\n                    this._insertNode(node.right, newNode);\n                }\n            }\n        }\n\n        visualize() {\n            return this._visualizeNode(this.root, 100, 0);\n        }\n\n        _visualizeNode(node, offset, level) {\n            if (!node) return \"\";\n\n            let left = this._visualizeNode(node.left, offset / 2, level + 1);\n            let right = this._visualizeNode(node.right, offset / 2, level + 1);\n\n            let connector = level === 0 ? '' : `<div class=\"connector\" style=\"left:${offset/2}px; height:20px; top:${level*60}px;\"></div>`;\n            let nodeHTML = `<div class=\"node\" style=\"margin-top:${level * 60}px;\">${node.value}</div>`;\n\n            return left + connector + nodeHTML + right;\n        }\n    }\n\n    const tree = new BinaryTree();\n    let valueCounter = 1;\n\n    function addNode() {\n        tree.add(valueCounter++);\n        document.getElementById(\"tree\").innerHTML = tree.visualize();\n    }\n</script>\n\n</body>\n</html>"
