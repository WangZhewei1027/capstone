    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Stack Implementation</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n        }\n        #stackOutput {\n            margin-top: 20px;\n            border: 1px solid #ccc;\n            padding: 10px;\n            height: 150px;\n            overflow-y: auto;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Stack Implementation</h1>\n<input type=\"text\" id=\"stackInput\" placeholder=\"Enter value\" />\n<button onclick=\"pushToStack()\">Push</button>\n<button onclick=\"popFromStack()\">Pop</button>\n<button onclick=\"peekStack()\">Peek</button>\n<button onclick=\"clearStack()\">Clear Stack</button>\n\n<div id=\"stackOutput\"></div>\n\n<script>\n    class Stack {\n        constructor() {\n            this.items = [];\n        }\n\n        push(element) {\n            this.items.push(element);\n        }\n\n        pop() {\n            if (this.isEmpty()) {\n                return \"Stack is empty\";\n            }\n            return this.items.pop();\n        }\n\n        peek() {\n            if (this.isEmpty()) {\n                return \"Stack is empty\";\n            }\n            return this.items[this.items.length - 1];\n        }\n\n        isEmpty() {\n            return this.items.length === 0;\n        }\n\n        clear() {\n            this.items = [];\n        }\n\n        toString() {\n            return this.items.join(', ');\n        }\n    }\n\n    const stack = new Stack();\n\n    function pushToStack() {\n        const inputField = document.getElementById('stackInput');\n        const value = inputField.value;\n        if (value) {\n            stack.push(value);\n            inputField.value = '';\n            updateStackOutput();\n        }\n    }\n\n    function popFromStack() {\n        stack.pop();\n        updateStackOutput();\n    }\n\n    function peekStack() {\n        alert(stack.peek());\n    }\n\n    function clearStack() {\n        stack.clear();\n        updateStackOutput();\n    }\n\n    function updateStackOutput() {\n        const outputDiv = document.getElementById('stackOutput');\n        outputDiv.innerHTML = stack.toString();\n    }\n</script>\n\n</body>\n</html>"
