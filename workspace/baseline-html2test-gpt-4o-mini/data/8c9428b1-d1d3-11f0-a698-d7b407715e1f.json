    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Bellman-Ford Algorithm Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n        }\n        #graph {\n            margin-bottom: 20px;\n        }\n        .node {\n            display: inline-block;\n            width: 30px;\n            height: 30px;\n            border: 1px solid #000;\n            border-radius: 50%;\n            text-align: center;\n            line-height: 30px;\n            background-color: #f0f0f0;\n        }\n        .edge {\n            display: inline-block;\n            margin: 0 10px;\n        }\n        .result {\n            margin-top: 20px;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Bellman-Ford Algorithm Visualization</h1>\n<div id=\"graph\"></div>\n<button onclick=\"runBellmanFord()\">Run Bellman-Ford Algorithm</button>\n\n<div class=\"result\" id=\"result\"></div>\n\n<script>\n    const vertices = ['A', 'B', 'C', 'D', 'E'];\n    const edges = [\n        { from: 'A', to: 'B', weight: -1 },\n        { from: 'A', to: 'C', weight: 4 },\n        { from: 'B', to: 'C', weight: 3 },\n        { from: 'B', to: 'D', weight: 2 },\n        { from: 'B', to: 'E', weight: 2 },\n        { from: 'D', to: 'C', weight: 5 },\n        { from: 'D', to: 'E', weight: -3 },\n        { from: 'E', to: 'A', weight: 1 }\n    ];\n\n    function drawGraph() {\n        const graphDiv = document.getElementById('graph');\n        graphDiv.innerHTML = '';\n\n        vertices.forEach(vertex => {\n            const node = document.createElement('div');\n            node.className = 'node';\n            node.innerText = vertex;\n            graphDiv.appendChild(node);\n        });\n\n        edges.forEach(edge => {\n            const edgeDiv = document.createElement('div');\n            edgeDiv.className = 'edge';\n            edgeDiv.innerText = `(${edge.from}, ${edge.to}, ${edge.weight})`;\n            graphDiv.appendChild(edgeDiv);\n        });\n    }\n\n    function runBellmanFord() {\n        const numVertices = vertices.length;\n        const distances = {};\n        const predecessors = {};\n\n        // Initialize distances and predecessors\n        for (const vertex of vertices) {\n            distances[vertex] = Infinity;\n            predecessors[vertex] = null;\n        }\n        distances['A'] = 0; // Starting node\n\n        // Relax edges\n        for (let i = 1; i < numVertices; i++) {\n            edges.forEach(edge => {\n                if (distances[edge.from] + edge.weight < distances[edge.to]) {\n                    distances[edge.to] = distances[edge.from] + edge.weight;\n                    predecessors[edge.to] = edge.from;\n                }\n            });\n        }\n\n        // Check for negative-weight cycles\n        let isNegativeCycle = false;\n        edges.forEach(edge => {\n            if (distances[edge.from] + edge.weight < distances[edge.to]) {\n                isNegativeCycle = true;\n            }\n        });\n\n        displayResult(distances, isNegativeCycle);\n    }\n\n    function displayResult(distances, isNegativeCycle) {\n        const resultDiv = document.getElementById('result');\n        resultDiv.innerHTML = '';\n\n        if (isNegativeCycle) {\n            resultDiv.innerHTML = '<strong>Graph contains a negative weight cycle.</strong>';\n        } else {\n            resultDiv.innerHTML = '<strong>Shortest distances from vertex A:</strong><br>';\n            for (const vertex in distances) {\n                resultDiv.innerHTML += `${vertex}: ${distances[vertex]}<br>`;\n            }\n        }\n    }\n\n    // Initial drawing of the graph\n    drawGraph();\n</script>\n\n</body>\n</html>"
