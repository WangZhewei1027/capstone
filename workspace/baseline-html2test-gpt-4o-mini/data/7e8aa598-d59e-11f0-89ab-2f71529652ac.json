    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Priority Queue Demo</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n        }\n        #queue {\n            margin-top: 20px;\n        }\n        .queue-item {\n            padding: 10px;\n            border: 1px solid #ccc;\n            margin: 5px 0;\n            border-radius: 5px;\n            display: flex;\n            justify-content: space-between;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Priority Queue Demonstration</h1>\n<label for=\"item\">Item:</label>\n<input type=\"text\" id=\"item\" placeholder=\"Enter item\">\n<label for=\"priority\">Priority:</label>\n<input type=\"number\" id=\"priority\" placeholder=\"Enter priority\" min=\"1\">\n<button onclick=\"enqueue()\">Add to Queue</button>\n<button onclick=\"dequeue()\">Remove from Queue</button>\n\n<div id=\"queue\">\n    <h2>Current Queue:</h2>\n    <div id=\"queueItems\"></div>\n</div>\n\n<script>\n    class PriorityQueue {\n        constructor() {\n            this.items = [];\n        }\n\n        enqueue(element, priority) {\n            const queueElement = { element, priority };\n            let added = false;\n\n            for (let i = 0; i < this.items.length; i++) {\n                if (queueElement.priority < this.items[i].priority) {\n                    this.items.splice(i, 0, queueElement);\n                    added = true;\n                    break;\n                }\n            }\n\n            if (!added) {\n                this.items.push(queueElement);\n            }\n        }\n\n        dequeue() {\n            return this.items.shift();\n        }\n\n        isEmpty() {\n            return this.items.length === 0;\n        }\n\n        printQueue() {\n            return this.items.map(item => item.element + ' (Priority: ' + item.priority + ')').join(', ');\n        }\n\n        renderQueue() {\n            const queueContainer = document.getElementById('queueItems');\n            queueContainer.innerHTML = '';\n            this.items.forEach(item => {\n                const div = document.createElement('div');\n                div.className = 'queue-item';\n                div.innerText = item.element + ' (Priority: ' + item.priority + ')';\n                queueContainer.appendChild(div);\n            });\n        }\n    }\n\n    const priorityQueue = new PriorityQueue();\n\n    function enqueue() {\n        const itemInput = document.getElementById('item');\n        const priorityInput = document.getElementById('priority');\n        const item = itemInput.value;\n        const priority = parseInt(priorityInput.value);\n\n        if (item && !isNaN(priority)) {\n            priorityQueue.enqueue(item, priority);\n            itemInput.value = '';\n            priorityInput.value = '';\n            priorityQueue.renderQueue();\n        } else {\n            alert(\"Please enter valid item and priority.\");\n        }\n    }\n\n    function dequeue() {\n        if (!priorityQueue.isEmpty()) {\n            priorityQueue.dequeue();\n            priorityQueue.renderQueue();\n        } else {\n            alert(\"Queue is empty.\");\n        }\n    }\n</script>\n\n</body>\n</html>"
