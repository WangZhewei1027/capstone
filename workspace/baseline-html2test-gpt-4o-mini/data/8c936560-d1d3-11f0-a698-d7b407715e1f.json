    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Priority Queue Demonstration</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n        }\n        #queue {\n            margin-top: 20px;\n        }\n        .queue-item {\n            padding: 10px;\n            border: 1px solid #ccc;\n            margin: 5px 0;\n            border-radius: 5px;\n        }\n        .high { background-color: #ffdddd; }\n        .medium { background-color: #ffffcc; }\n        .low { background-color: #ddffdd; }\n    </style>\n</head>\n<body>\n\n<h1>Priority Queue Demo</h1>\n<label for=\"priority\">Enter Priority (High, Medium, Low):</label>\n<input type=\"text\" id=\"priority\" placeholder=\"High, Medium, Low\">\n<label for=\"task\">Enter Task:</label>\n<input type=\"text\" id=\"task\" placeholder=\"Task description\">\n<button id=\"addBtn\">Add to Queue</button>\n\n<div id=\"queue\">\n    <h2>Priority Queue</h2>\n    <div id=\"queueList\"></div>\n</div>\n\n<script>\n    class PriorityQueue {\n        constructor() {\n            this.queue = [];\n        }\n\n        enqueue(task, priority) {\n            const item = { task, priority };\n            this.queue.push(item);\n            this.queue.sort((a, b) => this.getPriorityValue(a.priority) - this.getPriorityValue(b.priority));\n        }\n\n        getPriorityValue(priority) {\n            switch (priority.toLowerCase()) {\n                case 'high': return 1;\n                case 'medium': return 2;\n                case 'low': return 3;\n                default: return 4; // Unknown priority\n            }\n        }\n\n        dequeue() {\n            return this.queue.shift();\n        }\n\n        displayQueue() {\n            const queueList = document.getElementById('queueList');\n            queueList.innerHTML = '';\n\n            this.queue.forEach(item => {\n                const div = document.createElement('div');\n                div.className = `queue-item ${item.priority.toLowerCase()}`;\n                div.textContent = `${item.priority.toUpperCase()}: ${item.task}`;\n                queueList.appendChild(div);\n            });\n        }\n    }\n\n    const priorityQueue = new PriorityQueue();\n\n    document.getElementById('addBtn').addEventListener('click', () => {\n        const priority = document.getElementById('priority').value;\n        const task = document.getElementById('task').value;\n\n        if (priority && task) {\n            priorityQueue.enqueue(task, priority);\n            priorityQueue.displayQueue();\n            document.getElementById('priority').value = '';\n            document.getElementById('task').value = '';\n        } else {\n            alert('Please enter both priority and task.');\n        }\n    });\n</script>\n\n</body>\n</html>"
