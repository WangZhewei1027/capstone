    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Kruskal's Algorithm Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            background-color: #f4f4f4;\n            margin: 0;\n            padding: 20px;\n        }\n        h1 {\n            text-align: center;\n        }\n        #graph {\n            margin: 20px auto;\n            width: 80%;\n            height: 400px;\n            border: 1px solid #ccc;\n            position: relative;\n        }\n        .edge {\n            stroke: #ccc;\n            stroke-width: 2;\n        }\n        .edge.selected {\n            stroke: #ff6347;\n            stroke-width: 3;\n        }\n        .node {\n            fill: #3498db;\n            stroke: #2980b9;\n            stroke-width: 2;\n        }\n        .node:hover {\n            fill: #1abc9c;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Kruskal's Algorithm Visualization</h1>\n<svg id=\"graph\"></svg>\n<button id=\"run\">Run Kruskal's Algorithm</button>\n<div id=\"output\"></div>\n\n<script>\n    const edges = [\n        { source: 0, target: 1, weight: 4 },\n        { source: 0, target: 2, weight: 1 },\n        { source: 1, target: 2, weight: 2 },\n        { source: 1, target: 3, weight: 5 },\n        { source: 2, target: 3, weight: 8 },\n        { source: 1, target: 4, weight: 3 },\n        { source: 3, target: 4, weight: 7 },\n    ];\n\n    const nodes = [\n        { id: 0, x: 100, y: 100 },\n        { id: 1, x: 200, y: 50 },\n        { id: 2, x: 150, y: 150 },\n        { id: 3, x: 250, y: 200 },\n        { id: 4, x: 200, y: 300 },\n    ];\n\n    const svg = document.getElementById('graph');\n    const output = document.getElementById('output');\n\n    function drawGraph() {\n        // Draw edges\n        edges.forEach(edge => {\n            const line = document.createElementNS(\"http://www.w3.org/2000/svg\", \"line\");\n            line.setAttribute('class', 'edge');\n            line.setAttribute('x1', nodes[edge.source].x);\n            line.setAttribute('y1', nodes[edge.source].y);\n            line.setAttribute('x2', nodes[edge.target].x);\n            line.setAttribute('y2', nodes[edge.target].y);\n            svg.appendChild(line);\n        });\n\n        // Draw nodes\n        nodes.forEach(node => {\n            const circle = document.createElementNS(\"http://www.w3.org/2000/svg\", \"circle\");\n            circle.setAttribute('class', 'node');\n            circle.setAttribute('cx', node.x);\n            circle.setAttribute('cy', node.y);\n            circle.setAttribute('r', 20);\n            circle.setAttribute('data-id', node.id);\n            svg.appendChild(circle);\n        });\n    }\n\n    function find(parent, i) {\n        if (parent[i] === i) return i;\n        return find(parent, parent[i]);\n    }\n\n    function union(parent, rank, x, y) {\n        if (rank[x] > rank[y]) {\n            parent[y] = x;\n        } else if (rank[x] < rank[y]) {\n            parent[x] = y;\n        } else {\n            parent[y] = x;\n            rank[x]++;\n        }\n    }\n\n    function runKruskal() {\n        output.innerHTML = '';\n        svg.innerHTML = '';\n        drawGraph();\n\n        let result = [];\n        edges.sort((a, b) => a.weight - b.weight);\n\n        let parent = [];\n        let rank = [];\n        for (var node of nodes) {\n            parent[node.id] = node.id;\n            rank[node.id] = 0;\n        }\n\n        let e = 0;\n        let i = 0;\n        while (e < nodes.length - 1 && i < edges.length) {\n            const { source, target, weight } = edges[i++];\n            let x = find(parent, source);\n            let y = find(parent, target);\n\n            if (x !== y) {\n                e++;\n                result.push({ source, target, weight });\n                union(parent, rank, x, y);\n                // Highlight the selected edge\n                const lines = svg.getElementsByClassName('edge');\n                lines[(source + target) % edges.length].classList.add('selected');\n            }\n        }\n\n        output.innerHTML = 'Minimum Spanning Tree: <br>' + result.map(r => `(${r.source}, ${r.target}): ${r.weight}`).join('<br>');\n    }\n\n    document.getElementById('run').addEventListener('click', runKruskal);\n    drawGraph();\n</script>\n\n</body>\n</html>"
