    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Binary Search Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 2rem;\n            text-align: center;\n        }\n        #array-container {\n            margin: 1rem 0;\n        }\n        .array-item {\n            display: inline-block;\n            width: 30px;\n            height: 30px;\n            line-height: 30px;\n            margin: 2px;\n            border: 1px solid #333;\n            border-radius: 5px;\n        }\n        .highlight {\n            background-color: yellow;\n        }\n    </style>\n</head>\n<body>\n    <h1>Binary Search Visualization</h1>\n    <input type=\"text\" id=\"arrayInput\" placeholder=\"Enter sorted array (comma separated)\" />\n    <input type=\"number\" id=\"targetInput\" placeholder=\"Enter target number\" />\n    <button id=\"searchButton\">Search</button>\n    <div id=\"array-container\"></div>\n    <div id=\"result\"></div>\n\n    <script>\n        document.getElementById('searchButton').addEventListener('click', function() {\n            const arrayInput = document.getElementById('arrayInput').value.split(',').map(Number);\n            const target = Number(document.getElementById('targetInput').value);\n            const resultContainer = document.getElementById('result');\n            const arrayContainer = document.getElementById('array-container');\n\n            // Clear previous result\n            resultContainer.textContent = '';\n            arrayContainer.innerHTML = '';\n\n            // Display the array\n            arrayInput.forEach(num => {\n                const div = document.createElement('div');\n                div.textContent = num;\n                div.className = 'array-item';\n                arrayContainer.appendChild(div);\n            });\n\n            // Execute binary search\n            const index = binarySearch(arrayInput, target);\n\n            // Display result\n            if (index !== -1) {\n                resultContainer.textContent = `Element found at index: ${index}`;\n            } else {\n                resultContainer.textContent = 'Element not found';\n            }\n        });\n\n        function binarySearch(array, target) {\n            let left = 0;\n            let right = array.length - 1;\n\n            while (left <= right) {\n                const mid = Math.floor((left + right) / 2);\n                \n                // Highlight the middle element\n                highlightElement(mid);\n\n                if (array[mid] === target) {\n                    return mid;\n                }\n\n                if (array[mid] < target) {\n                    left = mid + 1;\n                } else {\n                    right = mid - 1;\n                }\n            }\n\n            return -1;\n        }\n\n        function highlightElement(index) {\n            const arrayItems = document.querySelectorAll('.array-item');\n            arrayItems.forEach((item, i) => {\n                item.classList.remove('highlight');\n                if (i === index) {\n                    item.classList.add('highlight');\n                }\n            });\n        }\n    </script>\n</body>\n</html>"
