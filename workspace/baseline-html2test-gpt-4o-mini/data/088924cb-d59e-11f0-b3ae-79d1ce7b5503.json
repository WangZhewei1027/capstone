    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>K-Means Clustering Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            text-align: center;\n            margin: 20px;\n        }\n        #canvas {\n            border: 1px solid black;\n            margin-top: 20px;\n        }\n    </style>\n</head>\n<body>\n    <h1>K-Means Clustering Visualization</h1>\n    <button onclick=\"runKMeans()\">Run K-Means</button>\n    <canvas id=\"canvas\" width=\"600\" height=\"400\"></canvas>\n\n    <script>\n        const canvas = document.getElementById('canvas');\n        const ctx = canvas.getContext('2d');\n\n        const colors = ['#FF5733', '#33FF57', '#3357FF', '#F1C40F', '#8E44AD']; // Colors for clusters\n        const dataPoints = [];\n        const k = 3; // Number of clusters\n\n        // Generate random data points\n        function generateDataPoints(numPoints) {\n            for (let i = 0; i < numPoints; i++) {\n                dataPoints.push({\n                    x: Math.random() * canvas.width,\n                    y: Math.random() * canvas.height,\n                    cluster: null\n                });\n            }\n        }\n\n        // Draw data points\n        function drawPoints() {\n            ctx.clearRect(0, 0, canvas.width, canvas.height);\n            dataPoints.forEach(point => {\n                ctx.fillStyle = point.cluster !== null ? colors[point.cluster] : '#000';\n                ctx.beginPath();\n                ctx.arc(point.x, point.y, 5, 0, Math.PI * 2);\n                ctx.fill();\n            });\n        }\n\n        // Calculate distance between two points\n        function distance(point1, point2) {\n            return Math.sqrt(Math.pow(point1.x - point2.x, 2) + Math.pow(point1.y - point2.y, 2));\n        }\n\n        // K-Means algorithm\n        function kMeans() {\n            let centroids = [];\n            // Initialize centroids\n            for (let i = 0; i < k; i++) {\n                centroids.push({\n                    x: Math.random() * canvas.width,\n                    y: Math.random() * canvas.height\n                });\n            }\n\n            let clustersChanged = true;\n\n            while (clustersChanged) {\n                // Assign clusters\n                clustersChanged = false;\n\n                dataPoints.forEach(point => {\n                    let distances = centroids.map(centroid => distance(point, centroid));\n                    let closestCluster = distances.indexOf(Math.min(...distances));\n\n                    if (point.cluster !== closestCluster) {\n                        clustersChanged = true;\n                        point.cluster = closestCluster;\n                    }\n                });\n\n                // Update centroids\n                centroids.forEach((centroid, index) => {\n                    let assignedPoints = dataPoints.filter(point => point.cluster === index);\n                    if (assignedPoints.length > 0) {\n                        centroid.x = assignedPoints.reduce((sum, point) => sum + point.x, 0) / assignedPoints.length;\n                        centroid.y = assignedPoints.reduce((sum, point) => sum + point.y, 0) / assignedPoints.length;\n                    }\n                });\n            }\n\n            drawPoints();\n        }\n\n        // Run the K-Means algorithm\n        function runKMeans() {\n            generateDataPoints(100);\n            kMeans();\n        }\n    </script>\n</body>\n</html>"
