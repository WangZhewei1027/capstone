    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Priority Queue Demo</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n        }\n        #queueDisplay {\n            margin-top: 20px;\n            border: 1px solid #ccc;\n            padding: 10px;\n            min-height: 100px;\n        }\n        .item {\n            margin: 5px 0;\n            padding: 5px;\n            border: 1px solid #007bff;\n            border-radius: 5px;\n            display: flex;\n            justify-content: space-between;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Priority Queue Demo</h1>\n<input type=\"text\" id=\"valueInput\" placeholder=\"Enter value\" />\n<input type=\"number\" id=\"priorityInput\" placeholder=\"Enter priority (lower means higher priority)\" />\n<button id=\"enqueueBtn\">Enqueue</button>\n<button id=\"dequeueBtn\">Dequeue</button>\n\n<div id=\"queueDisplay\"></div>\n\n<script>\n    class PriorityQueue {\n        constructor() {\n            this.items = [];\n        }\n\n        enqueue(value, priority) {\n            const newItem = { value, priority };\n            this.items.push(newItem);\n            this.items.sort((a, b) => a.priority - b.priority);\n            this.display();\n        }\n\n        dequeue() {\n            if (this.isEmpty()) {\n                alert(\"Queue is empty!\");\n                return;\n            }\n            const removedItem = this.items.shift();\n            this.display();\n            return removedItem;\n        }\n\n        isEmpty() {\n            return this.items.length === 0;\n        }\n\n        display() {\n            const queueDisplay = document.getElementById('queueDisplay');\n            queueDisplay.innerHTML = '';\n            this.items.forEach(item => {\n                const itemDiv = document.createElement('div');\n                itemDiv.className = 'item';\n                itemDiv.innerText = `Value: ${item.value}, Priority: ${item.priority}`;\n                queueDisplay.appendChild(itemDiv);\n            });\n        }\n    }\n\n    const queue = new PriorityQueue();\n\n    document.getElementById('enqueueBtn').addEventListener('click', () => {\n        const value = document.getElementById('valueInput').value;\n        const priority = parseInt(document.getElementById('priorityInput').value);\n        if (value && !isNaN(priority)) {\n            queue.enqueue(value, priority);\n            document.getElementById('valueInput').value = '';\n            document.getElementById('priorityInput').value = '';\n        } else {\n            alert(\"Please enter a valid value and priority.\");\n        }\n    });\n\n    document.getElementById('dequeueBtn').addEventListener('click', () => {\n        const removedItem = queue.dequeue();\n        if (removedItem) {\n            alert(`Dequeued: Value: ${removedItem.value}, Priority: ${removedItem.priority}`);\n        }\n    });\n</script>\n\n</body>\n</html>"
