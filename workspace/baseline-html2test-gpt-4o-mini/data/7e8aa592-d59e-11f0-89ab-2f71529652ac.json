    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Heap Demonstration</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n        }\n        #heapDisplay {\n            margin: 20px 0;\n            border: 1px solid #ccc;\n            padding: 10px;\n            width: 300px;\n            height: 200px;\n            overflow-y: auto;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Heap (Min/Max) Visualization</h1>\n<label for=\"numberInput\">Enter a number: </label>\n<input type=\"number\" id=\"numberInput\">\n<button onclick=\"insertNode()\">Insert</button>\n<button onclick=\"removeNode()\">Remove Min</button>\n<button onclick=\"removeMaxNode()\">Remove Max</button>\n\n<div id=\"heapDisplay\"></div>\n\n<script>\n    class MinHeap {\n        constructor() {\n            this.heap = [];\n        }\n\n        insert(val) {\n            this.heap.push(val);\n            this.bubbleUp();\n        }\n\n        bubbleUp() {\n            let index = this.heap.length - 1;\n            while (index > 0) {\n                let parentIndex = Math.floor((index - 1) / 2);\n                if (this.heap[index] >= this.heap[parentIndex]) break;\n                [this.heap[index], this.heap[parentIndex]] = [this.heap[parentIndex], this.heap[index]];\n                index = parentIndex;\n            }\n        }\n\n        removeMin() {\n            if (this.heap.length <= 1) return this.heap.pop();\n            const min = this.heap[0];\n            this.heap[0] = this.heap.pop();\n            this.bubbleDown();\n            return min;\n        }\n\n        bubbleDown() {\n            let index = 0;\n            const length = this.heap.length;\n            while (true) {\n                let leftChildIdx = 2 * index + 1;\n                let rightChildIdx = 2 * index + 2;\n                let leftChild, rightChild;\n                let swap = null;\n\n                if (leftChildIdx < length) {\n                    leftChild = this.heap[leftChildIdx];\n                    if (leftChild < this.heap[index]) {\n                        swap = leftChildIdx;\n                    }\n                }\n                if (rightChildIdx < length) {\n                    rightChild = this.heap[rightChildIdx];\n                    if ((swap === null && rightChild < this.heap[index]) || \n                        (swap !== null && rightChild < leftChild)) {\n                        swap = rightChildIdx;\n                    }\n                }\n                if (swap === null) break;\n                [this.heap[index], this.heap[swap]] = [this.heap[swap], this.heap[index]];\n                index = swap;\n            }\n        }\n\n        getHeap() {\n            return this.heap.slice(); \n        }\n\n        removeMax() {\n            return Math.max(...this.getHeap());\n        }\n    }\n\n    const minHeap = new MinHeap();\n    const heapDisplay = document.getElementById('heapDisplay');\n\n    function insertNode() {\n        const num = parseInt(document.getElementById('numberInput').value);\n        if (!isNaN(num)) {\n            minHeap.insert(num);\n            updateDisplay();\n        }\n    }\n\n    function removeNode() {\n        if (minHeap.getHeap().length > 0) {\n            minHeap.removeMin();\n            updateDisplay();\n        }\n    }\n\n    function removeMaxNode() {\n        const maxValue = minHeap.removeMax();\n        alert('Max Value Removed: ' + maxValue);\n        updateDisplay();\n    }\n\n    function updateDisplay() {\n        heapDisplay.innerHTML = \"Current Min Heap: \" + minHeap.getHeap().join(\", \");\n    }\n</script>\n\n</body>\n</html>"
