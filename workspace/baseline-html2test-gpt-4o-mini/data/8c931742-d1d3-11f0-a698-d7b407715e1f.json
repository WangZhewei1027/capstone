    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Weighted Graph Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            margin: 0;\n            padding: 20px;\n        }\n        canvas {\n            border: 1px solid black;\n        }\n    </style>\n</head>\n<body>\n    <h1>Weighted Graph Visualization</h1>\n    <canvas id=\"graphCanvas\" width=\"600\" height=\"400\"></canvas>\n    <script>\n        const canvas = document.getElementById('graphCanvas');\n        const ctx = canvas.getContext('2d');\n\n        // Graph data structure\n        const graph = {\n            A: { B: 1, C: 3 },\n            B: { A: 1, C: 1, D: 5 },\n            C: { A: 3, B: 1, D: 2 },\n            D: { B: 5, C: 2 }\n        };\n\n        const vertices = Object.keys(graph);\n        const vertexPositions = {\n            A: { x: 100, y: 200 },\n            B: { x: 250, y: 100 },\n            C: { x: 250, y: 300 },\n            D: { x: 400, y: 200 }\n        };\n\n        function drawGraph() {\n            ctx.clearRect(0, 0, canvas.width, canvas.height);\n\n            // Draw edges\n            for (let vertex of vertices) {\n                const edges = graph[vertex];\n                for (let adjacentVertex in edges) {\n                    drawEdge(vertex, adjacentVertex, edges[adjacentVertex]);\n                }\n            }\n\n            // Draw vertices\n            for (let vertex of vertices) {\n                drawVertex(vertex);\n            }\n        }\n\n        function drawVertex(vertex) {\n            const pos = vertexPositions[vertex];\n            ctx.fillStyle = 'blue';\n            ctx.beginPath();\n            ctx.arc(pos.x, pos.y, 20, 0, Math.PI * 2);\n            ctx.fill();\n            ctx.fillStyle = 'white';\n            ctx.font = '15px Arial';\n            ctx.fillText(vertex, pos.x - 5, pos.y + 5);\n        }\n\n        function drawEdge(vertex1, vertex2, weight) {\n            const pos1 = vertexPositions[vertex1];\n            const pos2 = vertexPositions[vertex2];\n\n            ctx.beginPath();\n            ctx.moveTo(pos1.x, pos1.y);\n            ctx.lineTo(pos2.x, pos2.y);\n            ctx.strokeStyle = 'black';\n            ctx.stroke();\n\n            // Midpoint for the weight label\n            const midX = (pos1.x + pos2.x) / 2;\n            const midY = (pos1.y + pos2.y) / 2;\n\n            ctx.fillStyle = 'black';\n            ctx.fillText(weight, midX, midY - 5);\n        }\n\n        drawGraph();\n    </script>\n</body>\n</html>"
