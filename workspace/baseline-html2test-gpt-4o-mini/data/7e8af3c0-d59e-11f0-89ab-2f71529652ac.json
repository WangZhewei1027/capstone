    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>K-Means Clustering Demo</title>\n    <style>\n        body { font-family: Arial, sans-serif; text-align: center; }\n        #canvas { border: 1px solid black; margin-top: 20px; }\n        button { margin-top: 20px; }\n    </style>\n</head>\n<body>\n    <h1>K-Means Clustering Visualization</h1>\n    <canvas id=\"canvas\" width=\"600\" height=\"400\"></canvas>\n    <button id=\"runKMeans\">Run K-Means</button>\n    \n    <script>\n        const canvas = document.getElementById('canvas');\n        const ctx = canvas.getContext('2d');\n\n        const colors = ['#FF5733', '#33FF57', '#3357FF', '#FFFF33', '#FF33FF'];\n        const points = [];\n        const centroids = [];\n        const k = 3;\n\n        function initializePoints() {\n            for (let i = 0; i < 100; i++) {\n                points.push({\n                    x: Math.random() * canvas.width,\n                    y: Math.random() * canvas.height\n                });\n            }\n        }\n\n        function initializeCentroids() {\n            for (let i = 0; i < k; i++) {\n                centroids.push({\n                    x: Math.random() * canvas.width,\n                    y: Math.random() * canvas.height\n                });\n            }\n        }\n\n        function drawPoints() {\n            ctx.clearRect(0, 0, canvas.width, canvas.height);\n            points.forEach(point => {\n                ctx.fillStyle = 'black';\n                ctx.fillRect(point.x, point.y, 5, 5);\n            });\n            centroids.forEach((centroid, index) => {\n                ctx.fillStyle = colors[index % colors.length];\n                ctx.beginPath();\n                ctx.arc(centroid.x, centroid.y, 10, 0, Math.PI * 2);\n                ctx.fill();\n            });\n        }\n\n        function assignPointsToCentroids() {\n            const clusters = Array.from({ length: k }, () => []);\n          \n            points.forEach(point => {\n                let closestIndex = null;\n                let closestDistance = Infinity;\n\n                centroids.forEach((centroid, index) => {\n                    const distance = Math.sqrt(Math.pow(point.x - centroid.x, 2) + Math.pow(point.y - centroid.y, 2));\n                    if (distance < closestDistance) {\n                        closestDistance = distance;\n                        closestIndex = index;\n                    }\n                });\n\n                clusters[closestIndex].push(point);\n            });\n\n            return clusters;\n        }\n\n        function updateCentroids(clusters) {\n            clusters.forEach((cluster, index) => {\n                if (cluster.length > 0) {\n                    const avgX = cluster.reduce((sum, p) => sum + p.x, 0) / cluster.length;\n                    const avgY = cluster.reduce((sum, p) => sum + p.y, 0) / cluster.length;\n                    centroids[index] = { x: avgX, y: avgY };\n                }\n            });\n        }\n\n        function kMeans() {\n            let prevCentroids;\n            do {\n                prevCentroids = centroids.map(c => ({ ...c }));\n                const clusters = assignPointsToCentroids();\n                updateCentroids(clusters);\n            } while (!centroids.every((centroid, index) => centroid.x === prevCentroids[index].x && centroid.y === prevCentroids[index].y));\n            drawPoints();\n        }\n\n        document.getElementById('runKMeans').onclick = () => {\n            initializePoints();\n            initializeCentroids();\n            drawPoints();\n            kMeans();\n        };\n\n        initializePoints(); // Initial points\n    </script>\n</body>\n</html>"
  },
