        insertNode(node, newNode) {\n            if (newNode.value < node.value) {\n                if (!node.left) {\n                    node.left = newNode;\n                } else {\n                    this.insertNode(node.left, newNode);\n                }\n            } else {\n                if (!node.right) {\n                    node.right = newNode;\n                } else {\n                    this.insertNode(node.right, newNode);\n                }\n            }\n        }\n\n        visualize() {\n            const bstDiv = document.getElementById(\"bst\");\n            bstDiv.innerHTML = ''; // Clear previous visualization\n            this.visualizeNode(this.root, bstDiv);\n        }\n\n        visualizeNode(node, parentDiv) {\n            if (!node) return;\n\n            const nodeDiv = document.createElement(\"div\");\n            nodeDiv.className = \"node\";\n            nodeDiv.innerText = node.value;\n\n            parentDiv.appendChild(nodeDiv);\n\n            if (node.left || node.right) {\n                const line = document.createElement(\"div\");\n                line.className = \"line\";\n                nodeDiv.appendChild(line);\n            }\n\n            if (node.left) {\n                const leftDiv = document.createElement(\"div\");\n                leftDiv.style.display = \"flex\";\n                leftDiv.style.justifyContent = \"center\";\n                leftDiv.style.alignItems = \"center\";\n                leftDiv.appendChild(this.visualizeNode(node.left, parentDiv));\n                parentDiv.appendChild(leftDiv);\n            }\n\n            if (node.right) {\n                const rightDiv = document.createElement(\"div\");\n                rightDiv.style.display = \"flex\";\n                rightDiv.style.justifyContent = \"center\";\n                rightDiv.style.alignItems = \"center\";\n                rightDiv.appendChild(this.visualizeNode(node.right, parentDiv));\n                parentDiv.appendChild(rightDiv);\n            }\n        }\n    }\n\n    const bst = new BinarySearchTree();\n\n    function insert() {\n        const value = parseInt(document.getElementById(\"valueInput\").value);\n        if (!isNaN(value)) {\n            bst.insert(value);\n            bst.visualize();\n            document.getElementById(\"valueInput\").value = ''; // Clear input\n        } else {\n            alert(\"Please enter a valid number\");\n        }\n    }\n</script>\n\n</body>\n</html>"
