    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Linear Regression Demo</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            text-align: center;\n            margin: 0;\n            padding: 20px;\n            background-color: #f4f4f9;\n        }\n        canvas {\n            border: 1px solid #000;\n            margin-top: 20px;\n        }\n        #inputs {\n            margin: 20px 0;\n        }\n    </style>\n</head>\n<body>\n    <h1>Linear Regression Demo</h1>\n    <div id=\"inputs\">\n        <label for=\"xValue\">Enter X value: </label>\n        <input type=\"number\" id=\"xValue\" placeholder=\"e.g., 5\">\n        <button onclick=\"addPoint()\">Add Point</button>\n    </div>\n    <canvas id=\"linearRegressionCanvas\" width=\"600\" height=\"400\"></canvas>\n    <h3 id=\"equation\"></h3>\n\n    <script>\n        const canvas = document.getElementById('linearRegressionCanvas');\n        const ctx = canvas.getContext('2d');\n        const points = [];\n        \n        function drawPoints() {\n            ctx.clearRect(0, 0, canvas.width, canvas.height);\n            ctx.fillStyle = 'lightblue';\n            points.forEach(point => {\n                ctx.beginPath();\n                ctx.arc(point.x, point.y, 5, 0, Math.PI * 2);\n                ctx.fill();\n            });\n        }\n\n        function drawLine(slope, intercept) {\n            ctx.beginPath();\n            ctx.moveTo(0, canvas.height - intercept);\n            ctx.lineTo(canvas.width, canvas.height - (slope * canvas.width + intercept));\n            ctx.strokeStyle = 'red';\n            ctx.lineWidth = 2;\n            ctx.stroke();\n        }\n\n        function calculateRegression() {\n            const n = points.length;\n            if (n < 2) return { slope: 0, intercept: 0 };\n\n            const sumX = points.reduce((sum, p) => sum + p.x, 0);\n            const sumY = points.reduce((sum, p) => sum + p.y, 0);\n            const sumXY = points.reduce((sum, p) => sum + p.x * p.y, 0);\n            const sumX2 = points.reduce((sum, p) => sum + p.x * p.x, 0);\n\n            const slope = (n * sumXY - sumX * sumY) / (n * sumX2 - sumX * sumX);\n            const intercept = (sumY - slope * sumX) / n;\n\n            return { slope, intercept };\n        }\n\n        function addPoint() {\n            const xValue = Number(document.getElementById('xValue').value);\n            const yValue = Math.random() * canvas.height; // Random Y value for demo\n\n            points.push({ x: xValue, y: canvas.height - yValue });\n            drawPoints();\n            const { slope, intercept } = calculateRegression();\n            drawLine(slope, intercept);\n            document.getElementById('equation').textContent = `y = ${slope.toFixed(2)}x + ${intercept.toFixed(2)}`;\n        }\n    </script>\n</body>\n</html>"
