    "content": "<html lang=\"en\">\n<head>\n<meta charset=\"UTF-8\" />\n<meta name=\"viewport\" content=\"width=device-width, initial-scale=1\" />\n<title>Queue Demonstration</title>\n<style>\n  body {\n    font-family: Arial, sans-serif;\n    margin: 2rem;\n    background: #f0f4f8;\n    color: #333;\n  }\n  h1 {\n    text-align: center;\n  }\n  #queueContainer {\n    margin-top: 1rem;\n    display: flex;\n    gap: 0.5rem;\n    flex-wrap: wrap;\n    justify-content: center;\n  }\n  .queue-item {\n    background-color: #4a90e2;\n    color: white;\n    min-width: 40px;\n    min-height: 40px;\n    line-height: 40px;\n    text-align: center;\n    border-radius: 5px;\n    font-weight: bold;\n    user-select: none;\n  }\n  #controls {\n    display: flex;\n    justify-content: center;\n    gap: 1rem;\n    margin-top: 1rem;\n    flex-wrap: wrap;\n  }\n  #controls > * {\n    font-size: 1rem;\n  }\n  input[type=\"text\"] {\n    padding: 0.4rem 0.6rem;\n    font-size: 1rem;\n    border-radius: 4px;\n    border: 1.5px solid #ccc;\n    width: 120px;\n  }\n  button {\n    padding: 0.5rem 1.2rem;\n    border: none;\n    background-color: #4a90e2;\n    color: white;\n    border-radius: 5px;\n    cursor: pointer;\n    font-weight: 600;\n    transition: background-color 0.3s;\n  }\n  button:hover:enabled {\n    background-color: #357ABD;\n  }\n  button:disabled {\n    background-color: #a3c0f5;\n    cursor: not-allowed;\n  }\n  #log {\n    margin-top: 2rem;\n    max-height: 150px;\n    overflow-y: auto;\n    background-color: #ffffff;\n    border: 1px solid #ddd;\n    padding: 1rem;\n    border-radius: 5px;\n    font-family: monospace;\n    font-size: 0.9rem;\n  }\n  #log p {\n    margin: 0.2rem 0;\n  }\n</style>\n</head>\n<body>\n<h1>Queue Data Structure Demonstration</h1>\n\n<div id=\"controls\">\n  <input type=\"text\" id=\"enqueueInput\" placeholder=\"Enter value\" maxlength=\"10\" />\n  <button id=\"enqueueBtn\">Enqueue</button>\n  <button id=\"dequeueBtn\" disabled>Dequeue</button>\n  <button id=\"peekBtn\" disabled>Peek</button>\n  <button id=\"clearBtn\" disabled>Clear</button>\n</div>\n\n<div id=\"queueContainer\" aria-label=\"Queue visualization\" role=\"list\" tabindex=\"0\">\n  <!-- Queue items will appear here -->\n</div>\n\n<div id=\"log\" aria-live=\"polite\" aria-atomic=\"true\">\n  <!-- Logs of operations -->\n</div>\n\n<script>\n  (function () {\n    const enqueueInput = document.getElementById('enqueueInput');\n    const enqueueBtn = document.getElementById('enqueueBtn');\n    const dequeueBtn = document.getElementById('dequeueBtn');\n    const peekBtn = document.getElementById('peekBtn');\n    const clearBtn = document.getElementById('clearBtn');\n    const queueContainer = document.getElementById('queueContainer');\n    const logDiv = document.getElementById('log');\n\n    // Internal queue implemented with an array\n    const queue = [];\n\n    // Helper: Log messages to the log container\n    function log(message) {\n      const p = document.createElement('p');\n      p.textContent = message;\n      logDiv.appendChild(p);\n      logDiv.scrollTop = logDiv.scrollHeight;\n    }\n\n    // Helper: Update buttons state based on queue contents\n    function updateButtons() {\n      const isEmpty = queue.length === 0;\n      dequeueBtn.disabled = isEmpty;\n      peekBtn.disabled = isEmpty;\n      clearBtn.disabled = isEmpty;\n      enqueueBtn.disabled = enqueueInput.value.trim() === '';\n    }\n\n    // Helper: Render the queue visually\n    function renderQueue() {\n      queueContainer.innerHTML = '';\n      queue.forEach((item, index) => {\n        const div = document.createElement('div');\n        div.className = 'queue-item';\n        div.textContent = item;\n        div.setAttribute('role', 'listitem');\n        if(index === 0){\n          div.title = 'Front';\n          div.style.border = '3px solid #ff8c00';\n        }\n        if(index === queue.length - 1){\n          div.title = (div.title ? div.title + ' & ' : '') + 'Rear';\n          if(index !== 0){\n            div.style.border = '3px solid #2ecc71';\n          }\n        }\n        queueContainer.appendChild(div);\n      });\n      if(queue.length === 0){\n        const emptyMsg = document.createElement('p');\n        emptyMsg.textContent = 'Queue is empty';\n        emptyMsg.style.fontStyle = 'italic';\n        emptyMsg.style.color = '#666';\n        queueContainer.appendChild(emptyMsg);\n      }\n    }\n\n    // Event Handlers\n    enqueueBtn.addEventListener('click', () => {\n      const value = enqueueInput.value.trim();\n      if(value === '') return;\n      queue.push(value);\n      log(`Enqueued: \"${value}\"`);\n      enqueueInput.value = '';\n      enqueueInput.focus();\n      renderQueue();\n      updateButtons();\n    });\n\n    dequeueBtn.addEventListener('click', () => {\n      if(queue.length === 0) return;\n      const value = queue.shift();\n      log(`Dequeued: \"${value}\"`);\n      renderQueue();\n      updateButtons();\n    });\n\n    peekBtn.addEventListener('click', () => {\n      if(queue.length === 0) return;\n      const value = queue[0];\n      log(`Peek: front value is \"${value}\"`);\n      renderQueue();\n      updateButtons();\n    });\n\n    clearBtn.addEventListener('click', () => {\n      if(queue.length === 0) return;\n      queue.length = 0;\n      log('Cleared the queue');\n      renderQueue();\n      updateButtons();\n      enqueueInput.focus();\n    });\n\n    enqueueInput.addEventListener('input', updateButtons);\n    enqueueInput.addEventListener('keydown', (e) => {\n      if(e.key === 'Enter' && !enqueueBtn.disabled){\n        enqueueBtn.click();\n      }\n    });\n\n    // Initial render\n    renderQueue();\n    updateButtons();\n  })();\n</script>\n\n</body>\n</html>"
