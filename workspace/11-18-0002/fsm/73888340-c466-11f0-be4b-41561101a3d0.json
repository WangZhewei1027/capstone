{
  "topic": "Interactive Application",
  "description": "Finite State Machine for an interactive queue exploration application.",
  "states": [
    {
      "name": "idle",
      "onEnter": "noop",
      "onExit": "noop",
      "on": {
        "ENQUEUE": "adding",
        "DEQUEUE": "removing"
      }
    },
    {
      "name": "adding",
      "onEnter": "updateQueueDisplay",
      "onExit": "noop",
      "on": {
        "ITEM_ADDED": "idle"
      }
    },
    {
      "name": "removing",
      "onEnter": "updateQueueDisplay",
      "onExit": "noop",
      "on": {
        "ITEM_REMOVED": "idle"
      }
    }
  ],
  "events": [
    "ENQUEUE",
    "DEQUEUE",
    "ITEM_ADDED",
    "ITEM_REMOVED"
  ],
  "notes": "The application allows users to enqueue and dequeue items from a queue, updating the display accordingly."
}