e].includes(connectedNode)) {\n                            adjacencyList[node].push(connectedNode);\n                        }\n                        // Ensure the connected node exists in the adjacency list\n                        if (!adjacencyList[connectedNode]) {\n                            adjacencyList[connectedNode] = [];\n                        }\n                        // Add the reverse connection for undirected graph representation\n                        if (!adjacencyList[connectedNode].includes(node)) {\n                            adjacencyList[connectedNode].push(node);\n                        }\n                    });\n                });\n\n                // Display the updated adjacency list\n                displayAdjacencyList();\n            }\n            document.getElementById('node').value = '';\n            document.getElementById('edges').value = '';\n        }\n\n        function displayAdjacencyList() {\n            document.getElementById('output').textContent = JSON.stringify(adjacencyList, null, 2);\n        }\n    </script>\n</body>\n</html>"
