    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>K-Nearest Neighbors Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            text-align: center;\n        }\n        canvas {\n            border: 1px solid black;\n        }\n        #controls {\n            margin-top: 20px;\n        }\n    </style>\n</head>\n<body>\n    <h1>K-Nearest Neighbors (KNN) Visualization</h1>\n    <canvas id=\"canvas\" width=\"500\" height=\"500\"></canvas>\n    <div id=\"controls\">\n        <label for=\"kValue\">Choose K: </label>\n        <input type=\"number\" id=\"kValue\" value=\"3\" min=\"1\" max=\"10\">\n        <button id=\"runKNN\">Run KNN</button>\n    </div>\n    \n    <script>\n        const canvas = document.getElementById('canvas');\n        const ctx = canvas.getContext('2d');\n\n        const dataPoints = [\n            { x: 100, y: 300, label: 'A' },\n            { x: 150, y: 200, label: 'A' },\n            { x: 200, y: 250, label: 'B' },\n            { x: 300, y: 150, label: 'B' },\n            { x: 250, y: 400, label: 'A' },\n            { x: 350, y: 350, label: 'B' }\n        ];\n\n        let userPoint = null;\n\n        const drawDataPoints = () => {\n            ctx.clearRect(0, 0, canvas.width, canvas.height);\n            dataPoints.forEach(point => {\n                ctx.fillStyle = point.label === 'A' ? 'blue' : 'red';\n                ctx.beginPath();\n                ctx.arc(point.x, point.y, 8, 0, Math.PI * 2);\n                ctx.fill();\n                ctx.closePath();\n            });\n\n            if (userPoint) {\n                ctx.fillStyle = 'green';\n                ctx.beginPath();\n                ctx.arc(userPoint.x, userPoint.y, 10, 0, Math.PI * 2);\n                ctx.fill();\n                ctx.closePath();\n\n                const neighbors = getNeighbors(userPoint.x, userPoint.y, Number(document.getElementById('kValue').value));\n                \n                neighbors.forEach(neighbor => {\n                    ctx.strokeStyle = 'yellow';\n                    ctx.beginPath();\n                    ctx.moveTo(userPoint.x, userPoint.y);\n                    ctx.lineTo(neighbor.x, neighbor.y);\n                    ctx.stroke();\n                    ctx.closePath();\n                });\n            }\n        };\n\n        const getNeighbors = (x, y, k) => {\n            return dataPoints.map(point => ({\n                point,\n                distance: Math.hypot(point.x - x, point.y - y)\n            }))\n            .sort((a, b) => a.distance - b.distance)\n            .slice(0, k)\n            .map(item => item.point);\n        };\n\n        canvas.addEventListener('click', (event) => {\n            const rect = canvas.getBoundingClientRect();\n            userPoint = {\n                x: event.clientX - rect.left,\n                y: event.clientY - rect.top\n            };\n            drawDataPoints();\n        });\n\n        document.getElementById('runKNN').addEventListener('click', drawDataPoints);\n\n        drawDataPoints();\n    </script>\n</body>\n</html>"
