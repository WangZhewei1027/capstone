    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Min/Max Heap Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n        }\n        #heapDisplay {\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            margin-bottom: 20px;\n        }\n        .node {\n            border: 1px solid #333;\n            border-radius: 5px;\n            padding: 10px;\n            margin: 5px;\n            width: 50px;\n            text-align: center;\n        }\n        #controls {\n            margin-bottom: 20px;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Min/Max Heap Visualization</h1>\n<div id=\"controls\">\n    <input type=\"number\" id=\"valueInput\" placeholder=\"Enter a value\" />\n    <select id=\"heapType\">\n        <option value=\"min\">Min Heap</option>\n        <option value=\"max\">Max Heap</option>\n    </select>\n    <button onclick=\"insertValue()\">Insert</button>\n</div>\n<div id=\"heapDisplay\"></div>\n\n<script>\n    class Heap {\n        constructor(type) {\n            this.type = type;\n            this.values = [];\n        }\n\n        insert(value) {\n            this.values.push(value);\n            this.bubbleUp();\n            this.display();\n        }\n\n        bubbleUp() {\n            let index = this.values.length - 1;\n            const parentIndex = this.getParentIndex(index);\n            while (index > 0 && this.compare(this.values[index], this.values[parentIndex])) {\n                [this.values[index], this.values[parentIndex]] = [this.values[parentIndex], this.values[index]];\n                index = parentIndex;\n            }\n        }\n\n        getParentIndex(index) {\n            return Math.floor((index - 1) / 2);\n        }\n\n        compare(a, b) {\n            return this.type === 'min' ? a < b : a > b;\n        }\n\n        display() {\n            const displayDiv = document.getElementById('heapDisplay');\n            displayDiv.innerHTML = '';\n            this.values.forEach(value => {\n                const node = document.createElement('div');\n                node.className = 'node';\n                node.textContent = value;\n                displayDiv.appendChild(node);\n            });\n        }\n    }\n\n    const heap = new Heap('min');\n\n    function insertValue() {\n        const valueInput = document.getElementById('valueInput');\n        const value = parseInt(valueInput.value);\n        if (!isNaN(value)) {\n            heap.insert(value);\n            valueInput.value = '';\n        } else {\n            alert('Please enter a valid number');\n        }\n    }\n\n    const heapTypeSelect = document.getElementById('heapType');\n    heapTypeSelect.addEventListener('change', function() {\n        const newType = this.value;\n        heap.type = newType;\n        heap.values = [];\n        heap.display();\n    });\n</script>\n\n</body>\n</html>"
