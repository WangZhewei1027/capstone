    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Binary Tree Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            text-align: center;\n        }\n        .tree-node {\n            display: inline-block;\n            padding: 10px;\n            border: 1px solid #000;\n            border-radius: 10px;\n            margin: 5px;\n            position: relative;\n        }\n        .connection {\n            position: absolute;\n            height: 50px;\n            width: 1px;\n            background: black;\n            left: 50%;\n            top: 100%;\n            transform: translateX(-50%);\n        }\n        .left-connection {\n            position: absolute;\n            top: 50px; /* Adjust as needed */\n            left: 0;\n        }\n        .right-connection {\n            position: absolute;\n            top: 50px; /* Adjust as needed */\n            right: 0;\n        }\n        .tree-container {\n            position: relative;\n            margin: auto;\n            width: fit-content;\n            padding-top: 20px;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Binary Tree Visualization</h1>\n<div id=\"treeContainer\" class=\"tree-container\"></div>\n\n<script>\n    class TreeNode {\n        constructor(value) {\n            this.value = value;\n            this.left = null;\n            this.right = null;\n        }\n    }\n\n    class BinaryTree {\n        constructor() {\n            this.root = null;\n        }\n\n        insert(value) {\n            const newNode = new TreeNode(value);\n            if (!this.root) {\n                this.root = newNode;\n            } else {\n                this._insertNode(this.root, newNode);\n            }\n        }\n\n        _insertNode(node, newNode) {\n            if (newNode.value < node.value) {\n                if (!node.left) {\n                    node.left = newNode;\n                } else {\n                    this._insertNode(node.left, newNode);\n                }\n            } else {\n                if (!node.right) {\n                    node.right = newNode;\n                } else {\n                    this._insertNode(node.right, newNode);\n                }\n            }\n        }\n\n        display(container) {\n            container.innerHTML = '';\n            this._displayNode(container, this.root, 0, 200);\n        }\n\n        _displayNode(container, node, level, offset) {\n            if (!node) return;\n\n            const nodeElement = document.createElement('div');\n            nodeElement.className = 'tree-node';\n            nodeElement.innerText = node.value;\n\n            const connectionLeft = document.createElement('div');\n            connectionLeft.className = 'connection left-connection';\n            const connectionRight = document.createElement('div');\n            connectionRight.className = 'connection right-connection';\n\n            // Place the node into the container\n            const positionLeft = offset - 25 + (level * 20);\n            const positionRight = offset + 25 + (level * 20);\n\n            if (node.left) {\n                connectionLeft.style.height = '50px'; // Length of the left connection\n                nodeElement.appendChild(connectionLeft);\n                container.appendChild(nodeElement);\n                connectionLeft.style.transform = `translateX(${-positionLeft}px)`;\n                this._displayNode(container, node.left, level + 1, positionLeft);\n            } else {\n                container.appendChild(nodeElement);\n            }\n\n            if (node.right) {\n                connectionRight.style.height = '50px'; // Length of the right connection\n                nodeElement.appendChild(connectionRight);\n                connectionRight.style.transform = `translateX(${positionRight}px)`;\n                this._displayNode(container, node.right, level + 1, positionRight);\n            }\n        }\n    }\n\n    const tree = new BinaryTree();\n    [7, 5, 10, 3, 6, 9, 12].forEach(num => tree.insert(num));\n\n    const treeContainer = document.getElementById('treeContainer');\n    tree.display(treeContainer);\n</script>\n\n</body>\n</html>"
