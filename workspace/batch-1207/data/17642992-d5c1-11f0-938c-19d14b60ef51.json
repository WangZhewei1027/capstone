    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Prim's Algorithm Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            text-align: center;\n            margin: 20px;\n        }\n        canvas {\n            border: 1px solid black;\n            margin-top: 20px;\n        }\n        button {\n            padding: 10px 20px;\n            margin: 10px;\n            cursor: pointer;\n        }\n        #result {\n            margin-top: 20px;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Prim's Algorithm Visualization</h1>\n<button onclick=\"runPrimsAlgorithm()\">Run Prim's Algorithm</button>\n<div id=\"result\"></div>\n<canvas id=\"graphCanvas\" width=\"800\" height=\"600\"></canvas>\n\n<script>\n    const graph = {\n        0: { 1: 2, 3: 6 },\n        1: { 0: 2, 2: 3, 3: 8, 4: 5 },\n        2: { 1: 3, 4: 7 },\n        3: { 0: 6, 1: 8, 4: 9 },\n        4: { 1: 5, 2: 7, 3: 9 }\n    };\n\n    const vertices = Object.keys(graph);\n    const edges = [];\n    for (const u in graph) {\n        for (const v in graph[u]) {\n            if (u < v) edges.push([u, v, graph[u][v]]);\n        }\n    }\n\n    let selectedEdges = [];\n\n    function runPrimsAlgorithm() {\n        const visited = {};\n        const pq = [];\n        const startVertex = 0;\n\n        visited[startVertex] = true;\n        for (const neighbor in graph[startVertex]) {\n            pq.push([startVertex, neighbor, graph[startVertex][neighbor]]);\n        }\n\n        while (pq.length > 0) {\n            pq.sort((a, b) => a[2] - b[2]);\n            const [u, v, weight] = pq.shift();\n\n            if (!visited[v]) {\n                visited[v] = true;\n                selectedEdges.push([u, v, weight]);\n\n                for (const neighbor in graph[v]) {\n                    if (!visited[neighbor]) {\n                        pq.push([v, neighbor, graph[v][neighbor]]);\n                    }\n                }\n            }\n        }\n\n        drawGraph();\n    }\n\n    function drawGraph() {\n        const canvas = document.getElementById('graphCanvas');\n        const ctx = canvas.getContext('2d');\n        ctx.clearRect(0, 0, canvas.width, canvas.height);\n\n        const radius = 20;\n        const positions = {};\n        const angleStep = (Math.PI * 2) / vertices.length;\n\n        vertices.forEach((vertex, index) => {\n            const angle = index * angleStep;\n            const x = canvas.width / 2 + Math.cos(angle) * 200;\n            const y = canvas.height / 2 + Math.sin(angle) * 200;\n            positions[vertex] = { x, y };\n\n            ctx.beginPath();\n            ctx.arc(x, y, radius, 0, Math.PI * 2);\n            ctx.fillStyle = 'lightblue';\n            ctx.fill();\n            ctx.stroke();\n            ctx.fillStyle = 'black';\n            ctx.fillText(vertex, x - 5, y + 5);\n        });\n\n        selectedEdges.forEach(([u, v, weight]) => {\n            ctx.beginPath();\n            ctx.moveTo(positions[u].x, positions[u].y);\n            ctx.lineTo(positions[v].x, positions[v].y);\n            ctx.strokeStyle = 'green';\n            ctx.lineWidth = 4;\n            ctx.stroke();\n\n            const midX = (positions[u].x + positions[v].x) / 2;\n            const midY = (positions[u].y + positions[v].y) / 2;\n            ctx.fillText(weight, midX, midY);\n        });\n\n        document.getElementById('result').innerHTML = \n            `Minimum Spanning Tree Edges: <br>${selectedEdges.map(edge => `${edge[0]} - ${edge[1]} (Weight: ${edge[2]})`).join('<br>')}`;\n    }\n</script>\n\n</body>\n</html>"
