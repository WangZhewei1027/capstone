padding-bottom: 5px;\n        }\n        \n        p {\n            line-height: 1.6;\n            margin-bottom: 15px;\n        }\n        \n        .steps {\n            margin: 20px 0;\n        }\n        \n        .step {\n            padding: 10px;\n            margin-bottom: 10px;\n            background: #e8f4fc;\n            border-left: 4px solid #3498db;\n            border-radius: 4px;\n        }\n        \n        .controls {\n            display: flex;\n            gap: 10px;\n            margin-bottom: 20px;\n            flex-wrap: wrap;\n        }\n        \n        button {\n            padding: 10px 15px;\n            background: #3498db;\n            color: white;\n            border: none;\n            border-radius: 5px;\n            cursor: pointer;\n            transition: background 0.3s;\n        }\n        \n        button:hover {\n            background: #2980b9;\n        }\n        \n        button:disabled {\n            background: #bdc3c7;\n            cursor: not-allowed;\n        }\n        \n        input[type=\"text\"] {\n            padding: 10px;\n            border: 2px solid #ddd;\n            border-radius: 5px;\n            flex-grow: 1;\n            min-width: 200px;\n        }\n        \n        .array-container {\n            display: flex;\n            flex-wrap: wrap;\n            gap: 10px;\n            margin-top: 20px;\n            min-height: 100px;\n            justify-content: center;\n            align-items: center;\n        }\n        \n        .array-element {\n            width: 50px;\n            height: 50px;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            background: #3498db;\n            color: white;\n            border-radius: 5px;\n            font-weight: bold;\n            transition: all 0.3s;\n            box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\n        }\n        \n        .array-element.divided {\n            background: #e74c3c;\n        }\n        \n        .array-element.conquered {\n            background: #2ecc71;\n        }\n        \n        .log {\n            margin-top: 20px;\n            background: #f8f9fa;\n            border-radius: 5px;\n            padding: 15px;\n            max-height: 200px;\n            overflow-y: auto;\n            border: 1px solid #e9ecef;\n        }\n        \n        .log-entry {\n            padding: 5px 0;\n            border-bottom: 1px solid #e9ecef;\n        }\n        \n        @media (max-width: 768px) {\n            .content {\n                flex-direction: column;\n            }\n            \n            .explanation {\n                margin-right: 0;\n            }\n        }\n    </style>\n</head>\n<body>\n    <div class=\"container\">\n        <header>\n            <h1>Divide and Conquer Algorithm</h1>\n            <p class=\"subtitle\">A Visual Demonstration of Breaking Problems into Smaller Parts</p>\n        </header>\n        \n        <div class=\"content\">\n            <div class=\"explanation\">\n                <h2>What is Divide and Conquer?</h2>\n                <p>Divide and Conquer is an algorithmic paradigm that works by recursively breaking down a problem into two or more sub-problems of the same or related type, until these become simple enough to be solved directly.</p>\n                \n                <div class=\"steps\">\n                    <div class=\"step\">\n                        <strong>Divide:</strong> Break the problem into smaller subproblems\n                    </div>\n                    <div class=\"step\">\n                        <strong>Conquer:</strong> Solve the subproblems recursively\n                    </div>\n                    <div class=\"step\">\n                        <strong>Combine:</strong> Combine the solutions to get the overall solution\n                    </div>\n                </div>\n                \n                <h2>Common Examples</h2>\n                <ul>\n                    <li>Merge Sort</li>\n                    <li>Quick Sort</li>\n                    <li>Binary Search</li>\n                    <li>Strassen's Matrix Multiplication</li>\n                    <li>Closest Pair of Points</li>\n                </ul>\n            </div>\n            \n            <div class=\"visualization\">\n                <h2>Merge Sort Visualization</h2>\n                <p>Enter numbers separated by commas to see how merge sort uses divide and conquer:</p>\n                \n                <div class=\"controls\">\n                    <input type=\"text\" id=\"arrayInput\" placeholder=\"e.g., 5, 2, 8, 1, 9, 3\" value=\"5, 2, 8, 1, 9, 3\">\n                    <button id=\"startBtn\">Start Sorting</button>\n                    <button id=\"resetBtn\">Reset</button>\n                    <button id=\"stepBtn\" disabled>Next Step</button>\n                </div>\n                \n                <div class=\"array-container\" id=\"arrayDisplay\">\n                    <!-- Array elements will be generated here -->\n                </div>\n                \n                <div class=\"log\" id=\"algorithmLog\">\n                    <div class=\"log-entry\">Enter numbers and click \"Start Sorting\" to begin.</div>\n                </div>\n            </div>\n        </div>\n    </div>\n\n    <script>\n        // DOM Elements\n        const arrayInput = document.getElementById('arrayInput');\n        const startBtn = document.getElementById('startBtn');\n        const resetBtn = document.getElementById('resetBtn');\n        const stepBtn = document.getElementById('stepBtn');\n        const arrayDisplay = document.getElementById('arrayDisplay');\n        const algorithmLog = document.getElementById('algorithmLog');\n        \n        // Algorithm state\n        let originalArray = [];\n        let steps = [];\n        let currentStep = 0;\n        let isRunning = false;\n        \n        // Initialize the visualization\n        function init() {\n            // Parse the input array\n            const input = arrayInput.value.trim();\n            originalArray = input.split(',').map(num => parseInt(num.trim())).filter(num => !isNaN(num));\n            \n            if (originalArray.length === 0) {\n                originalArray = [5, 2, 8, 1, 9, 3]; // Default array\n                arrayInput.value = originalArray.join(', ');\n            }\n            \n            // Generate the steps for merge sort\n            steps = generateMergeSortSteps([...originalArray]);\n            currentStep = 0;\n            \n            // Reset the display\n            arrayDisplay.innerHTML = '';\n            algorithmLog.innerHTML = '<div class=\"log-entry\">Ready to start merge sort visualization.</div>';\n            \n            // Render the initial array\n            renderArray(originalArray);\n            \n            // Enable/disable buttons\n            startBtn.disabled = false;\n            stepBtn.disabled = true;\n        }\n        \n        // Generate the steps for merge sort algorithm\n        function generateMergeSortSteps(arr) {\n            const steps = [];\n            \n            function mergeSort(arr, depth = 0, startIdx = 0) {\n                if (arr.length <= 1) {\n                    steps.push({\n                        type: 'base',\n                        array: [...arr],\n                        depth: depth,\n                        startIdx: startIdx,\n                        message: `Base case reached: [${arr}] is already sorted`\n                    });\n                    return arr;\n                }\n                \n                const mid = Math.floor(arr.length / 2);\n                const left = arr.slice(0, mid);\n                const right = arr.slice(mid);\n                \n                // Record the divide step\n                steps.push({\n                    type: 'divide',\n                    array: [...arr],\n                    left: [...left],\n                    right: [...right],\n                    depth: depth,\n                    startIdx: startIdx,\n                    message: `Dividing [${arr}] into left: [${left}] and right: [${right}]`\n                });\n                \n                // Recursively sort both halves\n                const sortedLeft = mergeSort(left, depth + 1, startIdx);\n                const sortedRight = mergeSort(right, depth + 1, startIdx + mid);\n                \n                // Merge the sorted halves\n                const merged = merge(sortedLeft, sortedRight);\n                \n                // Record the conquer step\n                steps.push({\n                    type: 'conquer',\n                    array: [...merged],\n                    depth: depth,\n                    startIdx: startIdx,\n                    message: `Merging [${sortedLeft}] and [${sortedRight}] into [${merged}]`\n                });\n                \n                return merged;\n            }\n            \n            function merge(left, right) {\n                let result = [];\n                let leftIndex = 0;\n                let rightIndex = 0;\n                \n                while (leftIndex < left.length && rightIndex < right.length) {\n                    if (left[leftIndex] < right[rightIndex]) {\n                        result.push(left[leftIndex]);\n                        leftIndex++;\n                    } else {\n                        result.push(right[rightIndex]);\n                        rightIndex++;\n                    }\n                }\n                \n                return result.concat(left.slice(leftIndex)).concat(right.slice(rightIndex));\n            }\n            \n            mergeSort(arr);\n            return steps;\n        }\n        \n        // Render the array with visual styling\n        function renderArray(arr, step = null) {\n            arrayDisplay.innerHTML = '';\n            \n            arr.forEach((num,