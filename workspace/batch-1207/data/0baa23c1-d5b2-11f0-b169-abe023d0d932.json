    "content": "<html>\n<head>\n    <title>Knapsack Problem</title>\n    <style>\n        table {\n            border-collapse: collapse;\n        }\n        th, td {\n            border: 1px solid #ddd;\n            padding: 8px;\n            text-align: left;\n        }\n    </style>\n</head>\n<body>\n    <h1>Knapsack Problem</h1>\n    <form id=\"knapsack-form\">\n        <label for=\"weight\">Weight:</label>\n        <input type=\"number\" id=\"weight\" name=\"weight\"><br><br>\n        <label for=\"value\">Value:</label>\n        <input type=\"number\" id=\"value\" name=\"value\"><br><br>\n        <label for=\"capacity\">Capacity:</label>\n        <input type=\"number\" id=\"capacity\" name=\"capacity\"><br><br>\n        <button id=\"submit\">Submit</button>\n    </form>\n    <div id=\"output\"></div>\n\n    <script>\n        const knapsackForm = document.getElementById('knapsack-form');\n        const outputDiv = document.getElementById('output');\n        const submitButton = document.getElementById('submit');\n        const weightInput = document.getElementById('weight');\n        const valueInput = document.getElementById('value');\n        const capacityInput = document.getElementById('capacity');\n\n        let output = '';\n\n        submitButton.addEventListener('click', () => {\n            const weight = parseInt(weightInput.value);\n            const value = parseInt(valueInput.value);\n            const capacity = parseInt(capacityInput.value);\n\n            if (weight <= capacity) {\n                output = `The optimal solution is:`;\n                output += ` Weight: 0, Value: ${value}`;\n                output += ` Capacity: ${capacity}`;\n                output += ` Total Value: ${value}`;\n                output += ` Total Weight: ${weight}`;\n                output += ` Remaining Capacity: ${capacity - weight}`;\n                output += `<br><br>`;\n                output += `The optimal solution is:`;\n                output += ` Weight: ${capacity}, Value: ${value * capacity / weight}`;\n                output += ` Capacity: ${capacity}`;\n                output += ` Total Value: ${value * capacity / weight}`;\n                output += ` Total Weight: ${capacity}`;\n                output += `<br><br>`;\n                output += `The optimal solution is:`;\n                output += ` Weight: 0, Value: ${value}`;\n                output += ` Capacity: ${capacity}`;\n                output += ` Total Value: ${value}`;\n                output += ` Total Weight: ${weight}`;\n                output += `<br><br>`;\n                output += `The optimal solution is:`;\n                output += ` Weight: ${capacity}, Value: ${value * capacity / weight}`;\n                output += ` Capacity: ${capacity}`;\n                output += ` Total Value: ${value * capacity / weight}`;\n                output += ` Total Weight: ${capacity}`;\n            } else {\n                output = 'Insufficient capacity to solve the problem.';\n            }\n\n            outputDiv.innerHTML = output;\n        });\n    </script>\n</body>\n</html>"
