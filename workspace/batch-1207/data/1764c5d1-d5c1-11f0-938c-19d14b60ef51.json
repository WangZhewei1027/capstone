    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Linear Regression Demo</title>\n    <script src=\"https://cdn.jsdelivr.net/npm/chart.js\"></script>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            background-color: #f4f4f4;\n            margin: 0;\n            padding: 20px;\n        }\n        canvas {\n            margin-top: 20px;\n        }\n        #inputData {\n            max-width: 600px;\n            margin: 20px 0;\n        }\n    </style>\n</head>\n<body>\n    <h1>Linear Regression Demo</h1>\n    <div>\n        <label for=\"dataInput\">Enter data points (x,y) separated by commas:</label><br>\n        <textarea id=\"dataInput\" rows=\"5\" cols=\"50\" placeholder=\"Example: 1,2, 2,3, 3,5, 4,7\"></textarea><br>\n        <button id=\"plotButton\">Plot Linear Regression</button>\n    </div>\n    <canvas id=\"myChart\" width=\"400\" height=\"200\"></canvas>\n\n    <script>\n        document.getElementById('plotButton').addEventListener('click', function() {\n            const input = document.getElementById('dataInput').value.trim();\n            const points = input.split(',').map(pair => pair.trim().split(',').map(Number)).filter(pair => pair.length === 2);\n            \n            if (points.length === 0) {\n                alert(\"Please enter valid data points.\");\n                return;\n            }\n\n            const xValues = points.map(p => p[0]);\n            const yValues = points.map(p => p[1]);\n            const { slope, intercept } = linearRegression(xValues, yValues);\n            const regressionLine = xValues.map(x => slope * x + intercept);\n\n            plotData(points, regressionLine);\n        });\n\n        function linearRegression(x, y) {\n            const n = x.length;\n            const sumX = x.reduce((a, b) => a + b, 0);\n            const sumY = y.reduce((a, b) => a + b, 0);\n            const sumXY = x.reduce((a, b, i) => a + b * y[i], 0);\n            const sumX2 = x.reduce((a, b) => a + b * b, 0);\n            \n            const slope = (n * sumXY - sumX * sumY) / (n * sumX2 - sumX * sumX);\n            const intercept = (sumY - slope * sumX) / n;\n\n            return { slope, intercept };\n        }\n\n        function plotData(points, regressionLine) {\n            const ctx = document.getElementById('myChart').getContext('2d');\n\n            const dataPoints = points.map(p => ({ x: p[0], y: p[1] }));\n            const regressionData = points.map((_, index) => ({ x: points[index][0], y: regressionLine[index] }));\n\n            const chart = new Chart(ctx, {\n                type: 'scatter',\n                data: {\n                    datasets: [{\n                        label: 'Data Points',\n                        data: dataPoints,\n                        backgroundColor: 'blue'\n                    }, {\n                        label: 'Regression Line',\n                        data: regressionData,\n                        borderColor: 'red',\n                        type: 'line',\n                        fill: false\n                    }]\n                },\n                options: {\n                    scales: {\n                        x: {\n                            title: {\n                                display: true,\n                                text: 'X values'\n                            }\n                        },\n                        y: {\n                            title: {\n                                display: true,\n                                text: 'Y values'\n                            }\n                        }\n                    }\n                }\n            });\n        }\n    </script>\n</body>\n</html>"
