    "content": "<html>\n<head>\n    <title>Binary Search Tree</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        \n        #tree {\n            border: 1px solid black;\n            border-collapse: collapse;\n            width: 50%;\n            margin: 20px auto;\n        }\n        \n        #tree tr, #tree td {\n            border: 1px solid black;\n            padding: 10px;\n            text-align: left;\n        }\n    </style>\n</head>\n<body>\n    <h1>Binary Search Tree (BST)</h1>\n    <form id=\"tree-form\">\n        <label for=\"root\">Root Node:</label>\n        <input type=\"text\" id=\"root\" name=\"root\" required>\n        <br><br>\n        <label for=\"left\">Left Child:</label>\n        <input type=\"text\" id=\"left\" name=\"left\" required>\n        <br><br>\n        <label for=\"right\">Right Child:</label>\n        <input type=\"text\" id=\"right\" name=\"right\" required>\n        <br><br>\n        <label for=\"leaf\">Leaf Node:</label>\n        <input type=\"text\" id=\"leaf\" name=\"leaf\" required>\n        <br><br>\n        <button type=\"submit\">Insert</button>\n    </form>\n    <div id=\"tree\"></div>\n\n    <script>\n        class Node {\n            constructor(value) {\n                this.value = value;\n                this.left = null;\n                this.right = null;\n            }\n        }\n\n        class BinarySearchTree {\n            constructor() {\n                this.root = null;\n            }\n\n            insert(value) {\n                if (this.root === null) {\n                    this.root = new Node(value);\n                } else {\n                    this._insert(this.root, value);\n                }\n            }\n\n            _insert(node, value) {\n                if (value < node.value) {\n                    if (node.left === null) {\n                        node.left = new Node(value);\n                    } else {\n                        this._insert(node.left, value);\n                    }\n                } else {\n                    if (node.right === null) {\n                        node.right = new Node(value);\n                    } else {\n                        this._insert(node.right, value);\n                    }\n                }\n            }\n\n            printTree() {\n                if (this.root !== null) {\n                    this._printTree(this.root);\n                }\n            }\n\n            _printTree(node) {\n                if (node !== null) {\n                    this._printTree(node.left);\n                    document.getElementById(\"tree\").innerHTML += `<tr><td>${node.value}</td><td>${node.left.value}</td><td>${node.right.value}</td></tr>`;\n                    this._printTree(node.right);\n                }\n            }\n        }\n\n        const bst = new BinarySearchTree();\n        document.getElementById(\"tree-form\").addEventListener(\"submit\", (e) => {\n            e.preventDefault();\n            const root = document.getElementById(\"root\").value;\n            const left = document.getElementById(\"left\").value;\n            const right = document.getElementById(\"right\").value;\n            const leaf = document.getElementById(\"leaf\").value;\n\n            bst.insert(root);\n            bst.insert(left);\n            bst.insert(right);\n            bst.insert(leaf);\n            bst.printTree();\n        });\n    </script>\n</body>\n</html>"
