    "content": "<html>\n<head>\n    <title>Array Example</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n    </style>\n</head>\n<body>\n    <h1>Array Example</h1>\n    <p id=\"array-length\"></p>\n    <p id=\"array-data\"></p>\n    <p id=\"array-values\"></p>\n\n    <script>\n        let array = [];\n        const lengthInput = document.getElementById('array-length');\n        const dataInput = document.getElementById('array-data');\n        const valuesInput = document.getElementById('array-values');\n\n        function addElement() {\n            const value = prompt('Enter a value:');\n            if (value !== null) {\n                array.push(value);\n                lengthInput.textContent = array.length;\n                dataInput.textContent = array.join(', ');\n                valuesInput.textContent = array.join(', ');\n            } else {\n                alert('Please enter a value.');\n            }\n        }\n\n        function deleteElement() {\n            if (array.length > 0) {\n                array.splice(array.length - 1, 1);\n                lengthInput.textContent = array.length;\n                dataInput.textContent = array.join(', ');\n                valuesInput.textContent = array.join(', ');\n            } else {\n                alert('Array is empty.');\n            }\n        }\n\n        function clearArray() {\n            array = [];\n            lengthInput.textContent = array.length;\n            dataInput.textContent = '';\n            valuesInput.textContent = '';\n        }\n\n        addElementButton.addEventListener('click', addElement);\n        deleteButton.addEventListener('click', deleteElement);\n        clearButton.addEventListener('click', clearArray);\n\n        function renderArray() {\n            const arrayValues = array.map((value, index) => `${index + 1}: ${value}`);\n            dataInput.value = arrayValues.join('\\n');\n        }\n\n        renderArray();\n    </script>\n</body>\n</html>"
