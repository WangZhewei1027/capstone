    "content": "<html>\n<head>\n    <title>Binary Search</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        #binary-search {\n            width: 50%;\n            margin: 40px auto;\n            text-align: center;\n        }\n    </style>\n</head>\n<body>\n    <h1>Binary Search</h1>\n    <input type=\"number\" id=\"search-term\" placeholder=\"Enter a number to search for\">\n    <button id=\"search-btn\">Search</button>\n    <div id=\"result\"></div>\n\n    <script>\n        const searchTerm = document.getElementById('search-term');\n        const searchBtn = document.getElementById('search-btn');\n        const resultDiv = document.getElementById('result');\n\n        searchBtn.addEventListener('click', search);\n\n        function search() {\n            const term = searchTerm.value.trim();\n            if (term === '') {\n                resultDiv.textContent = 'Please enter a number';\n                return;\n            }\n\n            const low = 1;\n            const high = 1000;\n\n            while (low <= high) {\n                const mid = Math.floor((low + high) / 2);\n\n                if (term < mid) {\n                    high = mid - 1;\n                } else if (term > mid) {\n                    low = mid + 1;\n                } else {\n                    resultDiv.textContent = `Number ${term} found at index ${mid}`;\n                    return;\n                }\n            }\n\n            resultDiv.textContent = `Number ${term} not found`;\n        }\n    </script>\n</body>\n</html>"
