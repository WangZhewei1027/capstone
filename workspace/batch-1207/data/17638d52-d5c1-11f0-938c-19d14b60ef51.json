    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Quick Sort Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            margin: 50px;\n        }\n        #arrayContainer {\n            display: flex;\n            align-items: flex-end;\n        }\n        .bar {\n            width: 30px;\n            background-color: teal;\n            margin: 0 2px;\n            text-align: center;\n            color: white;\n            display: flex;\n            align-items: flex-end;\n            justify-content: center;\n        }\n    </style>\n</head>\n<body>\n    <h1>Quick Sort Visualization</h1>\n    <div id=\"arrayContainer\"></div>\n    <button onclick=\"startQuickSort()\">Sort Random Array</button>\n\n    <script>\n        const arrayContainer = document.getElementById(\"arrayContainer\");\n        let array = generateRandomArray(10);\n\n        function generateRandomArray(size) {\n            return Array.from({ length: size }, () => Math.floor(Math.random() * 100));\n        }\n\n        function displayArray(array) {\n            arrayContainer.innerHTML = '';\n            array.forEach(value => {\n                const bar = document.createElement('div');\n                bar.classList.add('bar');\n                bar.style.height = `${value * 2}px`;\n                bar.textContent = value;\n                arrayContainer.appendChild(bar);\n            });\n        }\n\n        function partition(arr, low, high) {\n            const pivot = arr[high];\n            let i = low - 1;\n\n            for (let j = low; j <= high - 1; j++) {\n                if (arr[j] < pivot) {\n                    i++;\n                    [arr[i], arr[j]] = [arr[j], arr[i]];\n                    displayArray(arr);\n                    nextFrame();\n                }\n            }\n            [arr[i + 1], arr[high]] = [arr[high], arr[i + 1]];\n            displayArray(arr);\n            nextFrame();\n            return i + 1;\n        }\n\n        function quickSort(arr, low, high) {\n            if (low < high) {\n                const pi = partition(arr, low, high);\n                quickSort(arr, low, pi - 1);\n                quickSort(arr, pi + 1, high);\n            }\n        }\n\n        let frameTimeout;\n\n        function nextFrame() {\n            return new Promise(resolve => {\n                setTimeout(resolve, 500);  // Wait half a second between each frame\n            });\n        }\n\n        async function startQuickSort() {\n            array = generateRandomArray(10);\n            displayArray(array);\n            await quickSort(array, 0, array.length - 1);\n        }\n        \n        // Display initial array\n        displayArray(array);\n    </script>\n</body>\n</html>"
