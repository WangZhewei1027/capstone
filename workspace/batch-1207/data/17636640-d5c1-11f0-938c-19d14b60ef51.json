    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Priority Queue Demo</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n        }\n        #queue {\n            margin-top: 20px;\n        }\n        .queue-item {\n            padding: 5px;\n            border: 1px solid #ccc;\n            margin: 5px;\n            display: flex;\n            justify-content: space-between;\n        }\n    </style>\n</head>\n<body>\n\n    <h1>Priority Queue Demonstration</h1>\n    \n    <label for=\"priority\">Enter priority (higher number means higher priority):</label>\n    <input type=\"number\" id=\"priority\" min=\"1\" required>\n    <label for=\"value\">Enter value:</label>\n    <input type=\"text\" id=\"value\" required>\n    <button id=\"enqueue\">Enqueue</button>\n    <button id=\"dequeue\">Dequeue</button>\n    \n    <div id=\"queue\"></div>\n\n    <script>\n        class PriorityQueue {\n            constructor() {\n                this.items = [];\n            }\n\n            enqueue(value, priority) {\n                const queueItem = { value, priority };\n                let added = false;\n                for (let i = 0; i < this.items.length; i++) {\n                    if (queueItem.priority > this.items[i].priority) {\n                        this.items.splice(i, 0, queueItem);\n                        added = true;\n                        break;\n                    }\n                }\n                if (!added) {\n                    this.items.push(queueItem);\n                }\n            }\n\n            dequeue() {\n                return this.items.shift();\n            }\n\n            printQueue() {\n                const queueDiv = document.getElementById('queue');\n                queueDiv.innerHTML = '';\n                this.items.forEach(item => {\n                    const div = document.createElement('div');\n                    div.className = 'queue-item';\n                    div.innerText = `Value: ${item.value}, Priority: ${item.priority}`;\n                    queueDiv.appendChild(div);\n                });\n            }\n        }\n\n        const priorityQueue = new PriorityQueue();\n        \n        document.getElementById('enqueue').addEventListener('click', () => {\n            const value = document.getElementById('value').value;\n            const priority = parseInt(document.getElementById('priority').value);\n            if (value && !isNaN(priority)) {\n                priorityQueue.enqueue(value, priority);\n                priorityQueue.printQueue();\n                document.getElementById('value').value = '';\n                document.getElementById('priority').value = '';\n            } else {\n                alert('Please enter valid value and priority.');\n            }\n        });\n\n        document.getElementById('dequeue').addEventListener('click', () => {\n            const dequeuedItem = priorityQueue.dequeue();\n            if (dequeuedItem) {\n                alert(`Dequeued: Value: ${dequeuedItem.value}, Priority: ${dequeuedItem.priority}`);\n                priorityQueue.printQueue();\n            } else {\n                alert('Queue is empty!');\n            }\n        });\n    </script>\n</body>\n</html>"
