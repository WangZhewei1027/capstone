    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Depth-First Search (DFS) Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            margin: 20px;\n        }\n        #graph {\n            display: flex;\n            justify-content: center;\n            margin-bottom: 20px;\n        }\n        .node {\n            width: 30px;\n            height: 30px;\n            border: 2px solid #333;\n            border-radius: 50%;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            margin: 5px;\n            position: relative;\n            cursor: pointer;\n        }\n        .visited {\n            background-color: lightgreen;\n            border-color: green;\n        }\n    </style>\n</head>\n<body>\n    <h1>Depth-First Search (DFS) Visualization</h1>\n    <div id=\"graph\"></div>\n    <button id=\"start\">Start DFS</button>\n\n    <script>\n        const graph = {\n            A: ['B', 'C'],\n            B: ['D', 'E'],\n            C: ['F'],\n            D: [],\n            E: ['F'],\n            F: []\n        };\n\n        const nodes = Object.keys(graph);\n        const graphDiv = document.getElementById('graph');\n\n        nodes.forEach(node => {\n            const nodeDiv = document.createElement('div');\n            nodeDiv.classList.add('node');\n            nodeDiv.id = node;\n            nodeDiv.innerText = node;\n            graphDiv.appendChild(nodeDiv);\n        });\n\n        function dfs(start, visited = new Set()) {\n            visited.add(start);\n            document.getElementById(start).classList.add('visited');\n            console.log(start);\n\n            graph[start].forEach(neighbor => {\n                if (!visited.has(neighbor)) {\n                    dfs(neighbor, visited);\n                }\n            });\n        }\n\n        document.getElementById('start').addEventListener('click', () => {\n            const visitedNodes = new Set();\n            dfs(nodes[0], visitedNodes); // Start DFS from the first node\n        });\n    </script>\n</body>\n</html>"
