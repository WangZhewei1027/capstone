    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Sliding Window</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        #window {\n            width: 200px;\n            height: 300px;\n            position: relative;\n        }\n       .slide {\n            width: 50%;\n            height: 50%;\n            transition: all 0.5s ease-in-out;\n        }\n        @keyframes slide {\n            0% {\n                transform: translateX(-50%);\n            }\n            50% {\n                transform: translateX(50%);\n            }\n            100% {\n                transform: translateX(-50%);\n            }\n        }\n        @media screen and (max-width: 600px) {\n            #window {\n                display: flex;\n                justify-content: center;\n            }\n           .slide {\n                width: auto;\n            }\n        }\n    </style>\n</head>\n<body>\n    <div id=\"window\"></div>\n    <script>\n        const slides = document.querySelectorAll('.slide');\n        \n        // Function to add a slide to the window\n        function addSlide(slide) {\n            slides.forEach((slide) => slide.classList.add('slide'));\n        }\n\n        // Function to remove a slide from the window\n        function removeSlide(slide) {\n            slides.forEach((slide) => slide.classList.remove('slide'));\n        }\n\n        // Function to rotate the window by 90 degrees\n        function rotateWindow() {\n            var currentSlide = document.getElementById('window')..querySelector('.slide');\n            \n            if (!currentSlide) {\n                return;\n            }\n\n            var angle = Math.PI / 90;\n            \n            var newSlide = currentSlide.style.transform = `translate(${angle * 100}px)` + ` rotate(${angle * 180}deg)`;\n            \n            addSlide(newSlide);\n        }\n\n        // Function to toggle the visibility of a slide\n        function toggleSlide(slide) {\n            var slideId = slide.getAttribute('id');\n            \n            if (!slideId) {\n                return;\n            }\n\n            var slide = slides[slideId];\n            \n            if (slide.classList.contains('active')) {\n                slide.classList.remove('active');\n            } else {\n                slide.classList.add('active');\n            }\n        }\n\n        // Add some slide animations to the window\n        addSlide('Slide 1');\n        addSlide('Slide 2');\n        addSlide('Slide 3');\n\n        // Rotate the window\n        rotateWindow();\n    </script>\n</body>\n</html>"
