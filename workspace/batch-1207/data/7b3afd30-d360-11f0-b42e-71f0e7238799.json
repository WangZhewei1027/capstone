able += `<td><input type=\"number\" min=\"0\" max=\"1\" value=\"0\"></td>`;\n            }\n            table += '</tr>';\n        }\n\n        table += '</table>';\n        matrixContainer.innerHTML = table;\n    }\n\n    function calculateAdjacencyMatrix() {\n        const matrix = [];\n        const inputs = document.querySelectorAll('#matrixContainer input');\n\n        for (let i = 0; i < vertices; i++) {\n            const row = [];\n            for (let j = 0; j < vertices; j++) {\n                const inputIndex = i * vertices + j;\n                const value = parseInt(inputs[inputIndex].value);\n                row.push(value);\n            }\n            matrix.push(row);\n        }\n\n        console.log(matrix);\n        document.getElementById('output').innerText = 'Adjacency Matrix: ' + JSON.stringify(matrix);\n    }\n</script>\n\n</body>\n</html>"
