    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Adjacency List Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n        }\n        #graph {\n            border: 1px solid #ccc;\n            padding: 10px;\n            margin: 20px 0;\n        }\n        .node {\n            margin: 5px 0;\n        }\n    </style>\n</head>\n<body>\n\n<h1>Adjacency List Visualization</h1>\n<p>Enter the edges of the graph (e.g., A,B; A,C; B,D) and click \"Generate Graph\".</p>\n<input type=\"text\" id=\"edgesInput\" placeholder=\"Enter edges (A,B; A,C; B,D)\">\n<button onclick=\"generateGraph()\">Generate Graph</button>\n\n<div id=\"graph\"></div>\n\n<script>\n    function generateGraph() {\n        const input = document.getElementById('edgesInput').value;\n        const edges = input.split(';').map(edge => edge.trim());\n        const adjacencyList = {};\n\n        edges.forEach(edge => {\n            const [node1, node2] = edge.split(',').map(n => n.trim());\n            if (!adjacencyList[node1]) {\n                adjacencyList[node1] = [];\n            }\n            adjacencyList[node1].push(node2);\n            // If undirected, you might also add the reverse edge\n            if (!adjacencyList[node2]) {\n                adjacencyList[node2] = [];\n            }\n            adjacencyList[node2].push(node1); // Comment this line for directed graphs\n        });\n\n        displayGraph(adjacencyList);\n    }\n\n    function displayGraph(adjacencyList) {\n        const graphDiv = document.getElementById('graph');\n        graphDiv.innerHTML = ''; // Clear previous output\n\n        for (const node in adjacencyList) {\n            const nodeDiv = document.createElement('div');\n            nodeDiv.className = 'node';\n            nodeDiv.textContent = `${node}: ${adjacencyList[node].join(', ')}`;\n            graphDiv.appendChild(nodeDiv);\n        }\n    }\n</script>\n\n</body>\n</html>"
