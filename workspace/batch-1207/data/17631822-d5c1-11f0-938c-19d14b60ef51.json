    "content": "<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Weighted Graph Visualization</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n        }\n        #graph {\n            width: 600px;\n            height: 400px;\n            border: 1px solid #ccc;\n            position: relative;\n            margin: 20px auto;\n        }\n        .node {\n            width: 30px;\n            height: 30px;\n            background: lightblue;\n            border-radius: 50%;\n            position: absolute;\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            font-weight: bold;\n        }\n        .edge {\n            position: absolute;\n            border: 2px solid black;\n            width: 0;\n            height: 0;\n            transform-origin: 0 0;\n            font-weight: bold;\n        }\n    </style>\n</head>\n<body>\n\n<h2>Weighted Graph Visualization</h2>\n<div id=\"graph\"></div>\n\n<script>\n    // Define the weighted graph as a set of nodes and edges\n    const graph = {\n        nodes: [\n            { id: 'A', x: 50, y: 100 },\n            { id: 'B', x: 200, y: 50 },\n            { id: 'C', x: 300, y: 150 },\n            { id: 'D', x: 400, y: 100 },\n            { id: 'E', x: 500, y: 200 }\n        ],\n        edges: [\n            { from: 'A', to: 'B', weight: 2 },\n            { from: 'A', to: 'C', weight: 5 },\n            { from: 'B', to: 'D', weight: 3 },\n            { from: 'C', to: 'D', weight: 2 },\n            { from: 'C', to: 'E', weight: 4 },\n            { from: 'D', to: 'E', weight: 1 }\n        ]\n    };\n\n    const graphContainer = document.getElementById('graph');\n\n    // Function to draw nodes\n    function drawNode(node) {\n        const nodeDiv = document.createElement('div');\n        nodeDiv.className = 'node';\n        nodeDiv.style.left = `${node.x}px`;\n        nodeDiv.style.top = `${node.y}px`;\n        nodeDiv.textContent = node.id;\n        graphContainer.appendChild(nodeDiv);\n        return nodeDiv;\n    }\n\n    // Function to draw edges\n    function drawEdge(fromNode, toNode, weight) {\n        const x1 = fromNode.x + 15; // Adjust for node size\n        const y1 = fromNode.y + 15; // Adjust for node size\n        const x2 = toNode.x + 15; // Adjust for node size\n        const y2 = toNode.y + 15; // Adjust for node size\n\n        const edgeDiv = document.createElement('div');\n        edgeDiv.className = 'edge';\n        const length = Math.sqrt((x2 - x1) ** 2 + (y2 - y1) ** 2);\n        const angle = Math.atan2(y2 - y1, x2 - x1) * (180 / Math.PI);\n\n        edgeDiv.style.width = `${length}px`;\n        edgeDiv.style.transform = `translate(${x1}px, ${y1}px) rotate(${angle}deg)`;\n\n        // Adding the weight label\n        const label = document.createElement('span');\n        label.textContent = weight;\n        label.style.position = 'absolute';\n        label.style.left = `${(x1 + x2) / 2}px`;\n        label.style.top = `${(y1 + y2) / 2}px`;\n        label.style.transform = 'translate(-50%, -50%)';\n\n        graphContainer.appendChild(edgeDiv);\n        graphContainer.appendChild(label);\n    }\n\n    // Draw the graph\n    graph.nodes.forEach(drawNode);\n    graph.edges.forEach(edge => {\n        const fromNode = graph.nodes.find(node => node.id === edge.from);\n        const toNode = graph.nodes.find(node => node.id === edge.to);\n        drawEdge(fromNode, toNode, edge.weight);\n    });\n</script>\n\n</body>\n</html>"
