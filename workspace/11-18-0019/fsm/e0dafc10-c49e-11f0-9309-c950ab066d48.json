{
  "topic": "Binary Search Tree (BST) Visualization & Demo",
  "description": "FSM modeling user interactions for inserting, searching, deleting, clearing, and traversals in a BST visualization web app.",
  "states": [
    {
      "name": "empty_tree",
      "onEnter": "render",
      "onExit": "noop",
      "on": {
        "INSERT_CLICKED": "inserting",
        "ENTER_PRESSED": "inserting",
        "SEARCH_CLICKED": "search_no_tree",
        "DELETE_CLICKED": "delete_no_tree",
        "INORDER_CLICKED": "traversal_empty",
        "PREORDER_CLICKED": "traversal_empty",
        "POSTORDER_CLICKED": "traversal_empty",
        "LEVELORDER_CLICKED": "traversal_empty",
        "CLEAR_CLICKED": "empty_tree"
      }
    },
    {
      "name": "tree_ready",
      "onEnter": "render",
      "onExit": "noop",
      "on": {
        "INSERT_CLICKED": "inserting",
        "ENTER_PRESSED": "inserting",
        "SEARCH_CLICKED": "searching",
        "DELETE_CLICKED": "deleting",
        "INORDER_CLICKED": "traversal_inorder",
        "PREORDER_CLICKED": "traversal_preorder",
        "POSTORDER_CLICKED": "traversal_postorder",
        "LEVELORDER_CLICKED": "traversal_levelorder",
        "CLEAR_CLICKED": "clearing"
      }
    },
    {
      "name": "inserting",
      "onEnter": "parseAndInsert",
      "onExit": "render",
      "on": {
        "INSERT_SUCCESS": "tree_ready",
        "INSERT_FAIL_DUPLICATE": "duplicate_error",
        "INSERT_FAIL_INVALID_INPUT": "invalid_input"
      }
    },
    {
      "name": "duplicate_error",
      "onEnter": "showDuplicateMessage",
      "onExit": "noop",
      "on": {
        "INSERT_CLICKED": "inserting",
        "ENTER_PRESSED": "inserting",
        "CLEAR_CLICKED": "clearing"
      }
    },
    {
      "name": "invalid_input",
      "onEnter": "showInvalidInputMessage",
      "onExit": "noop",
      "on": {
        "INSERT_CLICKED": "inserting",
        "SEARCH_CLICKED": "searching",
        "DELETE_CLICKED": "deleting",
        "ENTER_PRESSED": "inserting"
      }
    },
    {
      "name": "search_no_tree",
      "onEnter": "showTreeEmptyMessageForSearch",
      "onExit": "noop",
      "on": {
        "INSERT_CLICKED": "inserting",
        "CLEAR_CLICKED": "empty_tree",
        "ENTER_PRESSED": "inserting"
      }
    },
    {
      "name": "delete_no_tree",
      "onEnter": "showTreeEmptyMessageForDelete",
      "onExit": "noop",
      "on": {
        "INSERT_CLICKED": "inserting",
        "CLEAR_CLICKED": "empty_tree",
        "ENTER_PRESSED": "inserting"
      }
    },
    {
      "name": "searching",
      "onEnter": "startSearchAndHighlight",
      "onExit": "clearHighlights",
      "on": {
        "SEARCH_FOUND": "search_done_found",
        "SEARCH_NOT_FOUND": "search_done_not_found",
        "SEARCH_FAIL_INVALID_INPUT": "invalid_input"
      }
    },
    {
      "name": "search_done_found",
      "onEnter": "showSearchFoundMessage",
      "onExit": "noop",
      "on": {
        "INSERT_CLICKED": "inserting",
        "DELETE_CLICKED": "deleting",
        "INORDER_CLICKED": "traversal_inorder",
        "CLEAR_CLICKED": "clearing"
      }
    },
    {
      "name": "search_done_not_found",
      "onEnter": "showSearchNotFoundMessage",
      "onExit": "noop",
      "on": {
        "INSERT_CLICKED": "inserting",
        "DELETE_CLICKED": "deleting",
        "CLEAR_CLICKED": "clearing"
      }
    },
    {
      "name": "deleting",
      "onEnter": "performDelete",
      "onExit": "render",
      "on": {
        "DELETE_SUCCESS": "tree_ready",
        "DELETE_SUCCESS_TREE_EMPTY": "empty_tree",
        "DELETE_FAIL_NOT_FOUND": "delete_not_found",
        "DELETE_FAIL_INVALID_INPUT": "invalid_input"
      }
    },
    {
      "name": "delete_not_found",
      "onEnter": "showDeleteNotFoundMessage",
      "onExit": "noop",
      "on": {
        "INSERT_CLICKED": "inserting",
        "DELETE_CLICKED": "deleting",
        "CLEAR_CLICKED": "clearing"
      }
    },
    {
      "name": "clearing",
      "onEnter": "clearTree",
      "onExit": "render",
      "on": {
        "CLEAR_CLICKED": "empty_tree",
        "INSERT_CLICKED": "inserting"
      }
    },
    {
      "name": "traversal_empty",
      "onEnter": "showTraversalEmptyMessage",
      "onExit": "noop",
      "on": {
        "INSERT_CLICKED": "inserting",
        "CLEAR_CLICKED": "empty_tree"
      }
    },
    {
      "name": "traversal_inorder",
      "onEnter": "showInOrderTraversal",
      "onExit": "noop",
      "on": {
        "INORDER_CLICKED": "traversal_inorder",
        "INSERT_CLICKED": "inserting",
        "CLEAR_CLICKED": "clearing"
      }
    },
    {
      "name": "traversal_preorder",
      "onEnter": "showPreOrderTraversal",
      "onExit": "noop",
      "on": {
        "PREORDER_CLICKED": "traversal_preorder",
        "INSERT_CLICKED": "inserting",
        "CLEAR_CLICKED": "clearing"
      }
    },
    {
      "name": "traversal_postorder",
      "onEnter": "showPostOrderTraversal",
      "onExit": "noop",
      "on": {
        "POSTORDER_CLICKED": "traversal_postorder",
        "INSERT_CLICKED": "inserting",
        "CLEAR_CLICKED": "clearing"
      }
    },
    {
      "name": "traversal_levelorder",
      "onEnter": "showLevelOrderTraversal",
      "onExit": "noop",
      "on": {
        "LEVELORDER_CLICKED": "traversal_levelorder",
        "INSERT_CLICKED": "inserting",
        "CLEAR_CLICKED": "clearing"
      }
    }
  ],
  "events": [
    "INSERT_CLICKED",
    "INSERT_SUCCESS",
    "INSERT_FAIL_DUPLICATE",
    "INSERT_FAIL_INVALID_INPUT",
    "SEARCH_CLICKED",
    "SEARCH_FOUND",
    "SEARCH_NOT_FOUND",
    "SEARCH_FAIL_INVALID_INPUT",
    "DELETE_CLICKED",
    "DELETE_SUCCESS",
    "DELETE_SUCCESS_TREE_EMPTY",
    "DELETE_FAIL_NOT_FOUND",
    "DELETE_FAIL_INVALID_INPUT",
    "CLEAR_CLICKED",
    "INORDER_CLICKED",
    "PREORDER_CLICKED",
    "POSTORDER_CLICKED",
    "LEVELORDER_CLICKED",
    "ENTER_PRESSED"
  ],
  "triggers": {
    "INSERT_CLICKED": "#insertBtn",
    "SEARCH_CLICKED": "#searchBtn",
    "DELETE_CLICKED": "#deleteBtn",
    "CLEAR_CLICKED": "#clearBtn",
    "INORDER_CLICKED": "#inOrderBtn",
    "PREORDER_CLICKED": "#preOrderBtn",
    "POSTORDER_CLICKED": "#postOrderBtn",
    "LEVELORDER_CLICKED": "#levelOrderBtn",
    "ENTER_PRESSED": "#inputValue",
    "INSERT_SUCCESS": "#insertBtn, #inputValue",
    "INSERT_FAIL_DUPLICATE": "#insertBtn, #inputValue",
    "INSERT_FAIL_INVALID_INPUT": "#insertBtn, #inputValue",
    "SEARCH_FOUND": "#searchBtn, #inputValue",
    "SEARCH_NOT_FOUND": "#searchBtn, #inputValue",
    "SEARCH_FAIL_INVALID_INPUT": "#searchBtn, #inputValue",
    "DELETE_SUCCESS": "#deleteBtn, #inputValue",
    "DELETE_SUCCESS_TREE_EMPTY": "#deleteBtn, #inputValue",
    "DELETE_FAIL_NOT_FOUND": "#deleteBtn, #inputValue",
    "DELETE_FAIL_INVALID_INPUT": "#deleteBtn, #inputValue"
  },
  "notes": "Selectors extracted from the HTML: IDs: #inputValue, #insertBtn, #searchBtn, #deleteBtn, #clearBtn, #inOrderBtn, #preOrderBtn, #postOrderBtn, #levelOrderBtn, #traversalResult, #output, #tree. Classes used in DOM for visualization: .node, .circle, .children, .connector.left, .connector.right, .highlight. Triggers map user actions (clicks and Enter key) to FSM events; internal result events are mapped to the initiating DOM selectors."
}